%o A064771 (Python)
%o A064771 from sympy import divisors
%o A064771 def isok(n):
%o A064771     dp = {0: 1}
%o A064771     for d in divisors(n)[:-1]:
%o A064771         u = {}
%o A064771         for k in dp.keys():
%o A064771             if (s := (d + k)) <= n:
%o A064771                 u[s] = dp.get(s, 0) + dp[k]
%o A064771                 if s == n and u[s] > 1:
%o A064771                     return False
%o A064771         for k,v in u.items():
%o A064771             dp[k] = v
%o A064771     return dp.get(n, 0) == 1
%o A064771 print([n for n in range(1, 1039) if isok(n)]) # _DarÃ­o Clavijo_, Sep 17 2024
%o A064771 (SageMath)
%o A064771 def isA064771(n: int) -> bool: return A033630(n) == 2
%o A064771 print([n for n in range(1039) if isA064771(n)])  # _Peter Luschny_, Nov 24 2025
