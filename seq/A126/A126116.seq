%p A126116 A010684 := proc(n) 1+2*(n mod 2) ; end proc:
%p A126116 A000032 := proc(n) coeftayl((2-x)/(1-x-x^2),x=0,n) ; end proc:
%p A126116 A126116 := proc(n) ((-1)^floor(n/2)*A010684(n)+2*A000032(n))/5 ; end proc: seq(A126116(n),n=0..80) ; # (End)
