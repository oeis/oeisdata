%o A075090 (Python)
%o A075090 from sympy import mobius, integer_nthroot
%o A075090 from oeis_sequences.OEISsequences import bisection
%o A075090 def A075090(n):
%o A075090     def f(x): return int(n+x+sum(mobius(k)*(integer_nthroot(x,k)[0]>>1) for k in range(2,x.bit_length())))
%o A075090     return bisection(f,n,n) # _Chai Wah Wu_, Sep 15 2025
