%N A195136 a(n) = ((n+1)^(n-1) + (n-1)^(n-1))/2 for n>=1.
%F A195136 E.g.f.: sinh(x*W(x)) = (W(x) - 1/W(x))/2 where W(x) = LambertW(-x)/(-x) = exp(x*W(x)) = Sum_{n>=0} (n+1)^(n-1)*x^n/n!.
%F A195136 a(n) = Sum_{k=0..floor((n-1)/2)} C(n-1,2*k) * n^(n-2*k-1).
%F A195136 a(n) ~ (exp(2) + exp(2/n)) * n^(n-1) / (2 * exp(1 + 3/(2*n))). - _Amiram Eldar_, Dec 29 2025
%e A195136 E.g.f.: A(x) = x + 2*x^2/2! + 10*x^3/3! + 76*x^4/4! + 776*x^5/5! + 9966*x^6/6! + 154400*x^7/7! + 2803256*x^8/8! + 58388608*x^9/9! + 1372684090*x^10/10! + ...
%e A195136 such that A(x) = sinh(x*W(x))
%e A195136 where W(x) = LambertW(-x)/(-x) begins
%e A195136 W(x) = 1 + x + 3*x^2/2! + 16*x^3/3! + 125*x^4/4! + 1296*x^5/5! + 16807*x^6/6! + 262144*x^7/7! + 4782969*x^8/8! + 100000000*x^9/9! + ... + (n+1)^(n-1)*x^n/n! + ...
%e A195136 and satisfies W(x) = exp(x*W(x)).
%e A195136 Also, A(x) = (W(x) - 1/W(x))/2 where
%e A195136 1/W(x) = 1 - x - x^2/2! - 4*x^3/3! - 27*x^4/4! - 256*x^5/5! - 3125*x^6/6! - 46656*x^7/7! - 823543*x^8/8! + ... + -(n-1)^(n-1)*x^n/n! + ...
%t A195136 Join[{1},Table[((n+1)^(n-1)+(n-1)^(n-1))/2,{n,2,30}]] (* _Harvey P. Dale_, Feb 06 2023 *)
%o A195136 (PARI) {a(n)=((n+1)^(n-1) + (n-1)^(n-1))/2}
%o A195136 for(n=1,30,print1(a(n),", "))
%o A195136 (PARI) {a(n)=sum(k=0,(n-1)\2,binomial(n-1,2*k)*n^(n-2*k-1))}
%o A195136 for(n=1,30,print1(a(n),", "))
%o A195136 (PARI) {a(n)=local(W=sum(m=0,n,(m+1)^(m-1)*x^m/m!)+x*O(x^n));n!*polcoeff(sinh(x*W),n)}
%o A195136 for(n=1,30,print1(a(n),", "))
