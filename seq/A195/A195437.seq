%o A195437 a195437_tabl = tail $ g 1 1 [0..] where
%o A195437    g m j xs = (filter ((== m) . (`mod` 2)) ys) : g (1 - m) (j + 2) xs'
%o A195437      where (ys,xs') = splitAt j xs
