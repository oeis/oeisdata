%e A337457 n=2: [1, 2, 0, -16, -320, -21888, -2648576, -494325760, ...];
%e A337457 n=3: [1, 6, 24, 0, -1728, -88704, -9621504, -1715198976, ...];
%e A337457 n=4: [1, 12, 120, 864, 0, -281088, -26873856, -4328017920, ...];
%e A337457 n=5: [1, 20, 360, 5600, 65920, 0, -66944000, -10207436800, ...];
