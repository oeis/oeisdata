%o A092782 (PARI)
%o A092782 strsub(s, vv, off=0)=
%o A092782 {
%o A092782     my( nl=#vv, r=[], ct=1 );
%o A092782     while ( ct <= #s,
%o A092782         r = concat(r, vv[ s[ct] + (1-off) ] );
%o A092782         ct += 1;
%o A092782     );
%o A092782     return( r );
%o A092782 }
