%F A092973 a(n, k) = !n!_k = Prod_{i=0, 1, 2, .., floor(2n/k)}_{0<|n-i*k|<=n} (n-i*k) = n(n-k)(n-2k)(n-3k)... . k=8.
%p A092973 T:=proc(n,k) local i,p;
%p A092973 p:=1;
%p A092973 for i from 0 to floor(2*n/k) do
%p A092973 if n-k*i <> 0 then p:=p*(n-k*i) fi; od:
%p A092973 p;
%p A092973 end;
