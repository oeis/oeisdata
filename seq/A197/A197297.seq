%o A197297 (Perl) use ntheory ":all"; my($max,$f)=(0); forprimes { $f=abs(chebyshev_theta($_)-$_); if ($f > $max) { say; $max=$f; } } 10000; # _Dana Jacobsen_, Dec 29 2015
