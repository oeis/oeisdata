%o A197945 (Haskell)
%o A197945 import Data.List (inits, isPrefixOf)
%o A197945 a197945 n = length $ takeWhile (`isPrefixOf` (show $ a096095 $ n+1)) $
%o A197945                                tail $ inits $ show $ a096095 n
%o A197945 (Python)
%o A197945 from itertools import islice
%o A197945 from os.path import commonprefix
%o A197945 def dws(s, t): # returns string s ++ t where s and t are digits strings, len(s) <= len(t)
%o A197945     return "".join(str(int(c)+int(d)) for c, d in zip(s.zfill(len(t)), t))
%o A197945 def agen(): # generator of terms
%o A197945     s, t = "1", "1"
%o A197945     while True:
%o A197945         yield len(commonprefix([s, t]))
%o A197945         s, t = t, dws(s, t)
%o A197945 print(list(islice(agen(), 59))) # _Michael S. Branicky_, Nov 08 2025
