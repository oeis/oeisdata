%t A217605 winv[n_]:=Times@@Cases[FactorInteger[n],{p_,k_}:>Prime[k]^PrimePi[p]];
%t A217605 Table[Length[Select[IntegerPartitions[n],winv[Times@@Prime/@#]==Times@@Prime/@#&]],{n,0,30}] (* _Gus Wiseman_, May 04 2019 *)
