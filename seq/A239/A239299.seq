%t A239299 b[n_, s_, u_, t_] := b[n, s, u, t] = If[n == 0, 1,
%t A239299     Sum[b[n - 1, Min[s, i], Min[u, If[s < i, i, u]],
%t A239299     Min[t, If[u < i, i + 1, t]]], {i, 1, t - 1}]];
%t A239299 a[n_] := b[n, n+1, n+1, n+1];
