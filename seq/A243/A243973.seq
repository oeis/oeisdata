%o A243973             count += 1
%o A243973         if count == n:
%o A243973           print(a,end=', ')
%o A243973           n += 1
%o A243973           k = 0
%o A243973           break
%o A243973       k += 1
%o A243973     else:
%o A243973       k += 1
