%e A147790 a(2) = round(3*3/2) = 4;
%e A147790 a(3) = round(4*3/2) = 6;
%e A147790 a(4) = round(6*3/2) = 9.
%t A147790 lst={};s=2;Do[s=Round[s*1.5];AppendTo[lst,s],{n,1,5!}];lst
