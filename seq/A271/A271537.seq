%I A271537 #11 Nov 05 2025 15:22:32
%S A271537 1,6,15,47,68,164,205,374,418,731,831,1240,1320,1977,2105,2938,3090,
%T A271537 4147,4435,5644,5864,7601,7765,9694,10135,12331,12804,15436,15833,
%U A271537 18937,19510,22950,23567,27519,28368,32628,33381,38381,39198,44722,45707,51735,52868
%N A271537 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 379", based on the 5-celled von Neumann neighborhood.
%C A271537 Initialized with a single black (ON) cell at stage zero.
%D A271537 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271537 Robert Price, <a href="/A271537/b271537.txt">Table of n, a(n) for n = 0..128</a>
%H A271537 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271537 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271537 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271537 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271537 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271537 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271537 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271537 code=379; stages=128;
%t A271537 rule=IntegerDigits[code,2,10];
%t A271537 g=2*stages+1; (* Maximum size of grid *)
%t A271537 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271537 ca=a;
%t A271537 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271537 PrependTo[ca,a];
%t A271537 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271537 k=(Length[ca[[1]]]+1)/2;
%t A271537 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271537 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271537 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
