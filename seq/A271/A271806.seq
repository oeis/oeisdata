%S A271806 3,1,27,-27,91,-79,147,-159,331,-311,419,-427,627,-583,683,-739,1195,
%T A271806 -1175,1411,-1411,1699,-1639,1819,-1887,2499,-2431,2651,-2675,3115,
%U A271806 -2959,2979,-3191,4459,-4439,4931,-4931,5475,-5415,5851,-5911,6739,-6639,7019
%N A271806 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 401", based on the 5-celled von Neumann neighborhood.
%C A271806 Initialized with a single black (ON) cell at stage zero.
%D A271806 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271806 Robert Price, <a href="/A271806/b271806.txt">Table of n, a(n) for n = 0..127</a>
%H A271806 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271806 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271806 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271806 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271806 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271806 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271806 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271806 code=401; stages=128;
%t A271806 rule=IntegerDigits[code,2,10];
%t A271806 g=2*stages+1; (* Maximum size of grid *)
%t A271806 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271806 ca=a;
%t A271806 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271806 PrependTo[ca,a];
%t A271806 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271806 k=(Length[ca[[1]]]+1)/2;
%t A271806 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271806 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271806 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A271806 Cf. A271803.
