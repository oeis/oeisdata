%I A271008 #12 Nov 05 2025 15:22:32
%S A271008 1,5,8,24,28,48,36,108,68,132,136,224,172,276,276,344,308,496,408,548,
%T A271008 560,652,604,860,688,1032,824,1144,972,1256,1224,1432,1356,1696,1392,
%U A271008 1836,1716,2016,1828,2144,2152,2496,2080,2632,2556,2948,2708,3248,3088
%N A271008 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 246", based on the 5-celled von Neumann neighborhood.
%C A271008 Initialized with a single black (ON) cell at stage zero.
%D A271008 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271008 Robert Price, <a href="/A271008/b271008.txt">Table of n, a(n) for n = 0..128</a>
%H A271008 Robert Price, <a href="/A271008/a271008.tmp.txt">Diagrams of the first 20 stages.</a>
%H A271008 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271008 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271008 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271008 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271008 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271008 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271008 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271008 code=246; stages=128;
%t A271008 rule=IntegerDigits[code,2,10];
%t A271008 g=2*stages+1; (* Maximum size of grid *)
%t A271008 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271008 ca=a;
%t A271008 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271008 PrependTo[ca,a];
%t A271008 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271008 k=(Length[ca[[1]]]+1)/2;
%t A271008 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271008 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A271008 nonn,easy
%O A271008 0,2
