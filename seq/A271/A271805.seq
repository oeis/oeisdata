%S A271805 1,5,10,42,47,143,160,324,329,665,690,1134,1151,1795,1856,2600,2605,
%T A271805 3805,3830,5266,5291,7015,7100,9004,9021,11537,11622,14358,14419,
%U A271805 17595,17812,21008,21013,25477,25502,30458,30483,35983,36068,42004,42029,48793,48918
%N A271805 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 401", based on the 5-celled von Neumann neighborhood.
%C A271805 Initialized with a single black (ON) cell at stage zero.
%D A271805 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271805 Robert Price, <a href="/A271805/b271805.txt">Table of n, a(n) for n = 0..128</a>
%H A271805 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271805 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271805 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271805 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271805 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271805 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271805 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271805 code=401; stages=128;
%t A271805 rule=IntegerDigits[code,2,10];
%t A271805 g=2*stages+1; (* Maximum size of grid *)
%t A271805 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271805 ca=a;
%t A271805 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271805 PrependTo[ca,a];
%t A271805 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271805 k=(Length[ca[[1]]]+1)/2;
%t A271805 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271805 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271805 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
