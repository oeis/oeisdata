%I A271694 #12 Nov 05 2025 15:22:33
%S A271694 1,5,21,76,296,1004,3984,15556,60308,241016,959044,3834608,15311436,
%T A271694 61288052,245072724,980065360
%N A271694 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 398", based on the 5-celled von Neumann neighborhood.
%C A271694 Initialized with a single black (ON) cell at stage zero.
%D A271694 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271694 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271694 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271694 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271694 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271694 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271694 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271694 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271694 code=398; stages=128;
%t A271694 rule=IntegerDigits[code,2,10];
%t A271694 g=2*stages+1; (* Maximum size of grid *)
%t A271694 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271694 ca=a;
%t A271694 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271694 PrependTo[ca,a];
%t A271694 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271694 k=(Length[ca[[1]]]+1)/2;
%t A271694 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271694 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271694 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A271694 Cf. A271693.
%K A271694 nonn,more
%O A271694 0,2
