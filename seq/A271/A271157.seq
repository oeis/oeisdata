%I A271157 #12 Nov 05 2025 15:22:32
%S A271157 1,5,21,85,320,1168,4360,16428,62348,238972,924028,3597324,14093900,
%T A271157 55467612,219171356,868441132
%N A271157 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 302", based on the 5-celled von Neumann neighborhood.
%C A271157 Initialized with a single black (ON) cell at stage zero.
%D A271157 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271157 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271157 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271157 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271157 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271157 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271157 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271157 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271157 code=302; stages=128;
%t A271157 rule=IntegerDigits[code,2,10];
%t A271157 g=2*stages+1; (* Maximum size of grid *)
%t A271157 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271157 ca=a;
%t A271157 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271157 PrependTo[ca,a];
%t A271157 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271157 k=(Length[ca[[1]]]+1)/2;
%t A271157 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271157 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271157 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A271157 Cf. A271156.
%K A271157 nonn,more
%O A271157 0,2
