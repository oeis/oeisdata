%I A271161 #12 Nov 05 2025 15:22:32
%S A271161 1,4,32,200,860,3448,14048,57108,227420,904432,3618540,14438616,
%T A271161 57738456,230833880,923143832,3692116492
%N A271161 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 305", based on the 5-celled von Neumann neighborhood.
%C A271161 Initialized with a single black (ON) cell at stage zero.
%D A271161 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271161 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271161 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271161 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271161 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271161 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271161 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271161 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271161 code=305; stages=128;
%t A271161 rule=IntegerDigits[code,2,10];
%t A271161 g=2*stages+1; (* Maximum size of grid *)
%t A271161 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271161 ca=a;
%t A271161 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271161 PrependTo[ca,a];
%t A271161 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271161 k=(Length[ca[[1]]]+1)/2;
%t A271161 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271161 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271161 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A271161 Cf. A271160.
%K A271161 nonn,more
%O A271161 0,2
