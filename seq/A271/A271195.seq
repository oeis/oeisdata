%S A271195 1,5,8,20,28,40,45,81,93,120,116,192,137,249,188,292,340,341,436,448,
%T A271195 500,521,548,709,660,757,725,900,932,976,965,1273,1080,1356,1312,1512,
%U A271195 1344,1692,1529,1760,1840,1973,1948,2165,2237,2237,2336,2524,2744,2692
%N A271195 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 310", based on the 5-celled von Neumann neighborhood.
%C A271195 Initialized with a single black (ON) cell at stage zero.
%D A271195 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271195 Robert Price, <a href="/A271195/b271195.txt">Table of n, a(n) for n = 0..128</a>
%H A271195 Robert Price, <a href="/A271195/a271195.tmp.txt">Diagrams of the first 20 stages.</a>
%H A271195 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271195 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271195 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271195 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271195 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271195 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271195 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271195 code=310; stages=128;
%t A271195 rule=IntegerDigits[code,2,10];
%t A271195 g=2*stages+1; (* Maximum size of grid *)
%t A271195 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271195 ca=a;
%t A271195 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271195 PrependTo[ca,a];
%t A271195 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271195 k=(Length[ca[[1]]]+1)/2;
%t A271195 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271195 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A271195 nonn,easy
%O A271195 0,2
