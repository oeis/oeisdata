%U A271053 3463,-3463,3951,-3951,4471,-4471,5023,-5023,5607,-5607,6223,-6223,6871,-6871,7551
%N A271053 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 253", based on the 5-celled von Neumann neighborhood.
%C A271053 Initialized with a single black (ON) cell at stage zero.
%D A271053 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271053 Robert Price, <a href="/A271053/b271053.txt">Table of n, a(n) for n = 0..127</a>
%H A271053 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015.
%H A271053 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271053 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271053 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271053 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271053 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%F A271053 Conjectures from _Colin Barker_, Nov 22 2017: (Start)
%F A271053 G.f.: (7 + 3*x + 22*x^2 + 3*x^3 + 7*x^4 - 15*x^5 - 4*x^6 + 9*x^7) / ((1 - x)^2*(1 + x)^3).
%F A271053 a(n) = 4*n^2 + 12*n - 9 for n>2 and even.
%F A271053 a(n) = -4*n^2 - 4*n + 17 for n>2 and odd.
%F A271053 a(n) = -a(n-1) + 2*a(n-2) + 2*a(n-3) - a(n-4) - a(n-5) for n>7.
%F A271053 (End)
%t A271053 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271053 code=253; stages=128;
%t A271053 rule=IntegerDigits[code,2,10];
%t A271053 g=2*stages+1; (* Maximum size of grid *)
%t A271053 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271053 ca=a;
%t A271053 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271053 PrependTo[ca,a];
%t A271053 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271053 k=(Length[ca[[1]]]+1)/2;
%t A271053 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271053 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271053 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A271053 Cf. A271051.
