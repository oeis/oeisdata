%S A271541 1,6,18,39,76,116,192,257,381,493,678,830,1067,1284,1640,1885,2294,
%T A271541 2694,3191,3584,4184,4804,5468,6069,6870,7742,8671,9400,10448,11573,
%U A271541 12794,13854,15179,16680,18156,19469,21113,23038,24775,26467,28568,30833,32925
%N A271541 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 382", based on the 5-celled von Neumann neighborhood.
%C A271541 Initialized with a single black (ON) cell at stage zero.
%D A271541 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271541 Robert Price, <a href="/A271541/b271541.txt">Table of n, a(n) for n = 0..128</a>
%H A271541 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271541 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271541 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271541 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271541 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271541 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271541 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271541 code=382; stages=128;
%t A271541 rule=IntegerDigits[code,2,10];
%t A271541 g=2*stages+1; (* Maximum size of grid *)
%t A271541 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271541 ca=a;
%t A271541 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271541 PrependTo[ca,a];
%t A271541 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271541 k=(Length[ca[[1]]]+1)/2;
%t A271541 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271541 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271541 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
%Y A271541 Cf. A271539.
%K A271541 nonn,easy
