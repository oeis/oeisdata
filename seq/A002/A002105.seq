%o A002105 (PARI) {a(n)=local(CF=1+x*O(x^n));if(n<1,return(0), for(k=1,n,CF=1/(1-(n-k+1)*(n-k+2)/2*x*CF));return(Vec(CF)[n]))}  /* _Paul D. Hanna_ */
