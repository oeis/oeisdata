%p A002573 v := proc(c,d) option remember; local i; if d < 0 or c < 0 then 0 elif d = c then 1 else add(v(i,d-c),i=1..2*c); fi; end; [ seq(v(2,n), n=1..50) ];
%t A002573 v[c_, d_] :=  v[c, d] = If[d < 0 || c < 0, 0, If[d == c, 1, Sum[v[i, d - c], {i, 1, 2*c}]]]; a[n_] := v[2, n]; Table[a[n], {n, 1, 35}] (* _Jean-FranÃ§ois Alcover_, Jan 30 2012, after Maple *)
