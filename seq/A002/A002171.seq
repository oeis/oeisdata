%o A002171 (PARI) {a(n) = my(A, p, e, x, y, a0, a1); if( n<0, 0, A = factor( 4*n + 1); prod( k=1, matsize(A)[1], [p, e] = A[k, ]; if( p==2, 0, p%4==3, (-p)^(e/2) * (1 + (-1)^e) / 2, forstep( i=1, sqrtint(p), 2, if( issquare( p - i^2, &y), x=i; break)); a0 = 1; y = a1 = x * (-1)^((x + y)\2) * if(y, 2, 1); for(i=2, e, x = y * a1 - p * a0; a0=a1; a1=x); a1 )))}; /* _Michael Somos_, Jun 18 2012 */
