%t A002890 (* For 25 terms, a PC computation lasts less than half an hour *) m = 48 (* max y exponent *); coes = CoefficientList[ Series[ Log[(1 + y^2)^2 - 2*y*(1 - y^2)*(Cos[2*Pi*u] + Cos[2*Pi*v])], {y, 0, m}], y] // Rest; nint[f_, {n_}] := If[n == 2 || OddQ[n], 0, Print[n]; Integrate[ Integrate[f, {u, 0, 1}], {v, 0, 1}]]; fy = MapIndexed[nint, coes].Table[y^k, {k, 1, m}]; CoefficientList[ Series[ Exp[fy/2], {y, 0, m}] , y^2] (* _Jean-FranÃ§ois Alcover_, Mar 19 2013 *)
