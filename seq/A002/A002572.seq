%p A002572 v := proc(c,d) option remember; local i; if d < 0 or c < 0 then 0 elif d = c then 1 else add(v(i,d-c),i=1..2*c); fi; end; [ seq(v(1,n), n=1..50) ];
%t A002572 v[c_, d_] := v[c, d] = If[d < 0 || c < 0, 0, If[d == c, 1, Sum[v[i, d - c], {i, 1, 2*c}]]]; a[n_] := v[1, n-1]; a[1] = 1; Table[a[n], {n, 1, 36}] (* _Jean-FranÃ§ois Alcover_, Oct 19 2011, after Maple *)
%o A002572 (PARI) v(c,d) = if ( d<0 || c<0, 0, if ( d==c, 1, sum(i=1,2*c, v(i,d-c) ) ) )
%o A002572 (PARI)
%o A002572 /* g.f. as given in the Elsholtz/Heuberger/Prodinger reference */
%o A002572 N=66;  q='q+O('q^N);
%o A002572 t=2;  /* t-ary: t=2 for A002572, t=3 for A176485, t=4 for A176503  */
%o A002572 L=2 + 2*ceil( log(N) / log(t) );
%o A002572 f(k)=(1-t^k)/(1-t);
%o A002572 la(j)=prod(i=1, j, q^f(i) / ( 1 - q^f(i) ) );
%o A002572 nm=sum(j=0, L, (-1)^j * q^f(j) * la(j) );
%o A002572 dn=sum(j=0, L, (-1)^j * la(j) );
%o A002572 gf=nm / dn;
%o A002572 Vec( gf )
%o A002572 /* _Joerg Arndt_, Dec 27 2012 */
%o A002572 (PARI) {a(n, k=2) = if( n<2 && k==2, n>=0, n<k || k<1, 0, n==k, 1, sum(i=2, min(n-k+1, 2*k-1), a(n-k+1, i)))}; /* _Michael Somos_, Dec 20 2016 */
