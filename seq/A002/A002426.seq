%o A002426 def A():
%o A002426     a, b, n = 1, 1, 1
%o A002426     yield a
%o A002426     while True:
%o A002426         yield b
%o A002426         n += 1
%o A002426         a, b = b, ((3 * (n - 1)) * a + (2 * n - 1) * b) // n
%o A002426 A002426 = A()
