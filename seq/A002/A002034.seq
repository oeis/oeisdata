%o A002034 (Python)
%o A002034 from sympy import factorial
%o A002034 def a(n):
%o A002034     m=1
%o A002034     while True:
%o A002034         if factorial(m)%n==0: return m
%o A002034         else: m+=1
%o A002034 [a(n) for n in range(1, 101)] # _Indranil Ghosh_, Apr 24 2017
%o A002034 (Python)
%o A002034 from sympy import factorint
%o A002034 def valp(n, p):
%o A002034     s = 0
%o A002034     while n: n //= p; s += n
%o A002034     return s
%o A002034 def K(p, e):
%o A002034     if e <= p: return e*p
%o A002034     t = e*(p-1)//p*p
%o A002034     while valp(t, p) < e: t += p
%o A002034     return t
%o A002034 def A002034(n):
%o A002034     return 1 if n == 1 else max(K(p, e) for p, e in factorint(n).items())
%o A002034 print([A002034(n) for n in range(1, 85)]) # _Michael S. Branicky_, Jun 09 2022 after _Charles R Greathouse IV_
