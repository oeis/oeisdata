%o A002321 (Python)
%o A002321 from functools import lru_cache
%o A002321 @lru_cache(maxsize=None)
%o A002321 def A002321(n):
%o A002321     if n == 0:
%o A002321         return 0
%o A002321     c, j = n, 2
%o A002321     k1 = n//j
%o A002321     while k1 > 1:
%o A002321         j2 = n//k1 + 1
%o A002321         c += (j2-j)*A002321(k1)
%o A002321         j, k1 = j2, n//j2
%o A002321     return j-c # _Chai Wah Wu_, Mar 30 2021
