%H A061871 Robert Israel, <a href="/A061871/b061871.txt">Table of n, a(n) for n = 1..10000</a>
%p A061871 filter:= proc(n) local L,i;
%p A061871   L:= convert(n,base,10);
%p A061871   abs(add((-1)^i*L[i],i=1..nops(L)))=2
%p A061871 end proc:
%p A061871 select(filter, [$1..1000]); # _Robert Israel_, Dec 23 2025
%t A061871 Do[ a = IntegerDigits[ n ]; l = Length[ a ]; e = o = {}; Do[ o = Append[ o, a[ [ 2k - 1 ] ] ], {k, 1, l/2 + .5} ]; Do[ e = Append[ e, a[ [ 2k ] ] ], {k, 1, l/2} ]; If[ Abs[ Apply[ Plus, o ] - Apply[ Plus, e ] ] == 2, Print[ n ] ], {n, 1, 1000} ]
%o A061871 (Python)
%o A061871 def ok(n): return abs(sum(di*(-1)**i for i, di in enumerate(map(int, str(n))))) == 2
%o A061871 print([k for k in range(365) if ok(k)]) # _Michael S. Branicky_, Dec 23 2025
