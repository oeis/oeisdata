%o A061394 (PARI) isA025487(n)=my(k=valuation(n, 2), t); n>>=k; forprime(p=3, default(primelimit), t=valuation(n, p); if(t>k, return(0), k=t); if(k, n/=p^k, return(n==1)))
%o A061394 [omega(n) | n <- [1..1000], isA025487(n)]
