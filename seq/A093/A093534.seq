%t A093534 l = {0}; s = 0; Do[s = s + n^2; If[ Mod[s, 10^Floor[ Log[10, n] + 1]] == n, AppendTo[l, n]], {n, 10^6}]; l (* _Robert G. Wilson v_, May 24 2004 *)
