%t A057175 lst={};Do[p=(9^n+1)/10;If[PrimeQ[p], AppendTo[lst, n]], {n, 7!}];lst (* _Vladimir Joseph Stephan Orlovsky_, Sep 29 2008 *)
%t A057175 Select[Range[4000], PrimeQ[(9^# + 1)/10] &] (* _Vincenzo Librandi_, Aug 03 2015 *)
