%t A253671 max = 500; ee = Table[2^n*EulerE[n, 1] + EulerE[n] - 1, {n, 0, max}]; A000111 = Table[Differences[ee, n] // First // Abs, {n, 0, max}]; Table[Quotient[A000111[[n + 1]], A000111[[n]]], {n, 1, max}] (* _Jean-François Alcover_, Jan 08 2015 *)
