%p A253830 G := product(1+(x*z)^j/(1-z), j = 1 .. 12): Gser := simplify(series(G, z = 0, 14)): for n to 12 do P[n] := coeff(Gser, z^n) end do: for n to 12 do seq(coeff(P[n], x^j), j = 1 .. n) end do;
