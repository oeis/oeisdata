%o A006751 (Python)
%o A006751 from functools import cache
%o A006751 from itertools import groupby
%o A006751 def LS(n): return int(''.join(str(len(list(g)))+k for k, g in groupby(str(n))))
%o A006751 @cache
%o A006751 def a(n): return 2 if n == 1 else LS(a(n-1))
%o A006751 print([a(n) for n in range(1, 14)]) # _Michael S. Branicky_, Jan 10 2026
