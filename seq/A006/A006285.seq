%o A006285 (Python)
%o A006285 from itertools import count, islice
%o A006285 from sympy import isprime
%o A006285 def A006285_gen(startvalue=1): # generator of terms
%o A006285     return filter(lambda n: not any(isprime(n-(1<<i)) for i in range(n.bit_length()-1,-1,-1)), count(max(startvalue+(startvalue&1^1),1),2))
%o A006285 A006285_list = list(islice(A006285_gen(),30)) # _Chai Wah Wu_, Nov 29 2023
