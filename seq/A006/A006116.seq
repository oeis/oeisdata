%e A006116 etc.
%p A006116 gf:= m-> add(x^n/mul(1-2^k*x, k=0..n), n=0..m):
%p A006116 a:= n-> coeff(series(gf(n), x, n+1), x, n):
%p A006116 seq(a(n), n=0..20);  # _Alois P. Heinz_, Apr 24 2012
%p A006116 # second Maple program:
%p A006116 b:= proc(n, m) option remember; `if`(n=0, 1,
%p A006116       2^m*b(n-1, m)+b(n-1, m+1))
%p A006116     end:
%p A006116 a:= n-> b(n, 0):
%p A006116 seq(a(n), n=0..25);  # _Alois P. Heinz_, Aug 08 2021
%t A006116 faq[n_, q_] = Product[(1-q^(1+k))/(1-q), {k, 0, n-1}]; qbin[n_, m_, q_] = faq[n, q]/(faq[m, q]*faq[n-m, q]); a[n_] := Sum[qbin[n, k, 2], {k, 0, n}]; a /@ Range[0, 19] (* _Jean-François Alcover_, Jul 21 2011 *)
%t A006116 Flatten[{1, RecurrenceTable[{a[n]==2*a[n-1]+(2^(n-1)-1)*a[n-2], a[0]==1, a[1]==2}, a, {n,1,15}]}] (* _Vaclav Kotesovec_, Aug 21 2013 *)
%t A006116 QP = QPochhammer; a[n_] := Sum[QP[2, 2, n]/(QP[2, 2, k]*QP[2, 2, n-k]), {k, 0, n}]; Table[a[n], {n, 0, 20}] (* _Jean-François Alcover_, Nov 23 2015 *)
%t A006116 Table[Sum[QBinomial[n, k, 2], {k, 0, n}], {n, 0, 19}] (* _Ivan Neretin_, Mar 28 2016 *)
%o A006116 (PARI) a(n)=polcoeff(sum(k=0, n, x^k/prod(j=0, k, 1-2^j*x+x*O(x^n))), n) \\ _Paul D. Hanna_, Dec 06 2007
%o A006116 (PARI) a(n,q=2)=sum(k=0,n,prod(i=1,n-k,(q^(i+k)-1)/(q^i-1))) \\ _Paul D. Hanna_, Nov 29 2008
