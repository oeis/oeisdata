%o A006889 (PARI) conseczerorec(n) = my(d=digits(n), i=0, r=0, x=#Str(n)); while(x > 0, while(d[x]==0, i++; x--); if(i > r, r=i); i=0; x--); r
%o A006889 a(n) = my(k=0); while(conseczerorec(2^k) < n, k++); k \\ _Felix Fröhlich_, Sep 27 2016
