%o A006753 (Python)
%o A006753 from sympy import factorint
%o A006753 def sd(n): return sum(map(int, str(n)))
%o A006753 def ok(n):
%o A006753   f = factorint(n)
%o A006753   return sum(f[p] for p in f) > 1 and sd(n) == sum(sd(p)*f[p] for p in f)
