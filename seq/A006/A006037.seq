%p A006037 isA006037 := proc(n)
%p A006037     isA005101(n) and not isA005835(n) ;
%p A006037 end proc:
%p A006037 for n from 1 do
%p A006037     if isA006037(n) then
%p A006037         print(n);
%p A006037     end if;
