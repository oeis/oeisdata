%t A006871 b[n_] := b[n] = If[n <= 1, n, Sum[k*b[k]*s[n-1, k], {k, 1, n-1}]/(n-1)]; s[n_, k_] := s[n, k] = Sum[b[n+1-j*k], {j, 1, Quotient[n, k]}]; B[n_] := B[n] = Function[x, Evaluate @ Sum[b[k]*x^k/k!, {k, 1, n}]]; a[n_] := Coefficient[Series[Exp[B[n+1][x]], {x, 0, n+2}], x, n+1]*(n+1)!; Table[a[n], {n, 0, 30}] (* _Jean-FranÃ§ois Alcover_, Feb 17 2014, after _Alois P. Heinz_ *)
