%t A006301 T[0, 0] = 1; T[n_, g_] /; g < 0 || g > n/2 = 0; T[n_, g_] := T[n, g] = ((4 n - 2)/3 T[n - 1, g] + (2 n - 3) (2 n - 2) (2 n - 1)/12 T[n - 2, g - 1] + 1/2 Sum[(2 k - 1) (2 (n - k) - 1) T[k - 1, i] T[n - k - 1, g - i], {k, 1, n - 1}, {i, 0, g}])/((n + 1)/6);
%t A006301 a[n_] := T[n, 2];
