%I A272218 #11 Nov 05 2025 15:22:33
%S A272218 1,5,20,100,352,1448,5304,21816,85649,340513,1365568,5470788,21883824,
%T A272218 87556401,350107285,1400656852
%N A272218 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 438", based on the 5-celled von Neumann neighborhood.
%C A272218 Initialized with a single black (ON) cell at stage zero.
%D A272218 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272218 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272218 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272218 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272218 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272218 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272218 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272218 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272218 code=438; stages=128;
%t A272218 rule=IntegerDigits[code,2,10];
%t A272218 g=2*stages+1; (* Maximum size of grid *)
%t A272218 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272218 ca=a;
%t A272218 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272218 PrependTo[ca,a];
%t A272218 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272218 k=(Length[ca[[1]]]+1)/2;
%t A272218 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272218 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272218 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272218 Cf. A272217.
%K A272218 nonn,more
%O A272218 0,2
