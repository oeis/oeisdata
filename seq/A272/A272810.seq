%I A272810 #11 Nov 05 2025 15:22:33
%S A272810 1,5,21,53,173,865,4029,16637,65381,262577,1043025,4111721,16309705,
%T A272810 64954017,259236073,1035773481
%N A272810 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 542", based on the 5-celled von Neumann neighborhood.
%C A272810 Initialized with a single black (ON) cell at stage zero.
%D A272810 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272810 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272810 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272810 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272810 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272810 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272810 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272810 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272810 code=542; stages=128;
%t A272810 rule=IntegerDigits[code,2,10];
%t A272810 g=2*stages+1; (* Maximum size of grid *)
%t A272810 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272810 ca=a;
%t A272810 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272810 PrependTo[ca,a];
%t A272810 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272810 k=(Length[ca[[1]]]+1)/2;
%t A272810 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272810 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272810 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272810 Cf. A272809.
%K A272810 nonn,more
%O A272810 0,2
