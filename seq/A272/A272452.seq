%I A272452 #11 Nov 05 2025 15:22:33
%S A272452 1,5,21,76,252,1108,5020,21364,86476,343440,1381412,5526704,22126848,
%T A272452 88572656,354349312,1417262424
%N A272452 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 478", based on the 5-celled von Neumann neighborhood.
%C A272452 Initialized with a single black (ON) cell at stage zero.
%D A272452 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272452 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272452 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272452 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272452 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272452 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272452 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272452 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272452 code=478; stages=128;
%t A272452 rule=IntegerDigits[code,2,10];
%t A272452 g=2*stages+1; (* Maximum size of grid *)
%t A272452 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272452 ca=a;
%t A272452 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272452 PrependTo[ca,a];
%t A272452 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272452 k=(Length[ca[[1]]]+1)/2;
%t A272452 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272452 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272452 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272452 Cf. A272451.
%K A272452 nonn,more
%O A272452 0,2
