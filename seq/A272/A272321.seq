%S A272321 4,4,19,1,51,-27,83,-19,115,-91,211,-147,291,-243,355,-187,387,-411,
%T A272321 715,-723,1055,-947,1075,-771,1067,-1107,1479,-1391,1735,-1595,1731,
%U A272321 -1195,1531,-1731,2467,-2483,3027,-2871,3339,-3255,3899,-3863,4395,-4083,4563
%N A272321 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 467", based on the 5-celled von Neumann neighborhood.
%C A272321 Initialized with a single black (ON) cell at stage zero.
%D A272321 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272321 Robert Price, <a href="/A272321/b272321.txt">Table of n, a(n) for n = 0..127</a>
%H A272321 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272321 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272321 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272321 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272321 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272321 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272321 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272321 code=467; stages=128;
%t A272321 rule=IntegerDigits[code,2,10];
%t A272321 g=2*stages+1; (* Maximum size of grid *)
%t A272321 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272321 ca=a;
%t A272321 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272321 PrependTo[ca,a];
%t A272321 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272321 k=(Length[ca[[1]]]+1)/2;
%t A272321 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272321 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272321 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A272321 Cf. A272315.
