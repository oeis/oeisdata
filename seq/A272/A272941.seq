%S A272941 1,6,23,52,113,202,323,484,701,970,1319,1732,2209,2782,3411,4112,4929,
%T A272941 5858,6955,8180,9521,10974,12583,14356,16253,18330,20559,22916,25461,
%U A272941 28142,30891,33768,36913,40370,44027,47908,52061,56446,61095,66048,71157,76474
%N A272941 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 563", based on the 5-celled von Neumann neighborhood.
%C A272941 Initialized with a single black (ON) cell at stage zero.
%D A272941 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272941 Robert Price, <a href="/A272941/b272941.txt">Table of n, a(n) for n = 0..128</a>
%H A272941 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272941 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272941 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272941 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272941 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272941 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272941 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272941 code=563; stages=128;
%t A272941 rule=IntegerDigits[code,2,10];
%t A272941 g=2*stages+1; (* Maximum size of grid *)
%t A272941 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272941 ca=a;
%t A272941 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272941 PrependTo[ca,a];
%t A272941 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272941 k=(Length[ca[[1]]]+1)/2;
%t A272941 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272941 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272941 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
