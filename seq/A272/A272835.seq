%I A272835 #11 Nov 05 2025 15:22:33
%S A272835 1,4,28,140,652,2724,11200,45532,183108,735184,2943200,11777752,
%T A272835 47112556,188440536,753734416,3014818088
%N A272835 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 545", based on the 5-celled von Neumann neighborhood.
%C A272835 Initialized with a single black (ON) cell at stage zero.
%D A272835 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272835 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272835 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272835 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272835 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272835 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272835 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272835 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272835 code=545; stages=128;
%t A272835 rule=IntegerDigits[code,2,10];
%t A272835 g=2*stages+1; (* Maximum size of grid *)
%t A272835 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272835 ca=a;
%t A272835 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272835 PrependTo[ca,a];
%t A272835 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272835 k=(Length[ca[[1]]]+1)/2;
%t A272835 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272835 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272835 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272835 Cf. A272834.
%K A272835 nonn,more
%O A272835 0,2
