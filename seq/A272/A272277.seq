%S A272277 1,5,20,57,233,980,3508,14868,58405,229204,920924,3674453,14688173,
%T A272277 58779657,235052852,939831460
%N A272277 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 454", based on the 5-celled von Neumann neighborhood.
%C A272277 Initialized with a single black (ON) cell at stage zero.
%D A272277 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272277 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272277 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272277 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272277 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272277 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272277 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272277 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272277 code=454; stages=128;
%t A272277 rule=IntegerDigits[code,2,10];
%t A272277 g=2*stages+1; (* Maximum size of grid *)
%t A272277 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272277 ca=a;
%t A272277 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272277 PrependTo[ca,a];
%t A272277 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272277 k=(Length[ca[[1]]]+1)/2;
%t A272277 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272277 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272277 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272277 Cf. A246326.
%K A272277 nonn,more
%O A272277 0,2
