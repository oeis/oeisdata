%S A272792 1,5,18,46,95,176,264,437,630,883,1131,1528,1936,2469,2981,3722,4471,
%T A272792 5368,6225,7377,8553,9878,11167,12888,14437,16397,18222,20419,22639,
%U A272792 25212,27528,30521,33457,36750,39798,43579,47235,51487,55456,60189,64626,69783
%N A272792 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 537", based on the 5-celled von Neumann neighborhood.
%C A272792 Initialized with a single black (ON) cell at stage zero.
%D A272792 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272792 Robert Price, <a href="/A272792/b272792.txt">Table of n, a(n) for n = 0..128</a>
%H A272792 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272792 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272792 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272792 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272792 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272792 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272792 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272792 code=537; stages=128;
%t A272792 rule=IntegerDigits[code,2,10];
%t A272792 g=2*stages+1; (* Maximum size of grid *)
%t A272792 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272792 ca=a;
%t A272792 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272792 PrependTo[ca,a];
%t A272792 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272792 k=(Length[ca[[1]]]+1)/2;
%t A272792 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272792 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272792 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
