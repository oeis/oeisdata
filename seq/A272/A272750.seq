%S A272750 1,5,13,29,49,89,97,165,177,273,277,385,389,545,549,701,697,889,917,
%T A272750 1057,1097,1301,1409,1557,1681,1925,1821,2213,2173,2465,2601,2821,
%U A272750 2853,3193,3253,3541,3481,3989,4057,4437,4493,4853,4797,5557,5381,5933,5749,6589
%N A272750 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 531", based on the 5-celled von Neumann neighborhood.
%C A272750 Initialized with a single black (ON) cell at stage zero.
%D A272750 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272750 Robert Price, <a href="/A272750/b272750.txt">Table of n, a(n) for n = 0..128</a>
%H A272750 Robert Price, <a href="/A272750/a272750.tmp.txt">Diagrams of the first 20 stages</a>
%H A272750 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272750 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272750 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272750 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272750 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272750 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272750 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272750 code=531; stages=128;
%t A272750 rule=IntegerDigits[code,2,10];
%t A272750 g=2*stages+1; (* Maximum size of grid *)
%t A272750 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272750 ca=a;
%t A272750 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272750 PrependTo[ca,a];
%t A272750 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272750 k=(Length[ca[[1]]]+1)/2;
%t A272750 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272750 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A272750 nonn,easy
%O A272750 0,2
