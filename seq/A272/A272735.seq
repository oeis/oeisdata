%I A272735 #12 Nov 05 2025 15:22:33
%S A272735 1,4,24,116,568,2500,10176,41116,164728,658292,2623192,10451916,
%T A272735 41644088,166056252,662379024,2643704140
%N A272735 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 521", based on the 5-celled von Neumann neighborhood.
%C A272735 Initialized with a single black (ON) cell at stage zero.
%D A272735 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272735 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272735 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272735 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272735 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272735 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272735 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272735 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272735 code=521; stages=128;
%t A272735 rule=IntegerDigits[code,2,10];
%t A272735 g=2*stages+1; (* Maximum size of grid *)
%t A272735 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272735 ca=a;
%t A272735 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272735 PrependTo[ca,a];
%t A272735 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272735 k=(Length[ca[[1]]]+1)/2;
%t A272735 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272735 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272735 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272735 Cf. A272734.
%K A272735 nonn,more
%O A272735 0,2
