%o A145198 (Python)
%o A145198 from operator import mul
%o A145198 from itertools import count, accumulate, islice
%o A145198 from collections import deque
%o A145198 def A145198_gen(): # generator of terms
%o A145198     aset, alist = set(), deque()
%o A145198     for k in count(1):
%o A145198         if k in aset:
%o A145198             aset.remove(k)
%o A145198         else:
%o A145198             yield k
%o A145198             aset |= set(k*d for d in accumulate(alist,mul))
%o A145198             alist.appendleft(k)
%o A145198 A145198_list = list(islice(A145198_gen(),50)) # _Chai Wah Wu_, Sep 01 2025
