%o A268834 (Python)
%o A268834 def A101080(n, k): return bin(n^k)[2:].count("1")
%o A268834 def A003188(n): return n^(n//2)
%o A268834 def A006068(n):
%o A268834     if n<2: return n
%o A268834     else:
%o A268834         m=A006068(n//2)
%o A268834         return 2*m + (n%2 + m%2)%2
%o A268834 def A268820(r, c): return c if r<1 else 0 if c<1 else A003188(1 + A006068(A268820(r - 1, c - 1)))
%o A268834 def a(r, c): return A101080(c, A268820(r, r + c))
