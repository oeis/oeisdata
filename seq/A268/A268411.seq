%F A268411 a(0)=0, a(2*n)=a(n); for odd n, a(2*n+1)=a(n); for even n, a(2*n+1)=1-a(n) or a(4*n)=a(n), a(4*n+1)=1-a(n), a(4*n+2)=a(4*n+3)=a(2*n+1);
%F A268411 also a(n+2^k)=1-a(n) for 0<=n<=2^(k-1)-1;
