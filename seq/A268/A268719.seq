%t A268719 a88[n_] := BitXor[n, Floor[n/2]];
%t A268719 a68[n_] := BitXor @@ Table[Floor[n/2^m], {m, 0, Floor[Log[2, n]]}];
%t A268719 a68[0] = 0;
%t A268719 T[n_, k_] := a88[a68[n] + a68[k]];
