%o A268719 (Python)
%o A268719 def a003188(n): return n^(n>>1)
%o A268719 def a006068(n):
%o A268719     s=1
%o A268719     while True:
%o A268719         ns=n>>s
%o A268719         if ns==0: break
%o A268719         n=n^ns
%o A268719         s<<=1
%o A268719     return n
%o A268719 def T(n, k): return a003188(a006068(n) + a006068(k))
