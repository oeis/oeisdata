%p A268138 # Alternative (which also gives an integer for n = 0):
%p A268138 f := n -> hypergeom([-n, -n], [1], 2):          # A001850
%p A268138 h := n -> hypergeom([-n,  n], [1], 2):          # A182626
%p A268138 g := n -> hypergeom([-n,  n, 1/2], [1, 1], -8): # A358388
