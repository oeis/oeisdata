%o A357009 (PARI) a(n) = sum(k=0, n\2, (2*k)!*(k+1)^(k-1)*stirling(n, 2*k, 2)/k!);
%o A357009 (PARI) my(N=30, x='x+O('x^N)); Vec(serlaplace(sum(k=0, N, (k+1)^(k-1)*(exp(x)-1)^(2*k)/k!)))
