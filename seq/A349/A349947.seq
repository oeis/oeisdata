%t A349947 t = {1, 1}; Do[t = Join[t, Riffle[Range[n], n], {n}], {n, 2, 100}];
%t A349947 u = Flatten[Partition[t, 2]];
%t A349947 v = Table[n (n + 1), {n, 1, 80}];
%t A349947 d = Delete[u, Map[{#} &, v]]; (* A349526 *)
%t A349947 p = Table[{d[[n]], d[[n + 1]]}, {n, 1, 150}];
