%o A384937 (PARI)
%o A384937 w(j,k,N) = {if(k>N,1, 1/(1 - sum(i=j,N, x^i * w(i,k+1,N-i+1))))}
%o A384937 Ax(N) = {Vec(w(1,1,N)+ O('x^(N+1)))}
