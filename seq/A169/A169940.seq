%o A169940 T(n)={ my(c=vector(n)); forstep(j=1<<n+1,2<<n,2, c[vecmax(Vec(Pol(binary(j))^2))-1]++);c} /* yields the n-th row of the triangle */
%o A169940 T(n,k)={ sum( j=1<<(n-1),1<<n-1, vecmax(Vec(Pol(binary(j*2+1))^2))==k )}
