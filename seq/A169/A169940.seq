%t A169940 row[n_] := Module[{dd, xx, mm}, dd = Join[{1}, PadLeft[IntegerDigits[#, 2], n-1], {1}]& /@ Range[0, 2^(n-1) - 1]; xx = (((x^Range[n, 0, -1]).#)& /@ dd)^2 // Expand; mm = Max[CoefficientList[#, x]]& /@ xx; Table[Count[mm, k], {k, 2, n+1}]]; Table[row[n], {n, 1, 12}] // Flatten (* _Jean-Fran√ßois Alcover_, Oct 10 2017 *)
