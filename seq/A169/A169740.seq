%t A169740 f[1]=1; f[n_?EvenQ]:=f[n]=f[n/2]; f[n_?OddQ]:=f[n]=f[n-1]+f[n-2]; a[n_]:=f[4*n+3]; Table[a[n], {n, 0, 100}] (* _Vincenzo Librandi_, May 27 2019 *)
%o A169740 (PARI) {f(n) = if(n==1,1, if(n%2==0, f(n/2), f(n-1) + f(n-2)))};
%o A169740 vector(50, n, n--; f(4*n+3)) \\ _G. C. Greubel_, May 29 2019
%o A169740 (SageMath)
%o A169740 def f(n):
%o A169740     if (n==1): return 1
%o A169740     elif (n%2==0): return f(n/2)
%o A169740     else: return f(n-1) + f(n-2)
%o A169740 [f(4*n+3) for n in (0..50)] # _G. C. Greubel_, May 29 2019
