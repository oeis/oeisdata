%o A130227 (Python)
%o A130227 from itertools import count, islice
%o A130227 from sympy.solvers.diophantine.diophantine import diop_DN
%o A130227 def A130227_gen(startvalue=1): # generator of terms >= startvalue
%o A130227     for n in count(max(startvalue,1)):
%o A130227         if len(d:=[a[1] for a in diop_DN(n,-1)]):
%o A130227             yield min(d)
%o A130227 A130227_list = list(islice(A130227_gen(),50)) # _Chai Wah Wu_, Dec 22 2025
