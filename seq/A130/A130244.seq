%o A130244 (Python)
%o A130244 def A130244(n):
%o A130244     if n<3: return 0
%o A130244     a, b, c = 2, 1, 0
%o A130244     while a < n:
%o A130244         c += n
%o A130244         a, b = b, a+b
%o A130244     return c-b # _Chai Wah Wu_, Sep 07 2025
