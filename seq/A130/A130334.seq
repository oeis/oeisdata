%F A130334 a(n) = n + A130335(n).
%o A130334 (Python)
%o A130334 from math import gcd
%o A130334 def A130334(n):
%o A130334     k, Tn, Tm = n+1, n*(n+1)//2, (n+1)*(n+2)//2
%o A130334     while gcd(Tn,Tm) != 1:
%o A130334         k += 1
%o A130334         Tm += k
%o A130334     return k # _Chai Wah Wu_, Sep 16 2021
