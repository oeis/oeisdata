%t A118696 spQ[n_] := Plus @@ Last /@ FactorInteger@n == 2; mdrQ[n_] := Mod[n, NestWhile[Times @@ IntegerDigits@# &, n, UnsameQ, All]] == 0; Select[ Range@9754, spQ@# && mdrQ@# &] (* _Robert G. Wilson v_, Aug 04 2006 *)
