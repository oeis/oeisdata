%p A118709 a:= n-> LinearAlgebra[Determinant](Matrix(n, shape=Circulant[[i^3$i=0..n-1]])):
%p A118709 seq(a(n), n=0..14);  # _Alois P. Heinz_, Feb 21 2026
%t A118709 Join[{1},Table[Det[Table[RotateRight[Range[0,i-1]^3,n],{n,0,i-1}]],{i,13}]] (* _Harvey P. Dale_, Oct 22 2012 *)
%t A118709 b[n_]:=(2*n^2-3*n-3+Sqrt[15n^2-18n-9]I)/2; c[n_]:=(2*n^2-3*n-3-Sqrt[15n^2-18n-9]I)/2; a[0]=1; a[n_]:=(-1)^(n-1)*(n-1)^2*n^(n-2)*(n^(2n)-b[n]^n-c[n]^n+(n^2-3n+3)^n)/24; Simplify[Array[a,14,0]] (* _Stefano Spezia_, Feb 21 2026 *)
