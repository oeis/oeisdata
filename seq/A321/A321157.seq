%p A321157 filter:= proc(x) local n,k,ct;
%p A321157    ct:= 0;
%p A321157    for n in select(t -> t >=3, NumberTheory:-Divisors(2*x)) do
%p A321157      k:= 2*(n^2-2*n+x)/(n*(n-1));
%p A321157      if k::posint and k >= 3 then ct:= ct+1; if ct > 7 then return false fi fi
%p A321157   od;
%p A321157   ct = 7
%p A321157 end proc:
