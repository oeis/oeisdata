%o A082379 (Python)
%o A082379 from fractions import Fraction
%o A082379 from functools import lru_cache
%o A082379 from itertools import count, islice, product
%o A082379 def period(x):
%o A082379     for p in range(1, len(x)):
%o A082379         if all(x[i] == x[i+p] for i in range(len(x)-p)):
%o A082379             return p
%o A082379     return len(x)
%o A082379 def exp(x):
%o A082379     return Fraction(len(x), period(x))
%o A082379 @lru_cache(maxsize=None)
%o A082379 def cexp(x):
%o A082379     if len(x) == 1: return 1
%o A082379     return max(exp(x), cexp(x[:-1]), cexp(x[1:]))
%o A082379 def agen(): # generator of 7/3-power-free terms
%o A082379     yield 1
%o A082379     stfs = set("0")
%o A082379     for n in count(1):
%o A082379         yield 2*len(stfs)
%o A082379         stfsnew = set(s+i for s in stfs for i in "01" if cexp(s+i) < Fraction(7, 3))
%o A082379         stfs = stfsnew
%o A082379 print(list(islice(agen(), 60))) # _Michael S. Branicky_, Dec 08 2025
