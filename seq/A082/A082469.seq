%o A082469 (Python)
%o A082469 from gmpy2 import is_prime, mpz
%o A082469 from itertools import count
%o A082469 def A082469(n):
%o A082469     if n == 1 or n%2 == 0 or n%5 == 0: return -1
%o A082469     return next(k for k in count(0) if is_prime(mpz("".join(str(i)+str(k) for i in range(1, n))+str(n))))
