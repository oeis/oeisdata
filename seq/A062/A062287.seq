%t A062287 palindromicQ[n_, b_:10] := TrueQ[IntegerDigits[n, b] == Reverse[IntegerDigits[n, b]]]; Select[Range[2, 10^4, 2], palindromicQ[#] && Plus@@Drop[DigitCount[#], {2, 10, 2}] == 0 &] (* _Alonso del Arte_, Feb 23 2012 *)
