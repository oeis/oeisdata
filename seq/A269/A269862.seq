%t A269862 f[n_] := (Times @@ Power[If[# == 1, 1, NextPrime@ #] & /@ First@ #, Last@ #] + 1)/2 &@ Transpose@ FactorInteger@ n; s = Select[Range@ 150, Xor[EvenQ@ f@ #, EvenQ@ #] &]; Table[Count[s, k_ /; k <= n], {n, 87}] (* _Michael De Vlieger_, Mar 17 2016 *)
