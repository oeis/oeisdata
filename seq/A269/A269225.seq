%t A269225 a[n_] := Block[{v=2^n, k=1}, While[++k! <= v]; k]; Array[a, 93, 0] (* _Giovanni Resta_, Jul 11 2016 *)
%t A269225 Module[{nn=30,f},f=Table[{k,k!},{k,nn}];Table[SelectFirst[f,#[[2]]>2^n&],{n,0,100}]][[;;,1]] (* _Harvey P. Dale_, Feb 19 2024 *)
