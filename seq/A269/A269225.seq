%o A269225 def a269225():
%o A269225    k = 1
%o A269225    f = 1
%o A269225    p = 1
%o A269225    n = 0
%o A269225    while True:
%o A269225       while f<=p:
%o A269225          k += 1
%o A269225          f *= k
%o A269225       yield k
%o A269225       p *= 2
