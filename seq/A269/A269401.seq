%o A269401 (definec (A269401 n) (cond ((<= n 1) n) ((zero? (A213719 n)) (+ 1 (* 2 (A269401 (- n (A269371 n)))))) (else (* 2 (A269401 (+ -1 (A269371 n)))))))
