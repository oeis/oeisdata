%t A051593 (* a3 = A000793  a4 = A051704 *) a3[n_] := Max[LCM @@@ IntegerPartitions[n]]; a4[n_] := (pp = Reap[ Do[ pk = p^k; If[pk <= n, Sow[pk]], {p, Prime[ Range[2, PrimePi[n]]]}, {k, 1, Ceiling[ Log[3, n]]}]][[2, 1]]; sel = Select[ IntegerPartitions[n, All, pp], Length[#] == Length[ Union[#] && !MatchQ[#, {___, x_, ___, y_, ___} /; GCD[x, y] != 1]] &]; Max[Times @@@ sel]); a4[0] = 1; a4[1] = a4[2] = a4[4] = a4[6] = 0; a[n_] := Max[a3[n - 2], a4[n - 1], a4[n]]; a[0] = a[1] = a[2] = 1; Table[a[n], {n, 0, 47}] (* _Jean-FranÃ§ois Alcover_, Sep 11 2012, from formula *)
%o A051593 (PARI) a(n)={my(m=1); forpart(p=n, if(sum(i=1, #p, p[i]-1)%2==0, m=max(m, lcm(Vec(p))))); m} \\ _Andrew Howroyd_, Jul 03 2018
