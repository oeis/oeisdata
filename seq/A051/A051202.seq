%o A051202 for n in range(3, 10**13):
%o A051202     a = abs(a1+2*a2-n)
%o A051202     if a == 0:
%o A051202         A051202_list.append(n)
%o A051202     a1, a2 = a, a1 # _Chai Wah Wu_, Apr 19 2015
