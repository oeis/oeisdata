%p A051202 t1:=[];
%p A051202 M:=200000:
%p A051202 a:=1: b:=1:
%p A051202 for n from 3 to M do c:=abs(2*a+b-n);
%p A051202 if c=0 then t1:=[op(t1),n]; fi;
%p A051202 a:=b; b:=c; od:
%p A051202 [seq(t1[n],n=1..nops(t1))]; # _N. J. A. Sloane_, Apr 16 2015
%o A051202 (Python)
