%o A332687 (Python)
%o A332687 from sympy import primenu, primepi
%o A332687 def A332687(n):
%o A332687     c, j = n-primenu(n), 1
%o A332687     while j <= n:
%o A332687         c += primepi(k:=n//j)*(-j+(j:=n//k+1))
%o A332687     return c # _Chai Wah Wu_, Jan 29 2026
