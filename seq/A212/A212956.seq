%S A212956 1,13,91,209,213,217,8,283,82,3150,105,3729,3548,1267,5218,3459,571,
%T A212956 1911,12088,858,4606,3171,5305,6890,5024,19149,14766,16640,15333,
%U A212956 39382,8692,49314,53630,123600,4120,57993,47136,20530,115743,30022,31457,76736,25842
%N A212956 Least k such that k*30^n-1 , k*30^n+1, and 2*k*30^n-1 are prime; that is, twin primes and a Sophie Germain prime.
%H A212956 Pierre CAMI, <a href="/A212956/b212956.txt">Table of n, a(n) for n = 1..125</a>
%e A212956 1*30^1-1 = 29, 1*30^1+1 = 31, 2*1*30^1-1 = 59. Because 29, 31, and 59 are prime, a(1) = 1.
%t A212956 Table[k = 1; While[! PrimeQ[k*30^n + 1] || ! PrimeQ[k*30^n - 1] || ! PrimeQ[2 k*30^n - 1], k++]; k, {n, 50}] (* _T. D. Noe_, Jun 01 2012 *)
%o A212956 (PFGW)
%o A212956 ' PFGW64 and SCRIPTIFY from Primeform Group
%o A212956 ' command: pfgw64 -f in.txt
%o A212956 ' in.txt file :
%o A212956 SCRIPT
%o A212956 DIM nn,0
%o A212956 DIM kk,0
%o A212956 DIMS tt
%o A212956 OPENFILEOUT myfil,b(n).txt
%o A212956 LABEL loopn
%o A212956 SET nn,nn+1
%o A212956 IF nn>125 THEN END
%o A212956 SET kk,0
%o A212956 LABEL loopk
%o A212956 SET kk,kk+1
%o A212956 SETS tt,%d,%d\,;nn;kk
%o A212956 PRP kk*30^nn-1,tt
%o A212956 IF ISPRP THEN GOTO a
%o A212956 IF ISPRIME THEN GOTO a
%o A212956 GOTO loopk
%o A212956 LABEL a
%o A212956 PRP kk*30^nn+1,tt
%o A212956 IF ISPRP THEN GOTO b
%o A212956 IF ISPRIME THEN GOTO b
%o A212956 GOTO loopk
%o A212956 LABEL b
%o A212956 PRP 2*kk*30^nn-1,tt
%o A212956 IF ISPRP THEN GOTO c
%o A212956 IF ISPRIME THEN GOTO c
%o A212956 GOTO loopk
%o A212956 LABEL c
%o A212956 WRITE myfil,tt
%o A212956 GOTO loopn
%Y A212956 Cf. A212955.
%K A212956 nonn
%O A212956 1,2
%A A212956 _Pierre CAMI_, Jun 01 2012
