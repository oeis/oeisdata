%t A212896 t = Compile[{{n, _Integer}}, Module[{s = 0},
%t A212896 (Do[If[Min[Abs[w - x], Abs[x - y], Abs[y - z]] <= 1, s = s + 1],
%t A212896 {w, 0, n}, {x, 0, n}, {y, 0, n}, {z, 0, n}]; s)]];
%t A212896 m = Map[t[#] &, Range[0, 40]]   (* A212896 *)
