%t A371285 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n], {p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A371285 Table[Times@@Prime/@Join@@Divisors/@prix[n],{n,100}]
%o A371285 (PARI) \\ here b(n) = A275700(n).
%o A371285 b(n) = vecprod([prime(d) | d<-divisors(n)])
%o A371285 a(n) = { my(f=factor(n)); prod(i=1, #f~, my([p,e]=f[i,]); b(primepi(p))^e) } \\ _Andrew Howroyd_, Nov 10 2025
