%t A371294 stableQ[u_,Q_]:=!Apply[Or,Outer[#1=!=#2&&Q[#1,#2]&,u,u,1],{0,1}];
%t A371294 bpe[n_]:=Join@@Position[Reverse[IntegerDigits[n,2]],1];
%t A371294 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A371294 Select[Range[1000],stableQ[bpe[#],Divisible]&&connectedQ[prix/@bpe[#]]&]
