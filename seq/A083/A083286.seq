%t A083286 s[x_, 0] := 0; s[x_, n_] := Floor[x*s[x, n - 1]] + 1
%t A083286 c[x_, n_] := ((1/x)^n) s[x, n]
%t A083286 t = N[c[3/2, 800], 120]
%t A083286 RealDigits[t, 10]  (* A083286 *)
