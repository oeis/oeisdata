%p A055584 T := (n, k) -> binomial(n, k)*hypergeom([4, k - n], [k + 1], -1):
%p A055584 for n from 0 to 7 do seq(simplify(T(n, k)), k = 0..n) od; # _Peter Luschny_, Sep 23 2024
