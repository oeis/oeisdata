%t A055549 normalQ[a_] := Block[{b=Transpose[a]}, a.b === b.a]; cnt[n_]:= Block[{c=0,q=n^2}, Do[If[normalQ@Partition[IntegerDigits[t-1,3,q]-1, n], c++ ],{t,3^q}]; c]; Array[cnt,4] (* _Giovanni Resta_, Apr 03 2006 *)
