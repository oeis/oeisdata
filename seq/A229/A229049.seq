%e A229049 G.f.: A(x) = 1 + 721*x + 7489441*x^2 + 137322405361*x^3 + 3249278494922881*x^4 +...
%e A229049 where
%e A229049 A(x) = 1/(1-x) + (6!/1!^6)*x/(1-x)^7 + (12!/2!^6)*x^2/(1-x)^13 + (18!/3!^6)*x^3/(1-x)^19 + (24!/4!^6)*x^4/(1-x)^25 + (30!/5!^6)*x^5/(1-x)^31 +...
%e A229049 Equivalently,
%e A229049 A(x) = 1/(1-x) + 720*x/(1-x)^7 + 7484400*x^2/(1-x)^13 + 137225088000*x^3/(1-x)^19 + 3246670537110000*x^4/(1-x)^25 + 88832646059788350720*x^5/(1-x)^31 +...+ A008979(n)*x^n/(1-x)^(6*n+1) +...
%p A229049 with(combinat):
%p A229049 a:= n-> add(multinomial(n+5*k, n-k, k$6), k=0..n):
%p A229049 seq(a(n), n=0..15);  # _Alois P. Heinz_, Sep 23 2013
%t A229049 Table[Sum[Binomial[n,k]*Binomial[n+k,k]*Binomial[n+2*k,k] *Binomial[n+3*k,k] *Binomial[n+4*k,k]*Binomial[n+5*k,k], {k, 0, n}],{n,0,20}] (* _Vaclav Kotesovec_, Sep 23 2013 *)
