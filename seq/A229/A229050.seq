%N A229050 G.f.: Sum_{n>=0} (n^2)!/n!^n * x^n / (1-x)^(n^2+1).
%H A229050 Alois P. Heinz, <a href="/A229050/b229050.txt">Table of n, a(n) for n = 0..26</a>
%F A229050 a(n) = Sum_{k=0..n} Product_{j=0..k-1} binomial(n+j*k,k).
%F A229050 a(n) ~ exp(-1/12) * n^(n^2-n/2+1) / (2*Pi)^((n-1)/2). - _Vaclav Kotesovec_, Sep 23 2013
%e A229050 G.f.: A(x) = 1 + 2*x + 9*x^2 + 1714*x^3 + 63079895*x^4 + 623361815288736*x^5 +...
%e A229050 where
%e A229050 A(x) = 1/(1-x) + x/(1-x)^2 + (4!/2!^2)*x^2/(1-x)^5 + (9!/3!^3)*x^3/(1-x)^10 + (16!/4!^4)*x^4/(1-x)^17 + (25!/5!^5)*x^5/(1-x)^26 +...
%e A229050 Equivalently,
%e A229050 A(x) = 1/(1-x) + x/(1-x)^2 + 6*x^2/(1-x)^5 + 1680*x^3/(1-x)^10 + 63063000*x^4/(1-x)^17 + 623360743125120*x^5/(1-x)^26 +...+ A034841(n)*x^n/(1-x)^(n^2+1) +...
%e A229050 Illustrate formula a(n) = Sum_{k=0..n} Product_{j=0..k-1} C(n+j*k,k) for initial terms:
%e A229050 a(0) = 1;
%e A229050 a(1) = 1 + C(1,1);
%e A229050 a(2) = 1 + C(2,1) + C(2,2)*C(4,2);
%e A229050 a(3) = 1 + C(3,1) + C(3,2)*C(5,2) + C(3,3)*C(6,3)*C(9,3);
%e A229050 a(4) = 1 + C(4,1) + C(4,2)*C(6,2) + C(4,3)*C(7,3)*C(10,3) + C(4,4)*C(8,4)*C(12,4)*C(16,4);
%e A229050 a(5) = 1 + C(5,1) + C(5,2)*C(7,2) + C(5,3)*C(8,3)*C(11,3) + C(5,4)*C(9,4)*C(13,4)*C(17,4) + C(5,5)*C(10,5)*C(15,5)*C(20,5)*C(25,5); ...
%e A229050 which numerically equals:
%e A229050 a(0) = 1;
%e A229050 a(1) = 1 + 1 = 2;
%e A229050 a(2) = 1 + 2 + 1*6 = 9;
%e A229050 a(3) = 1 + 3 + 3*10 + 1*20*84 = 1714;
%e A229050 a(4) = 1 + 4 + 6*15 + 4*35*120 + 1*70*495*1820 = 63079895;
%e A229050 a(5) = 1 + 5 + 10*21 + 10*56*165 + 5*126*715*2380 + 1*252*3003*15504*53130 = 623361815288736; ...
%p A229050 with(combinat):
%p A229050 a:= n-> add(multinomial(n+(k-1)*k, n-k, k$k), k=0..n):
%p A229050 seq(a(n), n=0..15);  # _Alois P. Heinz_, Sep 23 2013
%t A229050 Table[Sum[Product[Binomial[n+j*k,k],{j,0,k-1}],{k,0,n}],{n,0,10}] (* _Vaclav Kotesovec_, Sep 23 2013 *)
%o A229050 (PARI) {a(n)=polcoeff(sum(m=0, n, (m^2)!/m!^m*x^m/(1-x+x*O(x^n))^(m^2+1)), n)}
%o A229050 for(n=0,15,print1(a(n),", "))
%o A229050 (PARI) {a(n)=sum(k=0,n,prod(j=0,k-1,binomial(n+j*k,k)))}
%o A229050 for(n=0,15,print1(a(n),", "))
