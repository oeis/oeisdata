%e A157995 19=7+11+1, 31=13+17+1, 43=19+23+1, 53=23+29+1, 79=37+41+1, 101=47+53+1, ...
%p A157995 count:= 0: R:= NULL: p:= 2:
%p A157995 while count < 100 do
%p A157995   q:= p; p:= nextprime(p);
