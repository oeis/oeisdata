%p A157995 R; # _Robert Israel_, May 13 2020
%t A157995 lst={};Do[p0=Prime[n];p1=Prime[n+1];a=p0+p1+1;If[PrimeQ[a]&&(p1-p0)>2,AppendTo[lst,a]],{n,6!}];lst
