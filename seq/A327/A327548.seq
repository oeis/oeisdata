%t A327548 b[n_, i_] := b[n, i] = If[n == 0, {1, 0}, If[i < 1, {0, 0}, b[n, i - 1] + With[{p = 2^(i - 1) b[n - i, Min[n - i, i]]}, p + {0, p[[1]]}]]];
%t A327548 a[n_] := b[n, n][[2]];
