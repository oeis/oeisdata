%p A327442 seq(a(n), n=0..80);  # _Alois P. Heinz_, Jan 11 2020
%t A327442 a[0] = 0; a[n_] := a[n] = With[{phi = EulerPhi[n], a1 = a[n-1]}, If[phi>a1, a1+phi, a1-phi]];
