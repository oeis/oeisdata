%t A160694 p[x_] := Piecewise[ {{-x^2*((x - 8)*x^2 + (6*x - 4)*Pi), 0 <= x <= 1}, {2*x*((x^2 - 8*Sqrt[x^2 - 1] + 3)*x^2 + 12*ArcSec[x]*x^2 + Pi*(3 - 4*x) - 4*Sqrt[x^2 - 1] - 1/2), 1 < x <= Sqrt[2]}, {x*((-x^2 + 8*Sqrt[x^2 - 2] + 6*Pi - 5)* (x^2 + 1) - 16*x*ArcCsc[Sqrt[2 - 2/x^2]] - 24*(x^2 + 1)*ArcTan[Sqrt[x^2 - 2]] + 16*x*ArcTan[x*Sqrt[x^2 - 2]]), Sqrt[2] < x < Sqrt[3]}}]; a[n_] := Integrate[x^(2n)*p[x], {x, 0, Sqrt[3]}]; Table[a[n], {n, 0, 9}] // Denominator (* _Jean-FranÃ§ois Alcover_, Dec 26 2012, after _Eric W. Weisstein_ *)
