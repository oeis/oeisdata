%t A213558 b[n_] := n^3; c[n_] := n^3
%t A213558 t[n_, k_] := Sum[b[k - i] c[n + i], {i, 0, k - 1}]
%t A213558 TableForm[Table[t[n, k], {n, 1, 10}, {k, 1, 10}]]
%t A213558 Flatten[Table[t[n - k + 1, k], {n, 12}, {k, n, 1, -1}]]
