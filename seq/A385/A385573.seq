%t A385573 highs := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A385573 lows := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[Split[Rest[FoldList[Min, +\[Infinity], #]]]] &;
%t A385573 seqS = Table[N[Sin[n], 20], {n, 1, 4000}];  (* User: put your dense sequence S after seqS *)
%t A385573 indices = Range[Length[seqS]];
%t A385573 arrI = {};   (* start accumulating increasing partition array *)
%t A385573 Until[Last[arrI] == {}, AppendTo[arrI,  Flatten[Map[Position[seqS, #] &,
%t A385573      highs[seqS[[Complement[indices, Flatten[arrI]]]]][[1]]]]]];
%t A385573 Grid[Take[arrI, 12]]
%t A385573 arrD = {};   (* start accumulating decreasing partition array *)
%t A385573 Until[Last[arrD] == {}, AppendTo[arrD,
%t A385573    Flatten[Map[Position[seqS, #] &, lows[seqS[[Complement[indices, Flatten[arrD]]]]][[1]]]]]];
%t A385573 Grid[Take[arrD, 12]]
