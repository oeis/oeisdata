%p A385641 a := series(exp(x)*(BesselI(0, 2*x) + 2*int(BesselI(0, 2*x), x) + int(int(BesselI(0, 2*x), x), x)), x = 0, 30): seq(n!*coeff(a, x, n), n = 0 .. 29);
%t A385641 Module[{x}, CoefficientList[Series[1/(Sqrt[(1 + x)*(1 - 3*x)]*(1 - x)^2), {x, 0, 30}], x]] (* _Paolo Xausa_, Nov 03 2025 *)
