%t A305932 mx = 1000; g[n_] := g[n] = DigitCount[2^n, 10, 0]; f[n_] := Select[Range@mx, g@# == n &]; Table[f@n, {n, 0, 4}] // Flatten (* _Robert G. Wilson v_, Jun 20 2018 *)
