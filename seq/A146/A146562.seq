%o A146562 (Haskell)
%o A146562 import Data.List (findIndices)
%o A146562 a146562 n = a146562_list !! (n-1)
%o A146562 a146562_list = map (+ 1) $
%o A146562    findIndices (/= 0) $ zipWith (-) a064353_list $ cycle [1, 3]
