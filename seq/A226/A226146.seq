%N A226146 Numbers k such that k^2 is an average of three successive primes.
%H A226146 Robert Israel, <a href="/A226146/b226146.txt">Table of n, a(n) for n = 1..10000</a>
%p A226146 filter:= proc(k) local p,q,r,s;
%p A226146    q:= prevprime(k^2);
%p A226146    p:= prevprime(q);
%p A226146    r:= nextprime(q);
%p A226146    s:= nextprime(r);
%p A226146    k^2 = (p+q+r)/3 or k^2 = (q+r+s)/3
%p A226146 end proc:
%p A226146 select(filter, [seq(i,i=3..10000,2)]); # _Robert Israel_, Feb 16 2026
%t A226146 Select[Sqrt[Mean[#]]&/@Partition[Prime[Range[10^6]],3,1],IntegerQ] (* _Harvey P. Dale_, Oct 23 2021 *)
%o A226146 (C)
%o A226146 #include <stdio.h>
%o A226146 #include <stdlib.h>
%o A226146 #include <math.h>
%o A226146 #define TOP (1ULL<<30)
%o A226146 int main() {
%o A226146   unsigned long long i, j, p1, p2, r, s;
%o A226146   unsigned char *c = (unsigned char *)malloc(TOP/8);
%o A226146   memset(c, 0, TOP/8);
%o A226146   for (i=3; i < TOP; i+=2)
%o A226146     if ((c[i>>4] & (1<<((i>>1) & 7)))==0 /*&& i<(1ULL<<32)*/)
%o A226146         for (j=i*i>>1; j<TOP; j+=i)  c[j>>3] |= 1 << (j&7);
%o A226146   for (p2=2, p1=3, i=5; i < TOP; i+=2)
%o A226146     if ((c[i>>4] & (1<<((i>>1) & 7)))==0) {
%o A226146       s = p2 + p1 + i;
%o A226146       if ((s%3)==0) {
%o A226146         s/=3;
%o A226146         r = sqrt(s);
%o A226146         if (r*r==s) printf("%llu, ", r);
%o A226146       }
%o A226146       p2 = p1, p1 = i;
%o A226146     }
%o A226146   return 0;
%o A226146 }
