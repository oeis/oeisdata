%t A226108 testQ[n_] := n > 9 && Catch[Block[{d = IntegerDigits@n}, Do[If[! PrimeQ[ d[[j]] + 10*d[[i]]], Throw@False], {j, 2, Length@d}, {i, j-1}]; True]]; Select[Prime@ Range[10^5], testQ] (* _Giovanni Resta_, May 28 2013 *)
