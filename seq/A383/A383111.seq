%t A383111 Table[Length[Select[IntegerPartitions[n], Length[Select[Permutations[#], UnsameQ@@Length/@Split[#]&]]>1&]],{n,0,15}]
%o A383111 (Python)
%o A383111 from itertools import groupby
%o A383111 from sympy.utilities.iterables import partitions, multiset_permutations
%o A383111 def a(n):
%o A383111     c = 0
%o A383111     for p in partitions(n):
%o A383111         cp = 0
%o A383111         for mp in multiset_permutations(p):
%o A383111             passes, run_lengths = True, set()
%o A383111             for k, g in groupby(mp):
%o A383111                 rl = len(list(g))
%o A383111                 if rl in run_lengths:
%o A383111                     passes = False
%o A383111                     break
%o A383111                 run_lengths.add(rl)
%o A383111             if passes:
%o A383111                 cp += 1
%o A383111             if cp > 1:
%o A383111                 c += 1
%o A383111                 break
%o A383111     return c
%o A383111 print([a(n) for n in range(21)]) # _Michael S. Branicky_, Nov 02 2025
