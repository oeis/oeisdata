%o A383394 (Python)
%o A383394 from math import isqrt
%o A383394 from sympy import integer_nthroot, mobius
%o A383394 from oeis_sequences.OEISsequences import bisection, squarefreepi
%o A383394 def A383394(n):
%o A383394     def g(x):
%o A383394         c, l = squarefreepi(integer_nthroot(x,3)[0])+sum(mobius(k)*(integer_nthroot(x, k)[0]-1) for k in range(2, x.bit_length()))-1, 0
%o A383394         j = isqrt(x)
%o A383394         while j>1:
%o A383394             k2 = integer_nthroot(x//j**2,3)[0]+1
%o A383394             w = squarefreepi(k2-1)
%o A383394             c += j*(w-l)
%o A383394             l, j = w, isqrt(x//k2**3)
%o A383394         return c-l
%o A383394     def f(x): return n+x-sum(g(integer_nthroot(x, k)[0]) for k in range(2, x.bit_length()))
%o A383394     return bisection(f,n,n) # _Chai Wah Wu_, Aug 11 2025
