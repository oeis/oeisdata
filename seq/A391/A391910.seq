%e A391910   1, 2186, 743232, 92663424, 4693032960, 93943234560, 676457349120, 1352914698240;
%t A391910 T[n_,k_]:=Sum[(2j+1)^n*(-1)^(DigitCount[j,2,1]+k),{j,0,2^k-1}];Table[T[n,k],{n,0,8},{k,0,n}]//Flatten (* _James C. McMahon_, Dec 30 2025 *)
%o A391910 (PARI) rows(n) = my(v1 = vector(n+1, i, vector(i, j, 0)), v2 = v1); v1[1][1] = 1; v2[1][1] = 1; for(i=1, n, v1[i+1][1] = 1; v2[i+1][1] = 1; v2[i+1][i+1] = 1); for(i=2, n, for(j=1, i-1, v2[i+1][j+1] = v2[i][j+1] + v2[i][j])); for(i=1, n, for(j=i, n, v1[j+1][i+1] = 2*sum(k=0, (j-i)\2, v2[j+1][i+2*k+1]*v1[i+2*k+1][i]*2^(i*(j-i-2*k))))); v1
