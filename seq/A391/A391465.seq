%t A391465 PellP[n_]:=Fibonacci[n, 2];
%t A391465 Table[If[n==0,1,Sum[k *(PellP[k]+PellP[k+1]) Binomial[4 n,n-k],{k,1,n}]/n],{n,0,20}] (* _Vincenzo Librandi_, Dec 11 2025 *)
%o A391465 (PARI) pell(n) = ([2, 1; 1, 0]^n)[2, 1];
%o A391465 a(n) = if(n==0, 1, sum(k=1, n, k*(pell(k)+pell(k+1))*binomial(4*n, n-k))/n);
%o A391465 (Magma) M := Matrix(IntegerRing(), 2, 2, [2,1,1,0]);
%o A391465 a := function(n)
%o A391465     if n eq 0 then
%o A391465         return 1;
%o A391465     end if;
%o A391465     S := 0;
%o A391465     for k in [1..n] do
%o A391465         S +:= k * ((M^k)[2,1] + (M^(k+1))[2,1]) * Binomial(4*n, n-k);
%o A391465     end for;
%o A391465     return S div n;
%o A391465 end function;
%o A391465 seq := [ a(n) : n in [0..25] ];
%o A391465 seq; // _Vincenzo Librandi_, Dec 11 2025
