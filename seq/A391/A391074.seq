%p A391074 A391074 := proc(n) local f, e, k, t, m, M; f := ifactors(n)[2]; e := map(l -> l[2], f); k := nops(f); M := max(e); t := 0; for m from 2 to M do t := t + 2^nops(select(x -> m <= x, e)) - 1; end do; return NumberTheory:-tau(n) - 2^k - t; end proc: seq(A391074(n), n = 1 .. 88);
