%p A391886 A := proc(n, k) option remember; if n = 0 then 1 elif k = 0 then A(n-1, 0) + A(n-1,1) else A(n-1,k) + (k+1)*A(n-1,k+1) + k*A(n-1,k-1) fi end: seq(print(seq(A(n, k), k = 0..6)), n = 0..6); # _Peter Luschny_, Dec 22 2025
%o A391886 (PARI) A(m, n=m)={my(r=vectorv(m+1), v=vector(n+m+1, k, 1)); r[1] = v[1..n+1];
%o A391886 for(i=1, m, v=vector(#v-1, k, if(k==1, v[1] + v[2], v[k] + k*v[k+1] + (k-1)*v[k-1])); r[1+i] = v[1..n+1]); Mat(r)}
