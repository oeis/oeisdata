%t A391681 pas[y_,k_]:=Table[(-1)^j*k+Sum[(-1)^(i+j)*y[[i]],{i,j}],{j,0,Length[y]}];
%t A391681 cga[m_]:=Select[Table[pas[m,k],{k,0,Max[m]}],Min@@#>0&];
