%o A391245 (PARI)
%o A391245 up_to = 91;
%o A391245 A048720(b, c) = fromdigits(Vec(Pol(binary(b))*Pol(binary(c)))%2, 2);
%o A391245 A065621(n) = bitxor(n-1, n+n-1);
%o A391245 A391245_tr(x, y) = if(x*y == A048720(x, A065621(y)), x*y, 0);
%o A391245 A391245list(up_to) = { my(v = vector(up_to), i=0); for(n=1,oo, for(k=1,n, if(i++ > up_to, return(v)); v[i] = A391245_tr(n,k))); (v); };
%o A391245 v391245 = A391245list(up_to);
%o A391245 A391245(n) = v391245[n];
