%N A391011 Quartic-full (4-full) Achilles numbers.
%t A391011 With[{nn = 2^20}, Rest@ Union@ Flatten@ Table[If[GCD @@ FactorInteger[#][[;;,-1]] == 1, #, Nothing] &[a^7 *b^6 * c^5 * d^4], {d, Surd[nn, 4]}, {c, Surd[nn/(d^4), 5]}, {b, Surd[nn/(c^5 * d^4), 6]}, {a, Surd[nn/(b^6 * c^5 * d^4), 7] } ] ] (* _Michael De Vlieger_, Dec 04 2025 *)
%o A391011 (SageMath)
%o A391011 def isAchilles(n: int, k: int) -> bool:
%o A391011     factors = factor(n)
%o A391011     exps = [e for (_, e) in factors]
%o A391011     if any(e < k for e in exps): return False
%o A391011     return reduce(gcd, exps) == 1
%o A391011 def isA391011(n: int) -> bool: return isAchilles(n, 4)
%o A391011 print([n for n in range(2, 800001) if isA391011(n)])
