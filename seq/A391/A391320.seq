%o A391320 (Python)
%o A391320 from math import isqrt, prod
%o A391320 from sympy import primerange, integer_nthroot, primepi
%o A391320 from oeis_sequences.OEISsequences import bisection
%o A391320 def A391320(n):
%o A391320     def g(x,a,b,c,m): yield from (((d,) for d in enumerate(primerange(b+1,isqrt(x//c)+1),a+1)) if m==2 else (((a2,b2),)+d for a2,b2 in enumerate(primerange(b+1,integer_nthroot(x//c,m)[0]+1),a+1) for d in g(x,a2,b2,c*b2,m-1)))
%o A391320     def f(x): return int(n+x-sum(sum(primepi(isqrt(x)//prod(c[1] for c in a))-a[-1][0] for a in g(isqrt(x),0,1,1,i)) for i in range(3,x.bit_length()>>1)))
%o A391320     return bisection(f,n,n) # _Chai Wah Wu_, Dec 10 2025
