%t A391956 Round[Table[xi = x /. Solve[LaguerreL[n, x] == 0]; wi = Table[xi[[j]]/((n + 1)^2*LaguerreL[n + 1, xi[[j]]]^2), {j, 1, n}]; 1/Product[wi[[j]], {j, 1, n}], {n, 0, 15}]] (* _Vaclav Kotesovec_, Jan 16 2026 *)
