%F A391746 a(1) = A391747(1), and for n > 1, a(n) = a(n-1) + A391747(n).
%o A391746 (PARI)
%o A391746 up_to = 16384;
%o A391746 A195017(n) = { my(f); if(1==n, 0, f=factor(n); sum(i=1, #f~, f[i, 2] * (-1)^(1+primepi(f[i, 1])))); };
%o A391746 A391747(n) = (abs(A195017(n)) <= 1);
%o A391746 A391746_list(up_to_n) = { my(v=vector(up_to_n)); v[1] = A391747(1); for(i=2,#v,v[i] = v[i-1]+A391747(i)); (v); };
%o A391746 v391746 = A391746_list(up_to);
%o A391746 A391746(n) = v391746[n];
