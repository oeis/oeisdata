%e A391501 The a(1) = 11 composites k such that radical(k) = sopfr(k) = A350352(1) = 30 are 2^1*3^1*5^5 = 18750, 2^2*3^2*5^4 = 22500, 2^6*3^1*5^3 = 24000, 2^3*3^3*5^3 = 27000, 2^7*3^2*5^2 = 28800, 2^11*3^1*5^1 = 30720, 2^4*3^4*5^2 = 32400, 2^8*3^3*5^1 = 34560, 2^1*3^6*5^2 = 36450, 2^5*3^5*5^1 = 38880 and 2^2*3^7*5^1 = 43740.
%p A391501 A391501 := proc(n) local m, f, p, r, c, i, s; m := A350352(n); f := ifactors(m)[2]; p := [seq(f[i][1], i = 1 .. nops(f))]; r := nops(p); c := 0; s := proc(i, n) local e; if i = r then if n mod p[r] = 0 and 1 <= n/p[r] then c := c + 1; end if; else for e to floor((n - r + i)/p[i]) do s(i + 1, n - p[i]*e); end do; end if; end proc; s(1, m); c; end proc: seq(A391501(n), n = 1 .. 56);
