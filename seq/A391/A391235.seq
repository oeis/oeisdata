%t A391235 pas[y_,k_]:=Table[(-1)^j*k+Sum[(-1)^(i+j)*y[[i]],{i,j}],{j,0,Length[y]}];
%t A391235 nag[m_]:=Select[Table[pas[m,b],{b,1,Max[m]}],Min@@#>1&]!={};
%t A391235 Table[Length[Select[Join@@Permutations/@IntegerPartitions[n],nag]],{n,10}]
