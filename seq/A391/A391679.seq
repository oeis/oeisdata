%t A391679 pas[y_,k_]:=Table[(-1)^j*k+Sum[(-1)^(i+j)*y[[i]],{i,j}],{j,0,Length[y]}];
%t A391679 cga[m_]:=Select[Table[pas[m,b],{b,0,Max[m]}],Min@@#>=0&];
%t A391679 Table[Length[Select[Join@@Permutations/@IntegerPartitions[n], Min@@#>1&&Length[cga[#]]==0&]],{n,1,10}]
