%t A391629 prix[n_]:=If[n==1,{}, Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A391629 pas[y_,k_]:=Table[(-1)^j*k + Sum[(-1)^(i+j)*y[[i]],{i,j}],{j,0,Length[y]}];
%t A391629 Table[Length[Select[IntegerPartitions[n], Select[Table[pas[#,b],{b,1,Max[#]}], LessEqual@@#&]=={}&]],{n,20}]
