%H A391257 Antti Karttunen, <a href="/A391257/b391257.txt">Table of n, a(n) for n = 1..10000</a>
%o A391257 (PARI)
%o A391257 default(parisizemax,2^29);
%o A391257 memo_for_ps = Map();
%o A391257 ps(n) = if(n<2, n, my(v); if(mapisdefined(memo_for_ps,n,&v), v, v = if(n%2, ps(n\2)+ps(n\2+1), 'x*ps(n\2)); mapput(memo_for_ps,n,v); (v)));
%o A391257 p2r(p) = { my(v=Vecrev(Vec(p))); prod(i=1,#v,prime(i)^v[i]); };
%o A391257 is_A391257(n) = if(!(n%2) || 2!=bigomega(n), 0, my(f=factor(n), a = f[1,1], b = f[#f~,1], Pa = ps(a), Pb = if(b==a,Pa,ps(b)), Pn = ps(n)); (0==lift(Pn % Pa) || 0==lift(Pn % Pb)));
