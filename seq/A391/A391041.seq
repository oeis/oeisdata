%t A391041 A391041row[n_] := With[{m = 4*n}, Select[Prime[Range[2, PrimePi[m + 1]]], Divisible[m, # - 1] && CoprimeQ[#, m/(# - 1)] &]];
%t A391041 Array[A391041row, 25] (* _Paolo Xausa_, Nov 26 2025 *)
%o A391041 (PARI) row(n) = my(list=List()); forprime(p=3, 4*n+1, if (!((4*n) % (p-1)) && (gcd(p, 4*n/(p-1))==1), listput(list, p))); Vec(list);
