%o A391729 A003817(n) = (1<<(log(2*n+1)\log(2)))-1;
%o A391729 A006068(n) = { my(s=1, ns); while(1, ns = n >> s; if(0==ns, return(n)); n = bitxor(n, ns); s <<= 1); };
