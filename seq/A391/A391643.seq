%t A391643 pas[y_,k_]:=Table[(-1)^j*k+Sum[(-1)^(i+j)*y[[i]],{i,j}],{j,0,Length[y]}];
%t A391643 aga[m_]:=Select[Table[pas[m,b],{b,0,Max[m]}],Min@@#>=0&];
%t A391643 Table[Length[Select[Join@@Permutations/@IntegerPartitions[n],Length[aga[#]]==1&]],{n,1,10}]
