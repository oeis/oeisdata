%t A391981 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A391981 pas[y_,k_]:=Table[(-1)^j*k+Sum[(-1)^(i-j)*y[[i]],{i,j}],{j,0,Length[y]}];
