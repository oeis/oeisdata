%t A391754 rad[x_] := Times @@ FactorInteger[x][[All, 1]]; Table[s = Tuples[Range[b - 1], w]; c = 0; k = 1; m = (b - 1)^w; Until[k == m, If[And[UnsameQ @@ #, SameQ @@ Map[rad@ FromDigits[#, b] &, #]], c++] &@ NestList[RotateLeft, s[[k]], w - 1]; k++]; c/w, {b, 3, 50}]
