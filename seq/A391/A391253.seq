%o A391253 (PARI)
%o A391253 A280500sq(a, b) = { my(Pa=Pol(binary(a))*Mod(1, 2), Pb=Pol(binary(b))*Mod(1, 2)); if(0!=lift(Pa % Pb), 0, fromdigits(Vec(lift(Pa/Pb)), 2)); };
%o A391253 is_A391253(n) = if(!(n%2) || 2!=bigomega(n), 0, my(f=factor(n), a = f[1,1], b = f[#f~,1]); (0!=A280500sq(n,a) || 0!=A280500sq(n,b)));
