%o A391155 (PARI)
%o A391155 permcount(v) = {my(m=1, s=0, k=0, t); for(i=1, #v, t=v[i]; k=if(i>1&&t==v[i-1], k+1, 1); m*=t*k; s+=t); s!/m}
%o A391155 C(d,r) = {sum(i=1, #r, my(t=r[i]); if(d%t==0, t))}
%o A391155 D(u,v,r) = {prod(i=1, #u, prod(j=1, #v, my(g=gcd(u[i], v[j]));  C(u[i]*v[j]/g, r)^g))}
%o A391155 E(v,r) = {prod(i=2, #v, prod(j=1, i-1, my(g=gcd(v[i], v[j])); C(v[i]*v[j]/g, r)^g)) * prod(i=1, #v, my(t=v[i]); C(t,r)^((t+1)\2)*if(t%2, 1, C(t/2,r)))}
%o A391155 row(n) = {Vecrev(sum(k=0, n, my(s=0); forpart(p=k, forpart(q=n-k, my(r=concat(p,q)); s += prod(i=1, #p, 'x^p[i] - 1) * permcount(p) * permcount(q) * D(q,r,r) * E(p,r) )); s/(k!*(n-k)!)))}
