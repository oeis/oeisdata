%p A391003 issum:= proc(n,S) option remember; local m, Sp;
%p A391003   Sp:= select(`<=`,S,n);
%p A391003   if n > convert(Sp,`+`) then return false fi;
%p A391003   if member(n,Sp) then return true fi;
%p A391003   m:= max(Sp); Sp:= Sp minus {m};
%p A391003   procname(n-m,Sp) or procname(n,Sp)
%p A391003 end proc:
%p A391003 filter:= proc(n)
%p A391003   local S;
%p A391003   S:= numtheory:-divisors(n+1) minus {n+1};
%p A391003   issum(n,S) and issum(n+1,S)
%p A391003 end proc:
%p A391003 select(filter, [seq(ithprime(i),i=1..200)]); # _Robert Israel_, Nov 26 2025
%t A391003 pseudoPerfectQ[k_] := Module[{divs = Most[Divisors[k]]}, MemberQ[Total/@Subsets[ divs, Length[ divs]], k]];pp2Q[k_]:= Module[{divs = Most[Divisors[k+1]]}, MemberQ[Total/@Subsets[ divs, Length[ divs]], k]]; Select[Prime[Range[70]], pseudoPerfectQ[#+1]&&pp2Q[#]&] (* _James C. McMahon_, Nov 26 2025 *)
