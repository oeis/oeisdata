%t A391463 PellP[n_]:=Fibonacci[n, 2];
%t A391463 Table[If[n==0, 1, Sum[k *(k+1)*PellP[k+2]* Binomial[3 n, n-k], {k, 1, n}]/(2*n)], {n, 0, 20}] (* _Vincenzo Librandi_, Dec 11 2025 *)
%o A391463 (PARI) pell(n) = ([2, 1; 1, 0]^n)[2, 1];
%o A391463 a(n) = if(n==0, 1, sum(k=1, n, k*(k+1)*pell(k+2)*binomial(3*n, n-k))/(2*n));
%o A391463 (Magma) M := Matrix(IntegerRing(), 2, 2, [2,1,1,0]);
%o A391463 a := function(n)
%o A391463     if n eq 0 then
%o A391463         return 1;
%o A391463     end if;
%o A391463     S := 0;
%o A391463     for k in [1..n] do
%o A391463         S +:= k*(k+1)*(M^(k+2))[2,1] * Binomial(3*n, n-k);
%o A391463     end for;
%o A391463     return S div (2*n);
%o A391463 end function;
%o A391463 seq := [ a(n) : n in [0..25] ];
