%t A391753 rad[x_] := Times @@ FactorInteger[x][[All, 1]]; Table[s = Join @@ Table[{j, k}, {j, b - 1}, {k, j + 1, b - 1}]; c = 0; k = 1; m = Length[s] + 1; Until[k == m, c += Boole[rad[FromDigits[s[[k]], b] ] == rad[FromDigits[Reverse[s[[k]] ], b] ] ]; k++]; c, {b, 3, 100}]
