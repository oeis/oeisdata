%F A391734 Apparently, for n >= 3, a(n) = A245458(n) - 2.
%o A391734 (PARI)
%o A391734 A002110(n) = prod(i=1,n,prime(i));
%o A391734 A079276(n) = lift(prod(i=1,n-1,Mod(1/prime(i), prime(n))));
%o A391734 A391734(n) = (A002110(n-1) * A079276(n));
