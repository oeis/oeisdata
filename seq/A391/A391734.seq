%o A391734 A240673_tr(n, k) = { my(val = prod(j=1, n, prime(j))/prime(k)); val * lift(1/Mod(val, prime(k))); }; \\ The function "T" in A240673.
%o A391734 A391734(n) = A240673_tr(n, n);
