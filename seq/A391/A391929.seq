%e A391929 On row 10 the fixed points are: 0, 75741330, 92099070, 869259720, 1306416510, 1400639309, 1568479710, 2194157699, 2631314489, 2782797150, 2799154890, 3670538339, 3686896079, 3838378740, 4275535530, 4901213519, 5069053920, 5163276719, 5600433509, 6377594159, 6393951899, 6469693229, therefore a(10) = 22.
%o A391929 (PARI)
%o A391929 \\ Slow program:
%o A391929 A002110(n) = prod(i=1,n,prime(i));
%o A391929 is_fixed_point_of_A391930_row(n, k) = { my(orgk=k); for(i=1,n,if(((orgk-k)%prime(i))!=0, return(0)); k \= prime(i)); (1); };
%o A391929 A391929(n) = sum(k=0, A002110(n)-1, is_fixed_point_of_A391930_row(n, k));
%o A391929 (PARI)
%o A391929 \\ Use this for obtaining the actual fixed points:
%o A391929 memo_fp_lists = Map();
%o A391929 A002110(n) = prod(i=1,n,prime(i));
%o A391929 A391930_fp_lists(n) = { my(v); if(mapisdefined(memo_fp_lists,n,&v), v, v = if(!n, [0], my(prev_fps = A391930_fp_lists(n-1), new_fps = List([]), w = A002110(n-1)); for(i=1,#prev_fps,if((w%prime(n))>1, listput(new_fps,prev_fps[i] + w*lift(Mod(prev_fps[i]/(1-w), prime(n)))), for(d=0,prime(n)-1,if(d==((prev_fps[i]+(d*w))%prime(n)), listput(new_fps,(prev_fps[i]+(d*w))))))); Vec(new_fps)); mapput(memo_fp_lists,n,v); (v)); };
%o A391929 A391929(n) = #A391930_fp_lists(n);
%o A391929 (PARI)
%o A391929 \\ And this for computing just their count:
%o A391929 A002110(n) = prod(i=1,n,prime(i));
%o A391929 A391929(n) = if(n<2, 1+n, if(!((A002110(n-1)-1)%prime(n)), 2*prime(n), A391929(n-1)));
