%o A391844 (PARI)
%o A391844 A003415(n) = if(n<=1, 0, my(f=factor(n)); n*sum(i=1, #f~, f[i, 2]/f[i, 1]));
%o A391844 A020639(n) = if(1==n,n,vecmin(factor(n)[, 1]));
%o A391844 A053669(n) = forprime(p=2, , if(n%p, return(p))); \\ From A053669
