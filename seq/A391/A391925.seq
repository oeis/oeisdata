%o A391925 A048720(b, c) = fromdigits(Vec(Pol(binary(b))*Pol(binary(c)))%2, 2);
%o A391925 A391925_sq(n,k) = for(i=1,oo, my(Pni=Pol(binary(n*i))*Mod(1, 2), P_i=Pol(binary(i))*Mod(1, 2)); if(0==lift(Pni % P_i), if(k>1, k--, return(i))));
