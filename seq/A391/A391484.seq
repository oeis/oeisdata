%o A391484 EulerT(v)={Vec(exp(x*Ser(dirmul(v,vector(#v,n,1/n))))-1, -#v)}
%o A391484 T(n) = { my(M=matrix(n,n));
%o A391484   for(r=1, n, M[r,1]=1; for(k=1, r-1, my(j=r+1-k); M[k,j] = M[k,j-1] + EulerT(vector(j,i,if(i==j,0,M[i,k]-if(i>1,M[i-1,k]))))[j] ));
%o A391484   vector(n,r,vector(r,k,my(j=r+1-k); M[j,k] + M[k,j] - 1))
%o A391484 }
