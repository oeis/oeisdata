%p A391316 A391316 := proc(n) local b, d, i, p; d := A350352(n); p := [seq(op(1, ifactors(d)[2][i]), i = 1 .. nops(ifactors(d)[2]))]; b := proc(s, q) local i, j, v, t, c; option remember; t := nops(q); if t = 1 then if irem(s, q[1]) = 0 then return q[1]^(s/q[1]); else return infinity; end if; end if; c := infinity; for i while q[1]*i <= s - add(q[j], j = 2 .. t) do v := q[1]^i*b(s - q[1]*i, [op(q[2 .. t])]); if v < c then c := v; end if; end do; return c; end proc; b(d, p); end proc: seq(A391316(n), n = 1 .. 20);
