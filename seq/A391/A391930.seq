%T A391930 7,14,23,24,3,10,13,20,29,0,39,76,109,146,155,186,15,52,85,92,131,162,
%U A391930 201,28,31,68,107,138,177,184,7,44,83,114,123,160,193,20,59,60,99,136,169,206,5,36,75,112,145,152,191
%N A391930 Irregular triangle T(n, k) = Sum_{i=1..n} (k mod prime(i))*A002110(i-1), with n >= 1, and 1 <= k <= A002110(n), read by rows.
%C A391930 Each row of length A002110(n) lists a permutation of integers in range 0 .. A002110(n)-1, where A002110(n) is the n-th primorial number. See Schmitt paper.
%H A391930 Antti Karttunen, <a href="/A391930/b391930.txt">Table of n, a(n) for n = 1..32588; rows 1-6 of the triangle</a>
%H A391930 Matthias Schmitt, <a href="https://arxiv.org/abs/1404.0706">A function to calculate all relative prime numbers up to the product of the first n primes</a>, arXiv:1404.0706 [math.NT] (see pages 3-4).
%H A391930 <a href="/index/Pri#primorial_numbers">Index entries for sequences related to primorial numbers</a>.
%F A391930 a(n) = A276085(A391933(n)).
%e A391930 The table begin as:
%e A391930 1 |  1, 0;
%e A391930 2 |  3, 4, 1, 2, 5, 0;
%e A391930 3 |  9, 16, 19, 26, 5, 6, 15, 22, 25, 2, 11, 12, 21, 28, 1, 8, 17, 18, 27, 4, 7, 14, 23, 24, 3, 10, 13, 20, 29, 0;
%e A391930 4 | 39, 76, 109, 146, 155, 186, 15, 52, 85, 92, 131, 162, 201, 28, 31, 68, 107, 138, 177, 184, 7, 44, 83, 114, 123, ...
%o A391930 (PARI)
%o A391930 n_rows = 4;
%o A391930 A002110(n) = prod(i=1,n,prime(i));
%o A391930 A143293(n) = { if(n==0, return(1)); my(P=1, s=1); forprime(p=2, prime(n), s+=P*=p); s; };
%o A391930 A391930_list(n_rows) = { my(v = vector(A143293(n_rows)-1), i=0); for(row=1,n_rows, for(t=1, A002110(row), i++; v[i] = sum(k=1,row,((t % prime(k)) * A002110(k-1))))); (v); };
%o A391930 v391930 = A391930_list(n_rows);
%o A391930 A391930(n) = v391930[n];
%Y A391930 Cf. A002110 (row lengths), A143293 (the left edge of triangle), A276085, A391931 (permutation of natural numbers constructed from this table), A391933 [= A276086(a(n))].
%Y A391930 Cf. also A240673, A360407.
