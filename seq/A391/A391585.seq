%H A391585 <a href="/index/Ca#CARRYLESS">Index entries for sequences related to carryless arithmetic</a>.
%o A391585 (PARI) is_A391585(k) = (!k || (0==lift(Pol(binary(3*k)*Mod(1, 2)) % Pol(binary(k))*Mod(1, 2))));
%o A391585 (PARI)
%o A391585 up_to = 10001;
%o A391585 A391585list(up_to_n) = { my(v=vector(up_to_n), k=1, i=0); v[1] = 0; while(k<#v, i++; my(P3i=Pol(binary(3*i))*Mod(1, 2), P_i=Pol(binary(i))*Mod(1, 2)); if(0==lift(P3i % P_i), k++; v[k] = i)); (v); };
%o A391585 v391585 = A391585list(up_to);
%o A391585 A391585(n) = v391585[n];
