%T A391477 14717,18149,-1,149873,-1,-1,782147,-1,165541,336263
%N A391477 a(n) is the least prime that starts a sequence of consecutive primes whose sum is n^5, or -1 if there is no such prime.
%F A391477 a(n) = A390497(k) if n^5 = A050936(k), otherwise a(n) = -1.
%e A391477 a(3) = 41 because 3^5 = 243 is the sum of consecutive primes 41+43+47+53+59.
%e A391477 For n = 22 there are two ways to obtain 22^5 = 5153632, as the sum of the 318 consecutive primes from 14717 to 17761 and as the sum of the two consecutive primes 2576801 and 2576831, so a(22) = 14717.
%p A391477 N:= nextprime(30^5/2):
%p A391477 P:= select(isprime,[2,seq(i,i=3..N,2)]):
%p A391477 SP:= ListTools:-PartialSums([0,op(P)]): nSP:= nops(SP):
%p A391477 Res:= Vector(30,-1):
%p A391477 for n from 2 to 30 do
%p A391477   x:= n^5;
%p A391477   for i from 1 to nSP - 2 do
%p A391477     t:= SP[i] + x;
%p A391477     if t < SP[i+2] then break fi;
%p A391477     j:= ListTools:-BinarySearch(SP,t);
%p A391477     if j >= i+2 then Res[n]:= P[i]; break fi
%p A391477   od;
%p A391477 od:
%p A391477 convert(V,list);
%Y A391477 Cf. A050936, A390497.
%K A391477 sign,more
%O A391477 1,3
