%p A066470 A066470:= proc(q)
%p A066470 local k,n,t;
%p A066470 for n from 1 to q do
%p A066470 t:=0; for k from 2 to n-1 do if abs((n mod k)-k/2)<1 then t:=t+1; fi; od;
%p A066470 if t=5 then print(n); fi; od; end:
%p A066470 A066470(10^10); # _Paolo P. Lava_, Feb 22 2013
%t A066470 antid[n_] := Select[ Union[ Join[ Select[ Divisors[2n - 1], OddQ[ # ] && # != 1 & ], Select[ Divisors[2n + 1], OddQ[ # ] && # != 1 & ], 2n/Select[ Divisors[ 2*n], OddQ[ # ] && # != 1 &]]] }, # < n & ]]; Select[ Range[10^4], Length[ antid[ # ]] == 5 & ]
