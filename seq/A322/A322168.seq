%e A322168 row 1: 1/1
%e A322168 row 2: 1/2, 2/1
%e A322168 row 3: 1/3, 2/3, 3/2, 3/1
%e A322168 ...
%t A322168 row[n_] := Module[{v = Table[0, {2^(n-1)}], L = {{1, 0}, {1, 1}}}, For[k = 0, k <= Length[v]-1, k++, v[[k+1]] = Tr[Dot @@ Table[If[BitGet[k, b] == 1, Transpose[L], L], {b, 0, n-2}]]]; v]; row[1] = {2};
