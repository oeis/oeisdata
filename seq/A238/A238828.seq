%p A238828 g:=proc(n) option remember; local t1;
%p A238828 t1:=[2,3,6,14,34,84,208,515];
%p A238828 if n <= 7 then t1[n] else
%p A238828 3*g(n-1)-4*g(n-3)+g(n-4)+g(n-5)+3*g(n-6)-g(n-7); fi; end proc;
