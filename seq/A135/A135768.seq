%o A135768 (PARI) P(n)=n*(3*n-1)/2
%o A135768 isPent(t)=P(sqrtint((t*2)\3)+1)==t
%o A135768 for( i=1,999,for( j=1,(P(i)-1)\3, isPent(P(i)+P(j))&next(2)); print1(i","))
