%U A033621 803309,1027202,1064602,1215122,1252522,1403042,80399309,111222112,112060212,146979642
%N A033621 Numbers k such that k-1 is a palindrome in base 10, and k+1 is a palindrome in base 17.
%H A033621 Robert Israel, <a href="/A033621/b033621.txt">Table of n, a(n) for n = 1..61</a>
%e A033621 a(12) = 89 is a term because 89-1 = 88 is a palindrome in base 10, while 89+1 = 55_17 is a palindrome in base 17. - _Robert Israel_, Jan 14 2026
%p A033621 digrev:= proc(n,b) local L,i;
%p A033621   L:= convert(n,base,b);
%p A033621   add(L[-i]*b^(i-1),i=1..nops(L))
%p A033621 end proc:
%p A033621 b1:= 10: b2:= 17:
%p A033621 R:= $1..10,12: count:= 12:
%p A033621 for d from 2 while count < 50 do
%p A033621   if d::even then
%p A033621     for x from b2^(d/2-1) to b2^(d/2)-1 while count < 50 do
%p A033621       m:= x*b2^(d/2)+digrev(x,b2);
%p A033621       if digrev(m-2,b1) = m-2 then
%p A033621          R:= R, m-1; count:= count+1;
%p A033621       fi
%p A033621     od
%p A033621   else
%p A033621     for x from b2^((d-3)/2) to b2^((d-1)/2)-1 while count < 50 do
%p A033621       for y from 0 to b2-1 do
%p A033621         m:= x*b2^((d+1)/2)+y*b2^((d-1)/2) + digrev(x,b2);
%p A033621         if digrev(m-2,b1) = m-2 then
%p A033621           R:= R, m-1; count:= count+1;
%p A033621         fi
%p A033621     od od
%p A033621   fi
%p A033621 od:
%p A033621 R; # _Robert Israel_, Jan 14 2026
%o A033621 (PARI) isok(m) = my(da=digits(m-1, 10), db=digits(m+1, 17)); da==Vecrev(da) && db==Vecrev(db); \\ _Michel Marcus_, Jul 09 2020
