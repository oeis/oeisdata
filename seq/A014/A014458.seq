%t A014458 terms = 77; s = Normal[EllipticTheta[3, 0, q]^2*EllipticTheta[3, 0, q^3] + O[q]^(3*terms)][[1 ;; 2 terms]]; Partition[CoefficientList[s, q], 2][[All, 1]][[1 ;; terms]] (* _Jean-Fran√ßois Alcover_, Jul 04 2017 *)
