%e A014486    10: (oo)
%e A014486    12: ((o))
%e A014486    42: (ooo)
%e A014486    44: (o(o))
%e A014486    50: ((o)o)
%e A014486    52: ((oo))
%e A014486    56: (((o)))
%e A014486   170: (oooo)
%e A014486   172: (oo(o))
%e A014486   178: (o(o)o)
%e A014486   180: (o(oo))
%e A014486   184: (o((o)))
