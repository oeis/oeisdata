%N A014424 From table of maximal epacts e(p) and corresponding primes p, for x_1=2, x_{m+1} = (x_m)^2+1; sequence gives p.
%D A014424 R. K. Guy, How to factor a number, Proc. 5th Manitoba Conf. Numerical Math., Congress. Num. 16 (1975), 49-89.
%H A014424 Vincenzo Librandi, <a href="/A014424/b014424.txt">Table of n, a(n) for n = 1..62</a>
%t A014424 RecordPrimesUpTo[N_]:=Module[{rec={},maxe=0},Do[seq={2};e=0;
%t A014424 For[k=1,k<=p+1,k++,While[Length[seq]<2 k,AppendTo[seq,Mod[Last[seq]^2+1,p]]];
%t A014424 If[seq[[k]]===seq[[2 k]],e=k;Break[]]];
%t A014424 If[e>maxe,AppendTo[rec,p];maxe=e],{p,Prime/@Range[PrimePi[N]]}];rec];
%t A014424 RecordPrimesUpTo[100000] (* _Vincenzo Librandi_, Nov 17 2025 *)
%o A014424 (Magma) function RecordPrimesUpTo(N)
%o A014424     rec := []; maxe := 0;
%o A014424     for p in PrimesUpTo(N) do
%o A014424         seq := [2 mod p]; e := 0;
%o A014424         for k in [1..p+1] do
%o A014424             while #seq lt 2*k do Append(~seq, (seq[#seq]^2 + 1) mod p); end while;
%o A014424             if seq[k] eq seq[2*k] then e := k; break; end if;
%o A014424         end for;
%o A014424         if e gt maxe then Append(~rec, p); maxe := e; end if;
%o A014424     end for;
%o A014424     return rec;
%o A014424 end function;
%o A014424 RecordPrimesUpTo(90000); // _Vincenzo Librandi_, Nov 17 2025
