%t A014603 Union[ (-NumberFieldDiscriminant[ Sqrt[-#]] &) /@ Select[ Range[500], NumberFieldClassNumber[ Sqrt[-#]] == 2 &]] (* _Jean-François Alcover_, Jan 04 2012 *)
