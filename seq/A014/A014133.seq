%p A014133 isA014133 := proc(n)
%p A014133     local c,t ;
%p A014133     for c from 0 do
%p A014133         t := c*(c+1)/2 ;
%p A014133         if t > n then
%p A014133             return false;
%p A014133         end if;
%p A014133         if issqr(n-t) then
%p A014133             return true;
%p A014133         end if;
%p A014133     end do:
%p A014133 end proc:
%p A014133 for n from 0 to 100 do
%p A014133     if isA014133(n) then
%p A014133         printf("%d,",n) ;
%p A014133     end if;
%p A014133 end do: # _R. J. Mathar_, Oct 11 2014
%p A014133 # second Maple program:
%p A014133 q:= n-> ormap(x-> issqr(8*x+1), [n-i^2$i=0..isqrt(n)]):
%p A014133 select(q, [$0..100])[];  # _Alois P. Heinz_, Jan 14 2026
%t A014133 With[{nn=20},Select[Union[Flatten[Outer[Plus,Range[0,nn]^2,Accumulate[ Range[ 0,nn]]]]], #<=(nn(nn+1))/2&]] (* _Harvey P. Dale_, Dec 29 2019 *)
%t A014133 fQ[n_] := Block[{s = t = 0, lmt = 1 + Floor[(Sqrt[ 8n +1] -1)/2]}, While[t < lmt, If[ Mod[ Sqrt[n -t (t +1)/2], 1] == 0, s = 1; t = lmt]; t++]; s == 1]; Select[Range[0, 92], fQ] (* _Robert G. Wilson v_, Jan 14 2026 *)
