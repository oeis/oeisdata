%F A324306 G.f.: Sum_{n>=0} (2^n + 1)^n * x^n / (1 + 2^n*x)^(n+1).
%F A324306 G.f.: Sum_{n>=0} (2^n - 1)^n * x^n / (1 - 2^n*x)^(n+1).
%F A324306 a(n) = Sum_{k=0..n} binomial(n,k) * (2^n - 2^k)^(n-k).
%F A324306 a(n) = Sum_{k=0..n} (-1)^k * binomial(n,k) * (2^n + 2^k)^(n-k).
%F A324306 a(n) ~ 2^(n^2). - _Vaclav Kotesovec_, Mar 13 2019
%e A324306 G.f.: A(x) = 1 + 2*x + 14*x^2 + 464*x^3 + 62498*x^4 + 32904512*x^5 + 68218246274*x^6 + 561509337384704*x^7 + 18430991691638470658*x^8 + ...
%e A324306 such that
%e A324306 A(x) = 1/(1+x) + (2 + 1)*x/(1 + 2*x)^2 + (2^2 + 1)^2*x^2/(1 + 2^2*x)^3 + (2^3 + 1)^3*x^3/(1 + 2^3*x)^4 + (2^4 + 1)^4*x^4/(1 + 2^4*x)^5 + (2^5 + 1)^5*x^5/(1 + 2^5*x)^6 + (2^6 + 1)^6*x^6/(1 + 2^6*x)^7 + ...
%e A324306 Also,
%e A324306 A(x) = 1/(1-x) + (2 - 1)*x/(1 - 2*x)^2 + (2^2 - 1)^2*x^2/(1 - 2^2*x)^3 + (2^3 - 1)^3*x^3/(1 - 2^3*x)^4 + (2^4 - 1)^4*x^4/(1 - 2^4*x)^5 + (2^5 - 1)^5*x^5/(1 - 2^5*x)^6 + (2^6 - 1)^6*x^6/(1 - 2^6*x)^7 + ...
%e A324306 Explicitly,
%e A324306 A(x) = 1/(1+x) + 3*x/(1 + 2*x)^2 + 5^2*x^2/(1 + 2^2*x)^3 + 9^3*x^3/(1 + 2^3*x)^4 + 17^4*x^4/(1 + 2^4*x)^5 + 33^5*x^5/(1 + 2^5*x)^6 + 65^6*x^6/(1 + 2^6*x)^7 + ...
%e A324306 and
%e A324306 A(x) = 1/(1-x) + x/(1 - 2*x)^2 + 3^2*x^2/(1 - 2^2*x)^3 + 7^3*x^3/(1 - 2^3*x)^4 + 15^4*x^4/(1 - 2^4*x)^5 + 31^5*x^5/(1 - 2^5*x)^6 + 63^6*x^6/(1 - 2^6*x)^7 + ...
%t A324306 Table[1 + Sum[Binomial[n, k]*(2^n - 2^k)^(n - k), {k, 0, n-1}], {n, 0, 15}] (* _Vaclav Kotesovec_, Mar 13 2019 *)
%o A324306 (PARI) {a(n) = my(A = sum(m=0, n+1, (2^m + 1)^m*x^m/(1 + 2^m*x +x*O(x^n) )^(m+1) )); polcoeff(A, n)}
%o A324306 for(n=0, 20, print1(a(n), ", "))
%o A324306 (PARI) {a(n) = my(A = sum(m=0, n+1, (2^m - 1)^m*x^m/(1 - 2^m*x +x*O(x^n) )^(m+1) )); polcoeff(A, n)}
%o A324306 for(n=0, 20, print1(a(n), ", "))
%o A324306 (PARI) {a(n) = sum(k=0,n, binomial(n,k) * (2^n - 2^k)^(n-k) )}
%o A324306 for(n=0, 20, print1(a(n), ", "))
%o A324306 (PARI) {a(n) = sum(k=0,n, (-1)^k * binomial(n,k) * (2^n + 2^k)^(n-k) )}
%o A324306 for(n=0, 20, print1(a(n), ", "))
