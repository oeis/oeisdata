%p A182931 T := (n,k) -> n!*coeff(series(egf(k),x,n+1),x,n):
%p A182931 seq(print(seq(T(n,k),k=1..8)),n=0..8);
%t A182931 egf[k_] := Exp[Exp[x] (1 - Gamma[k, x]/Gamma[k])];
%t A182931 T[n_, k_] := n! SeriesCoefficient[egf[k], {x, 0, n}];
