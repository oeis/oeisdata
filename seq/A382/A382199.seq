%o A382199 (PARI) isok(k) = if (isprime(k), my(d=Set(digits(k))); for (i=1, #d, if (!isprime(2*k*d[i]+1), return(0))); return(1)); \\ _Michel Marcus_, Mar 18 2025
%o A382199 (Python)
%o A382199 from gmpy2 import is_prime, digits
%o A382199 from itertools import count, product
%o A382199 def ok(n): return is_prime(n) and all(is_prime(2*n*d+1) for d in map(int, set(digits(n))))
%o A382199 def agen(): # generator of terms
%o A382199     yield from (k for d in count(1) for p in product("123456789", repeat=d-1) for e in "1379" if ok(int("".join(p)+e)))
%o A382199 print([k for k in range(10**7) if ok(k)]) # _Michael S. Branicky_, Nov 02 2025
