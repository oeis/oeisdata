%U A270277 2055,-2007,2415,-2351,2851,-2859,3407,-3375,3843,-3763,4287,-4235,4823,-4819
%N A270277 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 137", based on the 5-celled von Neumann neighborhood.
%C A270277 Initialized with a single black (ON) cell at stage zero.
%D A270277 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270277 Robert Price, <a href="/A270277/b270277.txt">Table of n, a(n) for n = 0..127</a>
%H A270277 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270277 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270277 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270277 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270277 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270277 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270277 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270277 code=137; stages=128;
%t A270277 rule=IntegerDigits[code,2,10];
%t A270277 g=2*stages+1; (* Maximum size of grid *)
%t A270277 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270277 ca=a;
%t A270277 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270277 PrependTo[ca,a];
%t A270277 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270277 k=(Length[ca[[1]]]+1)/2;
%t A270277 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270277 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270277 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A270277 Cf. A270274.
