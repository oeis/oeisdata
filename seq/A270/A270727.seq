%S A270727 1,6,11,44,57,150,183,360,389,690,743,1180,1253,1846,2003,2780,2909,
%T A270727 3934,4099,5388,5597,7198,7407,9280,9557,11778,12119,14656,15085,
%U A270727 17982,18595,21804,22453,26146,26887,31104,31925,36750,37499,42864,43665,49658,50431
%N A270727 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 203", based on the 5-celled von Neumann neighborhood.
%C A270727 Initialized with a single black (ON) cell at stage zero.
%D A270727 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270727 Robert Price, <a href="/A270727/b270727.txt">Table of n, a(n) for n = 0..128</a>
%H A270727 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270727 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270727 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270727 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270727 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270727 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270727 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270727 code=203; stages=128;
%t A270727 rule=IntegerDigits[code,2,10];
%t A270727 g=2*stages+1; (* Maximum size of grid *)
%t A270727 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270727 ca=a;
%t A270727 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270727 PrependTo[ca,a];
%t A270727 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270727 k=(Length[ca[[1]]]+1)/2;
%t A270727 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270727 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270727 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
%Y A270727 Cf. A270725.
