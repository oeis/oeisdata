%S A270678 1,8,44,212,940,3857,15648,62928,251952,1005696,4021440,16072420,
%T A270678 64261572,256960036,1027642244,4110139460
%N A270678 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 189", based on the 5-celled von Neumann neighborhood.
%C A270678 Initialized with a single black (ON) cell at stage zero.
%D A270678 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270678 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270678 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270678 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270678 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270678 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270678 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270678 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270678 code=189; stages=128;
%t A270678 rule=IntegerDigits[code,2,10];
%t A270678 g=2*stages+1; (* Maximum size of grid *)
%t A270678 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270678 ca=a;
%t A270678 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270678 PrependTo[ca,a];
%t A270678 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270678 k=(Length[ca[[1]]]+1)/2;
%t A270678 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270678 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270678 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270678 Cf. A270677.
%K A270678 nonn,more
