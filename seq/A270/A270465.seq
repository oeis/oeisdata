%S A270465 1,8,4,40,17,112,20,180,61,324,41,436,116,644,73,820,193,1084,120,
%T A270465 1308,280,1628,148,1932,360,2280,236,2620,512,3040,276,3452,616,3988,
%U A270465 428,4200,952,4836,480,5568,912,6112,604,6592,1220,7248,656,8144,1232,8676
%N A270465 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 173", based on the 5-celled von Neumann neighborhood.
%C A270465 Initialized with a single black (ON) cell at stage zero.
%D A270465 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270465 Robert Price, <a href="/A270465/b270465.txt">Table of n, a(n) for n = 0..128</a>
%H A270465 Robert Price, <a href="/A270465/a270465.tmp.txt">Diagrams of the first 20 stages.</a>
%H A270465 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270465 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270465 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270465 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270465 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270465 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270465 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270465 code=173; stages=128;
%t A270465 rule=IntegerDigits[code,2,10];
%t A270465 g=2*stages+1; (* Maximum size of grid *)
%t A270465 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270465 ca=a;
%t A270465 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270465 PrependTo[ca,a];
%t A270465 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270465 k=(Length[ca[[1]]]+1)/2;
%t A270465 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270465 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A270465 nonn,easy
%O A270465 0,2
