%S A270219 1,5,10,38,47,131,152,292,317,577,614,994,1043,1575,1660,2280,2369,
%T A270219 3237,3338,4454,4567,5963,6112,7724,7885,9841,10038,12274,12507,15055,
%U A270219 15396,18000,18345,21453,21810,25422,25791,29939,30344,34964,35381,40601,41054
%N A270219 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 129", based on the 5-celled von Neumann neighborhood.
%C A270219 Initialized with a single black (ON) cell at stage zero.
%D A270219 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270219 Robert Price, <a href="/A270219/b270219.txt">Table of n, a(n) for n = 0..128</a>
%H A270219 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270219 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270219 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270219 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270219 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270219 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270219 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270219 code=129; stages=128;
%t A270219 rule=IntegerDigits[code,2,10];
%t A270219 g=2*stages+1; (* Maximum size of grid *)
%t A270219 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270219 ca=a;
%t A270219 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270219 PrependTo[ca,a];
%t A270219 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270219 k=(Length[ca[[1]]]+1)/2;
%t A270219 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270219 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270219 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
%Y A270219 Cf. A270217.
