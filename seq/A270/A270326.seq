%I A270326 #10 Nov 05 2025 15:22:32
%S A270326 1,5,37,185,785,3245,12853,50405,200077,796133,3177405,12692789,
%T A270326 50748197,202930829,811604581,3246194965
%N A270326 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 155", based on the 5-celled von Neumann neighborhood.
%C A270326 Initialized with a single black (ON) cell at stage zero.
%D A270326 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270326 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270326 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270326 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270326 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270326 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270326 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270326 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270326 code=155; stages=128;
%t A270326 rule=IntegerDigits[code,2,10];
%t A270326 g=2*stages+1; (* Maximum size of grid *)
%t A270326 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270326 ca=a;
%t A270326 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270326 PrependTo[ca,a];
%t A270326 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270326 k=(Length[ca[[1]]]+1)/2;
%t A270326 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270326 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270326 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270326 Cf. A270325.
%K A270326 nonn,more
%O A270326 0,2
