%F A270194 a(1) = 1, a(2n) = A269389(1+a(n)), a(2n+1) = A269399(a(n)).
%o A270194 (Scheme) ;; With memoization-macro definec.
%o A270194 (definec (A270194 n) (cond ((= 1 n) n) ((even? n) (A269389 (+ 1 (A270194 (/ n 2))))) (else (A269399 (A270194 (/ (- n 1) 2))))))
