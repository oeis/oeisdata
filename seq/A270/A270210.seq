%S A270210 1,5,5,41,12,109,16,209,20,337,28,501,28,689,52,913,60,1161,68,1457,
%T A270210 56,1757,120,2085,168,2429,216,2841,224,3249,312,3697,324,4193,332,
%U A270210 4753,296,5277,436,5821,564,6361,668,6997,712,7613,852,8257,932,8945,972
%N A270210 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 123", based on the 5-celled von Neumann neighborhood.
%C A270210 Initialized with a single black (ON) cell at stage zero.
%D A270210 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270210 Robert Price, <a href="/A270210/b270210.txt">Table of n, a(n) for n = 0..128</a>
%H A270210 Robert Price, <a href="/A270210/a270210.tmp.txt">Diagrams of the first 20 stages.</a>
%H A270210 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270210 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270210 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270210 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270210 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270210 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270210 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270210 code=123; stages=128;
%t A270210 rule=IntegerDigits[code,2,10];
%t A270210 g=2*stages+1; (* Maximum size of grid *)
%t A270210 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270210 ca=a;
%t A270210 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270210 PrependTo[ca,a];
%t A270210 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270210 k=(Length[ca[[1]]]+1)/2;
%t A270210 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270210 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A270210 nonn,easy
%O A270210 0,2
