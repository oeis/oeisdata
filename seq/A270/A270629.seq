%U A270629 -2953,3433,-3449,4049,-4021,4345,-4141,4657,-4661,5433,-5613,6317,-6189,6625
%N A270629 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 181", based on the 5-celled von Neumann neighborhood.
%C A270629 Initialized with a single black (ON) cell at stage zero.
%D A270629 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270629 Robert Price, <a href="/A270629/b270629.txt">Table of n, a(n) for n = 0..127</a>
%H A270629 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270629 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270629 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270629 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270629 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270629 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270629 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270629 code=181; stages=128;
%t A270629 rule=IntegerDigits[code,2,10];
%t A270629 g=2*stages+1; (* Maximum size of grid *)
%t A270629 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270629 ca=a;
%t A270629 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270629 PrependTo[ca,a];
%t A270629 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270629 k=(Length[ca[[1]]]+1)/2;
%t A270629 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270629 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270629 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A270629 Cf. A270626.
