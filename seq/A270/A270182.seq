%I A270182 #11 Nov 05 2025 15:22:31
%S A270182 1,5,41,209,913,3797,15409,62117,250601,1004249,4022933,16095565,
%T A270182 64410069,257685977,1030803317,4123341761
%N A270182 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 115", based on the 5-celled von Neumann neighborhood.
%C A270182 Initialized with a single black (ON) cell at stage zero.
%D A270182 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270182 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270182 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270182 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270182 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270182 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270182 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270182 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270182 code=115; stages=128;
%t A270182 rule=IntegerDigits[code,2,10];
%t A270182 g=2*stages+1; (* Maximum size of grid *)
%t A270182 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270182 ca=a;
%t A270182 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270182 PrependTo[ca,a];
%t A270182 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270182 k=(Length[ca[[1]]]+1)/2;
%t A270182 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270182 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270182 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270182 Cf. A270181.
%K A270182 nonn,more
%O A270182 0,2
