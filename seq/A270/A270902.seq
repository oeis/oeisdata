%S A270902 1,8,44,200,848,3544,14892,60788,245156,982444,3943484,15780348,
%T A270902 63144848,252667520,1010772776,4043327828
%N A270902 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 213", based on the 5-celled von Neumann neighborhood.
%C A270902 Initialized with a single black (ON) cell at stage zero.
%D A270902 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270902 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270902 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270902 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270902 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270902 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270902 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270902 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270902 code=213; stages=128;
%t A270902 rule=IntegerDigits[code,2,10];
%t A270902 g=2*stages+1; (* Maximum size of grid *)
%t A270902 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270902 ca=a;
%t A270902 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270902 PrependTo[ca,a];
%t A270902 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270902 k=(Length[ca[[1]]]+1)/2;
%t A270902 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270902 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270902 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270902 Cf. A270901.
%K A270902 nonn,more
