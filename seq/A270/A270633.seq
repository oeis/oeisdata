%U A270633 -84,544,-316,312,184,128,-4,408,-76,156,64,356,-260,336,336,-116
%N A270633 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 182", based on the 5-celled von Neumann neighborhood.
%C A270633 Initialized with a single black (ON) cell at stage zero.
%D A270633 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270633 Robert Price, <a href="/A270633/b270633.txt">Table of n, a(n) for n = 0..127</a>
%H A270633 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270633 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270633 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270633 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270633 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270633 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270633 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270633 code=182; stages=128;
%t A270633 rule=IntegerDigits[code,2,10];
%t A270633 g=2*stages+1; (* Maximum size of grid *)
%t A270633 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270633 ca=a;
%t A270633 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270633 PrependTo[ca,a];
%t A270633 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270633 k=(Length[ca[[1]]]+1)/2;
%t A270633 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270633 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270633 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A270633 Cf. A270630.
