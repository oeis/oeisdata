%S A270133 1,5,12,16,36,24,60,44,116,52,148,96,244,76,228,180,420,120,308,232,
%T A270133 556,248,548,368,812,308,556,548,936,640,832,836,1176,884,888,948,
%U A270133 1500,1092,1336,1368,1864,1232,1584,1692,2136,1768,1912,1984,2676,1828,2172
%N A270133 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 94", based on the 5-celled von Neumann neighborhood.
%C A270133 Initialized with a single black (ON) cell at stage zero.
%D A270133 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270133 Robert Price, <a href="/A270133/b270133.txt">Table of n, a(n) for n = 0..128</a>
%H A270133 Robert Price, <a href="/A270133/a270133.tmp.txt">Diagrams of the first 20 stages.</a>
%H A270133 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270133 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270133 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270133 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270133 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270133 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270133 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270133 code=94; stages=128;
%t A270133 rule=IntegerDigits[code,2,10];
%t A270133 g=2*stages+1; (* Maximum size of grid *)
%t A270133 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270133 ca=a;
%t A270133 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270133 PrependTo[ca,a];
%t A270133 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270133 k=(Length[ca[[1]]]+1)/2;
%t A270133 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270133 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A270133 nonn,easy
%O A270133 0,2
