%I A270721 #10 Nov 05 2025 15:22:32
%S A270721 1,4,28,133,656,2705,10709,44364,177969,715725,2861636,11469576,
%T A270721 45878364,183603688,734497160,2938408604
%N A270721 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 201", based on the 5-celled von Neumann neighborhood.
%C A270721 Initialized with a single black (ON) cell at stage zero.
%D A270721 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270721 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270721 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270721 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270721 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270721 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270721 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270721 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270721 code=201; stages=128;
%t A270721 rule=IntegerDigits[code,2,10];
%t A270721 g=2*stages+1; (* Maximum size of grid *)
%t A270721 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270721 ca=a;
%t A270721 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270721 PrependTo[ca,a];
%t A270721 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270721 k=(Length[ca[[1]]]+1)/2;
%t A270721 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270721 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270721 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270721 Cf. A270720.
%K A270721 nonn,more
%O A270721 0,2
