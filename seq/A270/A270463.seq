%S A270463 1,5,14,38,67,135,176,296,389,617,690,1034,1223,1675,1816,2428,2745,
%T A270463 3497,3818,4798,5259,6399,6768,8288,8845,10561,11182,13186,13887,
%U A270463 16147,16912,19444,20565,23525,24402,27798,29099,32739,34068,38340,40001,44545,45834
%N A270463 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 169", based on the 5-celled von Neumann neighborhood.
%C A270463 Initialized with a single black (ON) cell at stage zero.
%D A270463 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270463 Robert Price, <a href="/A270463/b270463.txt">Table of n, a(n) for n = 0..128</a>
%H A270463 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270463 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270463 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270463 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270463 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270463 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270463 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270463 code=169; stages=128;
%t A270463 rule=IntegerDigits[code,2,10];
%t A270463 g=2*stages+1; (* Maximum size of grid *)
%t A270463 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270463 ca=a;
%t A270463 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270463 PrependTo[ca,a];
%t A270463 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270463 k=(Length[ca[[1]]]+1)/2;
%t A270463 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270463 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270463 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
%Y A270463 Cf. A270461.
