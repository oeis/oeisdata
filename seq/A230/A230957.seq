%F A230957 a(n) = sum(A109449(n,k)*A000009(k): k=0..n).
%t A230957 T[n_, k_] := (n!/k!) SeriesCoefficient[(1 + Sin[x])/Cos[x], {x, 0, n - k}];
%t A230957 a[n_] := Sum[T[n, k] PartitionsQ[k], {k, 0, n}];
