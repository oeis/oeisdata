%o A070215 (Python)
%o A070215 from sympy import sieve
%o A070215 from functools import cache
%o A070215 @cache
%o A070215 def b(n, k=None): # A000586
%o A070215     if k == None: k = n
%o A070215     if n < 1: return int(n == 0)
%o A070215     return sum(b(n-p, p-1) for p in sieve.primerange(1, k+1))
%o A070215 def a(n): return b(sieve[n])
%o A070215 print([a(n) for n in range(1, 54)]) # _Michael S. Branicky_, Nov 30 2025
