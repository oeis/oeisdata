%o A338970 (Python)
%o A338970 # Using graphillion
%o A338970 from graphillion import GraphSet
%o A338970 def make_T_nk(n, k):
%o A338970     grids = []
%o A338970     for i in range(1, k + 1):
%o A338970         for j in range(1, n):
%o A338970             grids.append((i + (j - 1) * k, i + j * k))
%o A338970             if i < k:
%o A338970                 grids.append((i + (j - 1) * k, i + j * k + 1))
%o A338970     for i in range(1, k * n, k):
%o A338970         for j in range(1, k):
%o A338970             grids.append((i + j - 1, i + j))
%o A338970     return grids
%o A338970 def A339849(n, k):
%o A338970     universe = make_T_nk(n, k)
%o A338970     GraphSet.set_universe(universe)
%o A338970     cycles = GraphSet.cycles(is_hamilton=True)
%o A338970     return cycles.len()
%o A338970 def A338970(n):
%o A338970     return A339849(6, n)
