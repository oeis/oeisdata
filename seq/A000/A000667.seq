%o A000667 from itertools import islice, accumulate
%o A000667 def A000667_gen(): # generator of terms
%o A000667     blist = tuple()
%o A000667     while True:
%o A000667         yield (blist := tuple(accumulate(reversed(blist),initial=1)))[-1]
