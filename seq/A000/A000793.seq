%p A000793 A000793 := proc(n)
%p A000793     l := 1:
%p A000793     p := combinat[partition](n):
%p A000793     for i from 1 to combinat[numbpart](n) do
%p A000793         if ilcm( p[i][j] $ j=1..nops(p[i])) > l then
%p A000793             l := ilcm( p[i][j] $ j=1..nops(p[i]))
%p A000793         end if:
%p A000793     end do:
%p A000793     l ;
%p A000793 end proc:
