%t A000518 amax = 20; km0 = 10; Clear[dd]; L[a_, s_, km_] := Sum[ JacobiSymbol[ -a, 2 k + 1]/(2 k + 1)^s, {k, 0, km}]; d[1, n_, km_] := 2 (2 n - 1)! L[-1, 2 n, km] (2/Pi)^(2 n) // Round; d[a_ /; a > 1, n_, km_] := (2 n - 1)! L[-a, 2 n, km] (2 a/Pi)^(2 n)/Sqrt[a] // Round; dd[km_] := dd[km] = Table[d[a, 4, km], {a, 1, amax}]; dd[km0]; dd[km = 2 km0]; While[dd[km] != dd[km/2, km = 2 km]]; A000518 = dd[km] (* _Jean-Fran√ßois Alcover_, Feb 09 2016 *)
