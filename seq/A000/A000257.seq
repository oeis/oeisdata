%p A000257         option remember;
%p A000257         if n <=1 then
%p A000257                 1;
%p A000257         else
%p A000257                 4*(2*n-1)*procname(n-1)/(n+2) ;
%p A000257         end if ;
%p A000257 end proc: # _R. J. Mathar_, Dec 18 2011
%t A000257 CoefficientList[Series[1 + x HypergeometricPFQ[{1, 3/2}, {4}, 8 x], {x, 0, 10}], x]
%t A000257 (* Second program: *)
%t A000257 Join[{1}, Table[3*2^(n-1) CatalanNumber[n]/(n+2), {n, 30}]] (* _Harvey P. Dale_, Dec 18 2011 *)
%o A000257 (PARI)
%o A000257 C(n)=binomial(2*n, n)/(n+1);
%o A000257 a(n)=if(n==0, 1, 3*2^(n-1)*C(n)/(n+2) ); \\ _Joerg Arndt_, May 04 2013
%o A000257 (PARI) x='x+O('x^66); Vec( ((1-8*x)^(3/2) + 8*x^2 + 12*x - 1)/(32*x^2) ) \\ _Joerg Arndt_, May 04 2013
%o A000257 (PARI)
%o A000257 x='x; y='y; Fxy = 16*x^2*y^2 - (8*x^2+12*x-1)*y + x^2+11*x-1;
%o A000257 seq(N) = {
%o A000257   my(y0 = 1 + O('x^N), y1=0);
%o A000257   for (k = 1, N,
%o A000257     y1 = y0 - subst(Fxy, y, y0)/subst(deriv(Fxy, y), y, y0);
%o A000257     if (y1 == y0, break()); y0 = y1);
%o A000257   Vec(y0);
%o A000257 };
%o A000257 seq(24) \\ _Gheorghe Coserea_, Nov 30 2016
