%p A000727 with(numtheory): etr:= proc(p) local b; b:=proc(n) option remember; local d,j; if n=0 then 1 else add(add(d*p(d), d=divisors(j)) *b(n-j), j=1..n)/n fi end end: a:= etr(n-> -4): seq(a(n), n=0..81); # _Alois P. Heinz_, Sep 08 2008
%t A000727 etr[p_] := Module[{b}, b[n_] := b[n] = If[n == 0, 1, Sum[Sum[d*p[d], {d, Divisors[j]}]*b[n - j], {j, 1, n}]/n]; b]; a = etr[-4&]; Table[a[n], {n, 0, 81}] (* _Jean-Fran√ßois Alcover_, Mar 10 2014, after _Alois P. Heinz_ *)
%t A000727 a[ n_] := SeriesCoefficient[ QPochhammer[ x]^4, {x, 0, n}]; (* _Michael Somos_, Jun 12 2014 *)
%t A000727 nmax = 80; CoefficientList[Series[Sum[Sum[(-1)^(k+m) * (2*k+1) * q^(k*(k+1)/2 + m*(3*m-1)/2), {k, 0, nmax}], {m, -nmax, nmax}], {q, 0, nmax}], q] (* _Vaclav Kotesovec_, Dec 06 2015 *)
%o A000727 (PARI) {a(n) = my(A, p, e, x, y, a0, a1); if( n<0, 0, n = 6*n + 1; A = factor(n); prod( k=1, matsize(A)[1], [p, e] = A[k,]; if( p<5, 0, p%6==5, if(e%2, 0, (-1)^(e/2) * p^(e/2)), for( y=1, sqrtint(p\3), if( issquare( p - 3*y^2, &x), break)); a0=1; if( x%3!=1, x=-x); a1 = x = 2*x; for( i=2, e, y = x*a1 - p*a0; a0=a1; a1=y); a1)))}; /* _Michael Somos_, Aug 23 2006 */
