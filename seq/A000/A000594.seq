%o A000594 (Ruby)
%o A000594 def A000594(n)
%o A000594   ary = [0, 1]
%o A000594   (2..n).each{|i|
%o A000594     s, t, u = 0, 1, 0
%o A000594     (1..n).each{|j|
%o A000594       t += 9 * j
%o A000594       u += j
%o A000594       break if i <= u
%o A000594       s += (-1) ** (j % 2 + 1) * (2 * j + 1) * (i - t) * ary[-u]
%o A000594     }
%o A000594     ary << s / (i - 1)
%o A000594   }
%o A000594   ary[1..-1]
%o A000594 end
