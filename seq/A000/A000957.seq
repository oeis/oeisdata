%o A000957     f, c, n = 1, 1, 1
%o A000957     yield 0
%o A000957     while True:
%o A000957         yield f
%o A000957         n += 1
%o A000957         c = c * (4*n - 6) // n
%o A000957         f = (c - f) // 2
