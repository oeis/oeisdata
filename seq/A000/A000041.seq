%o A000041 @CachedFunction
%o A000041 def A000041(n):
%o A000041     if n == 0: return 1
%o A000041     S = 0; J = n-1; k = 2
%o A000041     while 0 <= J:
%o A000041         T = A000041(J)
%o A000041         S = S+T if is_odd(k//2) else S-T
%o A000041         J -= k if is_odd(k) else k//2
%o A000041         k += 1
%o A000041     return S
