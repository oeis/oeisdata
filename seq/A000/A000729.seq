%t A000729 a[ n_] := SeriesCoefficient[ (-1/16) EllipticThetaPrime[ 1, 0, q] EllipticTheta[ 1, -Pi/2, q]^3, {q, 0, 4 n + 1}]; (* _Michael Somos_, May 17 2015 *)
%o A000729 (PARI) {a(n) = my(A); if( n<0, 0, A = x * O(x^n); polcoeff( eta(x + A)^6, n))};
%o A000729 (PARI) {a(n) = my(A, p, e, x, y, a0, a1); if( n<0, 0, n = 4*n + 1; A = factor(n); prod( k=1, matsize(A)[1], [p, e] = A[k,]; if( p==2, 0, p%4==3, if( e%2, 0, p^e), forstep( i=1, sqrtint(p), 2, if( issquare( p - i^2, &y), x=i; break)); a0=1; a1 = y = 2*(x^2 - y^2); for( i=2, e, x = y*a1 - p^2*a0; a0=a1; a1=x); a1)))}; /* _Michael Somos_, Aug 21 2006 */
%o A000729 (PARI) {a(n)=local(tn=(sqrtint(8*n+1)+1)\2);polcoeff(sum(m=0,tn,(1+2*m)^2*x^(m^2+m)+x*O(x^n)) + 2*sum(m=0,tn,sum(k=1,tn,(1+4*(m^2+m-k^2))*x^(m^2+m+k^2)+x*O(x^n))),n)} /* _Paul D. Hanna_, Mar 15 2010 */
