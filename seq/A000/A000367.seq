%o A000367 (Python) # The objective of this implementation is efficiency.
%o A000367 # n -> [a(0), a(1), ..., a(n)] for n > 0.
%o A000367 from fractions import Fraction
%o A000367 def A000367_list(n):  # Bernoulli numerators
%o A000367     T = [0 for i in range(1, n+2)]
%o A000367     T[0] = 1; T[1] = 1
%o A000367     for k in range(2, n+1):
%o A000367         T[k] = (k-1)*T[k-1]
%o A000367     for k in range(2, n+1):
%o A000367         for j in range(k, n+1):
%o A000367             T[j] = (j-k)*T[j-1]+(j-k+2)*T[j]
%o A000367     a = 0; b = 6; s = 1
%o A000367     for k in range(1, n+1):
%o A000367         T[k] = s*Fraction(T[k]*k, b).numerator
%o A000367         h = b; b = 20*b - 64*a; a = h; s = -s
%o A000367     return T
%o A000367 print(A000367_list(100)) # _Peter Luschny_, Aug 09 2011
%o A000367 (Maxima)
%o A000367 B(n):=if n=0 then 1 else 2*n*sum((2*n+k-2)!*sum(((-1)^(j+1)*stirling1(2*n+j,j))/ ((k-j)!*(2*n+j)!),j,1,k),k,0,2*n);
