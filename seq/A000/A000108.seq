%o A000108 (SageMath) # Generalized algorithm of L. Seidel
%o A000108 def A000108_list(n) :
%o A000108     D = [0]*(n+1); D[1] = 1
%o A000108     b = True; h = 1; R = []
%o A000108     for i in range(2*n-1) :
%o A000108         if b :
%o A000108             for k in range(h,0,-1) : D[k] += D[k-1]
%o A000108             h += 1; R.append(D[1])
%o A000108         else :
%o A000108             for k in range(1,h, 1) : D[k] += D[k+1]
%o A000108         b = not b
%o A000108     return R
