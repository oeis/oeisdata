%p A000669 Method 1: a := [1,1]; for n from 3 to 30 do L := series( mul( (1-x^k)^(-a[k]),k=1..n-1)/(1-x^n)^b, x,n+1); t1 := coeff(L,x,n); R := series( 1+2*add(a[k]*x^k,k=1..n-1)+2*b*x^n, x, n+1); t2 := coeff(R,x,n); t3 := solve(t1-t2,b); a := [op(a),t3]; od: A000669 := n-> a[n];
%p A000669 Method 2, more efficient: with(numtheory): M := 1001; a := array(0..M); p := array(0..M); a[1] := 1; a[2] := 1; a[3] := 2; p[1] := 1; p[2] := 3; p[3] := 7;
%p A000669 Method 2, cont.: for m from 4 to M do t1 := divisors(m); t3 := 0; for d in t1 minus {m} do t3 := t3+d*a[d]; od: t4 := p[m-1]+2*add(p[k]*a[m-k],k=1..m-2)+t3; a[m] := t4/m; p[m] := t3+t4; od: # A000669 := n-> a[n]; A058757 := n->p[n];
%p A000669 # Method 3:
%p A000669 b:= proc(n, i) option remember; `if`(n=0, 1,
%p A000669       `if`(i<1, 0, add(binomial(a(i)+j-1, j)*
%p A000669        b(n-i*j, i-1), j=0..n/i)))
%p A000669     end:
%p A000669 a:= n-> `if`(n<2, n, b(n, n-1)):
%p A000669 seq(a(n), n=1..40);  # _Alois P. Heinz_, Jan 28 2016
%t A000669 b[n_, i_] := b[n, i] = If[n==0, 1, If[i<1, 0, Sum[Binomial[a[i]+j-1, j]* b[n-i*j, i-1], {j, 0, n/i}]]];
%t A000669 a[n_] := If[n<2, n, b[n, n-1]];
%t A000669 Array[a, 40] (* _Jean-François Alcover_, Jan 08 2021, after _Alois P. Heinz_ *)
%o A000669 (PARI) {a(n) = my(A, X); if( n<2, n>0, X = x + x * O(x^n); A = 1 / (1 - X); for(k=2, n, A /= (1 - X^k)^polcoeff(A, k)); polcoeff(A, n)/2)}; /* _Michael Somos_, Jul 25 2003 */
%o A000669 (SageMath)
%o A000669 from collections import Counter
%o A000669 def A000669_list(n):
%o A000669     list = [1] + [0] * (n - 1)
%o A000669     for i in range(1, n):
%o A000669         for p in Partitions(i + 1, min_length=2):
%o A000669             m = Counter(p)
%o A000669             list[i] += prod(binomial(list[s - 1] + m[s] - 1, m[s]) for s in m)
%o A000669     return list
%o A000669 print(A000669_list(20)) # _M. Eren Kesim_, Jun 21 2021
