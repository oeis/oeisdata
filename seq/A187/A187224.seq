%t A187224 jointRank[{seqA_,seqB_}]:={Flatten@Position[#1,{_,1}],Flatten@Position[#1,{_,2}]}&[Sort@Flatten[{{#1,1}&/@seqA,{#1,2}&/@seqB},1]];
