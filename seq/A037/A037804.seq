%N A037804 Number of i such that d(i)<d(i-1), where Sum{d(i)*6^i: i=0,1,...,m} is the base 6 representation of n.
%p A037804 A037804 := proc(n)
%p A037804     a := 0 ;
%p A037804     dgs := convert(n,base,6);
%p A037804     for i from 2 to nops(dgs) do
%p A037804         if op(i,dgs)<op(i-1,dgs) then
%p A037804             a := a+1 ;
%p A037804         end if;
%p A037804     end do:
%p A037804     a ;
%p A037804 end proc: # _R. J. Mathar_, Oct 16 2015
