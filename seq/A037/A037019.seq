%o A037019 (Python)
%o A037019 from math import prod
%o A037019 from sympy import factorint, prime
%o A037019 def a(n):
%o A037019     pf = factorint(n, multiple=True)
%o A037019     return prod(prime(i)**(pi-1) for i, pi in enumerate(pf[::-1], 1))
