%N A037810 Number of i such that d(i)<=d(i-1), where Sum{d(i)*3^i: i=0,1,...,m} is the base 3 representation of n.
%p A037810 A037810 := proc(n)
%p A037810     a := 0 ;
%p A037810     dgs := convert(n,base,3);
%p A037810     for i from 2 to nops(dgs) do
%p A037810         if op(i,dgs)<=op(i-1,dgs) then
%p A037810             a := a+1 ;
%p A037810         end if;
%p A037810     end do:
%p A037810     a ;
%p A037810 end proc: # _R. J. Mathar_, Oct 16 2015
