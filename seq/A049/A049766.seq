%F A049766 From _Mia Boudreau_, Dec 06 2025: (Start)
%F A049766 a(n) = Sum_{k=1..n} ((n mod k) + (k mod n)).
%F A049766 a(n) = A004125(n) + A161680(n).
%F A049766 a(n) = (3n^2 - n)/2 - Sum_{k=1..n}(k*floor(n/k)).
%F A049766 a(n) = (3n^2 - n)/2 - Sum_{j=1..n}(Sum_{k=(floor(n/(j+1))+1)..floor(n/j)}(k)). (End)
%p A049766 seq( add( `mod`(k, n) + `mod`(n, k), k = 1..n), n = 1..50); # _G. C. Greubel_, Dec 13 2019
%t A049766 T[n_, k_] := Mod[n, k] + Mod[k, n]; Map[Total[T[#, Range[#]]] &, Range[80]] (* _Carl Najafi_, Aug 24 2011 *)
%o A049766 (PARI) a(n) = sum(k=1, n, k%n + n%k); \\ _Michel Marcus_, Aug 22 2015
%o A049766 (Magma) [&+[ (k mod n) + (n mod k): k in [1..n]]: n in [1..50]]; // _G. C. Greubel_, Dec 13 2019
%o A049766 (SageMath) [sum((k%n) + (n%k) for k in (1..n)) for n in (1..50)] # _G. C. Greubel_, Dec 13 2019
%o A049766 (GAP) List([1..50], n-> Sum([1..n], k-> (k mod n) + (n mod k)) ); # _G. C. Greubel_, Dec 13 2019
%o A049766 (Java)
%o A049766 long a(long n) {
%o A049766   long s = 0, k0 = 1;
%o A049766   while (k0 <= n) {
%o A049766     long d = n / k0, k1 = n / d;
%o A049766     s += (k0 + k1) * (k1 - k0 + 1) / 2 * d;
%o A049766     k0 = k1 + 1;
%o A049766   }
%o A049766   return n * n + n * (n - 1) / 2 - s;
%o A049766 } // _Mia Boudreau_, Dec 07 2025
