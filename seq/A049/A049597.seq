%p A049597 a := n->sort(simplify(sum(product((1-q^i),i=n-r+1..n)/product((1-q^j),j=1..r), r=0..n))):T := proc(n,k) if k=n then n+1 elif k>n then 0 else coeff(a(k),q^(n-k)) fi end:seq(seq(T(n,k),k=0..n),n=0..21);
%t A049597 a [n_] := Sum[Product[1-q^i, {i, n-r+1, n}]/Product[1-q^j, {j, 1, r}], {r, 0, n}] // Simplify; T [n_, k_] := Which[k == n, n+1, k>n, 0, True, Coefficient[a[k], q^(n - k)]]; Table[Table[T[n, k], {k, 0, n}], {n, 0, 21}] // Flatten (* _Jean-Fran√ßois Alcover_, Feb 19 2015, after Maple *)
