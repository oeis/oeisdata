%t A049643 Join[{0},Rest[Accumulate[EulerPhi[Range[0,60]]]+1]] (* _Harvey P. Dale_, Oct 16 2018 *)
%t A049643 a[n_] := If[n == 0, 0, FareySequence[n] // Length];
%t A049643 Table[a[n], {n, 0, 100}] (* _Jean-François Alcover_, Jul 16 2022 *)
