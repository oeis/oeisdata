%p A049284 v := proc(c,d) option remember; local i; if d < 0 or c < 0 then 0 elif d = c then 1 else add(v(i,d-c),i=1..2*c); fi; end; [ seq(v(4,n), n=1..50) ];
%t A049284 v[c_, d_] := v[c, d] = If[d < 0 || c < 0, 0, If[d == c, 1, Sum[v[i, d-c], {i, 1, 2*c}]]]; Table[v[4, n], {n, 1, 40}] (* _Jean-FranÃ§ois Alcover_, Jan 10 2014, translated from Maple *)
