%p A330798 alias(C=binomial): T := (n, k) -> ((n+1)/(2*n+1))*C(2*n+1, n+k+1)*C(2*n+k, k):
%p A330798 seq(seq(T(n,k), k=0..n), n=0..8);
%t A330798 T[n_, k_]:= ((n+1)/(n+k+1))*Binomial[n,k]*Binomial[2*n+k,n];
