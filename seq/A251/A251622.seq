%t A251622 a[1] = 1; a[2] = 2;
%t A251622 a[n_] := a[n] = For[k = 1, True, k++, If[FreeQ[Array[a, n-1], k], If[!CoprimeQ[k, a[n-1]] || !CoprimeQ[k, a[n-2]], Return[k]]]];
