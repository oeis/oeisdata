%o A251561 (Python)
%o A251561 from sympy import isprime
%o A251561 def A251561(n):
%o A251561     if n == 2:
%o A251561         return 4
%o A251561     q,r = divmod(n,2)
%o A251561     if r :
%o A251561         if isprime(n):
%o A251561             return 2*n
%o A251561         return n
%o A251561     if isprime(q):
%o A251561         return q
