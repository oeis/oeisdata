%o A341961 A = 1/((1-2*x*B)*(1-3*x*C) +x*O(x^n));
%o A341961 B = 1/((1-1*x*A)*(1-3*x*C) +x*O(x^n));
%o A341961 C = 1/((1-1*x*A)*(1-2*x*B) +x*O(x^n)););
%o A341961 polcoeff(A,n)}
