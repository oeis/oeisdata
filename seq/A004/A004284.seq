%S A004284 1,10,111,10,11,1110,111,100,1101,110,11111,1110,1001,1110,111111,100,
%T A004284 101,11010,100111,110,111,111110,1011,11100,11011,10010,1101,1110,
%U A004284 10011,1111110,11111,1000,101001,1010,111111,11010,1001111,1001110,10101,1100,100001
%N A004284 Least positive multiple of n written in base 4 using only 0 and 1.
%H A004284 Robert Israel, <a href="/A004284/b004284.txt">Table of n, a(n) for n = 1..10000</a> (first 200 terms from Harvey P. Dale)
%e A004284    n | multiplier m | a(n) = m*n
%e A004284   ---+--------------+------------
%e A004284    1 |       1      |  1 =    1_4
%e A004284    2 |       2      |  4 =   10_4
%e A004284    3 |       7      | 21 =  111_4
%e A004284    4 |       1      |  4 =   10_4
%e A004284    5 |       1      |  5 =   11_4
%e A004284    6 |      14      | 84 = 1110_4
%e A004284    7 |       3      | 21 =  111_4
%e A004284    8 |       2      | 16 =  100_4
%e A004284    9 |       9      | 81 = 1101_4
%e A004284   10 |       2      | 20 =  110_4
%p A004284 N:= 100: # for a(1) .. a(N)
%p A004284 V:= Vector(N): count:= 0:
%p A004284 for x from 1 while count < N do
%p A004284   L:= convert(x,base,2);
%p A004284   y:= add(L[i]*4^(i-1),i=1..nops(L));
%p A004284   for z in NumberTheory:-Divisors(y) do
%p A004284     if z <= N and V[z] = 0 then V[z]:= add(L[i]*10^(i-1),i=1..nops(L)); count:= count+1 fi
%p A004284   od;
%p A004284 od:
%p A004284 convert(V,list); # _Robert Israel_, Jan 01 2026
%t A004284 lpm4[n_]:=Module[{k=1},While[Rest[Most[DigitCount[n k,4]]]!={0,0},k++]; FromDigits[IntegerDigits[n k,4]]]; Array[lpm4,40] (* _Harvey P. Dale_, Dec 22 2012 *)
