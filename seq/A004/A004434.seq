%o A004434 a004434 n = a004434_list !! (n-1)
%o A004434 a004434_list = filter (p 5 $ tail a000290_list) [1..] where
%o A004434    p k (q:qs) m = k == 0 && m == 0 ||
%o A004434                   q <= m && k >= 0 && (p (k - 1) qs (m - q) || p k qs m)
