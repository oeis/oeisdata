%t A004114 max = 28; etr[p_] := Module[{b}, b[n_] := b[n] = If[n == 0, 1, Sum[Sum[d*p[d], {d, Divisors[j]}]*b[n - j], {j, 1, n}]/n ]; b]; bb = etr[A004113]; A004113[n_] := If[n <= 1, n, 2*bb[n - 1]]; b[x_] := Sum[A004113[n] x^n, {n, 1, max}]; f[x_] := Sum[a[n] x^n, {n, 0, max}]; a[0] = a[1] = a[2] = 1; coes = CoefficientList[ Series[f[x] - (1 + b[x] - x*b[x] - b[x]^2/2 + b[x^2]/2), {x, 0, max}], x]; Table[a[n], {n, 0, max}] /. Solve[Thread[coes == 0]][[1]] (* _Jean-FranÃ§ois Alcover_, Jan 29 2013, after _Alois P. Heinz_ *)
