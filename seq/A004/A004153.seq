%p A004153 P:=proc(i) local a,b,c,n; for n from 1 by 1 to i do a:=evalf(floor(log10(n!)+1)); b:=n!; c:=0; while a>0 do c:=c+(b-(trunc(b/10)*10))*10^(a-1); b:=trunc(b/10); a:=a-1; od; print(c); od; end: P(100) # _Paolo P. Lava_, Aug 07 2008
