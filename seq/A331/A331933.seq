%e A331933                                   (o(o)(o))  (oooo(o))
%e A331933                                   (o(o(o)))  ((oo)(oo))
%e A331933                                              (o(o(oo)))
%e A331933                                              (o(oo(o)))
%e A331933                                              (oo(o)(o))
%e A331933                                              (oo(o(o)))
%e A331933                                              ((o)(o)(o))
