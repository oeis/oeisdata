%t A331680 disjointQ[u_]:=Apply[And,Outer[#1==#2||Intersection[#1,#2]=={}&,u,u,1],{0,1}];
%t A331680 strut[n_]:=If[n==1,{{}},Select[Join@@Function[c,Union[Sort/@Tuples[strut/@c]]]/@Rest[IntegerPartitions[n-1]],disjointQ]];
