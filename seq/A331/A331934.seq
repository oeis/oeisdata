%e A331934                                   ((o)(oo))  (oooo(o))
%e A331934                                   (o(o)(o))  ((o)(ooo))
%e A331934                                   (o(o(o)))  ((oo)(oo))
%e A331934                                              (o(o)(oo))
%e A331934                                              (o(o(oo)))
%e A331934                                              (o(oo(o)))
