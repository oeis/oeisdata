%N A183295 Complement of A115067.
%F A183295 (See the Mathematica code.)
%F A183295 a(n) = n+m if n > 3*m*(m+1)/2 and a(n) = n+m-1 otherwise, where m = floor(sqrt(2*(n-1)/3)). - _Chai Wah Wu_, Nov 05 2025
%t A183295 a=3/2; b=5/2;
%t A183295 F[n_]:=a*n^2+b*n;
%t A183295 R[n_]:=(n/a+((b-1)/(2a))^2)^(1/2);
%t A183295 G[n_]:=n-1+Ceiling[R[n]-(b-1)/(2a)];
%t A183295 Table[F[n], {n,60}]
%t A183295 Table[G[n], {n,100}]
