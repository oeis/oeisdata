%t A039686 t = Table[n^2, {n, 750}]; f[j_, k_] := Block[{n = j*10^Floor[1 + Log10@ k] + k}, If[IntegerQ@ Sqrt@ n, n, 0]]; Take[ Union@ Flatten@ Table[ f[t[[j]], t[[k]]], {j, 250}, {k, 750}], {2, 33}] (* _Robert G. Wilson v_, Jul 18 2011 *)
%t A039686 squareQ[n_] := IntegerQ[Sqrt[n]]; okQ[n_] := MatchQ[IntegerDigits[n], {a__ /; squareQ[FromDigits[{a}]], b__ /; First[{b}] > 0 && squareQ[FromDigits[ {b}]]}]; Select[Range[2000]^2, okQ] (* _Jean-Fran√ßois Alcover_, Dec 13 2016 *)
