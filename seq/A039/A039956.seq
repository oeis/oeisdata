%o A039956 (Python)
%o A039956 from math import isqrt
%o A039956 from sympy import mobius
%o A039956 def A039956(n):
%o A039956     def f(x): return int(n+x-sum(mobius(k)*(x//k**2+1>>1) for k in range(1, isqrt(x)+1,2)))
%o A039956     m, k = n, f(n)
%o A039956     while m != k: m, k = k, f(k)
%o A039956     return m<<1 # _Chai Wah Wu_, Nov 25 2025
