%p A001208 c2 :=array(0..8,[3,3,5,5,7,6,8,8,10]) ; c3 :=array(0..8,1..2,[[1,1],[1,1],[2,1],[2,1],[3,1],[2,2],[3,2],[3,2],[4,2]]); c4 :=array(0..8,1..3,[[0,0,0],[0,0,1],[1,0,1],[1,0,2],[2,0,2],[2,1,2],[3,1,2],[3,1,3],[4,1,3]]) ; for n from 23 to 100 do r := n mod 9 ; t := iquo(n,9) ; a2 := 6*t+c2[r] ; a3 := (2*t+c3[r,1])+(2*t+c3[r,2])*a2 ; printf("%a,",4*t+c4[r,1]+(2*t+c4[r,2])*a2+(3*t+c4[r,3])*a3) ; end: # _R. J. Mathar_, Apr 01 2006
%t A001208 ClearAll[c2, c3, c4, a]; Evaluate[ Array[c2, 9, 0]] = {3, 3, 5, 5, 7, 6, 8, 8, 10}; Evaluate[ Array[c3, {9, 2}, {0, 1}]] = {{1, 1}, {1, 1}, {2, 1}, {2,1}, {3, 1}, {2, 2}, {3, 2}, {3, 2}, {4, 2}}; Evaluate[ Array[c4, {9, 3}, {0, 1}]] = {{0, 0, 0}, {0, 0, 1}, {1, 0,1}, {1, 0, 2}, {2, 0, 2}, {2, 1, 2}, {3, 1, 2}, {3, 1, 3}, {4, 1,3}}; Evaluate[ Array[a, 19]] = {3, 8, 15, 26, 35, 52, 69, 89, 112, 146, 172, 212, 259, 302, 354, 418, 476, 548, 633}; a[n_] := (r = Mod[n, 9]; t = Quotient[n, 9]; a2 = 6t + c2[r]; a3 = (2t + c3[r, 1]) + (2t + c3[r, 2])*a2; 4t + c4[r, 1] + (2t + c4[r, 2])*a2 + (3t + c4[r, 3])*a3); Table[a[n], {n, 1, 48}] (* _Jean-FranÃ§ois Alcover_, Dec 19 2011, after _R. J. Mathar_'s Maple program *)
