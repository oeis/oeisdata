%F A001425 a(n) = Sum_{1*s_1+2*s_2+...=n} (fixA[s_1, s_2, ...]/(1^s_1*s_1!*2^s_2*s2!*...)) where fixA[s_1, s_2, ...] = Product_{i>=j>=1} f(i, j, s_i, s_j) where f(i, j, s_i, s_j) = {i=j, odd} (Sum_{d|i} (d*s_d))^((i*s_i^2+s_i)/2) or {i=j, even} (Sum_{d|i} (d*s_d))^(i*s_i^2/2) * (Sum_{d|i/2} (d*s_d))^s_i or {i != j} (Sum_{d|lcm(i, j)} (d*s_d))^(gcd(i, j)*s_i*s_j).
%F A001425 a(n) asymptotic to (n^binomial(n+1, 2))/n! = A023813(n)/A000142(n) ~ e^n*n^binomial(n, 2) / sqrt(2*pi*n).
%F A001425 a(n) = A001329(n) - A079183(n).
%o A001425 (PARI)
%o A001425 permcount(v) = {my(m=1, s=0, k=0, t); for(i=1, #v, t=v[i]; k=if(i>1&&t==v[i-1], k+1, 1); m*=t*k; s+=t); s!/m}
%o A001425 C(d, r)={sum(i=1, #r, my(t=r[i]); if(d%t==0, t))}
%o A001425 E(v) = {prod(i=2, #v, prod(j=1, i-1, my(g=gcd(v[i], v[j])); C(v[i]*v[j]/g, v)^g)) * prod(i=1, #v, my(t=v[i]); C(t,v)^((t+1)\2)*if(t%2, 1, C(t/2,v)))}
%o A001425 a(n) = {my(s=0); forpart(p=n, s += permcount(p)*E(p)); s/n!} \\ _Andrew Howroyd_, Dec 10 2025
