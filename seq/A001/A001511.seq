%e A001511 [ 6]  [ 2]  [ 1 2 2 ]
%e A001511 [ 7]  [ 1]  [ 1 3 1 ]
%e A001511 [ 8]  [ 4]  [ 1 4 ]
%e A001511 [ 9]  [ 1]  [ 2 1 1 1 ]
%e A001511 [10]  [ 2]  [ 2 1 2 ]
%e A001511 [11]  [ 1]  [ 2 2 1 ]
