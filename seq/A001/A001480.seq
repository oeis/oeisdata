%t A001480 nmax = 63; nextCuban[p_] := If[p1 = NextPrime[p]; Mod[p1, 3] > 1, nextCuban[p1], p1]; cubanPrimes = NestList[ nextCuban, 3, nmax ]; f[p_] := y /. ToRules[ Reduce[x > 0 && y > 0 && p == x^2 + 3*y^2, {x, y}, Integers]]; a[1] = 1; a[n_] := f[cubanPrimes[[n]]]; Table[ a[n] , {n, 1, nmax}] (* _Jean-Fran√ßois Alcover_, Oct 19 2011 *)
%o A001480 (Haskell)
%o A001480 a001480 n = a000196 $ (`div` 3) $ (a007645 n) - (a001479 n) ^ 2
