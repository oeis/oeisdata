%o A001003 (SageMath) # Generalized algorithm of L. Seidel
%o A001003 def A001003_list(n) :
%o A001003     D = [0]*(n+1); D[1] = 1/2
%o A001003     b = True; h = 2; R = [1]
%o A001003     for i in range(2*n-2) :
%o A001003         if b :
%o A001003             for k in range(h,0,-1) : D[k] += D[k-1]
%o A001003             h += 1;
%o A001003         else :
%o A001003             for k in range(1,h, 1) : D[k] += D[k-1]
%o A001003             R.append(D[h-1]);
%o A001003         b = not b
%o A001003     return R
%o A001003 A001003_list(24) # _Peter Luschny_, Jun 02 2012
