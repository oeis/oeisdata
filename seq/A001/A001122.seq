%o A001122 from itertools import islice
%o A001122 from sympy import nextprime, is_primitive_root
%o A001122 def A001122_gen(): # generator of terms
%o A001122     p = 2
%o A001122     while (p:=nextprime(p)):
%o A001122         if is_primitive_root(2,p):
%o A001122             yield p
%o A001122 A001122_list = list(islice(A001122_gen(),30)) # _Chai Wah Wu_, Feb 13 2023
