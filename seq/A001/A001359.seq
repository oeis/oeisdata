%p A001359 A001359 := proc(n)
%p A001359    option remember;
%p A001359    if n = 1
%p A001359       then 3;
%p A001359    else
%p A001359       p := nextprime(procname(n-1)) ;
%p A001359       while not isprime(p+2) do
%p A001359          p := nextprime(p) ;
%p A001359       end do:
%p A001359       p ;
%p A001359    end if;
%p A001359 end proc: # _R. J. Mathar_, Sep 03 2011
