%p A001349 # To produce all connected graphs on 4 nodes, for example (from _N. J. A. Sloane_, Oct 07 2013):
%p A001349 with(GraphTheory):
%p A001349 L:=[NonIsomorphicGraphs](4,output=graphs,outputform=adjacency, restrictto=connected):
%t A001349 <<"Combinatorica`"; max = 19; A000088 = Table[ NumberOfGraphs[n], {n, 0, max}]; f[x_] = 1 - Product[ 1/(1 - x^k)^a[k], {k, 1, max}]; a[0] = a[1] = a[2] = 1; coes = CoefficientList[ Series[ f[x], {x, 0, max}], x]; sol = First[ Solve[ Thread[ Rest[ coes + A000088 ] == 0]]]; Table[ a[n], {n, 0, max}] /. sol (* _Jean-François Alcover_, Nov 24 2011 *)
%t A001349 terms = 20;
%t A001349 mob[m_, n_] := If[Mod[m, n] == 0, MoebiusMu[m/n], 0];
%t A001349 EULERi[b_] := Module[{a, c, i, d}, c = {}; For[i = 1, i <= Length[b], i++, c = Append[c, i*b[[i]] - Sum[c[[d]]*b[[i - d]], {d, 1, i - 1}]]]; a = {}; For[i = 1, i <= Length[b], i++, a = Append[a, (1/i)*Sum[mob[i, d]*c[[d]], {d, 1, i}]]]; Return[a]];
%t A001349 permcount[v_] := Module[{m = 1, s = 0, k = 0, t}, For[i = 1, i <= Length[v], i++, t = v[[i]]; k = If[i > 1 && t == v[[i - 1]], k + 1, 1]; m *= t*k; s += t]; s!/m];
%t A001349 edges[v_] := Sum[GCD[v[[i]], v[[j]]], {i, 2, Length[v]}, {j, 1, i - 1}] + Total[Quotient[v, 2]];
%t A001349 a88[n_] := Module[{s = 0}, Do[s += permcount[p]*2^edges[p], {p, IntegerPartitions[n]}]; s/n!];
%t A001349 Join[{1}, EULERi[Array[a88, terms]]] (* _Jean-François Alcover_, Jul 28 2018, after _Andrew Howroyd_ *)
%o A001349 (SageMath)
%o A001349 property=lambda G: G.is_connected()
%o A001349 def a(n):
%o A001349     return len([1 for G in graphs(n) if property(G)])
