%o A001349 (Python)
%o A001349 from functools import lru_cache
%o A001349 from itertools import combinations
%o A001349 from fractions import Fraction
%o A001349 from math import prod, gcd, factorial
%o A001349 from sympy import mobius, divisors
%o A001349 from sympy.utilities.iterables import partitions
%o A001349 def A001349(n):
%o A001349     if n == 0: return 1
%o A001349     @lru_cache(maxsize=None)
%o A001349     def b(n): return int(sum(Fraction(1<<sum(p[r]*p[s]*gcd(r,s) for r,s in combinations(p.keys(),2))+sum((q>>1)*r+(q*r*(r-1)>>1) for q, r in p.items()),prod(q**r*factorial(r) for q, r in p.items())) for p in partitions(n)))
%o A001349     @lru_cache(maxsize=None)
%o A001349     def c(n): return n*b(n)-sum(c(k)*b(n-k) for k in range(1,n))
%o A001349     return sum(mobius(n//d)*c(d) for d in divisors(n,generator=True))//n # _Chai Wah Wu_, Jul 02-03 2024
