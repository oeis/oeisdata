%t A333485 ralensort[f_,c_]:=If[Length[f]!=Length[c],Length[f]>Length[c],OrderedQ[{f,c}]];
%t A333485 Join@@Table[Times@@Prime/@#&/@Sort[IntegerPartitions[n],ralensort],{n,0,8}]
%o A333485 (PARI)
%o A333485 C(sig)=prod(k=1,#sig,prime(sig[k]))
%o A333485 cmpLenLexDesc(x,y)=my(d=cmp(#x,#y)); if(d, d, -lex(x,y))
%o A333485 Row(n)={apply(C, vecsort([Vecrev(p) | p<-partitions(n)], cmpLenLexDesc, 4))}
%o A333485 { for(n=0, 7, print(Row(n))) } \\ _Andrew Howroyd_, Oct 07 2025
