%o A333077 (Python)
%o A333077 import networkx as nx
%o A333077 def deBruijn(n): return nx.MultiDiGraph(((0,0),(0,0))) if n==0 else nx.line_graph(deBruijn(n-1))
%o A333077 def A333077(n): return nx.graph_number_of_cliques(nx.complement(nx.Graph(deBruijn(n))))
