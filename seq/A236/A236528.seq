%o A236528 def b(x):
%o A236528   num = str(x)
%o A236528   n = 1
%o A236528   while n < 10**3:
%o A236528     new_num = str(x) + str(n)
%o A236528     if isprime(int(new_num)):
%o A236528       print(int(new_num))
%o A236528       x = new_num
%o A236528       n = 1
%o A236528     else:
%o A236528       n += 1
%o A236528 b(4)
