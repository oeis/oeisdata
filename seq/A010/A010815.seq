%o A010815 (Python)
%o A010815 from math import isqrt
%o A010815 def A010815(n):
%o A010815     m = isqrt(24*n+1)
%o A010815     return 0 if m**2 != 24*n+1 else ((-1)**((m-1)//6) if m % 6 == 1 else (-1)**((m+1)//6)) # _Chai Wah Wu_, Sep 08 2021
%o A010815 (Julia)
%o A010815 function A010815(n)
%o A010815     r = 24 * n + 1
%o A010815     m = isqrt(r)
%o A010815     m * m != r && return 0
%o A010815     iseven(div(m + m % 6, 6)) ? 1 : -1
%o A010815 end # _Peter Luschny_, Sep 09 2021
