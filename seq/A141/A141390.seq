%t A141390 ops5Q[n_] := CompositeQ[n] && GCD[n, 5] == 1 && MultiplicativeOrder[5, n]*(DivisorSum[n, EulerPhi[#]/MultiplicativeOrder[5, #] &] - 1) + 1 == n; Select[Range[6, 10^6], ops5Q] (* _Amiram Eldar_, Jun 24 2019 *)
