%t A190370 g[n_] := n + Floor[n*r/s] + Floor[n*t/s] + Floor[n*u/s];
%t A190370 h[n_] := n + Floor[n*r/t] + Floor[n*s/t] + Floor[n*u/t];
%t A190370 i[n_] := n + Floor[n*r/u] + Floor[n*s/u] + Floor[n*t/u];
