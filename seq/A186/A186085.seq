%p A186085 b:= proc(n, i) option remember; `if`(n=0, `if`(i=1, 1, 0),
%p A186085       `if`(n<0 or i<1, 0, add(b(n-i, i+j), j=-1..1)))
%p A186085     end:
%p A186085 a:= n-> `if`(n=0, 1, b(n-1, 1)):
%p A186085 seq(a(n), n=0..50);  # _Alois P. Heinz_, Jun 11 2013
%t A186085 b[n_, i_] := b[n, i] = If[n == 0, If[i == 1, 1, 0], If[n<0 || i<1, 0, Sum[b[n-i, i+j], {j, -1, 1}]]]; a[n_] := If[n == 0, 1, b[n-1, 1]]; Table[a[n], {n, 0, 50}] (* _Jean-FranÃ§ois Alcover_, Feb 03 2014, after _Alois P. Heinz_ *)
