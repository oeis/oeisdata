%t A323770 nmax = 21; CoefficientList[Series[x (2 - x) Exp[x/(1 - x)]/(2 (1 - x)^2), {x, 0, nmax}], x] Range[0, nmax]!
%t A323770 Table[Sum[Binomial[n - 1, k - 1] Binomial[k + 1, 2] n!/k!, {k, 0, n}], {n, 0, 21}]
