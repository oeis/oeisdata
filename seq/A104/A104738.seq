%o A104738 (MATLAB)
%o A104738 function [ A ] = A104738( N )
%o A104738 % Produce a(1) : a(N)
%o A104738 M = N;
%o A104738 R = [1:M];
%o A104738 v = 1;
%o A104738 A = 1;
%o A104738 count = 1;
%o A104738 n = 1;
%o A104738 while count < N
