%o A295511 (SageMath)
%o A295511 def EuclidTree(n): # with root 1
%o A295511     def DijkstraFusc(m):
%o A295511         a, b, k = 1, 0, m
%o A295511         while k > 0:
%o A295511             if k % 2 == 1: b += a
%o A295511             else: a += b
%o A295511             k = k >> 1
%o A295511         return b
%o A295511     DF = [DijkstraFusc(k) for k in (2^(n-1)..2^n)]
%o A295511     return [DF[j]/DF[j+1] for j in (0..2^(n-1)-1)]
%o A295511 def SchinzelSierpinski(l):
%o A295511     a, b = l.numerator(), l.denominator()
%o A295511     p, q = 1, 2
%o A295511     while q < 1000000000: # search limit
%o A295511         r = a*(q - 1)
%o A295511         if b.divides(r):
%o A295511             p = r // b + 1
%o A295511             if is_prime(p): return p/q
%o A295511         q = next_prime(q)
%o A295511     print("Search limit reached for ", l); return 0
%o A295511 def SSETree(level):
%o A295511     return [SchinzelSierpinski(l) for l in EuclidTree(level)]
%o A295511 # With the imperfection that Sage reduces 2/2 automatically to 1.
%o A295511 for level in (1..6): print(SSETree(level))
