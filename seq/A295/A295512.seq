%e A295512        -21       35      -35       21
%e A295512     -10  221  -77  55  -55  77  -221  10
%p A295512 EuclidTree := proc(n) local k, DijkstraFusc;
%p A295512 DijkstraFusc := proc(m) option remember; local a, b, n; a := 1; b := 0; n := m;
%p A295512 while n > 0 do if type(n, odd) then b := a+b else a := a+b fi; n := iquo(n,2) od; b end:
%p A295512 seq(DijkstraFusc(k)/DijkstraFusc(k+1), k=2^(n-1)..2^n-1) end:
%p A295512 SchinzelSierpinski := proc(l) local a, b, r, p, q, sgn;
%p A295512 a := numer(l); b := denom(l); q := 2; sgn := `if`(a < b, -1, 1);
%p A295512 while q < 1000000000 do r := a*(q - 1); if r mod b = 0 then p := r/b + 1;
%p A295512 if isprime(p) then return(sgn*p*q) fi fi; q := nextprime(q); od;
%p A295512 print("Search limit reached!", a, b) end:
%p A295512 Tree := level -> seq(SchinzelSierpinski(l), l=EuclidTree(level)): seq(Tree(n), n=1..6);
