%o A364835 from itertools import islice
%o A364835 from collections import Counter
%o A364835 def agen(): # generator of terms
%o A364835     an = 1; c = Counter([1])
%o A364835     while True: yield an; an = c[an+1] - c[an]; c[an] += 1
