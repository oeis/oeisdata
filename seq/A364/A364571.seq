%o A364571 A005940(n) = { my(p=2, t=1); n--; until(!n\=2, if((n%2), (t*=p), p=nextprime(p+1))); t };
%o A364571 A054429(n) = ((3<<#binary(n\2))-n-1); \\ From A054429
