%o A364448 (Python)
%o A364448 a = []
%o A364448 ban = [0 for n in range(160)]
%o A364448 for i in range(1000):
%o A364448     can = ban.index(0,1)
%o A364448     ban = [max(b-1,0) for b in ban]
