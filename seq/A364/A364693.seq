%t A364693 A364693[n_]:=With[{d=Divisors[2n]},Catch[For[i=3,i<Length[d]-1,i++,If[Divisible[2n/d[[i]]-2,d[[i]]-1],Throw[1]]];0]]; (* after _Jianing Song_ in A090466 *)
