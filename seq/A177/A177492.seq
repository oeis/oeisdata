%t A177492 f1[n_]:=Length[Last/@FactorInteger[n]]; f2[n_]:=Union[Last/@FactorInteger[n]]; lst={};Do[If[f1[n]>1&&f2[n]=={2},AppendTo[lst,n]],{n,0,8!}];lst
