%e A263856 S_1 = [01], a(1) = 1;
%e A263856 S_2 = [01, 11], a(2) = 2;
%e A263856 S_3 = [01, 101, 11], a(3) = 2;
%e A263856 S_4 = [01, 101, 11, 111], a(4) = 4;
