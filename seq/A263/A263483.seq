%p A263483 p:= 1:
%p A263483 for n from 1 to 100 do
%p A263483   p:= nextprime(p);
%p A263483   A[n]:= p + (p mod 6);
%p A263483 od:
%p A263483 seq(A[n],n=1..100); # _Robert Israel_, Jul 18 2018
%t A263483 Table[(p=Prime[n])+Mod[p,6],{n,100}]
