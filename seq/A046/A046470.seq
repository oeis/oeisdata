%o A046470 from math import isqrt, prod
%o A046470 from sympy import primerange, integer_nthroot, primepi
%o A046470 from oeis_sequences.OEISsequences import bisection
%o A046470 def A046470(n):
%o A046470     def g(x,a,b,c,m): yield from (((d,) for d in enumerate(primerange(b,isqrt(x//c)+1),a)) if m==2 else (((a2,b2),)+d for a2,b2 in enumerate(primerange(b,integer_nthroot(x//c,m)[0]+1),a) for d in g(x,a2,b2,c*b2,m-1)))
%o A046470     def f(x): return int(n+x-1-sum(sum(primepi((x>>1)//prod(c[1] for c in a))-a[-1][0] for a in g(x>>1,0,1,1,m)) for m in range(2,x.bit_length(),2)))
%o A046470     return bisection(f,n+1,n+1) # _Chai Wah Wu_, Dec 19 2025
