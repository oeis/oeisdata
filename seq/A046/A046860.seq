%p A046860 a:= proc(n, k) option remember; `if`([n, k]=[0$2], 1,
%p A046860       add(binomial(n, r)*2^(r*(n-r))*a(r, k-1), r=0..n-1))
%p A046860     end:
%p A046860 seq(seq(a(n,k), k=1..n), n=1..8);  # _Alois P. Heinz_, Apr 21 2020
%t A046860 a[n_ /; n >= 1, k_ /; k >= 1] := a[n, k] = Sum[ Binomial[n, r]*2^(r*(n - r))*a[r, k - 1], {r, 1, n - 1}]; a[_, 0] = 1; Flatten[ Table[ a[n, k], {n, 1, 8}, {k, 0, n - 1}]] (* _Jean-Fran√ßois Alcover_, Dec 12 2011, after formula *)
