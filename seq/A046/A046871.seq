%T A046871 256,289,324,361,400,432,441,484,500,529,576,605,625,648,676,729,784,
%U A046871 841,900,931,961,980,1024,1089,1156,1200,1225,1280,1296,1369,1444,1521
%N A046871 Numbers n such that sigma(2,n) divides sigma(4,n).
%C A046871 sigma_2(n) is the sum of the squares of the divisors of n (A001157).
%C A046871 sigma_4(n) is the sum of the 4th powers of the divisors of n (A001159).
%H A046871 Paolo P. Lava, <a href="/A046871/b046871.txt">Table of n, a(n) for n = 1..1000</a>
%e A046871 n=a(8)=48 of which divisor power sums for k=0,1,2,3,4 are 10,124,3410,131068, 5732210 resp. Here Sigma_2=3410 and Sigma_4=3410*1681.
%p A046871 with(numtheory);
%p A046871 List046871:=proc(q) local a,b,k,n;
%p A046871 for n from 1 to q do a:=divisors(n);  b:=nops(a);
%p A046871   if type(add(a[k]^4,k=1..b)/add(a[k]^2,k=1..b),integer) then print(n); fi;
%p A046871 od; end: List046871(10^6); # _Paolo P. Lava_, Apr 10 2013
%t A046871 Select[Range@ 1600, Divisible[DivisorSigma[4, #], DivisorSigma[2, #]] &] (* _Michael De Vlieger_, May 20 2017 *)
%o A046871 (Magma) [n: n in [1..1600] | IsZero(DivisorSigma(4,n) mod DivisorSigma(2,n))]; // _Bruno Berselli_, Apr 10 2013
%o A046871 (PARI) isok(n) = !(sigma(n, 4) % sigma(n, 2)); \\ _Michel Marcus_, May 21 2017
%Y A046871 Cf. A020487, A020486, A046841.
%K A046871 nonn
