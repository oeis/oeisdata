%U A046838 90625,99376,99625,99976,99995,99996,100000,100001,100025,100376,100625,109376,890625
%N A046838 Numbers k such that k^2 can be obtained from k by inserting a block of digits.
%C A046838 From _Robert Israel_, Oct 24 2025: (Start)
%C A046838 Every term is of one of the following types:
%C A046838   (a) x with d digits such that x^2 == x (mod 10^d).
%C A046838   (b) 10^r + x, x as in (a).
%C A046838   (c) 10^r - 10^d + x, x as in (a).
%C A046838 (End)
%H A046838 Robert Israel, <a href="/A046838/b046838.txt">Table of n, a(n) for n = 1..10000</a>
%p A046838 N:= 10: # for terms of <= N digits
%p A046838 R:= 0, seq(10^i,i=0..N-1),seq(10^i+1,i=1..N-1):
%p A046838 for d from 0 to N do
%p A046838    for b in [chrem([0,1],[2^d,5^d]),chrem([1,0],[2^d,5^d])] do
%p A046838      for r from 1 to N-d do
%p A046838        x:= b + (10^r-1)*10^d;
%p A046838        if floor(x^2/10^(ilog10(x^2)-r+1)) = 10^r-1 then R:= R, x fi;
%p A046838      od;
%p A046838      if b >= 10^(d-1) then
%p A046838        R:= R,b; # for a = 0
%p A046838      fi;
%p A046838      for r from 0 to N-d-1 do
%p A046838          x:= b + 10^(r+d);
%p A046838          if floor(x^2/10^(ilog10(x^2)-r)) = 10^r then R:= R, x fi;
%p A046838        od;
%p A046838 od od:
%p A046838 sort(convert({R},list));
%o A046838 (Python)
%o A046838 def ok(n):
%o A046838     s, t = str(n), str(n**2)
%o A046838     return any(s == t[:i]+t[j:] for i in range(len(t)) for j in range(i, len(t)+1))
%o A046838 print([k for k in range(900000) if ok(k)]) # _Michael S. Branicky_, Sep 12 2025
