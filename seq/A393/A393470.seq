%o A393470 (Python)
%o A393470 def row(n):
%o A393470     counts = [0] * (n - 1)
%o A393470     for i in range(n):
%o A393470         for j in range(n):
%o A393470             k = (i * j) // n
%o A393470             counts[k] += 1
%o A393470     return counts
%o A393470 # triangle:
%o A393470 a = []
%o A393470 for n in range(2, 13):
%o A393470     a.extend(row(n))
