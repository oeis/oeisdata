%e A393206 The a(10) = 5 partitions of A005101(10) = 54 are [54], [12, 42], [18, 36], [24, 30] and [12, 18, 24].
%p A393206 with(NumberTheory):
%p A393206 d := proc(i) option remember; sigma(i) > 2*i; end proc:
%p A393206 c := proc(i) option remember; `if`(i = 0, 0, c(i - 1) + `if`(d(i), i, 0)); end proc:
%p A393206 b := proc(n, i) option remember; `if`(n = 0, 1, `if`(i = 0 or c(i) < n, 0, b(n, i - 1) + `if`(i <= n and d(i), b(n - i, i - 1), 0))); end proc:
%p A393206 a := proc(n) option remember; local s, m, k, i;
%p A393206     s := proc(i, j, n) option remember; `if`(j >= n, i, s(i + 1, `if`(d(i + 1), j + 1, j), n)) end proc;
%p A393206     m := `if`(n = 1, 12, a(n - 1)[1]);
%p A393206     k := `if`(n = 1, 1, n - 1);
%p A393206     i := s(m, k, n);
%p A393206     [i, b(i, i)];
%p A393206 end proc:
