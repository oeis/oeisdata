%o A393332 (Python)
%o A393332 from itertools import groupby, count, islice
%o A393332 from gmpy2 import mpq, mpz, digits
%o A393332 def A393332_gen(): # generator of terms
%o A393332     c, m, r = mpq(2), mpz(1), mpq(1,10)
%o A393332     for k in count(0):
%o A393332         s = sorted(digits(m))
%o A393332         d = len(s)
%o A393332         f = sum(abs(mpq(len(list(g)),d)-r) for a,g in groupby(s))+mpq(10-len(set(s)),10)
%o A393332         if f < c:
%o A393332             c = f
%o A393332             yield k
%o A393332         m <<= 1
%o A393332 A393332_list = list(islice(A393332_gen(),15)) # _Chai Wah Wu_, Feb 17 2026
