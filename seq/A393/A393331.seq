%o A393331 (Python)
%o A393331 from itertools import groupby, count, islice
%o A393331 from gmpy2 import mpq, mpz, digits
%o A393331 def A393331_gen(): # generator of terms
%o A393331     c, m, r = mpq(1), mpz(1), mpq(1,10)
%o A393331     for k in count(0):
%o A393331         s = sorted(digits(m))
%o A393331         d = len(s)
%o A393331         f = sum((mpq(len(list(g)),d)-r)**2 for a,g in groupby(s))+mpq(10-len(set(s)),100)
%o A393331         if f < c:
%o A393331             c = f
%o A393331             yield k
%o A393331         m <<= 1
%o A393331 A393331_list = list(islice(A393331_gen(),15)) # _Chai Wah Wu_, Feb 17 2026
