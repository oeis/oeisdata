%N A393614 Numbers of the form a*b*c*r/60 where (a,b,c) is a primitive Pythagorean triple with inradius r.
%C A393614 Numbers of the form (x - y)^2*(x + y)*x*y^2*(x^2 + y^2)/30 where 1 <= y < x, x and y coprime and one of them even.
%H A393614 Robert Israel, <a href="/A393614/b393614.txt">Table of n, a(n) for n = 1..10000</a>
%F A393614 a(n) = A393526(n)/60.
%e A393614 a(3) = 102 is a term because (with x=4, y=1), the primitive Pythagorean triple (15, 8, 17) has inradius 3 and 15 * 8 * 17 * 3 / 60 = 102.
%p A393614 N:= 10^7: # for terms <= N
%p A393614 Res:= {}: count:= 0:
%p A393614 for x from 2 while x^6 - x^5 - x^2 + x  <= 30*N do
%p A393614   flag:= false;
%p A393614   for y from 1 to x-1 do
%p A393614     P:= (x - y)^2*(x + y)*x*y^2*(x^2 + y^2)/30;
%p A393614   if P > N then flag:= true; y1:= y; break fi;
%p A393614     if igcd(x, y) = 1 and (x+y)::odd  then
%p A393614       count:= count+1; Res:= Res union {P};
%p A393614     fi
%p A393614   od;
%p A393614   if flag then
%p A393614     for y from x-1 to y1+1 by -1 do
%p A393614     P:= (x - y)^2*(x + y)*x*y^2*(x^2 + y^2)/30;
%p A393614     if P > N then break fi;
%p A393614       if igcd(x, y) = 1 and (x+y)::odd  then
%p A393614          count:= count+1; Res:= Res union {P};
%p A393614     fi od fi
%p A393614 od:
%p A393614 sort(convert(Res, list));
