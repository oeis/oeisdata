%N A393060 Lexicographically earliest infinite sequence such that a(i) = a(j) => A393059(i) = A393059(j), for all i, j >= 1.
%C A393060 Restricted growth sequence transform of A393059.
%C A393060 For all i, j >= 1: A305800(i) = A305800(j) => a(i) = a(j) => A393063(i) = A393063(j).
%H A393060 Antti Karttunen, <a href="/A393060/b393060.txt">Table of n, a(n) for n = 1..65537</a>
%H A393060 <a href="/index/Pri#primorialbase">Index entries for sequences related to primorial base</a>.
%o A393060 (PARI)
%o A393060 up_to = 65537;
%o A393060 rgs_transform(invec) = { my(om = Map(), outvec = vector(length(invec)), u=1); for(i=1, length(invec), if(mapisdefined(om,invec[i]), my(pp = mapget(om, invec[i])); outvec[i] = outvec[pp] , mapput(om,invec[i],i); outvec[i] = u; u++ )); outvec; };
%o A393060 A276086(n) = { my(m=1, p=2); while(n, m *= (p^(n%p)); n = n\p; p = nextprime(1+p)); (m); };
%o A393060 A393059(n) = { my(m=1); fordiv(n,d,if(d<n, m = lcm(m,A276086(d)))); (m); };
%o A393060 v393060 = rgs_transform(vector(up_to, i, A393059(i)));
%o A393060 A393060(n) = v393060[n];
