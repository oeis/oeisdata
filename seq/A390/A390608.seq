%S A390608 0,1,2,2,4,4,6,4,6,8,10,8,12,12,16,8,16,12,18,16,24,20,22,16,20,24,18,
%T A390608 24,28,24,30,16,40,32,48,24,36,36,48,32,40,36,42,40,48,44,46,32,42,40,
%U A390608 64,48,52,36,80,48,72,56,58,48,60,60,72,32,96,60,66,64,88,72
%N A390608 a(n) = phi(n) * omega(n).
%C A390608 a(n) is the number of positive integers which are coprime to n multiplied by the number of distinct primes dividing n.
%C A390608 a(n) = phi(n) iff n is in A246655.
%C A390608 a(n) = cototient(n) iff n is in A007694.
%C A390608 a(n) = sigma(n) iff n is in A073567.
%C A390608 a(n) = usigma(n) iff n is in A063795.
%H A390608 Ridouane Oudra, <a href="/A390608/b390608.txt">Table of n, a(n) for n = 1..10000</a>
%F A390608 a(n) = A000010(n) * A001221(n).
%F A390608 a(n) = A122411(n) - A116512(n).
%F A390608 a(n) = Sum_{d|n} f(d) * phi(n/d), where f(n) = A014963(n) - 1.
%F A390608 a(n) = Sum_{d|n, d is a prime power} A057237(d) * phi(n/d).
%F A390608 a(n) is neither multiplicative nor additive, but it satisfies the mixed relation:
%F A390608 a(n*m) = phi(n) * a(m) + phi(m) * a(n), for all n, m such that gcd(n,m) = 1.
%e A390608 a(30) = phi(30)*omega(30) = 8*3 = 24.
%p A390608 with(numtheory): seq(phi(n)*nops(factorset(n)), n=1..120);
%t A390608 Table[EulerPhi[n]*PrimeNu[n], {n, 1, 120}]
%o A390608 (PARI) a(n) = my(f=factor(n)); eulerphi(f)*omega(f); \\ _Michel Marcus_, Nov 12 2025
