%o A390659 (PARI) a_vector(n) = my(v=vector(n+1)); for(i=0, n, v[i+1]=1+2*sum(j=0, (i-1)\2, v[j+1]*v[i-2*j])); v;
%o A390659 (Magma) a := [1]; for n in [1..30] do s := 0; for k in [0..Floor((n-1)/2)] do s +:= a[k+1] * a[n-2*k]; end for; Append(~a, 1 + 2*s); end for; a; // _Vincenzo Librandi_, Jan 12 2026
