%o A390435 (Python)
%o A390435 from math import isqrt
%o A390435 from sympy import integer_nthroot, mobius
%o A390435 from oeis_sequences.OEISsequences import bisection, squarefreepi
%o A390435 def A390435(n):
%o A390435     def f(x):
%o A390435         y = isqrt(x)
%o A390435         c, l = n+x+1, 0
%o A390435         j = isqrt(y)
%o A390435         while j>1:
%o A390435             k2 = integer_nthroot(y//j**2,3)[0]+1
%o A390435             w = squarefreepi(k2-1)
%o A390435             c -= j*(w-l)
%o A390435             l, j = w, isqrt(y//k2**3)
%o A390435         c -= squarefreepi(integer_nthroot(y,3)[0])-l+sum(mobius(k)*(integer_nthroot(y, k)[0]-1) for k in range(2, y.bit_length()))
%o A390435         return c
%o A390435     return bisection(f,n,n) # _Chai Wah Wu_, Nov 06 2025
