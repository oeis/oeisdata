%t A390378 primorial[n_] := Product[Prime[j], {j, n}]; Table[k = n + 1; While[Length@SequencePosition[IntegerDigits[primorial[k]], IntegerDigits[primorial[n]]] == 0, k++]; k, {n, 0, 9}]
%o A390378 (PARI) P(n) = vecprod(primes(n));
%o A390378 a(n) = my(k=n+1, sp=Str(P(n))); while (#strsplit(Str(P(k)), sp) < 2, k++); k; \\ _Michel Marcus_, Dec 18 2025
%o A390378 (Python)
%o A390378 from itertools import count
%o A390378 from gmpy2 import mpz, digits, next_prime
%o A390378 from sympy import primorial, prime
%o A390378 def A390378(n):
%o A390378     if n == 0: return 4
%o A390378     m = mpz(primorial(n))
%o A390378     p = prime(n+1)
%o A390378     s = digits(m)
%o A390378     for k in count(n+1):
%o A390378         m *= p
%o A390378         if s in digits(m):
%o A390378             return k
%o A390378         p = next_prime(p) # _Chai Wah Wu_, Dec 22 2025
