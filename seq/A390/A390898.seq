%N A390898 a(n) = Sum_{k>=0} (1/2)^(k+1) * |Stirling1(n+k,k)|.
%H A390898 Vincenzo Librandi, <a href="/A390898/b390898.txt">Table of n, a(n) for n = 0..300</a>
%F A390898 G.f.: (1/2) * Sum_{k>=0} Product_{j=0..k-1} (1/2 + j*x).
%F A390898 a(0) = 1; a(n) = -2*a(n-1) + 2*Sum_{k=0..n-1} binomial(n+1,k+1) * a(k) * a(n-1-k).
%F A390898 a(n) = 2^n * A032188(n+1).
%t A390898 a[0]=1;
%t A390898 a[n_]:=a[n]=-2 *a[n-1]+2 *Sum[Binomial[n+1,k+1] a[k]*a[n-1-k],{k,0,n-1}];
%t A390898 aList[N_]:=Table[a[n],{n,0,N}]; aList[17] (* _Vincenzo Librandi_, Nov 28 2025 *)
%o A390898 (PARI) a_vector(n) = my(v=vector(n+1)); v[1]=1; for(i=1, n, v[i+1]=-2*v[i]+2*sum(j=0, i-1, binomial(i+1, j+1)*v[j+1]*v[i-j])); v;
%o A390898 (Magma) function aVector(N)
%o A390898     v := [Integers()|1] cat [Integers()|0 : i in [1..N]];
%o A390898     for n in [1..N] do v[n+1] := -2*v[n] + 2* &+[Binomial(n+1,k+1)*v[k+1]*v[n-k] : k in [0..n-1]];
%o A390898 end for; return v; end function; aVector(17); // _Vincenzo Librandi_, Nov 28 2025
