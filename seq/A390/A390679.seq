%e A390679   [ 0,  1,  4,  5,  8,  9]
%e A390679   [ 2,  3,  6,  7, 10, 11]
%e A390679   [12, 13, 16, 17, 20, 21]
%e A390679   [14, 15, 18, 19, 22, 23]
%e A390679   [24, 25, 28, 29, 32, 33]
%e A390679   [26, 27, 30, 31, 34, 35]
%t A390679 A[i_,j_,n_]:=n(2i-(-1)^i-3)+2j-(-1)^j/2+(-1)^i-3/2; a[n_]:=Permanent[Table[A[i,j,n],{i,2n},{j,2n}]]; Join[{1},Array[a,10]]
%o A390679 (PARI) a(n) = matpermanent(matrix(2*n, 2*n, i, j, n*(2*i - (-1)^i - 3) + 2*j - (-1)^j/2 + (-1)^i - 3/2)); \\ _Michel Marcus_, Nov 15 2025
