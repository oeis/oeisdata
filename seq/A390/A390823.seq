%t A390823 nn = 5000; mm = Sqrt[nn]; i = 1; MapIndexed[Set[S[First[#2]], #1] &, Complement[Select[Range[mm], Not @* PrimePowerQ], Union@ Flatten@ Table[a^2*b^3, {b, Surd[mm, 3]}, {a, Sqrt[mm/b^3]} ] ] ]; Union@ Reap[While[j = 2; While[S[i]^j < nn, Sow[S[i]^j]; j++]; j > 2, i++] ][[-1, 1]]
%o A390823 (Python)
%o A390823 from math import isqrt
%o A390823 from sympy import integer_nthroot, primepi
%o A390823 from oeis_sequences.OEISsequences import bisection, squarefreepi
%o A390823 def A390823(n):
%o A390823     def g(x):
%o A390823         c, l, j = x-squarefreepi(integer_nthroot(x,3)[0])-primepi(x), 0, isqrt(x)
%o A390823         while j>1:
%o A390823             k2 = integer_nthroot(x//j**2,3)[0]+1
%o A390823             w = squarefreepi(k2-1)
%o A390823             c += j*(l-w)
%o A390823             l, j = w, isqrt(x//k2**3)
%o A390823         return c+l
%o A390823     def f(x): return n+x-sum(g(integer_nthroot(x, k)[0]) for k in range(2,x.bit_length()))
%o A390823     return bisection(f,n+1,n+1) # _Chai Wah Wu_, Dec 02 2025
