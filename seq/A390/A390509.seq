%t A390509 g[w_] := StringJoin[Map[ToString[#[[1]]] <> IntegerString[Length[#], 2] &, Split[Characters[w]]]];
%t A390509 s[n_] := Map[StringJoin[Map[ToString, Rest[IntegerDigits[#, 2]]]] &, Range[2, 2^(n + 1) - 1]];
%t A390509 m = Map[g, s[11]];
%t A390509 m1 = ToExpression /@ Characters /@ m;
%t A390509 Select[Range[Length[m1]], Count[m1[[#]], 0] == Count[m1[[#]], 1] &]
