%T A390604 1468,251,318,432,924,68,5027,76,1693,304,538,252,4225,103,673,431,
%U A390604 3230,123,12146,133,2159,1455,973,155,7533,730,2642,726,2964,188,5924,564
%N A390604 a(n) is the number of ways n^n can be written as s^t*u^v, where s, t, u, v are integers > 1 and s^t <= u^v.
%C A390604 a(n) is the number of solutions to A072103(x)*A072103(y) = A000312(n), where x <= y.
%F A390604 a(n) = Sum_{i=2..l/2} (tau(e(d[i])) - 1)*(tau(e(d[l-i+1])) - 1) + E(n), where d = Divisors(n^n), l = |d|, e(x) = gcd of the exponent list in the prime factorization of x, and E(n) = 0 if n is odd, otherwise E(n) = b*(b + 1)/2 with b = tau(e(d[(l + 1)/2])) - 1.
%e A390604 a(4) = 7 because 4^4 = 2^2*2^6 = 2^2*4^3 = 2^2*8^2 = 2^3*2^5 = 2^4*2^4 = 2^4*4^2 = 4^2*4^2.
%p A390604 with(NumberTheory):
%p A390604 A390604 := proc(n)
%p A390604     local d,l,i,e,g,t,a,b,p;
%p A390604     e := proc(x) local p; igcd(seq(p[2], p in ifactors(x)[2])) end proc;
%p A390604     d := sort(convert(Divisors(n^n),list));
%p A390604     l := nops(d);
%p A390604     a := 0;
%p A390604     for i from 2 to iquo(l,2) do
%p A390604         g := e(d[i]);
%p A390604         t := e(d[l-i+1]);
%p A390604         a := a + (tau(g)-1)*(tau(t)-1);
%p A390604     od;
%p A390604     if type(n, even) then
%p A390604         b := tau(e(d[(l+1)/2]))-1;
%p A390604         a := a + b*(b+1)/2;
%p A390604     fi;
%p A390604     a
%p A390604 end proc:
%p A390604 seq(A390604(n),n=1..55);
