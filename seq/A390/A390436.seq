%o A390436 (Python)
%o A390436 from math import isqrt
%o A390436 from sympy import integer_nthroot, mobius
%o A390436 from oeis_sequences.OEISsequences import bisection, squarefreepi
%o A390436 def A390436(n):
%o A390436     def g(x):
%o A390436         c, l = squarefreepi(integer_nthroot(x,3)[0])+sum(mobius(k)*(integer_nthroot(x, k)[0]-1) for k in range(2, x.bit_length()))-1, 0
%o A390436         j = isqrt(x)
%o A390436         while j>1:
%o A390436             k2 = integer_nthroot(x//j**2,3)[0]+1
%o A390436             w = squarefreepi(k2-1)
%o A390436             c += j*(w-l)
%o A390436             l, j = w, isqrt(x//k2**3)
%o A390436         return c-l
%o A390436     def f(x): return n+x-sum(g(integer_nthroot(x, k)[0]) for k in range(3, x.bit_length()))
%o A390436     return bisection(f,n,n) # _Chai Wah Wu_, Nov 08 2025
