%o A390651 (Python) # Using sample_gauss_kuzmin_distribution function from A084580.
%o A390651 from sympy import continued_fraction_convergents
%o A390651 coeffs = [0] + sample_gauss_kuzmin_distribution(100)
%o A390651 convergent_generator = continued_fraction_convergents(coeffs)
%o A390651 A390651 = [frac.numerator for frac in convergent_generator]
