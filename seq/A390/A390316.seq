%o A390316 (PARI) A390316_up_to_N(N) = { \\ gives terms <= N
%o A390316 my(A390254 = vector(N+1), e = vector(1+N), res = vector(1+N), len = N, sumk); \\ res[1+n] = -1 (resp. 1) means A390254(n) > (resp. -1) c*(4/3)^n
%o A390316 A390254[1] = 2; e[1] = 1/3;
%o A390316 for(n=1, N, A390254[1+n] = round(4*A390254[n]/3); e[1+n] = -kronecker(A390254[1+n], 3)/3);
%o A390316 for(n=0, N, sumk = 0; for(k=0, oo, if(n+k > len, A390254 = concat(A390254, round(4*A390254[1+len]/3)); e = concat(e, -kronecker(A390254[1+len+1], 3)/3); len++); sumk += (3/4)^k*e[1+n+k]; if(abs(sumk) >= (3/4)^k, res[1+n] = sign(sumk); break())));
%o A390316 select(n->res[n+1]==1, vector(N+1, i, i-1));
%o A390316 }
