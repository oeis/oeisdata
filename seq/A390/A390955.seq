%t A390955 nn = 45000; mm = Sqrt[nn]; i = 1; MapIndexed[Set[S[First[#2]], #1] &, Select[Range[mm], And[CompositeQ[#], SquareFreeQ[#]] &]]; Union@ Reap[While[j = 2; While[S[i]^j < nn, If[OddQ[#], Sow[#]] &[S[i]^j]; j++]; j > 2, i++] ][[-1, 1]]
%o A390955 (Python)
%o A390955 from math import isqrt
%o A390955 from sympy import mobius, integer_nthroot, primepi
%o A390955 from oeis_sequences.OEISsequences import bisection
%o A390955 def A390955(n):
%o A390955     def g(x): return int(sum(mobius(k)*(x//k**2+1>>1) for k in range(1, isqrt(x)+1, 2))-primepi(x))
%o A390955     def f(x): return n+x-sum(g(integer_nthroot(x,k)[0]) for k in range(2,x.bit_length()))
%o A390955     return bisection(f,n+1,n+1) # _Chai Wah Wu_, Dec 10 2025
