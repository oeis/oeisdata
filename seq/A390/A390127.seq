%o A390127 (Python)
%o A390127 from math import isqrt
%o A390127 from sympy import mobius, primepi, integer_nthroot
%o A390127 from oeis_sequences.OEISsequences import bisection
%o A390127 def A390127(n):
%o A390127     def g(x): return int(sum(mobius(k)*(x//k**2) for k in range(1, isqrt(x)+1))-primepi(x))
%o A390127     def f(x): return n-4+x+(y:=x.bit_length())-sum(g(integer_nthroot(x,k)[0]) for k in range(4,y))
%o A390127     return bisection(f,n+1000,n+1000) # _Chai Wah Wu_, Feb 04 2026
