%N A390784 a(n) is the number of positive integers k for which Sum_{i=1..r} p_i = Sum_{i=1..r} e_i = n, where p_1^e_1*...*p_r^e_r is the canonical prime factorization of k.
%C A390784 a(n) is the number of positive integers k for which A008472(k) = A001222(k) = n.
%H A390784 Felix Huber, <a href="/A390784/b390784.txt">Table of n, a(n) for n = 1..10000</a>
%F A390784 a(n) = Sum_{i=1..A024936(n)} T(n,i)*binomial(n-1,i-1), where T(n,i) is the number of partitions of n into i distinct prime parts.
%e A390784 The a(5) = 5 positive integers are 2^4*3^1 = 48, 2^3*3^2 = 72, 2^2*3^3 = 108, 2^1*3^4 = 162, 5^5 = 3125.
%p A390784 # Processes b and T (slightly adapted) by Alois P. Heinz (A219180).
%p A390784 b:=proc(n,i)
%p A390784    option remember;
%p A390784    `if`(n=0,[1],`if`(i<1,[],zip((x,y)->x+y,b(n,i-1),[0,`if`(ithprime(i)>n,[],b(n-ithprime(i),i-1))[]],0)))
%p A390784 end:
%p A390784 T:=proc(n)
%p A390784    local l;
%p A390784    l:=b(n,numtheory[pi](n));
%p A390784    while nops(l)>0 and l[-1]=0 do
%p A390784       l:=subsop(-1=NULL,l)
%p A390784    od;
%p A390784    `if`(nops(l)>0,subsop(1=NULL,l),l)
%p A390784 end:
%p A390784 A390784:=proc(n)
%p A390784    local i,l;
%p A390784    l:=T(n);
%p A390784    add(l[i]*binomial(n-1,i-1),i=1..nops(l))
%p A390784 end:
%p A390784 seq(A390784(n),n=1..47);
