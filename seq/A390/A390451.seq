%o A390451 (Python) # see links for a version suitable for producing b-file and a-file
%o A390451 from itertools import permutations
%o A390451 from sympy import divisors, isprime
%o A390451 def ok(n):
%o A390451     d = list(map(str, divisors(n)))
%o A390451     return n > 2 and sum(map(int, "".join(d)))%3 and any(isprime(int("".join(p))) for p in permutations(d))
%o A390451 print([k for k in range(190) if ok(k)]) # _Michael S. Branicky_, Nov 06 2025
