%e A390745   164: (2,3,3)
%e A390745   168: (2,2,4)
%e A390745   170: (2,2,2,2)
%t A390745 stc[n_]:=Differences[Prepend[Join @@ Position[Reverse[IntegerDigits[n,2]],1],0]]//Reverse;
%t A390745 uncha[tar_,ini_]:=(cur={ini}; Do[AppendTo[cur,tar[[k]]-If[k==1,ini,cur[[k]]]],{k,Length[tar]}];cur);
%t A390745 Select[Range[100],Length[Select[Table[uncha[stc[#],i], {i,Max[stc[#]]}],Min@@#>=1&]]==1&]
