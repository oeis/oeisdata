%o A390483 (Python)
%o A390483 from math import isqrt
%o A390483 from functools import lru_cache
%o A390483 @lru_cache(maxsize=None)
%o A390483 def A002321(n):
%o A390483     if n == 0:
%o A390483         return 0
%o A390483     c, j = n, 2
%o A390483     k1 = n//j
%o A390483     while k1 > 1:
%o A390483         j2 = n//k1 + 1
%o A390483         c += (j2-j)*A002321(k1)
%o A390483         j, k1 = j2, n//j2
%o A390483     return j-c
%o A390483 def A390483(n):
%o A390483     m = 1<<n
%o A390483     c, j, k1 = isqrt(m)+1, 1, m
%o A390483     while k1 > 1:
%o A390483         j2 = isqrt(m//k1) + 1
%o A390483         c += (j2-j)*A002321(k1)
%o A390483         j, k1 = j2, m//j2**2
%o A390483     return c-j # _Chai Wah Wu_, Nov 13 2025
