%F A390656 a(n) = 1 + 2 * Sum_{k=0..n-1} a(floor(k/2)) * a(n-1-k).
%t A390656 Clear[a]; a[0]=1; a[n_]:=a[n]=1+2*Sum[a[Floor[k/2]]*a[n-1-k],{k,0,n-1}]; Table[a[n],{n,0,40}] (* _Vincenzo Librandi_, Jan 11 2026 *)
%o A390656 (PARI) a_vector(n) = my(v=vector(n+1)); for(i=0, n, v[i+1]=1+2*sum(j=0, i-1, v[j\2+1]*v[i-j])); v;
%o A390656 (Magma) N := 40; a := [1]; for n in [1..N] do s := 1; for k in [0..n-1] do s +:= 2 * a[Floor(k/2) + 1] * a[n - k]; end for; Append(~a, s); end for; a; // _Vincenzo Librandi_, Jan 11 2026
