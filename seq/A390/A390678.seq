%t A390678 stc[n_]:=Differences[Prepend[Join @@ Position[Reverse[IntegerDigits[n,2]],1],0]]//Reverse;
%t A390678 uncha[tar_,ini_]:=(cur={ini}; Do[AppendTo[cur,tar[[k]]-If[k==1,ini,cur[[k]]]],{k,Length[tar]}];cur);
%t A390678 Select[Range[1000],Length[Select[Table[uncha[stc[#],i], {i,Max[stc[#]]}],Min@@#>=1&]]==0&&!MemberQ[stc[#],1]&]
