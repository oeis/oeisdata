%t A390952 With[{nn = 9000}, Union@ Flatten@ Table[If[EvenQ[#] && GCD @@ FactorInteger[#][[;; , -1]] == 1, #, Nothing] &[a^2*b^3], {b, Surd[nn, 3]}, {a, Sqrt[nn/b^3] } ] ]
%o A390952 (SageMath)
%o A390952 def isAchilles(n: int) -> bool:
%o A390952     factors = factor(n)
%o A390952     exps = [e for (_, e) in factors]
%o A390952     min_e = min(exps)
%o A390952     g = reduce(gcd, exps)
%o A390952     return g == 1 and min_e > 1
%o A390952 def isA390952(n): return 2.divides(n) and isAchilles(n)
%o A390952 print([n for n in range(1, 9001) if isA390952(n)])  # _Peter Luschny_, Nov 28 2025
