%o A390511 (Python)
%o A390511 from sympy import nextprime, prime
%o A390511 from itertools import count, islice
%o A390511 def agen(NN=10000): # generator of terms for n <= NN
%o A390511     n, pk, residues = 1, 1, "X"*NN
%o A390511     for k in count(1):
%o A390511         pk = nextprime(pk)
%o A390511         resk = pk%3
%o A390511         if resk == 2:
%o A390511             while residues.endswith("1"*n) and n <= NN:
%o A390511                 yield prime(k-n)
%o A390511                 n += 1
%o A390511         residues = residues[1:] + str(resk)
%o A390511 print(list(islice(agen(), 13))) # _Michael S. Branicky_, Jan 11 2026
