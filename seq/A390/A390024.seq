%e A390024 4*(4-7) = 6*(6-8), so (4,7,6,8) is in the list.
%t A390024 solnsM[m_, max_] := Module[{ans = {}, rhs = {}, u, v, w, lhs, matching},
%t A390024    Do[Do[AppendTo[rhs, {v*(v - w), v, w}], {w, max}], {v, m*(m + max)}];
%t A390024    rhs = GatherBy[rhs, First];
%t A390024    Do[lhs = m*(m - u); matching = Select[rhs, #[[1, 1]] == lhs &];
%t A390024     If[Length[matching] > 0, Do[AppendTo[ans,
%t A390024        Map[{m, u, #[[2]], #[[3]]} &, matching[[1]]]], {i,
%t A390024        Length[matching]}]], {u, max}];
%t A390024    ans = Flatten[ans, 1];
%t A390024    Select[Union[Map[Sort[{#, RotateLeft[#, 2]}][[1]] &,
%t A390024       Sort[Select[DeleteDuplicates[ans],
%t A390024         Length[Union[#]] == 4 &]]]], #[[1]] == m &]];
%t A390024 TableForm[solns = solnsM[4, 100],
%t A390024  TableHeadings -> {None, {"m", "u", "v", "w"}}]
%t A390024 Map[#[[2]] &, solns] (* u, A390024 *)
%t A390024 Map[#[[3]] &, solns] (* v, A390025 *)
%t A390024 Map[#[[4]] &, solns] (* w, A390026 *)
