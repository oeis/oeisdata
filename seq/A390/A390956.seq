%o A390956 (Python)
%o A390956 from math import isqrt
%o A390956 from sympy import mobius, integer_nthroot
%o A390956 from oeis_sequences.OEISsequences import bisection
%o A390956 def A390956(n):
%o A390956     def g(x): return int(sum(mobius(k)*((x>>1)//k**2+1>>1) for k in range(1, isqrt(x>>1)+1, 2))-1)
%o A390956     def f(x): return n+x-sum(g(integer_nthroot(x,k)[0]) for k in range(2,x.bit_length()))
%o A390956     return bisection(f,n+1,n+1) # _Chai Wah Wu_, Dec 10 2025
