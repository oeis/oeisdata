%U A390179 161394,163238,163248,165684,165964,167910,173218,181948,185440,190842,193954,211704,222186,227002
%N A390179 Numbers that can be represented as p^r + q^s with 4 distinct odd primes p, q, r, s.
%H A390179 Robert Israel, <a href="/A390179/b390179.txt">Table of n, a(n) for n = 1..10000</a>
%F A390179 a(n) == 0 (mod 2).
%e A390179 a(1) = 18138 = 7^5 + 11^3.
%p A390179 N:= 10^6: # for terms <= N
%p A390179 P:= select(isprime, [seq(i,i=3..floor(N^(1/3)),2)]):
%p A390179 R:= NULL:
%p A390179 for p in P do
%p A390179   for r in P do
%p A390179     if r = p then next fi;
%p A390179     v:= p^r;
%p A390179     if v > N then break fi;
%p A390179     for q in P do
%p A390179       if q >= p then break fi;
%p A390179       if q = r then next fi;
%p A390179       for s in P do
%p A390179         if member(s, {p,q,r}) then next fi;
%p A390179         w:= v + q^s;
%p A390179         if w > N then next fi;
%p A390179         R:= R,w
%p A390179 od od od od:
%p A390179 sort(convert({R},list)); # _Robert Israel_, Oct 30 2025
