%t A067116 Table[Floor[FromDigits[Flatten[IntegerDigits/@Range[n]]]/Total[Range[n]]],{n,20}] (* _Harvey P. Dale_, Jul 22 2022 *)
%o A067116 (PARI) digitsIn(x)= { local(d); if (x==0, return(1)); d=1 + log(x)\log(10); if (10^d == x, d++, if (10^(d-1) > x, d--)); return(d) }
%o A067116 Concat(a, b)= { return(a*10^digitsIn(b) + b) }
%o A067116 { c=0; s=0; for (n=1, 100, c=Concat(c, n); s+=n; write("b067116.txt", n, " ", floor(c/s)) ) } \\ _Harry J. Smith_, May 24 2010
