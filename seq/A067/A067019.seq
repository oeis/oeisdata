%o A067019 from oeis_sequences.OEISsequences import bisection
%o A067019 def A067019(n):
%o A067019     def g(x,a,b,c,m): yield from (((d,) for d in enumerate(primerange(b,isqrt(x//c)+1),a)) if m==2 else (((a2,b2),)+d for a2,b2 in enumerate(primerange(b,integer_nthroot(x//c,m)[0]+1),a) for d in g(x,a2,b2,c*b2,m-1)))
%o A067019     def f(x): return int(n+2+sum(sum(primepi(x//prod(c[1] for c in a))-a[-1][0] for a in g(x,0,1,1,m)) for m in range(2,x.bit_length()+1,2))+sum(sum(primepi((x>>1)//prod(c[1] for c in a))-a[-1][0] for a in g(x>>1,0,1,1,m)) for m in range(2,x.bit_length(),2)))
%o A067019     return bisection(f,n,n) # _Chai Wah Wu_, Dec 19 2025
