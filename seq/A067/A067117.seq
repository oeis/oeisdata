%t A067117 Table[Floor[FromDigits[Flatten[IntegerDigits/@Range[n]]]/n!],{n,30}] (* _Harvey P. Dale_, Aug 27 2014 *)
%o A067117 (PARI)
%o A067117 digitsIn(x)= { local(d); if (x==0, return(1)); d=1 + log(x)\log(10); if (10^d == x, d++, if (10^(d-1) > x, d--)); return(d) }
%o A067117 Concat(a, b)= { return(a*10^digitsIn(b) + b) }
%o A067117 { c=0; s=0; for (n=1, 200, c=Concat(c, n); s+=n; write("b067117.txt", n, " ", floor(c/n!)) ) } \\ _Harry J. Smith_, May 25 2010
