%o A067398 (Haskell)
%o A067398 a067398 :: Integer -> Integer
%o A067398 a067398 0 = 0
%o A067398 a067398 n = orm n n where
%o A067398    orm 1 v = v
%o A067398    orm u v = orm (shiftR u 1) (shiftL v 1) .|. if odd u then v else 0
