%p A116998 a:= proc(n) option remember; local k, t;
%p A116998       t:= nops(ifactors(a(n-1))[2]);
%p A116998       for k from 1+a(n-1) while nops(ifactors(k)[2])<t do od; k
%p A116998     end: a(1):=1:
