%o A034932 a034932 n k = a034932_tabl !! n !! k
%o A034932 a034932_row n = a034932_tabl !! n
%o A034932 a034932_tabl = iterate
%o A034932    (\ws -> zipWith ((flip mod 16 .) . (+)) ([0] ++ ws) (ws ++ [0])) [1]
