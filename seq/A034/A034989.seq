%N A034989 Smallest squares starting with a string of n 6's.
%t A034989 For[i = 1, i < 20, i++, zeros = 0; isFound = False;
%t A034989 While[ !isFound, low = Ceiling[ Sqrt[ 2/3*(10^i - 1)*10^zeros ] ]; high = Floor[ Sqrt[ (2/3*(10^i - 1) + 1)*10^zeros ] ]; isFound = (low <= high); zeros++ ]; Print[ low, " ", low^2 ] ]
