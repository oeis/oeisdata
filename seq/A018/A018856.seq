%t A018856 a[n_] := Block[{k = 1, m = Floor[ Log[10, n]]}, While[ Log[10, 2^k] < Floor[ Log[10, n]], k++ ]; While[ Quotient[2^k, 10^(Floor[k*Log[10, 2]] - m)] != n, k++ ]; k]; a[1] = 0; Array[a, 73] (* _Robert G. Wilson v_, Jun 02 2009 *)
