%o A210512 (Python)
%o A210512 from sympy import isprime
%o A210512 def xf(n): return int(str(n)*2+'3')
%o A210512 def ok(n): return isprime(xf(n))
%o A210512 print(list(map(xf, filter(ok, range(1, 140))))) # _Michael S. Branicky_, May 21 2021
