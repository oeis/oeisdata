%o A246056 {a(n)=local(A=1); A=sum(m=0, n, x^m/(1-x)^(2*m+1) * sum(k=0, m, binomial(m, k)^2*2^(m-k)*3^k*x^k) * sum(k=0, m, binomial(m, k)^2*x^k) +x*O(x^n)); polcoeff(A, n)}
%o A246056 for(n=0, 25, print1(a(n), ", "))
%o A246056 (PARI) /* By a binomial identity: */
%o A246056 {a(n)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2 * 2^(m-k)* sum(j=0, k, binomial(k, j)^2 * 3^j * x^j)+x*O(x^n))), n)}
%o A246056 for(n=0, 25, print1(a(n), ", "))
%o A246056 (PARI) /* By a binomial identity: */
%o A246056 {a(n)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2 * sum(j=0, k, binomial(k, j)^2 * 2^(k-j) * 3^j * x^j)+x*O(x^n))), n)}
%o A246056 for(n=0, 25, print1(a(n), ", "))
