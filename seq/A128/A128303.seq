%o A128303 (Python)
%o A128303 from math import isqrt, prod
%o A128303 from sympy import primerange, integer_nthroot, primepi
%o A128303 from oeis_sequences.OEISsequences import bisection
%o A128303 def A128303(n):
%o A128303     def g(x,a,b,c,m): yield from (((d,) for d in enumerate(primerange(b,isqrt(x//c)+1),a)) if m==2 else (((a2,b2),)+d for a2,b2 in enumerate(primerange(b,integer_nthroot(x//c,m)[0]+1),a) for d in g(x,a2,b2,c*b2,m-1)))
%o A128303     def f(x): return int(n+x+((t:=primepi(s:=isqrt(x)))*(t-1)>>1)-sum(primepi(x//p) for p in primerange(s+1)))
%o A128303     m = bisection(f,n,n)**2
%o A128303     return int(sum(primepi(m//prod(c[1] for c in a))-a[-1][0] for a in g(m,0,1,1,4))) # _Chai Wah Wu_, Jan 02 2026
