%t A193792 z = 8; a = 1; b = 3;
%t A193792 p[n_, x_] := (a*x + b)^n
%t A193792 q[n_, x_] := 1 + x^n ; q[n_, 0] := q[n, x] /. x -> 0;
%t A193792 t[n_, k_] := Coefficient[p[n, x], x^k]; t[n_, 0] := p[n, x] /. x -> 0;
