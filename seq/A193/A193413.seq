%o A193413 (Python)
%o A193413 def ok(n):
%o A193413     s = str(n)
%o A193413     if s[0] != s[-1]: return False
%o A193413     r = s.rfind(min(s))
%o A193413     first, last = list(s[:r]), list(s[r:])
%o A193413     return first == sorted(first, reverse=True) and last == sorted(last)
%o A193413 print([k for k in range(1, 2223) if ok(k)]) # _Michael S. Branicky_, Feb 01 2026
