%p A298647       `if`(min(y, v, x-max(y, v))<0, 0, `if`(x=0, 1, (l-> add(add(
%p A298647       `if`(y=v+(j-i)/2, 1, z)*b(x-1, y+i, v+j), i=l), j=l))([-1, 1]))))
%p A298647     end:
