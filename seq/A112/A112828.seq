%o A112828 (Python)
%o A112828 from sympy import bernoulli, n_order
%o A112828 def period_length(denom):
%o A112828     while denom % 2 == 0: denom //= 2
%o A112828     while denom % 5 == 0: denom //= 5
%o A112828     if denom == 1: return 0
%o A112828     return n_order(10, denom)
%o A112828 A112828 = [period_length(bernoulli(i).denominator) for i in range(0, 150, 2)]  # _Jwalin Bhatt_, Oct 30 2025
