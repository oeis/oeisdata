%o A112829 (Python)
%o A112829 from sympy import bernoulli, n_order
%o A112829 def period_length(denom):
%o A112829     while denom % 2 == 0: denom //= 2
%o A112829     while denom % 5 == 0: denom //= 5
%o A112829     if denom == 1: return 0
%o A112829     return n_order(10, denom)
%o A112829 A112829 = [i//period_length(bernoulli(i).denominator) for i in range(2, 150, 2)]  # _Jwalin Bhatt_, Nov 03 2025
