%t A379311 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A379311 Table[Length[Select[prix[n],#==1||PrimeQ[#]&]],{n,100}]
%o A379311 (PARI) A080339(n) = n == 1 || isprime(n);
%o A379311 a(n) = {my(f = factor(n)); sum(i = 1, #f~, f[i, 2] * A080339(primepi(f[i, 1])));} \\ _Amiram Eldar_, Jan 17 2026
