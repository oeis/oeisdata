%t A356544 SeparatedPairQ[A_][B_] := AnyTrue[A, And @@ MapThread[SubsetQ, {#, B}] &];
%t A356544 Table[Length[With[{X = Range[n]},
%t A356544 Select[Cases[Subsets@Subsets@X, {{}, ___, X}],
%t A356544    F |-> SubsetQ[F, Intersection @@@ Subsets[F, {2}]]
