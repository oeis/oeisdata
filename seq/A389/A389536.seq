%o A389536 (PARI) /* Using Functional Equation in Definition */
%o A389536 {a(n) = my(A=x); for(i=0, #binary(n),
%o A389536 A = subst(A, x, x^2*(1 + 2*x) +x^2*O(x^n) )/x ); polcoef(A, n)}
%o A389536 for(n=1, 40, print1(a(n), ", "))
%o A389536 (PARI) /* Using Infinite Product Formula */
%o A389536 {F(n) = my(G=x); if(n==0, G=x, if(n==1, G = 1 + 2*x, G = 1 + (F(n-1) - 1)^2/2 * F(n-1) )); G}
%o A389536 {a(n) = my(A = prod(k=0, #binary(n), F(k) +x*O(x^n))); polcoef(A, n)}
%o A389536 for(n=1, 40, print1(a(n), ", "))
