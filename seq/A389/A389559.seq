%o A389559 (Python)
%o A389559 from mpmath import iv
%o A389559 def a_upto(limit):
%o A389559     sum_, highs = iv.mpf('0'), [0]
%o A389559     for i in range(1, 1+limit):
%o A389559         t = 2*i + 1
%o A389559         sum_ += iv.cos(t)**t
%o A389559         if sum_ > 0:
%o A389559             highs.append(i)
%o A389559             sum_ = 0
%o A389559     return highs
%o A389559 print(a_upto(40000)) # _Jwalin Bhatt_, Nov 13 2025
