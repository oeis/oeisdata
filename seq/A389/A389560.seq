%o A389560 (Python)
%o A389560 from mpmath import iv
%o A389560 def a_upto(limit):
%o A389560     sum_, lows = iv.mpf('0'), [0]
%o A389560     for i in range(1, 1+limit):
%o A389560         t = 2*i + 1
%o A389560         sum_ += iv.cos(t)**t
%o A389560         if sum_ < 0:
%o A389560             lows.append(i)
%o A389560             sum_ = 0
%o A389560     return lows
%o A389560 print(a_upto(11000)) # _Jwalin Bhatt_, Nov 13 2025
