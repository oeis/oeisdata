%N A389896 Lexicographically earliest infinite sequence such that a(i) = a(j) => A007814(i) = A007814(j), A343029(i) = A343029(j) and A343030(i) = A343030(j) for all i, j >= 1.
%C A389896 Restricted growth sequence transform of the triple [A007814(n), A343029(n), A343030(n)].
%C A389896 For all i, j >= 1: A389897(i) = A389897(j) => a(i) = a(j) => A361020(i) = A361020(j).
%H A389896 Antti Karttunen, <a href="/A389896/b389896.txt">Table of n, a(n) for n = 1..65537</a>
%o A389896 (PARI)
%o A389896 up_to = 65537;
%o A389896 rgs_transform(invec) = { my(om = Map(), outvec = vector(length(invec)), u=1); for(i=1, length(invec), if(mapisdefined(om,invec[i]), my(pp = mapget(om, invec[i])); outvec[i] = outvec[pp] , mapput(om,invec[i],i); outvec[i] = u; u++ )); outvec; };
%o A389896 A007814(n) = valuation(n, 2);
%o A389896 A343029(n) = { my(t=1, ret=0); for(i=0, if(n, logint(n, 2)), if(bittest(n, i), ret+=t, t=!t)); ret; };
%o A389896 A343030(n) = { my(t=0, ret=0); for(i=0, if(n, logint(n, 2)), if(bittest(n, i), ret+=t, t=!t)); ret; };
%o A389896 Aux389896(n) = [A007814(n), A343029(n), A343030(n)];
%o A389896 v389896 = rgs_transform(vector(up_to,n,Aux389896(n)));
%o A389896 A389896(n) = v389896[n];
