%o A389601 (Python)
%o A389601 from sympy.combinatorics.partitions import IntegerPartition
%o A389601 from sympy.utilities.iterables import partitions, multiset_permutations
%o A389601 def compositions(n): yield from (c for p in partitions(n) for c in multiset_permutations(IntegerPartition(p).partition))
%o A389601 def diffs(t): return [t[i]-t[i-1] for i in range(1, len(t))]
%o A389601 def a(n): return sum(1 for c in compositions(n) if len(t:=diffs([0]+c))==len(set(t)))
%o A389601 print([a(n) for n in range(20)]) # _Michael S. Branicky_, Oct 25 2025
