%o A389480 (Python)
%o A389480 from math import gcd
%o A389480 from sympy import totient
%o A389480 def A389480(n,k):
%o A389480     if k == 0 or k == n or 2*k == n:
%o A389480         return 1
%o A389480     else:
%o A389480         return totient(n//gcd(n,k))//(1+(n//gcd(n,k))%2)
