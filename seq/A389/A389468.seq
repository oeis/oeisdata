%o A389468 A003961(n) = { my(f=factor(n)); for (i=1, #f~, f[i, 1] = nextprime(f[i, 1]+1)); factorback(f); };
%o A389468 A389468list(up_to_row_n) = { my(lista=List([])); for(n=1, up_to_row_n, for(k=n,A003961(n)-1,if(1==gcd(k,n), listput(lista,k)))); (lista); };
