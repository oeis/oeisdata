%e A389924     5: (2,1)
%e A389924    10: (2,2)
%e A389924    19: (3,1,1)
%e A389924    20: (2,3)
%e A389924    37: (3,2,1)
%e A389924    38: (3,1,2)
%e A389924    40: (2,4)
%e A389924    71: (4,1,1,1)
%e A389924    73: (3,3,1)
