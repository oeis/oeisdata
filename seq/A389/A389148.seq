%o A389148 from math import prod
%o A389148 from itertools import count, islice
%o A389148 from sympy import factorint, isprime
%o A389148 def A389148_gen(): # generator of terms
%o A389148     g, p, r = [set()], 0, {}
%o A389148     for c in count(2):
%o A389148         f = factorint(c)
%o A389148         d = g[-1]^set(i for i, j in f.items() if j&1)
%o A389148         for h in g:
%o A389148             if (t:=prod(d^h)) not in r:
%o A389148                 r[t] = c-1
%o A389148         g.append(d)
%o A389148         if isprime(c):
%o A389148             p = c-2
%o A389148         elif any(not c%q for q in (2,3,5,7,11)):
%o A389148             continue
%o A389148         elif max(f.values())>1:
%o A389148             continue
%o A389148         else:
%o A389148             for b in range(c-2,p,-1):
%o A389148                 u = d^g[b]
%o A389148                 for a in range(b-1,1,-1):
%o A389148                     e = prod(u^g[a])
%o A389148                     if e in r and r[e]<a:
%o A389148                         break
%o A389148                 else:
%o A389148                     continue
%o A389148                 break
%o A389148             else:
%o A389148                 yield c
%o A389148 A389148_list = list(islice(A389148_gen(),10)) # _Chai Wah Wu_, Sep 25 2025, updated Oct 05 2025
