%N A389898 Lexicographically earliest infinite sequence such that a(i) = a(j) => A126760(i) = A126760(j), A343029(i) = A343029(j) and A343030(i) = A343030(j) for all i, j >= 1.
%H A389898 Antti Karttunen, <a href="/A389898/b389898.txt">Table of n, a(n) for n = 1..65537</a>
%o A389898 (PARI)
%o A389898 up_to = 65537;
%o A389898 rgs_transform(invec) = { my(om = Map(), outvec = vector(length(invec)), u=1); for(i=1, length(invec), if(mapisdefined(om,invec[i]), my(pp = mapget(om, invec[i])); outvec[i] = outvec[pp] , mapput(om,invec[i],i); outvec[i] = u; u++ )); outvec; };
%o A389898 A126760(n) = {n && n\=3^valuation(n, 3)<<valuation(n, 2); n%3+n\6*2 };
%o A389898 A343029(n) = { my(t=1, ret=0); for(i=0, if(n, logint(n, 2)), if(bittest(n, i), ret+=t, t=!t)); ret; };
%o A389898 A343030(n) = { my(t=0, ret=0); for(i=0, if(n, logint(n, 2)), if(bittest(n, i), ret+=t, t=!t)); ret; };
%o A389898 Aux389898(n) = [A126760(n), A343029(n), A343030(n)];
%o A389898 v389898 = rgs_transform(vector(up_to,n,Aux389898(n)));
%o A389898 A389898(n) = v389898[n];
