%o A389791 (Python)
%o A389791 from sympy import isprime, nextprime
%o A389791 from itertools import count, islice, product
%o A389791 def agen(): # generator of terms
%o A389791     an, t1, t2 = 1, "11", "13"
%o A389791     while True:
%o A389791         yield an
%o A389791         s = str(an)
%o A389791         for an in count(1):
%o A389791             if not isprime(an):
%o A389791                 u = str(an)
%o A389791                 if t1 not in s+u: continue
%o A389791                 if any(s[i:]+u[:j] == t1 and t2.startswith(u[k:]) and t2[len(u[k:])]!= '0' for i in range(len(s)) for j in range(1, len(u)+1) for k in range(len(u))):
%o A389791                     break
%o A389791         t1, t2 = t2, str(nextprime(int(t2)))
%o A389791 print(list(islice(agen(), 56))) # _Michael S. Branicky_, Oct 15 2025
