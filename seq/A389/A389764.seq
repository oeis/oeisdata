%t A389764 seq[lim_] := Union[Table[m^k - 1, {k, 2, Log2[lim + 1]}, {m, 3, Surd[lim + 1, k], 2}] // Flatten]; seq[6000]
%o A389764 (PARI) list(lim) = {my(s = List());  for(k = 2, logint(lim+1, 2), forstep(m = 3, sqrtnint(lim+1, k), 2, listput(s, m^k - 1))); Set(s);}
