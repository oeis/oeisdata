%e A389579   17  26  40  47  49  50   75   78
%e A389579   23  35  38  41  44  46   55   72
%e A389579   29  32  39  42  48  52   62   69
%e A389579   36  45  57  58  60  63   66   68
%e A389579   51  54  64  70  80  99  113  118
%t A389579 highs := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389579     Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A389579 lows := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389579     Split[Rest[FoldList[Min, +\[Infinity], #]]]] &;
%t A389579 seqS = Table[N[FractionalPart[Tan[n]], 20], {n, 1, 1000}];
%t A389579 (* User:put your dense sequence S after seqS *)
%t A389579 indices = Range[Length[seqS]];
%t A389579 arrI = {};   (*start accumulating increasing partition array*)
%t A389579 Until[Last[arrI] == {}, AppendTo[arrI, Flatten[Map[Position[seqS, #] &,
%t A389579      highs[seqS[[Complement[indices, Flatten[arrI]]]]][[1]]]]]];
%t A389579 Grid[Take[arrI, 12]]
%t A389579 arrD = {};   (*start accumulating decreasing partition array*)
%t A389579 Until[Last[arrD] == {}, AppendTo[arrD, Flatten[Map[Position[seqS, #] &,
%t A389579      lows[seqS[[Complement[indices, Flatten[arrD]]]]][[1]]]]]];
%t A389579 Grid[Take[arrD, 12]]
