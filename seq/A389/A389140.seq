%o A389140 from functools import cache
%o A389140 def children(s):
%o A389140     return set(int(s[:i]+str(q>>1)+s[j:]) for i in range(len(s)) for j in range(i+1, len(s)+1) if (w:=s[i:j])[0]!='0' and (q:=int(w))&1==0)
%o A389140 @cache
%o A389140 def a(n):
%o A389140     if n.bit_count() == 1: return 1
%o A389140     if set(s:=str(n)) <= set("13579"): return n
%o A389140     return min(a(c) for c in children(s))
