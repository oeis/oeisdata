%o A031399 (Python)
%o A031399 from itertools import count, islice
%o A031399 from sympy import primefactors
%o A031399 from sympy.solvers.diophantine.diophantine import diop_DN
%o A031399 def A031399_gen(startvalue=1): # generator of terms >= startvalue
%o A031399     return filter(lambda n: not (any(p&3==3 for p in primefactors(n)) or len(diop_DN(n,-1))), count(max(startvalue,1)))
%o A031399 A031399_list = list(islice(A031399_gen(),50)) # _Chai Wah Wu_, Dec 22 2025
