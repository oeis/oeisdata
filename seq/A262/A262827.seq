%t A262827 SQ[n_]:=IntegerQ[Sqrt[n]]
%t A262827 Do[r=0;Do[If[SQ[n-x^3-y^4-2*z^4],r=r+1],{x,0,n^(1/3)},{y,0,(n-x^3)^(1/4)},{z,0,((n-x^3-y^4)/2)^(1/4)}];Print[n," ",r];Continue,{n,0,100}]
