%p A279644 a:= n-> b(n$2):
%p A279644 seq(a(n), n=0..15);
%t A279644 b[n_, k_] := b[n, k] = If[n == 0, 1, Sum[Binomial[n-1, j-1]*j^k*b[n-j, k], {j, 1, n}]];
%t A279644 a[n_] := b[n, n];
