%S A279473 1,11,11,111,1011,10111,111011,1010111,11111011,100010111,1100111011,
%T A279473 11001010111,110011111011,1111100010111,10001100111011,
%U A279473 110011001010111,1101110011111011,11010101100010111,110101011100111011,1101010101001010111,11010101110011111011
%N A279473 Binary representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 158", based on the 5-celled von Neumann neighborhood.
%C A279473 Initialized with a single black (ON) cell at stage zero.
%D A279473 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A279473 Robert Price, <a href="/A279473/b279473.txt">Table of n, a(n) for n = 0..126</a>
%H A279473 Robert Price, <a href="/A279473/a279473.tmp.txt">Diagrams of first 20 stages</a>
%H A279473 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A279473 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A279473 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A279473 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A279473 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A279473 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A279473 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A279473 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A279473 code = 158; stages = 128;
%t A279473 rule = IntegerDigits[code, 2, 10];
%t A279473 g = 2 * stages + 1; (* Maximum size of grid *)
%t A279473 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A279473 ca = a;
%t A279473 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A279473 PrependTo[ca, a];
%t A279473 (* Trim full grid to reflect growth by one cell at each stage *)
%t A279473 k = (Length[ca[[1]]] + 1)/2;
%t A279473 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A279473 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
