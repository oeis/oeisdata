%S A279602 1,0,101,0,11101,1000,1010101,101000,100000101,11101000,10101010101,
%T A279602 110101000,1101100000101,1011011101000,101010101,1111100110101000,
%U A279602 1001100000101,111010001011101000,11101010001010101,10101000101010101000,1100101111100000101
%N A279602 Binary representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 177", based on the 5-celled von Neumann neighborhood.
%C A279602 Initialized with a single black (ON) cell at stage zero.
%D A279602 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A279602 Robert Price, <a href="/A279602/b279602.txt">Table of n, a(n) for n = 0..126</a>
%H A279602 Robert Price, <a href="/A279602/a279602.tmp.txt">Diagrams of first 20 stages</a>
%H A279602 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A279602 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A279602 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A279602 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A279602 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A279602 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A279602 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A279602 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A279602 code = 177; stages = 128;
%t A279602 rule = IntegerDigits[code, 2, 10];
%t A279602 g = 2 * stages + 1; (* Maximum size of grid *)
%t A279602 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A279602 ca = a;
%t A279602 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A279602 PrependTo[ca, a];
%t A279602 (* Trim full grid to reflect growth by one cell at each stage *)
%t A279602 k = (Length[ca[[1]]] + 1)/2;
%t A279602 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A279602 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
