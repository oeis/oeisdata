%p A279567       b(n-1, i-h+1, max(m, j)-h))(max(0, min(m-1, j))), j=1..i))
%p A279567     end:
%p A279567 a:= n-> b(n, 1, 0):
%p A279567 seq(a(n), n=0..30);  # _Alois P. Heinz_, Feb 23 2017
%t A279567 b[n_, i_, m_] := b[n, i, m] = If[n == 0, 1, Sum[b[n-1, i-#+1, Max[m, j]-#]& @ Max[0, Min[m-1, j]], {j, 1, i}]]; a[n_] := b[n, 1, 0]; Table[a[n], {n, 0, 30}] (* _Jean-Fran√ßois Alcover_, Jul 10 2017, after _Alois P. Heinz_ *)
