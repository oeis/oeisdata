%S A279546 1,0,101,0,11101,1000,1000101,100000,100011101,10101000,10000000101,
%T A279546 1111100000,1010001011101,1000101000,111000100000101,10100011100000,
%U A279546 10000010101011101,1110000000101000,1010101111100000101,10001011100000,111110001000101011101
%N A279546 Binary representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 169", based on the 5-celled von Neumann neighborhood.
%C A279546 Initialized with a single black (ON) cell at stage zero.
%D A279546 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A279546 Robert Price, <a href="/A279546/b279546.txt">Table of n, a(n) for n = 0..126</a>
%H A279546 Robert Price, <a href="/A279546/a279546.tmp.txt">Diagrams of first 20 stages</a>
%H A279546 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A279546 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A279546 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A279546 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A279546 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A279546 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A279546 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A279546 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A279546 code = 169; stages = 128;
%t A279546 rule = IntegerDigits[code, 2, 10];
%t A279546 g = 2 * stages + 1; (* Maximum size of grid *)
%t A279546 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A279546 ca = a;
%t A279546 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A279546 PrependTo[ca, a];
%t A279546 (* Trim full grid to reflect growth by one cell at each stage *)
%t A279546 k = (Length[ca[[1]]] + 1)/2;
%t A279546 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A279546 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
