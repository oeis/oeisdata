%S A279827 1,11,11,111,1011,10111,101011,1000111,11011011,110110111,1101101011,
%T A279827 11011000111,110111011011,1101010110111,11010101101011,
%U A279827 110101011000111,1101000111011011,11011011010110111,110110110101101011,1101101101011000111,11011011000111011011
%N A279827 Binary representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 206", based on the 5-celled von Neumann neighborhood.
%C A279827 Initialized with a single black (ON) cell at stage zero.
%D A279827 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A279827 Robert Price, <a href="/A279827/b279827.txt">Table of n, a(n) for n = 0..126</a>
%H A279827 Robert Price, <a href="/A279827/a279827.tmp.txt">Diagrams of first 20 stages</a>
%H A279827 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A279827 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A279827 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A279827 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A279827 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A279827 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A279827 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A279827 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A279827 code = 206; stages = 128;
%t A279827 rule = IntegerDigits[code, 2, 10];
%t A279827 g = 2 * stages + 1; (* Maximum size of grid *)
%t A279827 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A279827 ca = a;
%t A279827 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A279827 PrependTo[ca, a];
%t A279827 (* Trim full grid to reflect growth by one cell at each stage *)
%t A279827 k = (Length[ca[[1]]] + 1)/2;
%t A279827 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A279827 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
