%p A050870 A050186 := proc(n,k)
%p A050870         if n = 0 then
%p A050870                 1;
%p A050870         else
%p A050870         add (numtheory[mobius](d)*binomial(n/d,k/d),d =numtheory[divisors](igcd(n,k))) ;
%p A050870         end if;
%p A050870 end proc:
%p A050870 A050870 := proc(n,k)
%p A050870         binomial(n,k)-A050186(n,k) ;
