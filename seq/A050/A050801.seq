%t A050801 Select[Range[5350], Reduce[0 < x <= y && #^2 == x^3 + y^3, {x,y}, Integers] =!= False &] (* _Jean-FranÃ§ois Alcover_, Mar 30 2011 *)
%t A050801 Sqrt[#]&/@Union[Select[Total/@(Tuples[Range[500],2]^3),IntegerQ[ Sqrt[ #]]&]] (* _Harvey P. Dale_, Mar 06 2012 *)
