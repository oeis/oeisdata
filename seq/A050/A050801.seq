%o A050801 (PARI) nstart=1; astart=3; n=nstart; a=astart-1; until (0, a++; a2=a^2; b1=((a2/2)^(1/3))\1; for (b=b1, a, b3=b^3; c1=1; if (a2 > b3, c1=((a2-b3)^(1/3))\1;); for (c=c1, b, d=b3 + c^3; if (d > a2 && c == 1, break(2)); if (d > a2, break); if (a2 == d, print(n, " ", a); write("b050801.txt", n, " ", a); n++; break(2))))) \\ _Harry J. Smith_, Jan 15 2009
