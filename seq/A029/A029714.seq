%p A029714 s:= proc(n) local m; m:= 1; while not type(m!/n, integer) do m:= m+1 od; m end: a:= n-> add(s(3^k), k=0..n-1): seq(a(n), n=1..70); # _Alois P. Heinz_, Sep 14 2008
%t A029714 S[n_] := S[n] = Module[{k=1}, While[True, If[Divisible[k!, n], Return[k], k++] ] ];
%t A029714 a[n_] := S /@ Divisors[3^n] // Total;
%t A029714 a /@ Range[0, 49] (* _Jean-FranÃ§ois Alcover_, Nov 17 2020 *)
%o A029714 (Python)
%o A029714 from itertools import count
%o A029714 from sympy import multiplicity
%o A029714 def A029714(n):
%o A029714     a, m, c = 0,0,0
%o A029714     for k in count(1):
%o A029714         c += multiplicity(3,k)
%o A029714         if c+k>=m:
%o A029714             a+=k*(min(c+k-m,n-m)+1)
%o A029714             m = c+k+1
%o A029714         if c+k >= n:
%o A029714             return 3*a-2 # _Chai Wah Wu_, Feb 24 2026
