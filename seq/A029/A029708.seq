%F A029708 a(n) = (A029710(n) + A031505(n+1))/2 = A029710(n) + 2 = A031505(n+1) - 2.
%t A029708 f[n_]:=PrimeQ[n-2]&&PrimeQ[n+2]; lst={};Do[If[f[n],AppendTo[lst,n]],{n,7,8!,2}];lst (* _Vladimir Joseph Stephan Orlovsky_, Oct 09 2009 *)
