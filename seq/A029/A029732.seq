%t A029732 lst={};Do[p=Prime[n];If[IntegerDigits[p,16]==Reverse[IntegerDigits[p,16]],AppendTo[lst,p]],{n,7!}];lst (* _Vladimir Joseph Stephan Orlovsky_, Jul 31 2009 *)
