%F A120512 g.f.: P(z) = z / (1-z) * (1 + sum(z^(m^2) * (2 + 1 / (1 - z^(m^2))), m=0..infinity))
%p A120512 p := proc(n)
%p A120512 if n=1 then return 1; end if;
%p A120512 for j from p(n-1)+1 to infinity do
%p A120512 if A120501(j) = n then return j; fi; od;
%p A120512 end proc;
