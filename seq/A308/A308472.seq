%o A308472 (Python)
%o A308472 def dprod(n):
%o A308472     x = str(n)
%o A308472     start = 1
%o A308472     for q in x:
%o A308472         start *= int(q)
%o A308472     return start
%o A308472 def dsum(n):
%o A308472     x = str(n)
%o A308472     start = 0
%o A308472     for q in x:
%o A308472         start += int(q)
%o A308472     return start
%o A308472 seq_1 = [n for n in range(1,10000) if dprod(n) != 0 and n % (dsum(dprod(n))) == 0]
%o A308472 print(seq_1)
