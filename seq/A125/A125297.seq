%o A125297 from functools import cache
%o A125297 @cache
%o A125297 def b(n, s, l): # n, sum, lcm
%o A125297     if n == 0: return s and s%l == 0
%o A125297     return b(n-1, s, l) + b(n-1, s + n, lcm(l, n))
%o A125297 def a(n): return b(n, 0, 1)
