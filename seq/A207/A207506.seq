%t A207506 f[n_] := Module[{x = n, l}, l = IntegerDigits[x];
%t A207506    While[x > 0,  x = x - First[l];
%t A207506     l = Join[Rest[l], IntegerDigits[x]]; ]; Abs[ x]] ;
%t A207506 Select[Range[0, 10000], f[#] == 0 &] (* _Robert Price_, Apr 04 2020 *)
