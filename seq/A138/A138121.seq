%t A138121 less[run1_, run2_] := (lg1 = run1 // Length; lg2 = run2 // Length; lg = Max[lg1, lg2]; r1 = If[lg1 == lg, run1, PadRight[run1, lg, 0]]; r2 = If[lg2 == lg, run2, PadRight[run2, lg, 0]]; Order[r1, r2] != -1); row[n_] := Join[Array[1 &, {PartitionsP[n - 1]}], Sort[Reverse /@ Select[IntegerPartitions[n], FreeQ[#, 1] &], less]] // Flatten // Reverse; Table[row[n], {n, 1, 9}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 15 2013 *)
