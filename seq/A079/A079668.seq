%o A079668 (Python)
%o A079668 from itertools import islice
%o A079668 def agen(): # generator of terms
%o A079668     a = [1]
%o A079668     yield 1
%o A079668     while True:
%o A079668         counts = []
%o A079668         for d in range(min(a), max(a)+1):
%o A079668             c = a.count(d)
%o A079668             counts.extend([c, d])
%o A079668         a += counts
%o A079668         yield from counts
%o A079668 print(list(islice(agen(), 84))) # _Michael S. Branicky_, Aug 24 2025
