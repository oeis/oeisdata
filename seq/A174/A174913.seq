%o A174913 (Python)
%o A174913 from itertools import islice
%o A174913 from sympy import isprime, nextprime
%o A174913 def agen(): # generator of terms
%o A174913     p1, p2 = 3, 5
%o A174913     while True:
%o A174913         if p2-p1 == 2 and isprime(2*p1+p2): yield p1
%o A174913         p1, p2 = p2, nextprime(p2)
%o A174913 print(list(islice(agen(), 46))) # _Michael S. Branicky_, Dec 07 2025
