%e A248206 a(1) = 43:
%e A248206 0^4 + 0^3 + 0^2 + 0 + 43 = 43;
%e A248206 1^4 + 1^3 + 1^2 + 1 + 43 = 47;
%e A248206 2^4 + 2^3 + 2^2 + 2 + 43 = 73;
%e A248206 3^4 + 3^3 + 3^2 + 3 + 43 = 163;
%e A248206 4^4 + 4^3 + 4^2 + 4 + 43 = 383;
%e A248206 5^4 + 5^3 + 5^2 + 5 + 43 = 823;
%e A248206 6^4 + 6^3 + 6^2 + 6 + 43 = 1597;
%e A248206 7^4 + 7^3 + 7^2 + 7 + 43 = 2843;
%e A248206 all eight are primes.
%t A248206 Select[f=k^4+k^3+k^2+k; k={0,1,2,3,4,5,6,7}; Prime[Range[2000000]], And @@ PrimeQ[#+f] &]
