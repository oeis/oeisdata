%t A248217 CoefficientList[Series[E^(2 x) (-1 + E^(6 x)), {x, 0, 50}], x]*Table[n!, {n, 0, 50}] (* _Stefano Spezia_, Sep 07 2018 *)
%t A248217 Table[Length[FromDigits /@ Select[IntegerDigits /@ (Sum[10^i, {i, 0, n - 1}]*FromDigits /@ Select[Flatten[DeleteDuplicates[Permutations /@ (PadLeft[#, n] & /@ Flatten[Table[Select[IntegerPartitions[9 i, n], Max[#] < 10 &], {i, 1, n - 1}], 1])], 1], First[#] != 9 &]), (Count[#, 9] == 1 && Sort[DeleteDuplicates[#]] != {1, 8, 9}) &]], {n, 1, 7}]  (* number of anomalous cancellation of 88...88911...11 with n 1's and n 8's, _Xiaohan Zhang_, Nov 21 2019 *)
