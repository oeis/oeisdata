%t A381455 hwt[n_]:=Total[Cases[FactorInteger[n],{p_,k_}:>PrimePi[p]*k]];
%t A381455 sqfacs[n_]:=If[n<=1,{{}},Join@@Table[(Prepend[#,d]&)/@Select[sqfacs[n/d],Min@@#>=d&],{d,Select[Rest[Divisors[n]],PrimePowerQ]}]];
%t A381455 Table[Length[Union[Sort[hwt/@#]&/@sqfacs[n]]],{n,100}]
%o A381455 (PARI) \\ F(e,k) gives one polynomial for each partition of e.
%o A381455 F(e,k)={ my(v=partitions(e)); vector(#v, i, my(p=v[i]); sum(j=1, #p, x^(p[j]*k))) }
%o A381455 a(n)={ my(f=factor(n),R=[0]); for(i=1, #f~, my(v=F(f[i,2], primepi(f[i,1]))); R=concat(vector(#v, i, vector(#R, j, v[i]+R[j])))); #Set(R) } \\ _Andrew Howroyd_, Nov 21 2025
