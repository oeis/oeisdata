%o A080910 (Python)
%o A080910 import numpy
%o A080910 def A080910(n):
%o A080910   def is_valid(m):
%o A080910     return m[0] != "0" or len(m) == 1
%o A080910   n = str(n)
%o A080910   arr = []
%o A080910   len_n = len(n)
%o A080910   # Deletion
%o A080910   if len_n > 1:
%o A080910     for i in range(len_n):
%o A080910       if is_valid(n[:i]+n[(i+1):]):
%o A080910         arr.append(int(n[:i]+n[(i+1):]))
%o A080910   # Insertion
%o A080910   for i in range(len_n+1):
%o A080910     for j in range(10):
%o A080910       if is_valid(n[:i]+str(j)+n[i:]):
%o A080910         arr.append(int(n[:i]+str(j)+n[i:]))
%o A080910   # Substitution
%o A080910   for i in range(len_n):
%o A080910     for j in range(10):
%o A080910       if is_valid(n[:i]+str(j)+n[(i+1):]):
%o A080910         arr.append(int(n[:i]+str(j)+n[(i+1):]))
%o A080910   return len(numpy.unique(arr)) - 1 # _Zhuorui He_, Oct 01 2025
