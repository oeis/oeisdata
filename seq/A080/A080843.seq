%o A080843 (PARI)
%o A080843 strsub(s, vv, off=0)=
%o A080843 {
%o A080843     my( nl=#vv, r=[], ct=1 );
%o A080843     while ( ct <= #s,
%o A080843         r = concat(r, vv[ s[ct] + (1-off) ] );
%o A080843         ct += 1;
%o A080843     );
%o A080843     return( r );
%o A080843 }
%o A080843 t=[0];  for (k=1, 10, t=strsub( t, [[0,1], [0,2], [0]], 0 ) );  t
