%N A280573 E.g.f. satisfies: A(x - Integral 2*A(x) dx) = x + Integral 3*A(x) dx.
%F A280573 E.g.f. A(x) satisfies:
%F A280573 (1) A(x - Integral 2*A(x) dx) = x + Integral 3*A(x) dx.
%F A280573 (2) A(x) = x + 5 * G( (2*A(x) + 3*x)/5 ), where G(x) = Integral A(x) dx.
%F A280573 (3) A(x) = -3*x/2 + 5/2 * Series_Reversion(x - Integral 2*A(x) dx).
%F A280573 (4) A( (2*A(x) + 3*x)/5 ) = (A'(x) - 1)/(2*A'(x) + 3).
%F A280573 (5) A'(x - Integral 2*A(x) dx) = (1 + 3*A(x))/(1 - 2*A(x)).
%F A280573 a(n) = Sum_{k=0..n-1} A277410(n,k) * 2^k * 5^(n-k-1).
%e A280573 E.g.f.: A(x) = x + 5*x^2/2! + 55*x^3/3! + 1075*x^4/4! + 30825*x^5/5! + 1174725*x^6/6! + 56153575*x^7/7! + 3241453075*x^8/8! + 219981653625*x^9/9! + 17205716877125*x^10/10! + 1527315775776375*x^11/11! + 152004555650445875*x^12/12! +...
%e A280573 Let G(x) = Integral A(x) dx, then A(x - 2*G(x)) = x + 3*G(x) where
%e A280573 G(x) = x^2/2! + 5*x^3/3! + 55*x^4/4! + 1075*x^5/5! + 30825*x^6/6! + 1174725*x^7/7! + 56153575*x^8/8! + 3241453075*x^9/9! + 219981653625*x^10/10! + 17205716877125*x^11/11! + 1527315775776375*x^12/12! +...
%e A280573 Also, A(x) = x + 5 * G( (2*A(x) + 3*x)/5 ).
%e A280573 RELATED SERIES.
%e A280573 We have (2*A(x) + 3*x)/5 = Series_Reversion( x - Integral 2*A(x) dx ), where
%e A280573 (2*A(x) + 3*x)/5 =  x + 2*x^2/2! + 22*x^3/3! + 430*x^4/4! + 12330*x^5/5! + 469890*x^6/6! + 22461430*x^7/7! + 1296581230*x^8/8! + 87992661450*x^9 + 6882286750850*x^10 + 610926310310550*x^11 + 60801822260178350*x^12 +...
%e A280573 Further, A( (2*A(x) + 3*x)/5 ) = (A'(x) - 1)/(2*A'(x) + 3), which begins
%e A280573 A( (2*A(x) + 3*x)/5 ) = x + 7*x^2/2! + 107*x^3/3! + 2665*x^4/4! + 93005*x^5/5! + 4201015*x^6/6! + 233920155*x^7/7! + 15535390105*x^8/8! + 1201670102125*x^9/9! + 106329616511975*x^10/10! + 10612821894707675*x^11/11! + 1181462628283585225*x^12/12! +...
%t A280573 m = 21; A[_] = 0;
%t A280573 Do[A[x_] = -3x/2 + 5/2 InverseSeries[x-Integrate[2A[x], x] + O[x]^m], {m}];
%t A280573 CoefficientList[A[x], x] * Range[0, m-1]! // Rest (* _Jean-Fran√ßois Alcover_, Sep 30 2019 *)
%o A280573 (PARI) /* A(x) = x + (p+q)*G((p*A(x) + q*x)/(p+q)) ; G(x) = Integral A(x) dx: */
%o A280573 {a(n, p=2, q=3) = my(A=x, G); for(i=1, n, G = intformal(A +x*O(x^n)); A = x + (p+q)*subst(G, x, (p*A + q*x)/(p+q)) +x*O(x^n)); n!*polcoeff(A, n)}
%o A280573 for(n=1, 30, print1(a(n, 2, 3), ", "))
%o A280573 (PARI) /* A(x - Integral p*A(x) dx) = x + Integral q*A(x) dx: */
%o A280573 {a(n, p=2, q=3) = my(A=[1], F=x); for(i=1, n, A=concat(A, 0); F=x*Ser(A); G=intformal(F); A[#A] = -polcoeff(subst(F, x, x - p*G) - q*G, #A) ); n!*A[n]}
%o A280573 for(n=1, 30, print1(a(n, 2, 3), ", "))
%o A280573 (PARI) /* Informal code to generate the first N terms: */
%o A280573 {N=20; p=2; q=3; A=x; for(i=1, N, G=intformal(A +x*O(x^N)); A = x + (p+q)*subst(G, x, (p*A + q*x)/(p+q))); Vec(serlaplace(A))}
