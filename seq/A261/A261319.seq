%t A261319 TGF[1, x_] := x^2/(1 - x^2); TGF[n_, x_] := x^n/(1 + x)*Product[1/(1 - (2*j - 1)*x), {j, 1, n}];
%t A261319 T[0, 0] := 1; T[_, 0] := 0; T[0, _] := 0; T[t_, n_] := Coefficient[Series[TGF[n, x], {x, 0, t}], x^t];
%t A261319 CC[t_, n_] := Sum[T[t, m], {m, 0, n}]
