%F A261724 a(n) = (1/12)*(-3^(n - 2)*(n^2 + 5*n + 18) + (1/64)*(2^(2*n + 5) + 3*2^n*(n^4 + 2*n^3 + 19*n^2 + 42*n + 64) - 16*(n^6 - 9*n^5 + 43*n^4 - 91*n^3 + 112*n^2 - 32*n + 8))).
