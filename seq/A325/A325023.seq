%t A325023 Select[Range[10^6], And[Mod[#3, #1] == 0, IntegerQ[#1 (#1 - #2)/#3]] & @@ Prepend[DivisorSigma[{0, 1}, #], #] &] (* _Michael De Vlieger_, Mar 24 2019 *)
