%o A012781 (Python)
%o A012781 def a(n, adict={0:0, 1:1, 2:4}):
%o A012781     if n in adict:
%o A012781         return adict[n]
%o A012781     adict[n]=5*a(n-1) - 4*a(n-2) + a(n-3)
