%t A318160 b[n_, i_, k_] := b[n, i, k] = If[n == 0, 1, If[i == 0, 0, Sum[b[n - j, i - 1, k], {j, 0, Min[n, k]}]]];
%t A318160 a[n_] := If[n == 0, 1, b[n, n, Ceiling[n/2]] - b[n, n, Ceiling[n/2] - 1]];
