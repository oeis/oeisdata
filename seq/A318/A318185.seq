%t A318185 totra[n_]:=totra[n]=If[n==1,{{}},Join@@Table[Select[Union[Sort/@Tuples[totra/@c]],Complement[Union@@#,#]=={}&],{c,IntegerPartitions[n-1]}]];
%t A318185 Table[Length[totra[n]],{n,20}]
%o A318185 (PARI)
%o A318185 transitiveQ(t) = {my(S = Set(t), U = Set([]), i); for(i=1, #S, if(#S[i], U = setunion(U, Set(S[i])));); return(#setminus(U, S) == 0);};
%o A318185 allmultisets_by_sizes(tt, parts) = {my(r = #parts, bounds = vector(r), L = List()); my(i, sz, len, idx, acc); for(i=1, r, sz = parts[i]; len = #tt[sz]; if(len == 0, return([])); bounds[i] = [1, len];); forvec(idx = bounds, acc = vector(r, i, tt[parts[i]][ idx[i] ]); listput(L, vecsort(acc)); ); return(Set(Vec(L)));};
%o A318185 A(N) = {my(n = N); if(n == 1, return([1])); my(tt = vector(n), k, parts, cand, good, i); tt[1] = [ [] ]; for(k=2, n, good = List(); forpart(parts = k-1, cand = allmultisets_by_sizes(tt, parts); for(i=1, #cand, if(transitiveQ(cand[i]), listput(good, cand[i])););); tt[k] = Set(Vec(good));); return(vector(n, k, #tt[k]));};
%o A318185 a(n) = A(n)[n];
