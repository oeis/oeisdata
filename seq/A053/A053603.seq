%o A053603 (PARI)
%o A053603 istriang(n)={n>0 && issquare(8*n+1);}
%o A053603 a(n) = { my(t=1, ct=0, j=1); while (t<n, ct+=istriang(n-t); j+=1; t+=j;); ct; }
