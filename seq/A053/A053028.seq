%t A053028 Lucas[n_] := Fibonacci[n+1] + Fibonacci[n-1]; badP={}; Do[p=Prime[n]; k=1; While[k<p&&Mod[Lucas[k], p]>0, k++ ]; If[k==p, AppendTo[badP, p]], {n, 200}]; badP
