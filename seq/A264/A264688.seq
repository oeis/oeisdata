%o A264688 (Python)
%o A264688 from collections import Counter
%o A264688 from gmpy2 import digits, fac, mpz
%o A264688 def ok(k):
%o A264688     c = Counter(digits(fac(mpz(k))))
%o A264688     return sum(c[d] for d in set(digits(k))) == k
