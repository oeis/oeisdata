%o A072921 def agen(): # generator of terms
%o A072921     an, anp1 = 1, 2
%o A072921     while True:
%o A072921         yield an
%o A072921         an, anp1 = anp1, 2*anp1 - an + sum(map(int, str(anp1)))
%o A072921 print(list(islice(agen(), 44))) # _Michael S. Branicky_, Oct 01 2024
