%o A153336 (Python)
%o A153336 import math
%o A153336 def C(n,r):
%o A153336     f=math.factorial
%o A153336     return f(n)/f(r)/f(n-r)
%o A153336 def A153336(n):
%o A153336     return str((2*n+1)*2**(2*n-2)-2*(2*n-1)*C(2*n-2,n-1)) # _Indranil Ghosh_, Feb 19 2017
