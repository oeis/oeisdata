%p A361893 A361893 := (n, k) -> n!*binomial(n - 1, k - 1)/(n - k)!:
%p A361893 seq(seq(A361893(n,k), k = 0..n), n = 0..9);
%p A361893 # Using the egf.:
%p A361893 egf := 1 + (x*y/(1 - x*y))*exp(y/(1 - x*y)): ser := series(egf, y, 10):
%p A361893 poly := n -> convert(n!*expand(coeff(ser, y, n)), polynom):
%p A361893 row := n -> seq(coeff(poly(n), x, k), k = 0..n): seq(print(row(n)), n = 0..6);
