%e A361868    68: {1,1,7}
%e A361868    72: {1,1,1,2,2}
%t A361868 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A361868 Select[Range[100],Max@@prix[#]>=2*Median[prix[#]]&]
