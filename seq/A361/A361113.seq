%o A361113 from sympy import primepi, integer_nthroot
%o A361113 def A361113(n):
%o A361113     def f(x): return int(n+sum(primepi(integer_nthroot(x,k)[0]) for k in range(1,x.bit_length())))
%o A361113     m, k = n, f(n)
%o A361113     while m != k: m, k = k, f(k)
