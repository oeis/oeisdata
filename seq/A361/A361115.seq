%o A361115 (Python)
%o A361115 from sympy import primepi, integer_nthroot
%o A361115 def A361115(n):
%o A361115     def f(x): return int(n+sum(primepi(integer_nthroot(x,k)[0]) for k in range(1,x.bit_length())))
%o A361115     m, k = n, f(n)
%o A361115     while m != k: m, k = k, f(k)
%o A361115     return int(not m%3) # _Chai Wah Wu_, Dec 05 2025
