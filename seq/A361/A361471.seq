%t A361471 delta[l_, m_] := Complement[Join[l, m], Intersection[l, m]];
%t A361471 Nabl[s_, n_] := (d = {}; Do[d = delta[d, s*j], {j, Range[n]}]; d);
%t A361471 Table[Length[Nabl[Range[1, 4], n]], {n, 100}]
