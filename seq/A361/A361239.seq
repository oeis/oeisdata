%o A361239 u(n,k,r) = {r*binomial(n*(2*k-1) + r, n)/(n*(2*k-1) + r)}
%o A361239 R(n,k) = {if(n==0, 1, u(n, k, 1)/((k-1)*n+1) + sumdiv(gcd(k,n-1), d, if(d>1, eulerphi(d)*u((n-1)/d, k, 2*k/d)/k)))}
%o A361239 T(n, k) = {(R(n, k) + u(n\2, k, if(n%2, k, 1)))/2}
