%o A247843 (Python)
%o A247843 for n in range(10**5):
%o A247843   s,t = str(n),str(n+1)
%o A247843   s2,t2 = str(n**2),str((n+1)**2)
%o A247843   c = 0
%o A247843   for i in s:
%o A247843     if s2.count(i):
%o A247843       c += 1
%o A247843       break
%o A247843   for j in t:
%o A247843     if t2.count(j):
%o A247843       c += 1
%o A247843       break
%o A247843   if not c:
%o A247843     print(n,end=', ')
