%t A247026 (* This program is not suitable to compute a large number of terms. *)
%t A247026 nmax = 8;
%t A247026 f[a_][b_] /; Length[a]==Length[b] := Table[b[[a[[i]]]], {i, 1, Length[a]}];
%t A247026 A[n_, k_] := Nest[f[#], Range[n], k]& /@ Tuples[Range[n], {n}] // Union // Length;
%t A247026 Table[A[n-k, k], {n, 0, nmax}, {k, n, 0, -1}] // Flatten (* _Jean-Fran√ßois Alcover_, May 05 2019 *)
