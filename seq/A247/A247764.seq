%o A247764 a247764 n = a247764_list !! (n-1)
%o A247764 a247764_list = 0 : f (fromList [1..9]) where
%o A247764    f s = x : f (s' `union`
%o A247764          fromList (map (+ 10 * x) $ dropWhile (/= mod x 10) digs))
