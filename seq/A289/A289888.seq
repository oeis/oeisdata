%S A289888 1,1,3,11,23,55,111,175,351,863,1727,2751,5503,5503,11007,43775,87551,
%T A289888 87551,175103,699391,1398783,1398783,2797567,11710463,23420927,
%U A289888 20275199,40550399,185253887,370507775,332759039,665518079,2955608063,5911216127,13947502591
%N A289888 Decimal representation of the diagonal from the origin to the corner of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 605", based on the 5-celled von Neumann neighborhood.
%C A289888 Initialized with a single black (ON) cell at stage zero.
%D A289888 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A289888 Robert Price, <a href="/A289888/b289888.txt">Table of n, a(n) for n = 0..126</a>
%H A289888 Robert Price, <a href="/A289888/a289888.tmp.txt">Diagrams of first 20 stages</a>
%H A289888 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A289888 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A289888 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A289888 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A289888 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A289888 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A289888 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A289888 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A289888 code = 605; stages = 128;
%t A289888 rule = IntegerDigits[code, 2, 10];
%t A289888 g = 2 * stages + 1; (* Maximum size of grid *)
%t A289888 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A289888 ca = a;
%t A289888 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A289888 PrependTo[ca, a];
%t A289888 (* Trim full grid to reflect growth by one cell at each stage *)
%t A289888 k = (Length[ca[[1]]] + 1)/2;
%t A289888 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A289888 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
