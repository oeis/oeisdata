%e A275046 For n = 5 there are binomial(10,5) = 252 binary strings with 5 zeros and 5 ones; seven out of this 252 binary strings contain as substrings w1=10101101 or w2=1110101, i.e.
%e A275046    0123456789
%e A275046    ----------
%e A275046 1  0001110101 contains w2 at offset 3
%e A275046 2  0010101101 contains w1 at offset 2
%e A275046 3  0011101010 contains w2 at offset 2
%e A275046 4  0101011010 contains w1 at offset 1
%e A275046 5  0111010100 contains w2 at offset 1
%e A275046 6  1010110100 contains w1 at offset 0
%e A275046 7  1110101000 contains w2 at offset 0
%e A275046 Therefore a(5) = 252 - 7 = 245.
%t A275046 a[n_] := SeriesCoefficient[(1 + x^2 y^3 + x^2 y^4 + x^3 y^4 - x^3 y^6) / (1 - x - y + x^2 y^3 - x^3 y^3 - x^4 y^4 - x^3 y^6 + x^4 y^6), {x, 0, n}, {y, 0, n}]; Table[a[n], {n, 0, 27}] (* _Jean-Fran√ßois Alcover_, Aug 20 2018 *)
%o A275046 (PARI)
%o A275046 r1 = (1+x^2*y^3+x^2*y^4+x^3*y^4-x^3*y^6);
%o A275046 r2 = (1-x-y+x^2*y^3-x^3*y^3-x^4*y^4-x^3*y^6+x^4*y^6);
%o A275046 diag(expr, N=22, var=variables(expr)) = {
%o A275046   my(a = vector(N));
%o A275046   for (k = 1, #var, expr = taylor(expr, var[#var - k + 1], N));
%o A275046   for (n = 1, N, a[n] = expr;
%o A275046     for (k = 1, #var, a[n] = polcoeff(a[n], n-1)));
%o A275046   return(a);
%o A275046 };
%o A275046 diag(r1/r2, 28)
%o A275046 F  = (x + 1)*(4*x^20 + 8*x^19 - 23*x^18 - 63*x^17 - 62*x^16 - 26*x^15 + 43*x^14 + 11*x^13 + 182*x^12 + 56*x^11 - x^10 + 203*x^9 - 66*x^8 - 154*x^7 + 286*x^6 - 368*x^5 + 233*x^4 - 75*x^3 - 8*x^2 + 20*x - 4)*(y^4 - y^3)  - (12*x^17 + 48*x^16 + 72*x^15 + 49*x^14 - 23*x^13 - 57*x^12 - 91*x^11 - 137*x^10 - 84*x^9 - 34*x^8 - 91*x^7 + 62*x^6 + 24*x^5 - 34*x^4 + 41*x^3 - 10*x^2 - 3*x - 3)*y^2 + (x^15 + 4*x^14 + 6*x^13 + 3*x^12 - 6*x^11 - 11*x^10 - 11*x^9 - 8*x^8 - 3*x^7 + 12*x^6 + 11*x^4 + 5*x^3 - 6*x^2 - 4)*y - x^4 + x + 1;
%o A275046 \\ test: y = Ser(diag(r1/r2, 100)); 0 == subst(F, 'y, y)
%o A275046 (PARI)
%o A275046 x='x; y='y; t='t;
%o A275046 seq(N) = {
%o A275046   my(Fx = substvec(F, [x, y], [t, x]), y0 = 1 + O('t^N), y1=0, n=1);
%o A275046   while (n++,
%o A275046     y1 = y0 - subst(Fx, 'x, y0)/subst(deriv(Fx, 'x), 'x, y0);
%o A275046     if (y1 == y0, break()); y0 = y1); Vec(y0);
%o A275046 };
