%t A022562 EulerInvTransform[seq_] := Module[{final = {}}, For[i = 1, i <= Length[seq], i++, AppendTo[final, i*seq[[i]] - Sum[final[[d]]*seq[[i - d]], {d, i - 1}]]]; Table[Sum[MoebiusMu[i/d]*final[[d]], {d, Divisors[i]}]/i, {i, Length[seq]}]];
