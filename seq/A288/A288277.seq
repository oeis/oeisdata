%F A288277 G.f.: -4*y*(y-1)^14*(18995313191166*y^13 + 602583747147072*y^12 + 3880832501643076*y^11 + 259447266126966*y^10 - 24577880734142257*y^9 + 10075843752456953*y^8 + 45406701745704921*y^7 - 44360505974166179*y^6 - 5860774604042624*y^5 + 22759971294835512*y^4 - 8598423383057104*y^3 - 18688742922288*y^2 + 464831946526080*y - 48608581644864)/(y-2)^41, where y=A000108(x).
%t A288277 Q[0, 1, 0] = 1; Q[n_, f_, g_] /; n < 0 || f < 0 || g < 0 = 0;
%t A288277 Q[n_, f_, g_] := Q[n, f, g] = 6/(n + 1) ((2n - 1)/3 Q[n - 1, f, g] + (2n - 1)/3 Q[n - 1, f - 1, g] + (2n - 3) (2n - 2) (2n - 1)/12 Q[n - 2, f, g - 1] + 1/2 Sum[l = n - k; Sum[v = f - u; Sum[j = g - i; Boole[l >= 1 && v >= 1 && j >= 0] (2k - 1) (2l - 1) Q[k - 1, u, i] Q[l - 1, v, j], {i, 0, g}], {u, 1, f}], {k, 1, n}]);
