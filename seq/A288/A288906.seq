%S A288906 1,1,5,7,19,63,7,255,15,1023,159,3839,575,15871,1151,62463,6399,
%T A288906 249855,59903,929791,41983,4112383,165887,16449535,724991,65667071,
%U A288906 3940351,260308991,16269311,1049624575,15237119,4264558591,60882943,16252927999,1860304895
%N A288906 Decimal representation of the diagonal from the origin to the corner of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 529", based on the 5-celled von Neumann neighborhood.
%C A288906 Initialized with a single black (ON) cell at stage zero.
%D A288906 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A288906 Robert Price, <a href="/A288906/b288906.txt">Table of n, a(n) for n = 0..126</a>
%H A288906 Robert Price, <a href="/A288906/a288906.tmp.txt">Diagrams of first 20 stages</a>
%H A288906 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A288906 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A288906 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A288906 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A288906 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A288906 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A288906 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A288906 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A288906 code = 529; stages = 128;
%t A288906 rule = IntegerDigits[code, 2, 10];
%t A288906 g = 2 * stages + 1; (* Maximum size of grid *)
%t A288906 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A288906 ca = a;
%t A288906 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A288906 PrependTo[ca, a];
%t A288906 (* Trim full grid to reflect growth by one cell at each stage *)
%t A288906 k = (Length[ca[[1]]] + 1)/2;
%t A288906 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A288906 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
