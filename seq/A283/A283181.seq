%S A283181 1,0,3,1,12,3,57,22,192,63,905,374,3112,967,14521,5974,49672,14583,
%T A283181 231201,94734,796144,252551,3702897,1523494,12764368,3742503,59032785,
%U A283181 24255246,203800816,64659247,947938497,390005534,3267689700,958022425,15112453350
%N A283181 Decimal representation of the x-axis, from the left edge to the origin, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 593", based on the 5-celled von Neumann neighborhood.
%C A283181 Initialized with a single black (ON) cell at stage zero.
%D A283181 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A283181 Robert Price, <a href="/A283181/b283181.txt">Table of n, a(n) for n = 0..126</a>
%H A283181 Robert Price, <a href="/A283181/a283181.tmp.txt">Diagrams of first 20 stages</a>
%H A283181 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A283181 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A283181 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A283181 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A283181 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A283181 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A283181 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A283181 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A283181 code = 593; stages = 128;
%t A283181 rule = IntegerDigits[code, 2, 10];
%t A283181 g = 2 * stages + 1; (* Maximum size of grid *)
%t A283181 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A283181 ca = a;
%t A283181 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A283181 PrependTo[ca, a];
%t A283181 (* Trim full grid to reflect growth by one cell at each stage *)
%t A283181 k = (Length[ca[[1]]] + 1)/2;
%t A283181 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A283181 Table[FromDigits[Part[ca[[i]] [[i]], Range[1, i]], 2], {i, 1, stages - 1}]
