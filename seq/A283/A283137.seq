%U A283137 110000100110110,1100010000000100,11000000111100010,110110001001110000,1101100000000110110,11011001101000000100
%N A283137 Binary representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 585", based on the 5-celled von Neumann neighborhood.
%C A283137 Initialized with a single black (ON) cell at stage zero.
%D A283137 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A283137 Robert Price, <a href="/A283137/b283137.txt">Table of n, a(n) for n = 0..126</a>
%H A283137 Robert Price, <a href="/A283137/a283137.tmp.txt">Diagrams of first 20 stages</a>
%H A283137 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A283137 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A283137 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A283137 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A283137 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A283137 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A283137 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A283137 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A283137 code = 585; stages = 128;
%t A283137 rule = IntegerDigits[code, 2, 10];
%t A283137 g = 2 * stages + 1; (* Maximum size of grid *)
%t A283137 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A283137 ca = a;
%t A283137 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A283137 PrependTo[ca, a];
%t A283137 (* Trim full grid to reflect growth by one cell at each stage *)
%t A283137 k = (Length[ca[[1]]] + 1)/2;
%t A283137 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A283137 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
