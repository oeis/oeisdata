%S A283016 1,1,1,9,1,41,1,169,1,681,1,2729,1,10921,1,35497,16385,223913,262145,
%T A283016 172713,1587201,2665129,278529,16149161,29556737,54110889,95158273,
%U A283016 172713,320354305,17476265,1434468353,6843049,5519769601,698460841,25168707585,4668695209
%N A283016 Decimal representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 549", based on the 5-celled von Neumann neighborhood.
%C A283016 Initialized with a single black (ON) cell at stage zero.
%D A283016 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A283016 Robert Price, <a href="/A283016/b283016.txt">Table of n, a(n) for n = 0..126</a>
%H A283016 Robert Price, <a href="/A283016/a283016.tmp.txt">Diagrams of first 20 stages</a>
%H A283016 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A283016 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A283016 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A283016 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A283016 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A283016 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A283016 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A283016 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A283016 code = 549; stages = 128;
%t A283016 rule = IntegerDigits[code, 2, 10];
%t A283016 g = 2 * stages + 1; (* Maximum size of grid *)
%t A283016 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A283016 ca = a;
%t A283016 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A283016 PrependTo[ca, a];
%t A283016 (* Trim full grid to reflect growth by one cell at each stage *)
%t A283016 k = (Length[ca[[1]]] + 1)/2;
%t A283016 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A283016 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 2], {i ,1, stages - 1}]
%Y A283016 Cf. A283013, A283014, A283015.
