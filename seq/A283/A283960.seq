%t A283960 a[n_]:= If[n<7, 1, (Sum[a[n-j] , {j, 5}] +  a[n - 1] a[n - 5])/a[n - 6]]; Table[a[n], {n, 30}] (* _Indranil Ghosh_, Mar 18 2017 *)
%o A283960 (PARI) a(n) = if(n<7, 1, (sum(j=1, 5, a(n - j)) + a(n - 1)*a(n - 5))/a(n - 6));
