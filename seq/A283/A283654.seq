%o A283654 (Python)
%o A283654 import math
%o A283654 f=math.factorial
%o A283654 def C(n,r): return f(n)/f(r)/f(n - r)
%o A283654 def T(n,m): return sum([(-1)**j*C(m,j)*(2**(m - j) - 1)**n for j in range (0, m+1)])
