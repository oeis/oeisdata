%t A283845 Table[Fibonacci[Max[m, n - m + 1]], {n, 20}, {m, n}] // Flatten (* _Indranil Ghosh_, Apr 01 2017 *)
%o A283845 (PARI)
%o A283845 tabl(nn) = {for(n=1, nn, for(m=1, n, print1(fibonacci(max(m, n - m + 1)),", ");); print(););}
