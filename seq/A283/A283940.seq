%o A283940 (Python)
%o A283940 from sympy import sqrt
%o A283940 import math
%o A283940 def s(n): return 1 if n<1 else s(n - 1) + 1 +
%o A283940 int(math.floor(n*sqrt(3)))
%o A283940 def p(n): return n + 1 + sum([int(math.floor((n - k)/sqrt(3))) for k in range(0, n+1)])
%o A283940 v=[s(n) for n in range(0, 101)]
%o A283940 u=[p(n) for n in range(0, 101)]
%o A283940 def w(i,j): return u[i - 1] + v[j - 1] + (i - 1) * (j - 1) - 1
