%o A283939 (Python)
%o A283939 from sympy import sqrt
%o A283939 import math
%o A283939 def s(n): return 1 if n<1 else s(n - 1) + 1 +
%o A283939 int(math.floor(n*sqrt(2)))
%o A283939 def p(n): return n + 1 + sum([int(math.floor((n - k)/sqrt(2))) for k in range(0, n+1)])
%o A283939 v=[s(n) for n in range(0, 101)]
%o A283939 u=[p(n) for n in range(0, 101)]
%o A283939 def w(i,j): return u[i - 1] + v[j - 1] + (i - 1) * (j - 1) - 1
%o A283939 for n in range(1, 11):
