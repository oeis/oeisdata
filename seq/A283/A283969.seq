%t A283969 r = GoldenRatio^2; z = 120;
%t A283969 s[0] = 1; s[n_] := s[n] = s[n - 1] + 1 + Floor[n*r];
%t A283969 Table[n + 1 + Sum[Floor[(n - k)/r], {k, 0, n}], {n, 0, z}] (* A283968 *)
