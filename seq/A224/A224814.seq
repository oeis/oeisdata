%F A224814 a(n) = a(n-1) +a(n-4) -a(n-6) +a(n-7) -3*a(n-9) +4*a(n-10) +5*a(n-12) -2*a(n-13) +3*a(n-15) -8*a(n-16) +a(n-18) -4*a(n-19) +3*a(n-21) -4*a(n-22) -3*a(n-24) -5*a(n-27) +8*a(n-28) +7*a(n-30) -2*a(n-31) -9*a(n-33) +2*a(n-34) +5*a(n-36) +4*a(n-37) +a(n-39) -6*a(n-40) -3*a(n-42) +2*a(n-43) +2*a(n-45) +a(n-46) -a(n-48) -a(n-49) +a(n-52).
