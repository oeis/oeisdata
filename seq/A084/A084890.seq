%t A084890 ader[n_] := ader[n] =Switch[n, 0 | 1, 0, _, If[PrimeQ[n], 1, Sum[Module[{p, e}, {p, e} = pe; n e/p], {pe, FactorInteger[n]}]]];
%t A084890 T[n_, k_] := ader[n k];
