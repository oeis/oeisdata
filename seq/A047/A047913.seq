%t A047913 a[n_, n_] = 1; a[n_?Positive, k_?Positive] := a[n, k] = Sum[a[i, k-n], {i, 1, 2*n}]; a[n_, k_] = 0; Table[a[n, k], {k, 1, 12}, {n, k, 1, -1}] // Flatten (* _Jean-Fran√ßois Alcover_, Oct 21 2013 *)
