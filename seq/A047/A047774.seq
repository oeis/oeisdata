%H A047774 L. W. Beineke and R. E. Pippert, <a href="http://dx.doi.org/10.4153/CJM-1974-006-x">Enumerating dissectable polyhedra by their automorphism groups</a>, Canad. J. Math., 26 (1974), 50-67.
%F A047774 Reference gives complicated recurrence.
%p A047774 # T=A001764
%p A047774 T := proc(n)
%p A047774     if n < 0 then
%p A047774         0;
%p A047774     else
%p A047774         (3*n)!/n!/(2*n+1)! ;
%p A047774     end if;
%p A047774 end proc:
%p A047774 # U=A047749
%p A047774 U := proc(n)
%p A047774     if type(n,'integer') then
%p A047774         if type(n,'even') then
%p A047774             T(n/2) ;
%p A047774         else
%p A047774             (3*n-1)/(n+1)*T((n-1)/2) ;
%p A047774         end if;
%p A047774     else
%p A047774         0 ;
%p A047774     end if;
%p A047774 end proc:
%p A047774 # V=A047750
%p A047774 V := proc(n)
%p A047774     if type(n,'integer') then
%p A047774         if type(n,'even') then
%p A047774             2*U(n+1)-U(n) ;
%p A047774         else
%p A047774             2*U(n+1) ;
%p A047774         end if;
%p A047774     else
%p A047774         0;
%p A047774     end if;
%p A047774 end proc:
%p A047774 K := proc(n)
%p A047774     if n < 1 then
%p A047774         0 ;
%p A047774     elif n = 1 then
%p A047774         1;
%p A047774     else
%p A047774         U((n-5)/12) ;
%p A047774     end if;
%p A047774 end proc:
%p A047774 J := proc(n)
%p A047774     if type((n-5)/12,'integer') then
%p A047774         T((n-5)/12)-K(n) ;
%p A047774         %/2 ;
%p A047774     else
%p A047774         0;
%p A047774     end if ;
%p A047774 end proc:
%p A047774 Q := proc(n)
%p A047774     if type((n-2)/6,'integer') then
%p A047774         U((n-2)/6) ;
%p A047774     else
%p A047774         0 ;
%p A047774     end if;
%p A047774 end proc:
%p A047774 N := proc(n)
%p A047774     if type((n-2)/6,'integer') then
%p A047774         T((n-2)/6)-Q(n) ;
%p A047774         %/2 ;
%p A047774     else
%p A047774         0;
%p A047774     end if ;
%p A047774 end proc:
%p A047774 DD := proc(n)
%p A047774     2*U((n-1)/3)+V((n-2)/3)-2*K(n)-Q(n) ;
%p A047774     %/2 ;
%p A047774 end proc:
%p A047774 OO := proc(n)
%p A047774     if type((n-2)/6,'integer') then
%p A047774         T((n-2)/6)-Q(n) ;
%p A047774         %/2 ;
%p A047774     else
%p A047774         0;
%p A047774     end if ;
%p A047774 end proc:
%p A047774 C := proc(n)
%p A047774     if n = 1 then
%p A047774         0;
%p A047774       elif modp(n,3) = 1 then
%p A047774         T((n-1)/3)-DD(n) ;
%p A047774         %/2 ;
%p A047774     else
%p A047774         U((2*n-1)/3)-2*DD(n)-4*J(n) -2*K(n)-2*N(n)-2*OO(n)-Q(n) ;
%p A047774         %/4 ;
%p A047774     end if;
%p A047774 end proc:
