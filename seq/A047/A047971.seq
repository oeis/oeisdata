%t A047971 nmax = 6;
%t A047971 se = Series[ 1/Product[1 - q^k*x, {k, 0, 3}], {x, 0, nmax}];
%t A047971 row[n_] := CoefficientList[ SeriesCoefficient[se, n], q];
%t A047971 Flatten[ Table[ row[n], {n, 0, nmax}]] (* _Jean-FranÃ§ois Alcover_, Dec 19 2011 *)
