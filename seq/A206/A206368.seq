%o A206368 (Python)
%o A206368 from math import prod; from sympy import factorint
%o A206368 def A206369(n): return prod((lambda x:x[0]+int((x[1]<<1)>=p+1))(divmod(p**(e+1), p+1)) for p, e in factorint(n).items())
%o A206368 for n in range(1,10001):
