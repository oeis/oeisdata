%o A260682 (Python)
%o A260682 from itertools import count, islice
%o A260682 from sympy import factorint
%o A260682 def A260682_gen(): return (6*n+1 for n in count(0) if all(e&1^1 for p,e in factorint(6*n+1).items() if p % 3 == 2))
%o A260682 A260682_list = list(islice(A260682_gen(),30)) # _Chai Wah Wu_, Nov 15 2025
