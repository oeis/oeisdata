%o A274111 (PARI)
%o A274111 x='x; y='y;
%o A274111 Fxy = x^2*(1-2*x+x^2-x^4)*y^3 + 2*x*(1-x)^2*y^2 + (1-3*x+x^2)*y - 1;
%o A274111 seq(N) = {
