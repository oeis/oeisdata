%e A274965 sqrt( (A(x) + A(-x))/2 ) = 1 + x^2 + 4*x^4 + 19*x^6 + 92*x^8 + 446*x^10 + 2150*x^12 + 10280*x^14 + 48761*x^16 + 229558*x^18 + 1073278*x^20 + 4986624*x^22 + 23037102*x^24 + 105877968*x^26 + 484337300*x^28 +...+ A275751(n)*x^(2*n) +...
%e A274965 sqrt( x*(A(x) - A(-x))/2 ) = x + 2*x^3 + 8*x^5 + 36*x^7 + 166*x^9 + 770*x^11 + 3574*x^13 + 16560*x^15 + 76516*x^17 + 352498*x^19 + 1619014*x^21 + 7414134*x^23 + 33855996*x^25 + 154181234*x^27 + 700333366*x^29 +...+ A275752(n)*x^(2*n+1) +...
