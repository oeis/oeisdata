%S A274224 1,3,4,12,16,48,68,204,256,768,1088,3264,4112,12336,17476,52428,65536,
%T A274224 196608,278528,835584,1052672,3158016,4473856,13421568,16777472,
%U A274224 50332416,71304256,213912768,269488144,808464432,1145324612,3435973836,4294967296,12884901888
%N A274224 Decimal representation of the x-axis, from the left edge to the origin, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 22", based on the 5-celled von Neumann neighborhood.
%C A274224 Initialized with a single black (ON) cell at stage zero.
%D A274224 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A274224 Robert Price, <a href="/A274224/b274224.txt">Table of n, a(n) for n = 0..126</a>
%H A274224 Robert Price, <a href="/A274224/a274224.tmp.txt">Diagrams of first 20 stages</a>
%H A274224 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A274224 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A274224 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A274224 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A274224 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A274224 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A274224 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A274224 code=22; stages=128;
%t A274224 rule=IntegerDigits[code,2,10];
%t A274224 g=2*stages+1; (* Maximum size of grid *)
%t A274224 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A274224 ca=a;
%t A274224 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A274224 PrependTo[ca,a];
%t A274224 (* Trim full grid to reflect growth by one cell at each stage *)
%t A274224 k=(Length[ca[[1]]]+1)/2;
%t A274224 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A274224 Table[FromDigits[Part[ca[[i]][[i]],Range[1,i]],2], {i,1,stages-1}]
