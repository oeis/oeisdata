%o A085546 (Python) # see links for an alternate program suitable for generating b-file
%o A085546 from math import isqrt
%o A085546 from itertools import count, islice, permutations
%o A085546 def agen(verbose=False): # generator of terms
%o A085546     adict, n, seen = dict(), 2, set()
%o A085546     for m in count(10):
%o A085546         k = "".join(sorted(str(m**2)))
%o A085546         if k in seen: continue
%o A085546         seen.add(k)
%o A085546         ub = isqrt(int(k[::-1]))
%o A085546         v = 1 + sum(1 for i in range(m+1, ub+1) if "".join(sorted(str(i**2))) == k)
%o A085546         if v > 0 and v not in adict:
%o A085546             if verbose: print("FOUND", v, m)
%o A085546             adict[v] = m
%o A085546             while n in adict: yield adict[n]; n += 1
%o A085546 print(list(islice(agen(), 10))) # _Michael S. Branicky_, Oct 07 2025, edited Oct 09 2025
