%S A287753 1,1,10,1,111,0,1100,11,11000,110,111001,1001,1110110,10110,11101001,
%T A287753 100101,111011010,1000110,1110110001,10001101,11101101010,100010110,
%U A287753 111011100001,1000011101,1110110100010,10001001110,11101110110001,100000001101,111011011101010
%N A287753 Binary representation of the diagonal from the corner to the origin of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 350", based on the 5-celled von Neumann neighborhood.
%C A287753 Initialized with a single black (ON) cell at stage zero.
%D A287753 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A287753 Robert Price, <a href="/A287753/b287753.txt">Table of n, a(n) for n = 0..126</a>
%H A287753 Robert Price, <a href="/A287753/a287753.tmp.txt">Diagrams of first 20 stages</a>
%H A287753 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A287753 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A287753 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A287753 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A287753 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A287753 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A287753 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A287753 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A287753 code = 350; stages = 128;
%t A287753 rule = IntegerDigits[code, 2, 10];
%t A287753 g = 2 * stages + 1; (* Maximum size of grid *)
%t A287753 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A287753 ca = a;
%t A287753 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A287753 PrependTo[ca, a];
%t A287753 (* Trim full grid to reflect growth by one cell at each stage *)
%t A287753 k = (Length[ca[[1]]] + 1)/2;
%t A287753 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A287753 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
