%p A287654 N:= 2^10: # to get a(1) to a(N)
%p A287654 C[1]:= 1:
%p A287654 C[2]:= 2:
%p A287654 C[3]:= 2:
%p A287654 C[4]:= 2:
%p A287654 for n from 5 to N do
%p A287654 C[n]:= C[C[n-1]] + C[C[n-C[n-1]] + C[n-C[n-2]]];
%p A287654 od:
%p A287654 S:=seq(C[n], n=1..N);
