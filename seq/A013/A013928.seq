%o A013928 from math import isqrt
%o A013928 from sympy import mobius
%o A013928 def A013928(n): return sum(mobius(k)*((n-1)//k**2) for k in range(1,isqrt(n-1)+1)) # _Chai Wah Wu_, Jan 03 2024
