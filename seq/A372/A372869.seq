%o A372869 (Python) # Using `sample_gauss_kuzmin_distribution` function from A084580.
%o A372869 from sympy import floor, continued_fraction_convergents
%o A372869 from collections import deque
%o A372869 from os.path import commonprefix
%o A372869 def reliable_digits_from_cf(coeffs, prec):
%o A372869     frac_lower, frac_upper = deque(continued_fraction_convergents(coeffs+[1]), maxlen=2)
%o A372869     order = 10**prec
%o A372869     trunc_lower = floor(frac_lower*order) / order
%o A372869     trunc_upper = floor(frac_upper*order) / order
%o A372869     return commonprefix([f'{trunc_lower:.{prec}f}'[2:], f'{trunc_upper:.{prec}f}'[2:]])
%o A372869 num_coeffs = 180
%o A372869 coeffs = [0] + sample_gauss_kuzmin_distribution(num_coeffs)
%o A372869 num = reliable_digits_from_cf(coeffs, prec=200)
%o A372869 A372869 = [int(d) for d in num]
