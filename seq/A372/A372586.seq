%p A372586 binds:= proc(n) local L,i; L:= convert(n,base,2); add(L[i]*i,i=1..nops(L)) end proc;
%p A372586 pinds:= proc(n) local F,t; F:= ifactors(n)[2]; add(NumberTheory:-pi(t[1])*t[2],t=F) end proc;
%p A372586 select(t -> (binds(t) +pinds(t))::odd, [$1..200]); # _Robert Israel_, Jan 05 2026
%t A372586 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A372586 bix[n_]:=Join@@Position[Reverse[IntegerDigits[n,2]],1];
%t A372586 Select[Range[100],OddQ[Total[bix[#]]+Total[prix[#]]]&]
