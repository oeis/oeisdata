%t A372693 f[p_, e_] := p^(2^(-1 + Position[Reverse@ IntegerDigits[e, 2], _?(# == 0 &)])); s[1] = 1; s[n_] := s[n] = Times @@ (Flatten@ (f @@@ FactorInteger[n]) + 1);
%t A372693 Select[Range[10^5], (s1 = s[#]) > 1 && s1 == s[# + 1] &]
%o A372693 (PARI) s(n) = {my(f = factor(n), k); prod(i = 1, #f~, k = apply(x -> 1 - x, binary(f[i, 2])); prod(j = 1, #k, if(k[j], f[i, 1]^(2^(#k-j)) + 1, 1)));}
%o A372693 lista(kmax) = {my(s1 = s(1), s2); for(k = 2, kmax, s2 = s(k); if(s1 > 1 && s1 == s2, print1(k - 1, ", ")); s1 = s2);}
