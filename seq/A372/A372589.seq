%p A372589 gbi:= proc(n) local L,i;
%p A372589   L:= convert(n,base,2);
%p A372589   max(select(i->L[i]=1,[$1..nops(L)]))
%p A372589 end proc:
%p A372589 gpi:= proc(n) NumberTheory:-pi(max(NumberTheory:-PrimeFactors(n))) end proc:
%p A372589 filter:= n -> (gbi(n)+gpi(n))::even:
