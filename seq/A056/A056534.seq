%p A056534 ordered_pair_perm := proc(upto_n) local a,i,j; a := []; for i from 1 to upto_n do for j in sort(divisors(i)) do a := [op(a),binomial(((i/j) + j - 1),2)+j]; od; od; RETURN(a); end;
