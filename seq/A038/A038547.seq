%o A038547 (Haskell)
%o A038547 import Data.List  (find)
%o A038547 import Data.Maybe (fromJust)
%o A038547 a038547 n = fromJust $ find ((== n) . length . divisors) [1,3..]
%o A038547    where divisors m = filter ((== 0) . mod m) [1..m]
