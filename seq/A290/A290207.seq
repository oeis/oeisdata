%S A290207 1,10,110,1100,10100,101000,1011000,11010000,111010000,1001100000,
%T A290207 11001100000,100001000000,1011001000000,10000110000000,
%U A290207 101010110000000,1000000100000000,10101000100000000,110101111000000000,1001101111000000000,11010011010000000000
%N A290207 Binary representation of the diagonal from the origin to the corner of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 726", based on the 5-celled von Neumann neighborhood.
%C A290207 Initialized with a single black (ON) cell at stage zero.
%D A290207 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A290207 Robert Price, <a href="/A290207/b290207.txt">Table of n, a(n) for n = 0..126</a>
%H A290207 Robert Price, <a href="/A290207/a290207.tmp.txt">Diagrams of first 20 stages</a>
%H A290207 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A290207 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A290207 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A290207 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A290207 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A290207 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A290207 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A290207 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A290207 code = 726; stages = 128;
%t A290207 rule = IntegerDigits[code, 2, 10];
%t A290207 g = 2 * stages + 1; (* Maximum size of grid *)
%t A290207 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A290207 ca = a;
%t A290207 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A290207 PrependTo[ca, a];
%t A290207 (* Trim full grid to reflect growth by one cell at each stage *)
%t A290207 k = (Length[ca[[1]]] + 1)/2;
%t A290207 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A290207 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
