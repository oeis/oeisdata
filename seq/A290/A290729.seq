%t A290729 a[n_] := Product[{p, e} = pe; If[p <= 3, p^e, (p^e - p^(e-1))/2 + (p^(e-1) - p^(Mod[e+1, 2]))/(2*(p+1)) + 1], {pe, FactorInteger[n]}];
%t A290729 r = 2; Reap[For[j=1, j <= 10^4, j = j+1, t = a[j]/j; If[t<r, r = t; Sow[j] ]]][[2, 1]] (* _Jean-FranÃ§ois Alcover_, Oct 02 2018, after _Hugo Pfoertner_ *)
