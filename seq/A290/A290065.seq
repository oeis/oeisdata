%S A290065 1,1,7,15,7,15,63,127,479,959,255,511,1023,2047,3071,6143,24063,48127,
%T A290065 102399,204799,344063,688127,2949119,5898239,25133055,50266111,
%U A290065 117309439,234618879,536346623,1072693247,1912340479,3824680959,7716339711,15432679423
%N A290065 Decimal representation of the diagonal from the origin to the corner of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 633", based on the 5-celled von Neumann neighborhood.
%C A290065 Initialized with a single black (ON) cell at stage zero.
%D A290065 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A290065 Robert Price, <a href="/A290065/b290065.txt">Table of n, a(n) for n = 0..126</a>
%H A290065 Robert Price, <a href="/A290065/a290065.tmp.txt">Diagrams of first 20 stages</a>
%H A290065 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A290065 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A290065 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A290065 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A290065 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A290065 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A290065 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A290065 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A290065 code = 633; stages = 128;
%t A290065 rule = IntegerDigits[code, 2, 10];
%t A290065 g = 2 * stages + 1; (* Maximum size of grid *)
%t A290065 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A290065 ca = a;
%t A290065 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A290065 PrependTo[ca, a];
%t A290065 (* Trim full grid to reflect growth by one cell at each stage *)
%t A290065 k = (Length[ca[[1]]] + 1)/2;
%t A290065 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A290065 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
%Y A290065 Cf. A290062, A290063, A290064.
