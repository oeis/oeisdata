%o A077426 def A077426_gen(startvalue=3): # generator of terms >= startvalue
%o A077426     return filter(lambda n: len(diop_DN(n,-1)), count(max(startvalue+(startvalue&1^1),3),2)) # _Chai Wah Wu_, Dec 21 2025
