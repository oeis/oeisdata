%o A068834 (Python)
%o A068834 from math import isqrt
%o A068834 from itertools import count, islice
%o A068834 def c(k): return isqrt(r:=sum(map(int, str(k))))**2 == r
%o A068834 def agen(): # generator of terms
%o A068834     c1, c2, c3, c4 = list(map(c, [1, 4, 9, 16]))
%o A068834     for k in count(5):
%o A068834         if all([c1, c2, c3, c4]): yield (k-4)**2
%o A068834         c1, c2, c3, c4 = c2, c3, c4, c(k*k)
%o A068834 print(list(islice(agen(), 32))) # _Michael S. Branicky_, Feb 11 2026
