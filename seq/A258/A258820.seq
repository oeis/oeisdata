%t A258820 max = 15; coes = Table[ PadRight[ CoefficientList[ BernoulliB[n, x], x], max], {n, 0, max-1}]; inv = Inverse[coes] // Numerator; t[n_, k_] := inv[[n, k]]; t[n_, k_] /; k == n+1 = 1; Table[t[n-k+1, k], {n, 2, max+1}, {k, 2, Floor[n/2]+1}] // Flatten (* _Jean-Fran√ßois Alcover_, Jul 22 2015 *)
