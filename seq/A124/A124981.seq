%o A124981 (PARI)
%o A124981 isA022544(n)={ local(cnt=0,y2) ; for(x=0,sqrtint(n), y2=n-x^2 ; if( y2>=x^2 && issquare(y2), return(0) ; ) ; ) ; return(1) ; }
%o A124981 isA124981(n)={ return( (n%2) && isA022544(n)) ; }
