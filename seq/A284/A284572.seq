%o A284572 (Scheme) ;; With memoization-macro definec.
%o A284572 (definec (A284572 n) (cond ((= 1 n) n) ((even? n) (A005117 (+ 1 (A284572 (/ n 2))))) (else (A065642 (+ 1 (A284572 (/ (- n 1) 2)))))))
