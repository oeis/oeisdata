%p A188642 isA007602 := proc(n) if A007954(n) = 0 then false; else `if`(n mod A007954(n) = 0,true,false) ; end if; end proc:
%p A188642 A188642 := proc(n) if isA007602(n) then 1; else 0 ; end if ; end proc:
