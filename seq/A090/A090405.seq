%o A090405 (Python)
%o A090405 from sympy import isprime
%o A090405 def a(n):
%o A090405     if n<2: return 2
%o A090405     else:
%o A090405         if isprime(n + 1 + (n%2 == 1) + (n==1)): return 1
%o A090405         else: return 0 # _Indranil Ghosh_, Mar 31 2017
