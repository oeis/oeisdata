%p A090822 K:= proc(L)
%p A090822 local n,m,k,i,b;
%p A090822 m:= 0;
%p A090822 n:= nops(L);
%p A090822 for k from 1 do
%p A090822   if k*(m+1) > n then return(m) fi;
%p A090822   b:= L[-k..-1];
%p A090822   for i from 1 while i*k <= n and L[-i*k .. -(i-1)*k-1] = b do od:
%p A090822   m:= max(m, i-1);
%p A090822 od:
%p A090822 end proc:
%p A090822 A[1]:= 1:
%p A090822 for i from 2 to 220 do
%p A090822   A[i]:= K([seq(A[j],j=1..i-1)])
%p A090822 od:
%p A090822 seq(A[i],i=1..220); # _Robert Israel_, Jul 02 2015
%t A090822 ClearAll[a]; reversed = {a[2]=1, a[1]=1}; blocs[len_] := Module[{bloc1, par, pos}, bloc1 = Take[reversed, len]; par = Partition[ reversed, len]; pos = Position[par, bloc_ /; bloc != bloc1, 1, 1]; If[pos == {}, Length[par], pos[[1, 1]] - 1]]; a[n_] := a[n] = Module[{an}, an = Table[{blocs[len], len}, {len, 1, Quotient[n-1, 2]}] // Sort // Last // First; PrependTo[ reversed, an]; an]; A090822 = Table[a[n], {n, 1, 99}] (* _Jean-FranÃ§ois Alcover_, Aug 13 2012 *)
