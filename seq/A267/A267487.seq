%p A267487 isA267487 := proc(p)
%p A267487     if isprime(p) then
%p A267487         A001221(p+1) ;
%p A267487         simplify(modp(% &^ (p-1),p^2) =1 );
%p A267487     else
%p A267487         false;
%p A267487     end if;
%p A267487 end proc:
%p A267487 p := 2;
