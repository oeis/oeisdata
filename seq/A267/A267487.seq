%p A267487     if isA267487(p) then
%p A267487         printf("%d\n",p) ;
%p A267487     end if;
%p A267487     p := nextprime(p) ;
%p A267487 end do: # _R. J. Mathar_, Jan 23 2016
%t A267487 Select[Prime[Range[3200]], Mod[PrimeNu[# + 1], #^2]^(# - 1) == 1 &] (* _G. C. Greubel_, Apr 25 2017 *)
