%t A052174 c = Binomial; T[n_, m_] /; EvenQ[n-m] := (k = (n-m)/2; c[n+1, k]*c[n, k] - c[n+1, k]*c[n, k-1]); T[n_, m_] /; OddQ[n-m] := (k = (n-m-1)/2; c[n+1, k]*c[n, k+1] - c[n+1, k+1]*c[n, k-1]); Table[T[n, m], {n, 0, 9}, {m, 0, n}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 13 2015, after _Michel Marcus_ *)
%o A052174 (PARI) tabl(nn) = {alias(C, binomial); for (n=0, nn, for (k=0, n, if (!((n-k) % 2), kk = (n-k)/2; tnk = C(n+1,kk)*C(n,kk) - C(n+1,kk)*C(n,kk-1), kk = (n-k-1)/2; tnk = C(n+1,kk)*C(n,kk+1) - C(n+1,kk+1)*C(n,kk-1)); print1(tnk, ", ");); print(););} \\ _Michel Marcus_, Oct 12 2014
