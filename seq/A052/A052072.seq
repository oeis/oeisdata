%o A052072 (Python)
%o A052072 def A052072(n):
%o A052072     a, b, c = 0, 0, 0
%o A052072     for i in range(10**9):
%o A052072         s = str(c)
%o A052072         for d in set(s):
%o A052072             if s.count(d) != n:
%o A052072                 break
%o A052072         else:
%o A052072             return c
%o A052072         c += a + b + 1
