%o A113081 (PARI) /* Generalized Cook-Kleber Recurrence */ T(n,k,q=3)=if(n==0,1,if(n<0 || k<=0,0,if(n==1,k, if(n>=k,sum(j=1,k,T(n-1,k+(q-1)*j)), sum(j=1,n+1,(-1)^(j-1)*binomial(n+1,j)*T(n,k-j))))))
%o A113081 (PARI) /* Matrix Power Recurrence (Paul D. Hanna) */ T(n,k,q=3)=local(M=matrix(n+1,n+1));for(r=1,n+1, for(c=1,r, M[r,c]=if(r==c,1,if(c>1,(M^q)[r-1,c-1])+(M^q)[r-1,c]))); return((M^k)[n+1,1])
