%o A363408 from gmpy2 import digits
%o A363408 def okA176189(n): return "2" not in digits(n*n, 3)
%o A363408 print([k**2 for k in range(1000) if okA176189(k)]) # _Michael S. Branicky_, Jun 07 2023
