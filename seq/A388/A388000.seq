%C A388000 a(11), if it exists, is greater than 10^9. - _Lyle Blosser_, Jan 18 2026
%e A388000 80 is in the sequence since phi(80) = 32 = 22 + 10 = pi(80) + tau(80).
%t A388000 Select[Range[400], EulerPhi[#] == PrimePi[#] + DivisorSigma[0, #] &] (* _Amiram Eldar_, Oct 14 2025 *)
%o A388000 (Python)
%o A388000 from math import prod
%o A388000 from itertools import count, islice
%o A388000 from sympy import factorint, nextprime
%o A388000 def agen(): # generator of terms
%o A388000     pk, pik = 2, 0
%o A388000     for k in count(1):
%o A388000         if k == pk:
%o A388000             pk, pik = nextprime(pk), pik + 1
%o A388000         f = factorint(k)
%o A388000         phik = prod((p - 1)*p**(e-1) for p, e in f.items())
%o A388000         tauk = prod(e+1 for e in f.values())
%o A388000         if phik == pik + tauk:
%o A388000             yield k
%o A388000 print(list(islice(agen(), 10))) # _Michael S. Branicky_, Jan 19 2026
