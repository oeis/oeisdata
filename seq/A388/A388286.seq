%t A388286 f1[p_, e_] := If[e == 1, 1, 0];
%t A388286 f2[p_, e_] := (p - 1)*p^(e - 1); f2[p_, 1] := p;
%t A388286 a[n_] := Module[{f = FactorInteger[n]}, (n * Times @@ f1 @@@ f + Times @@ f2 @@@ f) / 2]; Array[a, 100] (* _Amiram Eldar_, Oct 05 2025 *)
