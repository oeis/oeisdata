%t A388549 nn = 2^21; i = 1; k = 2; MapIndexed[Set[S[First[#2]], #1] &, Select[Range@ Sqrt[nn], 1 == Min[#] < Max[#] &@ FactorInteger[#][[All, -1]] &] ]; Union@ Reap[While[j = 3; While[S[i]^j < nn, Sow[S[i]^j]; j++]; j > 3, k++; i++] ][[-1, 1]]
%o A388549 (Python)
%o A388549 from math import isqrt
%o A388549 from sympy import integer_nthroot, mobius, factorint
%o A388549 from oeis_sequences.OEISsequences import bisection, squarefreepi
%o A388549 def A388549(n):
%o A388549     def g(x):
%o A388549         c, l = squarefreepi(integer_nthroot(x,3)[0])+sum(mobius(k)*(integer_nthroot(x, k)[0]-1) for k in range(2, x.bit_length()))-2, 0
%o A388549         j = isqrt(x)
%o A388549         while j>1:
%o A388549             k2 = integer_nthroot(x//j**2,3)[0]+1
%o A388549             w = squarefreepi(k2-1)
%o A388549             c += j*(w-l)
%o A388549             l, j = w, isqrt(x//k2**3)
%o A388549         return c-l
%o A388549     def f(x):
%o A388549         z = isqrt(x)
%o A388549         return n+2+x+g(z)+sum(mobius(k)*((w:=integer_nthroot(x,k)[0])-1)+squarefreepi(w)+g(w) for k in range(3, x.bit_length()))-sum(isqrt(z//k**3) for k in range(1, integer_nthroot(z, 3)[0]+1) if all(d<=1 for d in factorint(k).values()))
%o A388549     return bisection(f,n,n) # _Chai Wah Wu_, Dec 02 2025
