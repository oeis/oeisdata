%e A281865 a(6) = 1: {1,2,3,4,6}.
%e A281865 a(7) = 2: {1,2,3,4,6}, {3,4,5,6,7}.
%e A281865 a(8) = 6: {1,2,3,4,6}, {1,3,6,7,8}, {1,4,5,7,8}, {2,3,5,7,8}, {2,4,5,6,8}, {3,4,5,6,7}.
%p A281865 b:= proc(n, i, t) option remember;
%p A281865       `if`(i<t, 0, `if`(n=0, `if`(t=0, 1, 0),
%p A281865       `if`(t<1 or n<t*(t+1)/2 or n>(t+1)*(2*i-t)/2, 0,
%p A281865       `if`(i>n, 0, b(n-i, i-1, t-1))+b(n, i-1, t))))
%p A281865     end:
%p A281865 a:= proc(n) option remember; `if`(n<0, 0, a(n-1)+add(
%p A281865        b(j^2-n, n-1, 4), j=isqrt(n-10)..isqrt(5*n-10)))
%p A281865     end:
