%p A227345 b:= proc(n, i, t) option remember; `if`(n=0, `if`(t>1, x, 1),
%p A227345       expand(`if`(i<1, 0, `if`(t>1, x, 1)*b(n, i-1, iquo(t, 2))+
%p A227345       `if`(i>n, 0, `if`(t=2, x, 1)*b(n-i, i-1, iquo(t, 2)+2)))))
%p A227345     end:
%p A227345 T:= n-> (p->seq(coeff(p, x, i), i=1..n))(b(n$2, 0)):
%p A227345 seq(T(n), n=1..20);  # _Alois P. Heinz_, Jul 16 2013
%t A227345 b[n_, i_, t_] := b[n, i, t] = If[n == 0, If[t>1, x, 1], Expand[If[i<1, 0, If[t>1, x, 1]*b[n, i-1, Quotient[t, 2]] + If[i>n, 0, If[t == 2, x, 1] * b[n-i, i-1, Quotient[t, 2]+2]]]]]; T[n_] := Function[{p}, Table[Coefficient[p, x, i], {i, 1, n}]][b[n, n, 0]]; Table[T[n], {n, 1, 20}] // Flatten (* _Jean-FranÃ§ois Alcover_, Feb 18 2015, after _Alois P. Heinz_ *)
