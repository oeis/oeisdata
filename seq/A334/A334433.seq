%t A334433 Join@@Table[Times@@Prime/@#&/@Sort[IntegerPartitions[n]],{n,0,8}]
%o A334433 (PARI)
%o A334433 cmpLenLexAsc(x,y)=my(d=cmp(#x,#y)); if(d, d, lex(x,y))
%o A334433 C(sig)=prod(k=1,#sig,prime(sig[k]))
%o A334433 Row(n)={apply(C, vecsort([Vecrev(p) | p<-partitions(n)], cmpLenLexAsc))}
%o A334433 { for(n=0, 9, print(Row(n))) } \\ _Andrew Howroyd_, Oct 07 2025
