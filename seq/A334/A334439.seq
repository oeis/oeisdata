%t A334439 revlensort[f_,c_]:=If[Length[f]!=Length[c],Length[f]<Length[c],OrderedQ[{c,f}]];
%t A334439 Join@@Table[Sort[IntegerPartitions[n],revlensort],{n,0,8}]
%o A334439 (PARI)
%o A334439 cmpLenLexDesc(x,y)=my(d=cmp(#x,#y)); if(d, d, -lex(x,y))
%o A334439 Row(n)={concat(vecsort([Vecrev(p) | p<-partitions(n)], cmpLenLexDesc))}
%o A334439 { for(n=1, 7, print(Row(n))) } \\ _Andrew Howroyd_, Oct 06 2025
