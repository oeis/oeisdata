%t A334438 revlensort[f_,c_]:=If[Length[f]!=Length[c],Length[f]<Length[c],OrderedQ[{c,f}]];
%t A334438 Join@@Table[Times@@Prime/@#&/@Sort[IntegerPartitions[n],revlensort],{n,0,8}]
%o A334438 (PARI)
%o A334438 C(sig)=prod(k=1,#sig,prime(sig[k]))
%o A334438 cmpLenLexDesc(x,y)=my(d=cmp(#x,#y)); if(d, d, -lex(x,y))
%o A334438 Row(n)={apply(C, vecsort([Vecrev(p) | p<-partitions(n)], cmpLenLexDesc))}
%o A334438 { for(n=0, 9, print(Row(n))) } \\ _Andrew Howroyd_, Oct 06 2025
