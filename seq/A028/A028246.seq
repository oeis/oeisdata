%o A028246 def A163626_row(n) :
%o A028246     x = polygen(ZZ,'x')
%o A028246     A = []
%o A028246     for m in range(0, n, 1) :
%o A028246         A.append((-x)^m)
%o A028246         for j in range(m, 0, -1):
%o A028246             A[j - 1] = j * (A[j - 1] - A[j])
%o A028246     return list(A[0])
