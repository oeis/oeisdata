%S A107423 15,35,143,899,1763,5183,19043,79523,213443,359999,412163,685583,
%T A107423 777923,1192463,1695203,2585663,4536899,5143823,5673923,7225343,
%U A107423 12446783,12659363,12830723,17040383,17892899,18818243,20684303,25100099
%N A107423 Numbers k equal prime(m)*prime(m+1) such that k+1 is a square and k-1 is an even semiprime.
%H A107423 Robert Israel, <a href="/A107423/b107423.txt">Table of n, a(n) for n = 1..10000</a>
%e A107423 a(1)=15 because 3*5=15: 15+1=4^2 and 15-1=2*7;
%e A107423 a(2)=35 because 5*7=35: 35+1=6^2 and 35-1=2*17;
%e A107423 a(3)=143 because 11*13=143: 143+1=12^2 and 143-1=2*71;
%e A107423 a(4)=899 because 29*31=899: 899+1=30^2 and 899-1=2*449;
%p A107423 Res:= NULL: count:= 0:
%p A107423 q:= 2:
%p A107423 while count < 100 do
%p A107423   p:= q; q:= nextprime(p);
%p A107423   k:= p*q;
%p A107423   if issqr(k+1) and isprime((k-1)/2) then
%p A107423     count:= count+1; Res:= Res,k;
%p A107423   fi
%p A107423 od:
%p A107423 Res; # _Robert Israel_, Feb 12 2026
