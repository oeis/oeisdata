%o A199631 from itertools import permutations
%o A199631 def afull(): return sorted(t for p in permutations("0123456789") if p[0]!='0' and len(s:=str((t:=int("".join(p)))**3))==30 and all(s.count(d)==3 for d in "123456789"))
