%t A045920 f[n_]:=Plus@@Last/@FactorInteger[n];lst={};Do[If[f[n]==f[n+1],AppendTo[lst,n]],{n,0,6!}];lst (* _Vladimir Joseph Stephan Orlovsky_, May 12 2010 *)
