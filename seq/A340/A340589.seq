%t A340589 Table[Sum[Sum[Sum[Sign[Mod[k, 2] + Mod[j, 2] + Mod[i, 2] + Mod[n - i - j - k, 2]], {i, j, Floor[(n - j - k)/2]}], {j, k, Floor[(n - k)/3]}], {k, Floor[n/4]}], {n, 0, 100}]
