%t A340596 facs[n_]:=If[n<=1,{{}},Join@@Table[Map[Prepend[#,d]&,Select[facs[n/d],Min@@#>=d&]],{d,Rest[Divisors[n]]}]];
%t A340596 Table[Length[Select[facs[n],Length[#]==PrimeNu[n]&]],{n,100}]
