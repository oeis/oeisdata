%t A111530 T[n_, k_] := T[n, k] = Which[n<0 || k<0, 0, k==0 || k==1, 1, n==0, k!, True, (T[n-1, k+1]-T[n-1, k])/n - Sum[T[n, j]*T[n-1, k-j], {j, 1, k-1}]];
