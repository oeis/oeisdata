%U A232243 2,1,3,2,3,1,3,3,5,2,3,3,0,1,3,2,4,3,3,3,2,2,5,4,0,-1,1,-1,-1,0,2,2,2
%N A232243 a(n) = wt(n^2) - wt(n), where wt(n) = A000120(n) is the binary weight function.
%C A232243 A077436 gives n for which this sequence is zero.
%C A232243 A094694 gives n for which this sequence is negative.
%F A232243 a(n) = A159918(n) - A000120(n).
%e A232243 a(5) : 5 is 101, 25 is 11001, so a(5) is 3 - 2 = 1.
%e A232243 a(23) : 23 is 10111, 529 is 10001001, so a(23) = 3 - 4 = -1.
%o A232243 (JavaScript)
%o A232243 function bitCount(n) {
%o A232243 var i,c,s;
%o A232243 c=0;
%o A232243 s=n.toString(2);
%o A232243 for (i=0;i<s.length;i++) if (s.charAt(i)==1) c++;
%o A232243 return c;
%o A232243 }
%o A232243 for (i=0;i<100;i++) document.write(bitCount(i*i)-bitCount(i)+", ");
