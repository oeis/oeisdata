%o A232243 (JavaScript)
%o A232243 function bitCount(n) {
%o A232243    var i,c,s;
%o A232243    c=0;
%o A232243    s=n.toString(2);
%o A232243    for (i=0;i<s.length;i++)
%o A232243       if (s.charAt(i)==1)
%o A232243          c++;
%o A232243    return c;
%o A232243 }
%o A232243 for (i=0;i<100;i++) document.write(bitCount(i*i)-bitCount(i)+", ");
