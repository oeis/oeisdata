%t A069948 f[n_] := f[n] = Sum[(k + n)!^3/((k + n)!*(k!^3)*E), {k, 0, Infinity}]; Table[ f[n], {n, 0, 13}] (* or *)
%t A069948 Table[n!^2*HypergeometricPFQ[{n + 1, n + 1}, {1, 1}, 1]/Exp[1], {n, 0, 13}] (* _Robert G. Wilson v_, Jan 11 2007 *)
