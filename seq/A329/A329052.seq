%o A329052 EulerXY(A)={my(j=serprec(A,x)); exp(sum(i=1, j, 1/i * subst(subst(A + x * O(x^(j\i)), x, x^i), y, y^i)))}
%o A329052 R(n)={my(A=O(x)); for(j=1, 2*n, A = if(j%2, 1, y)*x*EulerXY(A)); A};
%o A329052 P(n)={my(r1=R(n), r2=x*EulerXY(r1), s=r1+r2-r1*r2); Vec(EulerXY(s))}
%o A329052 { my(A=P(10)); for(n=0, #A\2, for(k=0, #A\2, print1(polcoef(A[n+k+1], k), ", ")); print) }
