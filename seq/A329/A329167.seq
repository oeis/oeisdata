%t A329167 MultiFactorial[n_, k_] := If[n < 1, 1, n*MultiFactorial[n - k, k]];
%t A329167 Select[Range[1000], (x = MultiFactorial[#, 4] - 32; x > 0 && PrimeQ[x]) &]
