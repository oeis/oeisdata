%t A095072 Select[Prime[Range[500]], Differences[DigitCount[#, 2]] == {1} &]
%o A095072 (PARI) isA095072(n)=my(v=binary(n));#v==2*sum(i=1,#v,v[i])+1&&isprime(n)
