%F A115873 a(2^k) = 1, a(2n) = a(n).
%F A115873 From _Antti Karttunen_, Dec 20 2025: (Start)
%F A115873 a(n) = Min_{k>=1 such that k == n*k XOR A048724(A048720(n-1, k))}.
%F A115873 a(n) <= A003817(n) < 2*n.
%F A115873 (End)
%t A115873 X[a_, b_] := Module[{A, B, C, x},
%t A115873    A = Reverse@IntegerDigits[a, 2];
%t A115873    B = Reverse@IntegerDigits[b, 2];
%t A115873    C = Expand[
%t A115873       Sum[A[[i]]*x^(i - 1), {i, 1, Length[A]}]*
%t A115873       Sum[B[[i]]*x^(i - 1), {i, 1, Length[B]}]];
%t A115873    PolynomialMod[C, 2] /. x -> 2];
%t A115873 T[n_, k_] := Module[{x = BitXor[n - 1, 2 n - 1], k0 = k},
%t A115873      For[i = 1, True, i++, If[n*i == X[x, i],
%t A115873      If[k0 == 1, Return[i], k0--]]]];
%t A115873 a[n_] := T[n, 1];
%t A115873 Table[a[n], {n, 1, 100}] (* _Jean-FranÃ§ois Alcover_, Jan 04 2022 *)
%o A115873 (PARI)
%o A115873 A048720(b, c) = fromdigits(Vec(Pol(binary(b))*Pol(binary(c)))%2, 2);
%o A115873 A065621(n) = bitxor(n-1, n+n-1);
%o A115873 A115873(n) = forstep(k=1,oo,2,if((n*k)==A048720(A065621(n),k),return(k))); \\ _Antti Karttunen_, Dec 16 2025
%o A115873 (PARI)
%o A115873 A006068(n) = { my(s=1, ns); while(1, ns = n >> s; if(0==ns, return(n)); n = bitxor(n, ns); s <<= 1); };
%o A115873 A115873(n) = forstep(k=1,oo,2,my(t=bitxor(k,k*n)); if(!(hammingweight(t)%2) && A006068(t) == 2*A048720(k,n-1), return(k))); \\ _Antti Karttunen_, Dec 20 2025
