%S A273855 1,5,5,17,9,29,21,65,33,69,69,141,129,201,233,325,309,337,353,465,525,
%T A273855 649,673,789,825,937,989,1169,1257,1421,1453,1677,1657,1729,1777,1945,
%U A273855 2053,2261,2405,2625,2705,2829,2901,3181,3285,3557,3709,4017,4041,4145
%N A273855 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 998", based on the 5-celled von Neumann neighborhood.
%C A273855 Initialized with a single black (ON) cell at stage zero.
%D A273855 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273855 Robert Price, <a href="/A273855/b273855.txt">Table of n, a(n) for n = 0..128</a>
%H A273855 Robert Price, <a href="/A273855/a273855.tmp.txt">Diagrams of the first 20 stages</a>
%H A273855 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273855 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273855 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273855 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273855 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273855 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273855 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273855 code=998; stages=128;
%t A273855 rule=IntegerDigits[code,2,10];
%t A273855 g=2*stages+1; (* Maximum size of grid *)
%t A273855 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273855 ca=a;
%t A273855 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273855 PrependTo[ca,a];
%t A273855 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273855 k=(Length[ca[[1]]]+1)/2;
%t A273855 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273855 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A273855 nonn,easy
%O A273855 0,2
