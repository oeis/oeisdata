%I A273267 #10 Nov 05 2025 15:22:34
%S A273267 1,8,45,208,896,3652,14664,58140,233932,933464,3734428,14938420,
%T A273267 59764528,239091748,956487288,3825849520
%N A273267 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 621", based on the 5-celled von Neumann neighborhood.
%C A273267 Initialized with a single black (ON) cell at stage zero.
%D A273267 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273267 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273267 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273267 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273267 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273267 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273267 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273267 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273267 code=621; stages=128;
%t A273267 rule=IntegerDigits[code,2,10];
%t A273267 g=2*stages+1; (* Maximum size of grid *)
%t A273267 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273267 ca=a;
%t A273267 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273267 PrependTo[ca,a];
%t A273267 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273267 k=(Length[ca[[1]]]+1)/2;
%t A273267 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273267 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273267 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A273267 Cf. A273266.
%K A273267 nonn,more
%O A273267 0,2
