%I A273247 #10 Nov 05 2025 15:22:34
%S A273247 1,4,29,152,696,3008,12552,51432,207592,833000,3340028,13364284,
%T A273247 53499372,214105632,856510780,3426475304
%N A273247 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 617", based on the 5-celled von Neumann neighborhood.
%C A273247 Initialized with a single black (ON) cell at stage zero.
%D A273247 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273247 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273247 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273247 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273247 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273247 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273247 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273247 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273247 code=617; stages=128;
%t A273247 rule=IntegerDigits[code,2,10];
%t A273247 g=2*stages+1; (* Maximum size of grid *)
%t A273247 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273247 ca=a;
%t A273247 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273247 PrependTo[ca,a];
%t A273247 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273247 k=(Length[ca[[1]]]+1)/2;
%t A273247 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273247 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273247 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A273247 Cf. A273246.
%K A273247 nonn,more
%O A273247 0,2
