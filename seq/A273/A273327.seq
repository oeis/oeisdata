%S A273327 1,5,17,61,257,1073,4433,18161,73937,299633,1210193,4875761,19608017,
%T A273327 78746993,315932753,1266565361
%N A273327 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 646", based on the 5-celled von Neumann neighborhood.
%C A273327 Initialized with a single black (ON) cell at stage zero.
%D A273327 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273327 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273327 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273327 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273327 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273327 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273327 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%F A273327 Conjecture: a(n) = 19*4^(n-2) - 16*3^(n-3) + 1, n>2. - _Lars Blomberg_, Jul 18 2016
%t A273327 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273327 code=646; stages=128;
%t A273327 rule=IntegerDigits[code,2,10];
%t A273327 g=2*stages+1; (* Maximum size of grid *)
%t A273327 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273327 ca=a;
%t A273327 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273327 PrependTo[ca,a];
%t A273327 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273327 k=(Length[ca[[1]]]+1)/2;
%t A273327 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273327 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273327 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A273327 Cf. A273326.
%K A273327 nonn,more
%O A273327 0,2
