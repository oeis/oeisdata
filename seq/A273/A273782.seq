%S A273782 3,17,27,33,40,48,56,64,72,80,88,96,104,112,120,128,136,144,152,160,
%T A273782 168,176,184,192,200,208,216,224,232,240,248,256,264,272,280,288,296,
%U A273782 304,312,320,328,336,344,352,360,368,376,384,392,400,408,416,424,432
%N A273782 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 929", based on the 5-celled von Neumann neighborhood.
%C A273782 Initialized with a single black (ON) cell at stage zero.
%D A273782 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273782 Robert Price, <a href="/A273782/b273782.txt">Table of n, a(n) for n = 0..127</a>
%H A273782 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273782 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273782 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273782 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273782 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273782 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%F A273782 Conjectures from _Colin Barker_, May 30 2016: (Start)
%F A273782 a(n) = 8*(1+n) for n>3.
%F A273782 a(n) = 2*a(n-1)-a(n-2) for n>5.
%F A273782 G.f.: (3+11*x-4*x^2-4*x^3+x^4+x^5) / (1-x)^2.
%F A273782 (End)
%t A273782 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273782 code=929; stages=128;
%t A273782 rule=IntegerDigits[code,2,10];
%t A273782 g=2*stages+1; (* Maximum size of grid *)
%t A273782 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273782 ca=a;
%t A273782 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273782 PrependTo[ca,a];
%t A273782 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273782 k=(Length[ca[[1]]]+1)/2;
%t A273782 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273782 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273782 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A273782 Cf. A273780.
