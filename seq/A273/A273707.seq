%S A273707 1,4,21,33,68,105,144,193,277,329,409,433,568,625,760,817,1013,1105,
%T A273707 1277,1369,1553,1661,1841,1993,2205,2297,2589,2705,3025,3125,3381,
%U A273707 3553,3881,4105,4389,4593,4933,5053,5385,5681,6009,6257,6585,6889,7245,7573,7841
%N A273707 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 873", based on the 5-celled von Neumann neighborhood.
%C A273707 Initialized with a single black (ON) cell at stage zero.
%D A273707 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273707 Robert Price, <a href="/A273707/b273707.txt">Table of n, a(n) for n = 0..128</a>
%H A273707 Robert Price, <a href="/A273707/a273707.tmp.txt">Diagrams of the first 20 stages</a>
%H A273707 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273707 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273707 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273707 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273707 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273707 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273707 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273707 code=873; stages=128;
%t A273707 rule=IntegerDigits[code,2,10];
%t A273707 g=2*stages+1; (* Maximum size of grid *)
%t A273707 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273707 ca=a;
%t A273707 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273707 PrependTo[ca,a];
%t A273707 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273707 k=(Length[ca[[1]]]+1)/2;
%t A273707 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273707 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A273707 nonn,easy
