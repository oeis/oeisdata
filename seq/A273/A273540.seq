%I A273540 #11 Nov 05 2025 15:22:34
%S A273540 1,6,23,60,121,218,343,540,777,1082,1419,1892,2413,3030,3659,4560,
%T A273540 5533,6638,7775,9176,10625,12234,13795,15884,18053,20414,22759,25544,
%U A273540 28329,31330,34119,37964,41945,46186,50459,55252,60093,65222,70175,76168,82241,88634
%N A273540 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 779", based on the 5-celled von Neumann neighborhood.
%C A273540 Initialized with a single black (ON) cell at stage zero.
%D A273540 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273540 Robert Price, <a href="/A273540/b273540.txt">Table of n, a(n) for n = 0..128</a>
%H A273540 N. J. A. Sloane, <a href="https://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273540 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273540 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273540 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273540 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273540 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273540 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273540 code=779; stages=128;
%t A273540 rule=IntegerDigits[code,2,10];
%t A273540 g=2*stages+1; (* Maximum size of grid *)
%t A273540 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273540 ca=a;
%t A273540 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273540 PrependTo[ca,a];
%t A273540 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273540 k=(Length[ca[[1]]]+1)/2;
%t A273540 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273540 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273540 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
%Y A273540 Cf. A273538.
%K A273540 nonn,easy
%O A273540 0,2
