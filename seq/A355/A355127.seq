%p A355127 b:= proc(n, m, x, y) option remember; `if`(n=0, 1, add(
%p A355127       add((s-> `if`({i, j}={0} or min(s)<1 or max(s)>m, 0,
%p A355127         b(n-1, m, s[])))([x+i, y+j]), j=-1..1), i=-1..1))
%p A355127     end:
%p A355127 a:= n-> add(add(b(n-1, n, x, y), x=1..n), y=1..n):
%p A355127 seq(a(n), n=1..20);  # _Alois P. Heinz_, Jun 20 2022
%t A355127 b[n_, m_, x_, y_] := b[n, m, x, y] = If[n == 0, 1, Sum[Sum[With[{s = {x + i, y + j}}, If[Union@{i, j} == {0} || Min[s] < 1 || Max[s] > m, 0, b[n - 1, m, Sequence @@ s]]], {j, -1, 1}], {i, -1, 1}]];
%t A355127 a[n_] := Sum[Sum[b[n - 1, n, x, y], {x, 1, n}], {y, 1, n}];
%t A355127 Table[a[n], {n, 1, 20}] (* _Jean-Fran√ßois Alcover_, Sep 10 2022, after _Alois P. Heinz_ *)
