%o A171223 (Python)
%o A171223 import re
%o A171223 from itertools import product
%o A171223 from fractions import Fraction
%o A171223 from collections import Counter
%o A171223 k = Counter()
%o A171223 for a,b,c,d,e,f,g,h in product(("", "+", "-", "*", "/"), repeat=8):
%o A171223     s1 = f"1{a}2{b}3{c}4{d}5{e}6{f}7{g}8{h}9"
%o A171223     s2 = re.sub(r'(\d+)', r'Fraction(\1)', s1)
%o A171223     t = eval(s2)
%o A171223     if t > 0 and t.denominator == 1:
%o A171223         k[t.numerator] += 1
%o A171223 print([k[i] for i in range(1, 54)]) # _Mariko Doi_, Dec 17 2025 [edited by _Michael S. Branicky_, Dec 19 2025]
