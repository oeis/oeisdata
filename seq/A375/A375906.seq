%F A375906 A055600(a(n)) = A003586(a(n)) + 1 = A005109(n).
%t A375906 With[{lim = 10^10}, Position[Sort@ Flatten@ Table[2^i*3^j + 1, {i, 0, Log2[lim]}, {j, 0, Log[3, lim/2^i]}], _?PrimeQ] // Flatten]
%o A375906 (PARI) lista(lim) = {my(s = List()); for(i = 0, logint(lim, 2), for(j = 0, logint(lim >> i, 3), listput(s, 2^i * 3^j + 1))); s = Set(s); for(i = 1, #s, if(isprime(s[i]), print1(i, ", ")));}
