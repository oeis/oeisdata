%t A216232 A216232[n_, k_]:= A216232[n,k]= If[n>k+2 || k>n+4, 0, If[n==0 || k==0, 1, A216232[n-1,k] + A216232[n,k-1]]];
%t A216232 T[n_, k_]:= A216232[k,n-k];
%t A216232 Table[T[n,k], {n,0,12}, {k,0,n}]//Flatten (* _G. C. Greubel_, Feb 04 2026 *)
%o A216232 (Magma)
%o A216232 A216232:= function(n,k)
%o A216232    if n gt k+2 or k gt n+4 then return 0;
%o A216232    elif n eq 0 or k eq 0 then return 1;
%o A216232    else return $$(n-1,k) + $$(n,k-1);
%o A216232    end if;
%o A216232 end function;
%o A216232 T:= func< n,k | A216232(k,n-k) >;
%o A216232 [T(n,k): k in [0..n], n in [0..12]]; // _G. C. Greubel_, Feb 04 2026
