%p A319869 a:=(n,k)->add((floor((n-j)/k)-floor((n-j-1)/k))*(mul(n-i-j+k+1,i=1..j)),j=1..k-1) + add((floor(j/k)-floor((j-1)/k))*(mul(j-i+1,i=1..k)),j=1..n): seq(a(n,5),n=1..40); # _Muniru A Asiru_, Sep 30 2018
%t A319869 k:=5; a[n_]:=Sum[(Floor[(n-j)/k]-Floor[(n-j-1)/k])*Product[n-i-j+k+1, {i,1,j }] , {j,1,k-1}] + Sum[(Floor[j/k]-Floor[(j-1)/k])*Product[j-i+1, {i,1,k}], {j,1,n}]; Array[a, 50] (* _Stefano Spezia_, Sep 30 2018 *)
