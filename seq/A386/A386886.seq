%t A386886 T[n_, k_] := If[Mod[n, 2] == 1, Binomial[n, k] - n*Binomial[(n - 1)/2, k - 1], Binomial[n, k] - (1/2)*n*(Binomial[n/2, k - 1] + Binomial[(n - 2)/2, k - 1])]
%t A386886 Table[T[n, k], {n, 0, 20}, {k, 3, n}] // Flatten
%o A386886 (Maxima)
%o A386886 T(n, k) := if mod(n, 2) = 1 then binomial(n, k) - n*binomial((n - 1)/2, k - 1) else binomial(n, k) - (n/2)*(binomial(n/2, k - 1) + binomial((n - 2)/2, k - 1))$
%o A386886 create_list(T(n, k), n, 3, 20, k, 3, n);
