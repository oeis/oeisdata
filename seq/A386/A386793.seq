%o A386793 def T23(n):
%o A386793     if n < 0:
%o A386793         return QQ(0)
%o A386793     if n== 0:
%o A386793         return QQ(1)
%o A386793     Tn = (14*n^3 - 21*n^2 + 15*n - 4) * T23(n-1)
%o A386793     Tn -= (57*n^3 - 171*n^2 + 202*n - 88) * T23(n-2)
%o A386793     Tn += (106*n^3 - 477*n^2 + 773*n - 444) * T23(n-3)
%o A386793     Tn -= (90*n^3 - 540*n^2 + 1104*n - 768) * T23(n-4)
%o A386793     Tn += (16*n^3 - 120*n^2 + 296*n - 240) * T23(n-5)
%o A386793     Tn += (19*n^3 - 171*n^2 + 494*n - 456) * T23(n-6)
%o A386793     Tn /= n^3
%o A386793     return Tn
