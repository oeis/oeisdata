%I A106105 #19 Nov 25 2025 10:01:06
%S A106105 5,59,557,569,577,587,599,659,757,857,859,5557,5569,5657,5659,5669,
%T A106105 5689,5779,5857,5867,5869,5879,5897,5987,6569,6577,6599,6659,6857,
%U A106105 6959,7559,7577,7589,7757,7759,8597,8599,9587,9857,9859,55579,55589,55667,55697
%N A106105 Primes with minimal digit = 5.
%H A106105 Robert Israel, <a href="/A106105/b106105.txt">Table of n, a(n) for n = 1..10000</a>
%p A106105 nextcand:= proc(n) local L,nL,Lp,i,j;
%p A106105   L:= convert(n,base,10); nL:= nops(L);
%p A106105   if min(L) > 5 then
%p A106105     for i from 2 to nL do
%p A106105       if L[i] < 9 then
%p A106105         Lp:= [5$(i-1),L[i]+1,op(L[i+1..nL])];
%p A106105         return add(Lp[j]*10^(j-1),j=1..nL);
%p A106105       fi
%p A106105     od;
%p A106105     return 5*(10^(nL+1)-1)/9;
%p A106105   else
%p A106105     for i from nL to 1 by -1 do
%p A106105       if L[i] < 5 then
%p A106105         Lp:= [5$i,op(L[i+1..nL])];
%p A106105         return add(Lp[j]*10^(j-1),j=1..nL)
%p A106105       fi
%p A106105     od;
%p A106105   fi;
%p A106105   n
%p A106105 end proc;
%p A106105 R:= NULL: count:= 0:
%p A106105 x:= 0:
%p A106105 while count < 100 do
%p A106105   x:= nextcand(x+1);
%p A106105   if isprime(x) then R:= R,x; count:= count+1; fi;
%p A106105 od:
%p A106105 R; # _Robert Israel_, Nov 24 2025
%t A106105 Select[Prime[Range[800]], Min[IntegerDigits[ # ]]==5&]
%Y A106105 Cf. A106101, A106102, A106103, A106104, A106106, A106107.
%K A106105 nonn,base,changed
