%t A106053 t[0, 0] = t[1, 0] = t[1, 1] = t[1, 2] = 1;
%t A106053 t[n_ /; n >= 0, k_ /; k >= 0] /; k <= 2n := t[n, k] = t[n-1, k] + t[n-1, k-1] + t[n-1, k-2] + t[n-2, k-2];
%t A106053 t[n_, k_] /; n<0 || k<0 || k>2n = 0;
%t A106053 a[n_] := t[n-1, n-2];
