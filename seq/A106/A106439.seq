%o A106439 a106439 n = a106439_list !! (n-1)
%o A106439 a106439_list = filter belge1 [1..] where
%o A106439    belge1 x = x == (head $ dropWhile (< x) $
%o A106439                     scanl (+) 1 $ cycle (map (read . return) $ show x))
