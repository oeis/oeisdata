%t A360480 rad[x_] := rad[x] = Times @@ FactorInteger[x][[All, 1]]; Table[k = rad[n]; Count[Range[n], _?(Nor[CoprimeQ[#1, n], Divisible[#2, k], Divisible[k, #2]] & @@ {#, rad[#]} &)], {n, 88}]
