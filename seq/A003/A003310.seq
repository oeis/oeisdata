%o A003310 (Haskell)
%o A003310 a003310 n = a003310_list !! (n-1)
%o A003310 a003310_list = f [3..] where
%o A003310    f (x:xs) = x : f (g xs) where
%o A003310      g zs = us ++ g vs where (us, _:vs) = splitAt (x - 1) zs
