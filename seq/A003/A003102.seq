%t A003102 k=1; lc=1; Table[While[r=Floor[lc^(1/n)]; Union[Mod[lc,Range[r]]]=={0}, k++; good=lc; lc=LCM[lc,k]]; m=2; While[r=Floor[(m*good)^(1/n)]; Union[Mod[m*good,Range[r]]]=={0}, m++ ]; m=m-1; m*good, {n,16}] (* _T. D. Noe_, Aug 01 2006 *)
