%o A003842 (Python) def A003842(length):
%o A003842     a = [1]
%o A003842     while len(a)<length: a = [j for i in a for j in [[],[1,2],[1]][i]]
%o A003842     return a[:length] # _Nicholas Stefan Georgescu_, Jun 14 2022
%o A003842 (Python)
%o A003842 def aupto(nn):
%o A003842     S, Fnm2, Fnm1 = [1, 2], 1, 2
%o A003842     while len(S) < nn+1:
%o A003842         S += S[:min(Fnm2, nn+1-len(S))]
%o A003842         Fnm2, Fnm1 = Fnm1, Fnm1+Fnm2
%o A003842     return S
%o A003842 print(aupto(104)) # _Michael S. Branicky_, Jun 06 2022
%o A003842 (Python)
%o A003842 from math import isqrt
%o A003842 def A003842(n): return n+2-((m:=(n+2+isqrt(5*(n+2)**2)>>1)-n-2)+isqrt(5*m**2)>>1) # _Chai Wah Wu_, Aug 26 2022
