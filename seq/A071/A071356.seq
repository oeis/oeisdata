%o A071356 (SageMath)
%o A071356 def A071356_list(n):  # n>=1
%o A071356     T = [0]*(n+1); R = [1]
%o A071356     for m in (1..n-1):
%o A071356         a,b,c = 1,0,0
%o A071356         for k in range(m,-1,-1):
%o A071356             r = a + 2*(b + c)
%o A071356             if k < m : T[k+2] = u;
%o A071356             a,b,c = T[k-1],a,b
%o A071356             u = r
%o A071356         T[1] = u; R.append(u)
%o A071356     return R
%o A071356 A071356_list(25)  # _Peter Luschny_, Nov 01 2012
