%o A117870 (Python)
%o A117870 def pdeg(p): return p.bit_length()-1
%o A117870 def pmod(a,b):
%o A117870     db = pdeg(b)
%o A117870     while a and pdeg(a)>=db: a^=b<<(pdeg(a)-db)
%o A117870     return a
%o A117870 def pgcd(a,b):
%o A117870     while b: a,b = b,pmod(a,b)
%o A117870     return a
%o A117870 def a(n):
%o A117870     x=2; c0,c1=1,x^1; g0,g1=1,x; out=[]
%o A117870     for i in range(1,n+1):
%o A117870         if i>1: c0,c1=c1,((c1<<1)^c1)^c0; g0,g1=g1,(g1<<1)^g0
%o A117870         if pgcd(c1,g1)!=1: out.append(i)
%o A117870     return out
