%t A117762 a[n_]= If[n==1, 6, Prime[n]*(Prime[n]^2 -1)/2];
%t A117762 Table[a[n], {n,40}]
%t A117762 Join[{6}, Table[Prime[n] (Prime[n]^2 - 1)/2, {n, 2, 40}]] (* _Vincenzo Librandi_, Sep 29 2015 *)
