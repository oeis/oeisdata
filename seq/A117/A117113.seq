%o A117113 (Python)
%o A117113 from collections import Counter
%o A117113 from num2words import num2words
%o A117113 def n2w(n): return "".join(c for c in num2words(n).replace(" and", "") if c.isalpha())
%o A117113 def a(n): return (Counter(n2w(n)) - Counter(n2w(n-1))).total()
%o A117113 print([a(n) for n in range(1, 103)]) # _Michael S. Branicky_, Jan 13 2026
