%S A176209 8,13,20,24,29,33,34,40,44,45,53,57,58,62,68,72,73,77,80,85,89,90,94,
%T A176209 97,104,108,109,113,116,120,125,129,130,134,137,141,148,152,153,157,
%U A176209 160,164,168,173,177,178,182,185,189,193,194,200,204,205,209,212,216,220
%N A176209 Sums of at least 2 squares s, for s >= 4.
%H A176209 Daniel Mondot, <a href="/A176209/b176209.txt">Table of n, a(n) for n = 1..10000</a>
%e A176209 8 = 4+4, 13 = 9+4, 20 = 16+4, 24 = 16+4+4, 29 = 25+4, ...
%t A176209 f1[n_]:=(x=n;While[x>3,x-=Floor[Sqrt[x]]^2];x);f2[n_]:=(x=n;k=0;While[x>3,k++;x-=Floor[Sqrt[x]]^2];k);lst={};Do[If[f1[n]==0&&f2[n]>1,AppendTo[lst,n]],{n,6!}];lst
%o A176209 (C)
%o A176209 #include <stdio.h>
%o A176209 #include <math.h>
%o A176209 void main()
%o A176209 {
%o A176209   unsigned int n, i, k, ln, j;
%o A176209   for (n=1; ln<10000; ++n)
%o A176209   {
%o A176209     for (i=n, j=0; i>=4; ++j) { k=sqrt(i); i -= k*k; }
%o A176209     if ((i==0) && (j>=2)) printf("%u %u\n", ++ln, n);
%o A176209   }
%o A176209 } // _Daniel Mondot_, Jan 20 2026
