%F A129874 From _Thomas Scheuerle_, Jan 13 2026: (Start)
%F A129874 The generating function A(x) satisfies: (8*x^2 - 1)*A(x)^3 + (12*x^2 - 2*x - 2)*A(x)^2 + (10*x^2 - 1)*A(x) + (8*x^3 + 9*x^2 + 2*x) = 0.
%F A129874 a(2*n) = Sum_{k=1..n} A129873(2*n - 2*k)*binomial(3*k-3, k-1)/(2*k-1), with A129873(0) = 1.
%F A129874 a(2*n+1) = (Sum_{k=1..n} A129873(2*n - 2*k+1)*(binomial(3*k-3, k-1)/(2*k-1))) + 2*Sum_{r=1..n} (binomial(3*(n-r)+1, n-r)/((n-r)+1))*(binomial(3*r-3, r-1)/(2*r-1)), for n > 0 and A129873(0) = 1. (End)
%o A129874 (PARI)
%o A129874 listA129873(m) = {my(S = t*O(t)); for (n= 1, m+1, S = (- 8*t - 3 - (8*t^4 - t^2)*S^3 - (24*t^3 - 2*t^2 - 4*t)*S^2)/(24*t^2 + 2*t - 3); ); return(vector(m, i, polcoeff(S, i, t))); }
%o A129874 h_even(n,a129873) = {sum(k=1,n, a129873[1+2*n-2*k]*(binomial(3*k-3, k-1)/(2*k-1))); }
%o A129874 h_odd(n,a129873) = {sum(k=1,n, a129873[1+2*n-2*k+1]*(binomial(3*k-3, k-1)/(2*k-1)))+2*sum(r=1,n,(binomial(3*(n-r)+1,n-r)/((n-r)+1))*(binomial(3*r-3, r-1)/(2*r-1))); }
%o A129874 a(n) = {if(n==1, 2, my(a129873 = concat([1], listA129873(n))); if(n%2==0,h_even(n/2,a129873), h_odd((n-1)/2,a129873))); }
