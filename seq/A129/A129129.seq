%t A129129 Array[Times @@ # & /@ Prime@ IntegerPartitions@ # &, 9, 0] // Flatten (* _Michael De Vlieger_, Aug 07 2018 *)
%t A129129 b[n_, i_] := b[n, i] = If[n == 0 || i == 1, {2^n}, Join[(# Prime[i]&) /@ b[n - i, Min[n - i, i]], b[n, i - 1]]];
%t A129129 T[n_] := b[n, n];
