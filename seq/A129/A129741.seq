%t A129741 a[0] = a[1] = a[2] = a[3] = 1; a[n_] := a[n] = (a[n - 1] a[n - 3] + a[n - 2]^2)/a[n - 4]; t = Array[a, 30]; t2 = {}; ps = {}; Do[f = Transpose[FactorInteger[t[[n]]]][[1]]; c = Complement[f, ps]; t2 = Join[t2, c]; ps = Union[ps, c], {n, 4, 30}]; t2 (* _T. D. Noe_, Nov 19 2013 *)
