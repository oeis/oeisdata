%t A129705 fib[n_Integer?Positive] := fib[n] = fib[n - 1] + fib[n - 2]; fib[0] = 0; fib[1] = fib[2] = 1; t[n_, m_] = Sum[fib[i], {i, 0, n}] - Sum[i, {i, 0, m}]; Table[Table[t[n, m], {m, 0, n}], {n, 0, 10}]
