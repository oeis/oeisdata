%e A109166 is prime.
%o A109166 (Python)
%o A109166 from gmpy2 import is_prime, next_prime
%o A109166 from itertools import count, islice
%o A109166 def agen(): # generator of terms
%o A109166     p1, p2 = 2, 3
%o A109166     for k in count(1):
%o A109166         t = int("".join(str(i) for i in range(p1, p2+1)))
%o A109166         if is_prime(t):
%o A109166             yield k
%o A109166         p1, p2 = p2, next_prime(p2)
%o A109166 print(list(islice(agen(), 48))) # _Michael S. Branicky_, Jan 12 2026
