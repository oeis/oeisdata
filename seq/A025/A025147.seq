%t A025147 p[_, 0] = 1; p[k_, m_] := p[k, m] = If[m < k, 0, p[k+1, m-k] + p[k+1, m]]; Table[p[2, m], {m, 0, 59}] (* _Jean-FranÃ§ois Alcover_, Apr 17 2014, after _Reinhard Zumkeller_ *)
%o A025147 (Haskell)
%o A025147 a025147 = p 2 where
%o A025147    p _ 0 = 1
%o A025147    p k m = if m < k then 0 else p (k + 1) (m - k) + p (k + 1) m
