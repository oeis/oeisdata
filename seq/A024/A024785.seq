%o A024785 def alst():
%o A024785   primes, alst = [2, 3, 5, 7], []
%o A024785   while len(primes) > 0:
%o A024785     alst += sorted(primes)
%o A024785     candidates = set(int(d+str(p)) for p in primes for d in "123456789")
