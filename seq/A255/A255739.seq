%t A255739 mn = Infinity; k = 1; lst = {}; While[k < 2501, a = N[ Abs[ Im[ ZetaZero[
%t A255739 k]] - Round[ Im[ ZetaZero[ k]] ]], 32]; If[a < mn, AppendTo[lst, k];
%t A255739 Print[k]; mn = a]; k++]; lst (* _Robert G. Wilson v_, Sep 29 2015 *)
