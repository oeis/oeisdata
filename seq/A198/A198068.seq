%e A198068 [3]        0, 1, 1
%e A198068 [4]       0, 1, 2, 1
%e A198068 [5]     0, 1, 1, 1, 1
%e A198068 [6]    0, 1, 2, 2, 3, 2
%e A198068 [7]  0, 1, 1, 1, 1, 2, 1
