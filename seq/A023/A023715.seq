%o A023715 def ok(n):
%o A023715     mask, c = 3, 0
%o A023715     for i in range(n.bit_length() >> 1):
%o A023715         if (mask&n) >> (i << 1) == 2: c += 1
%o A023715         mask <<= 2
%o A023715     return c == 2
