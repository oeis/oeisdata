%p A023531 A023531 := proc(n)
%p A023531     option remember ;
%p A023531     local m,t ;
%p A023531     for m from 0 do
%p A023531         t := m*(m+3)/2 ;
%p A023531         if t > n then
%p A023531             return 0 ;
%p A023531         elif t = n then
%p A023531             return 1 ;
%p A023531         end if;
%p A023531     end do:
%p A023531 end proc:
