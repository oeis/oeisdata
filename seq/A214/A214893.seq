%t A214893 nxt[{a_,b_}]:=Module[{c=a+b},{b,If[PrimeQ[c],c,c/FactorInteger[c][[1,1]]]}]; Transpose[NestList[nxt,{18,5},82]][[1]] (* _Harvey P. Dale_, Oct 19 2012 *)
