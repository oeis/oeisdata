%o A005150 (Python)
%o A005150 # program without string operations
%o A005150 def sign(n): return int(n > 0)
%o A005150 def say(a):
%o A005150     r = 0
%o A005150     p = 0
%o A005150     while a > 0:
%o A005150         c = 3 - sign((a % 100) % 11) - sign((a % 1000) % 111)
%o A005150         r += (10 * c + (a % 10)) * 10**(2*p)
%o A005150         a //= 10**c
%o A005150         p += 1
%o A005150     return r
%o A005150 a = 1
%o A005150 for i in range(1, 26):
%o A005150     print(i, a)
%o A005150     a = say(a)
