%o A005150 say :: Integer -> Integer
%o A005150 say = read . concatMap saygroup . group . show
%o A005150 where saygroup s = (show $ length s) ++ [head s]
%o A005150 look_and_say :: [Integer]
%o A005150 look_and_say = 1 : map say look_and_say
