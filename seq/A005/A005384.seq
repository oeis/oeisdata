%o A005384 (Python)
%o A005384 from sympy import isprime, nextprime
%o A005384 def ok(p): return isprime(2*p+1)
%o A005384 def aupto(limit): # only test primes
%o A005384   alst, p = [], 2
%o A005384   while p <= limit:
%o A005384     if ok(p): alst.append(p)
%o A005384     p = nextprime(p)
%o A005384   return alst
%o A005384 print(aupto(1559)) # _Michael S. Branicky_, Feb 03 2021
