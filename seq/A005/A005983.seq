%p A005983 b:= proc(u, o, t) option remember; `if`(u+o=0, 1, add(`if`(t=4,
%p A005983       b(o-j, u+j-1, 1), b(u+j-1, o-j, t+1)), j=1..o))
%p A005983     end:
%p A005983 a:= n-> b(0, 4*n+1, 0):
%p A005983 seq(a(n), n=0..20);  # _Alois P. Heinz_, Oct 06 2013
%t A005983 b[u_, o_, t_] := b[u, o, t] = If[u+o == 0, 1, Sum[If[t == 4, b[o-j, u+j-1, 1], b[u+j-1, o-j, t+1]], {j, 1, o}]] ; a[n_] := b[0, 4*n+1, 0]; Table[a[n], {n, 1, 20}] (* _Jean-Fran√ßois Alcover_, Nov 25 2014, after _Alois P. Heinz_ *)
