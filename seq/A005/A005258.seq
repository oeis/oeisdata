%o A005258 (Python)
%o A005258 def A005258(n):
%o A005258     m, g = 1, 0
%o A005258     for k in range(n+1):
%o A005258         g += m
%o A005258         m *= (n+k+1)*(n-k)**2
%o A005258         m //= (k+1)**3
