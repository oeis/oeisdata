%o A005043 (SageMath)
%o A005043 def ms():
%o A005043     a, b, c, d, n = 0, 1, 1, -1, 1
%o A005043     yield 1
%o A005043     while True:
%o A005043         yield -b + (-1)^n*d
%o A005043         n += 1
%o A005043         a, b = b, (3*(n-1)*n*a+(2*n-1)*n*b)/((n+1)*(n-1))
%o A005043         c, d = d, (3*(n-1)*c-(2*n-1)*d)/n
%o A005043 A005043 = ms()
