%p A005245 with(numtheory):
%p A005245 a:= proc(n) option remember;
%p A005245       `if`(n=1, 1, min(seq(a(i)+a(n-i), i=1..n/2),
%p A005245        seq(a(d)+a(n/d), d=divisors(n) minus {1, n})))
%p A005245     end:
%p A005245 seq(a(n), n=1..100); # _Alois P. Heinz_, Apr 18 2012
%t A005245 a[n_] := a[n] = If[n == 1, 1,
%t A005245    Min[Table[a[i] + a[n-i], {i, 1, n/2}] ~Join~
%t A005245    Table[a[d] + a[n/d], {d, Divisors[n] ~Complement~ {1, n}}]]];
%t A005245 Table[a[n], {n, 1, 100}] (* _Jean-François Alcover_, Dec 12 2013, after _Alois P. Heinz_ *)
%o A005245 (PARI) A005245(n /* start by calling this with the largest needed n */, lim/* see below */) = { local(d); n<6 && return(n);
%o A005245 if(n<=#A005245, A005245[n]&return(A005245[n]) /* return memoized result if available */,
%o A005245 A005245=vector(n) /* create vector if needed - should better reuse existing data if available */);
%o A005245 for(i=1, n-1, A005245[i] || A005245[i]=A005245(i,lim)); /* compute all previous elements */
%o A005245 A005245[n]=min( vecmin(vector(min(n\2,if(lim>0,lim,n)), k, A005245[k]+A005245[n-k])) /* additive possibilities - if lim>0 is given, consider a(k)+a(n-k) only for k<=lim - we know it is save to use lim=1 up to n=2e7 */, if( isprime(n), n, vecmin(vector((-1+#d=divisors(n))\2, i, A005245[d[i+1]]+A005245[d[ #d-i]]))/* multiplicative possibilities */))}
%o A005245 \\ See also the Python program by Tim Peters at A005421.
%o A005245 \\ _M. F. Hasler_, Jan 30 2008
%o A005245 (Haskell)
%o A005245 import Data.List (genericIndex)
%o A005245 a005245 n = a005245_list `genericIndex` (n-1)
%o A005245 a005245_list = 1 : f 2 [1] where
%o A005245    f x ys = y : f (x + 1) (y : ys) where
%o A005245      y = minimum $
%o A005245          (zipWith (+) (take (x `div` 2) ys) (reverse ys)) ++
%o A005245          (zipWith (+) (map a005245 $ tail $ a161906_row x)
%o A005245                       (map a005245 $ reverse $ init $ a161908_row x))
%o A005245 -- _Reinhard Zumkeller_, Mar 08 2013
%o A005245 (Python)
%o A005245 from functools import lru_cache
%o A005245 from itertools import takewhile
%o A005245 from sympy import divisors
%o A005245 @lru_cache(maxsize=None)
%o A005245 def A005245(n): return min(min(A005245(a)+A005245(n-a) for a in range(1,(n>>1)+1)),min((A005245(d)+A005245(n//d) for d in takewhile(lambda d:d*d<=n,divisors(n)) if d>1),default=n)) if n>1 else 1 # _Chai Wah Wu_, Apr 29 2023
