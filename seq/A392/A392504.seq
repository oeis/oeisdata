%o A392504 (Python)
%o A392504 from sympy import isprime, prime
%o A392504 from itertools import count, product
%o A392504 def a(n):
%o A392504     if n < 5: return -1
%o A392504     pn = prime(n)
%o A392504     S = sorted(set(str(pn)))
%o A392504     for d in count(len(S)):
%o A392504         for p in product(S, repeat=d):
%o A392504             if p[0] == "0" or len(set(p)) != len(S): continue
%o A392504             t = int("".join(p))
%o A392504             if t != pn and isprime(t):
%o A392504                 return t
%o A392504 print([a(n) for n in range(1, 56)]) # _Michael S. Branicky_, Feb 20 2026
