%T A392662 46,48,50,52,54,56,58,59,61,63,65,67,69,71,73,75,76,78,80,82,84,85,88,
%U A392662 90,92,93,95,98,99,101,102,104,107,109,110,111,114,116,118
%N A392662 Indices of the lower Wythoff partition sequence of the triangular numbers; see A392660.
%C A392662 The sequences A392662 and A392663 partition the positive integers, A000027.
%F A392662 A003056(A392660).
%t A392662 generateSequenceFast[seq_] := Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392662    s = Range[200]*(1 + Range[200])/2; (* User: put your sequence s here. *)
%t A392662    a = ConstantArray[0, seqLen];
%t A392662    b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392662    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392662     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392662     If[pb > seqLen, Break[]];
%t A392662     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392662     While[pa <= seqLen && used[[pa]], pa++];
%t A392662     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]]; used[[pa]] = True; n++];
%t A392662    {Take[a, n], Take[b, n - 1]}];
%t A392662 u = generateSequenceFast[s]
%t A392662 s1 = First[u]  (* A392660 *)
%t A392662 s2 = Last[u]   (* A392661 *)
%t A392662 w = Sort[Join[s1, s2]]
%t A392662 Flatten[Flatten[Position[w, #]] & /@s1] (* A392662 *)
%t A392662 Flatten[Flatten[Position[w, #]] & /@s2] (* A392663 *)
