%t A392132 A000129[n_]:=Fibonacci[n,2]; CirculantMatrix[l_List?VectorQ]:=NestList[RotateRight, RotateRight[l], Length[l] - 1]; a[n_]:=Permanent[CirculantMatrix[A000129[Range[0,n-1]]]]; Join[{1},Array[a,13]] (* or *)
