%t A392165 Join[{1},Select[Differences[Table[f[n],{n,1,400}]],#==1&->"Index"]+1]
%o A392165 (Python)
%o A392165 from itertools import count, islice
%o A392165 from networkx import empty_graph, find_cliques
%o A392165 from sympy import factorint
%o A392165 def A392165_gen(): # generator of terms
%o A392165     def is_squarefree(n): return max(factorint(n).values(),default=1)<2
%o A392165     c, G = 0, empty_graph([])
%o A392165     for n in count(1,2):
%o A392165         if is_squarefree(n):
%o A392165             G.add_node(n)
%o A392165             G.add_edges_from((a,n) for a in G if a!=n and is_squarefree(a+n))
%o A392165             if (m:=max(len(c) for c in find_cliques(G,nodes=[n])))>c:
%o A392165                 yield n
%o A392165                 c = m
%o A392165 A392165_list = list(islice(A392165_gen(),20)) # _Chai Wah Wu_, Jan 07 2026
