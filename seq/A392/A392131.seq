%e A392131   [0, 1, 2, 5]
%t A392131 A000129[n_]:=Fibonacci[n,2]; CirculantMatrix[l_List?VectorQ]:=NestList[RotateRight, RotateRight[l], Length[l] - 1]; a[n_]:=Det[CirculantMatrix[A000129[Range[0,n-1]]]]; Join[{1},Array[a,13]] (* or *)
%t A392131 A000129[i_]:=Fibonacci[i,2]; a[n_]:=Det[ToeplitzMatrix[Reverse[Array[A000129,n,0]],Join[{A000129[n-1]}, Array[A000129, n-1,0]]]]; Join[{1},Array[a, 13]]
