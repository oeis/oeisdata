%p A392648 q:= proc(n) option remember; local b, l, ll:
%p A392648       l, b:= sort([(numtheory[divisors](n) minus {1, n})[]]),
%p A392648       proc(m, i) option remember; `if`(m=0, 1, `if`(i<1 or ll[i]<m, 0,
%p A392648         (t-> t+`if`(t>1, 0, `if`(l[i]>m, 0, b(m-l[i], i-1))))(b(m, i-1))))
%p A392648       end; ll:= ListTools[PartialSums](l); is(b(n, nops(l))>1)
%p A392648     end:
%p A392648 select(q, [$1..1000])[];  # _Alois P. Heinz_, Jan 22 2026
%o A392648 (Python)
%o A392648 from sympy import divisors
%o A392648 def isA392648(n: int) -> int:
%o A392648     if n == 0: return False
%o A392648     divs = divisors(n)[1:-1]
%o A392648     dp = [0] * (n + 1)
%o A392648     dp[0] = 1
%o A392648     for d in divs:
%o A392648         for j in range(n, d - 1, -1):
%o A392648             dp[j] += dp[j - d]
%o A392648     return dp[n] > 1
%o A392648 print([n for n in range(350) if isA392648(n)])
%o A392648 (PARI) isok(k) = my(d=setminus(divisors(k), Set([1, k]))); forsubset(#d, s, my(x=sum(i=1,#s,d[s[i]])); if (x==k, forsubset(#d, t, if((s!=t) && (sum(i=1,#t,d[t[i]]) == k), return(1))))); \\ _Michel Marcus_, Jan 22 2026
