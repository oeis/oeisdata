%T A392125 52,55,56,57,59,60,63,64,66,67,68,69,71,72,74,75,76,79,82,83,84,85,88,
%U A392125 93,95,96,98,99,100,101,102,107,111,112,113,115,124,128
%N A392125 Numbers k such that (t(k) - greatest prime < t(k)) > (- t(k) + least prime > t(k)), where t(k) = A000217(k).
%C A392125 This sequence together with A181902 and A392123 partition the set of positive integers greater than 1.
%t A392125 f[x_] := f[x] = (x^2 + x)/2;
%t A392125 u[n_] := NextPrime[f[n], -1]; v[n_] := NextPrime[f[n]];
%t A392125 s1 = Select[Range[1000], f[#] - v[#] < u[#] - f[#] &]    (* A392123 *)
%t A392125 s2 = Select[Range[1000], f[#] - v[#] == u[#] - f[#] &]   (* A181902 *)
%t A392125 s3 = Select[1+Range[1000], f[#] - v[#] > u[#] - f[#] &]  (* A392125 *)
%Y A392125 Cf. A390788, A392123, A181902.
