%T A392661 946,1035,1128,1225,1326,1431,1540,1653,1830,1953,2080,2211,2346,2485,
%U A392661 2628,2775,3003,3160,3321,3486,3741,3828,4005,4186,4465,4656,4753,5050
%N A392661 Upper Wythoff partition sequence of the triangular numbers (A003086); see A392660.
%t A392661 Clear[generateSequenceFast];
%t A392661 s = Range[200]*(1 + Range[200])/2  (* User: put your sequence s here.*)
%t A392661 generateSequenceFast[seq_] := Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392661    a = ConstantArray[0, seqLen];
%t A392661    b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392661    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392661     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392661     If[pb > seqLen, Break[]];
%t A392661     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392661     While[pa <= seqLen && used[[pa]], pa++];
%t A392661     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]];
%t A392661     used[[pa]] = True; n++];
%t A392661    {Take[a, n], Take[b, n - 1]}];
%t A392661 u = generateSequenceFast[s]
%t A392661 First[u]   (* A392660*)
%t A392661 Last[u]    (* A392661*)
