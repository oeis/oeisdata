%o A392175 (Python)
%o A392175 from itertools import count, islice
%o A392175 from math import gcd
%o A392175 def A392175_gen(): # generator of terms
%o A392175     p, q = 0, 1
%o A392175     for k in count(1):
%o A392175         yield q%k
%o A392175         m = q//(k*(q-p))+1
%o A392175         p, q = p*k*m+q, k*m*q
%o A392175         p //= (r:=gcd(p,q))
%o A392175         q //= r
%o A392175 A392175_list = list(islice(A392175_gen(),34)) # _Chai Wah Wu_, Jan 18 2026
