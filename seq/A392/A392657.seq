%N A392657 Upper Wythoff partition sequence of the squares; see A392656.
%t A392657 Clear[generateSequenceFast];
%t A392657 s = Range[200]  (* User: put your sequence s here. *)
%t A392657 generateSequenceFast[seq_] := Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392657    a = ConstantArray[0, seqLen]; b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392657    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392657     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392657     If[pb > seqLen, Break[]];
%t A392657     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392657     While[pa <= seqLen && used[[pa]], pa++];
%t A392657     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]];
%t A392657     used[[pa]] = True; n++]; {Take[a, n], Take[b, n - 1]}];
%t A392657 (* User: in the next line, put your sequence *)
%t A392657 u = generateSequenceFast[s]
%t A392657 First[u]  (* A392656 *)
%t A392657 Last[u]   (* A392657 *)
