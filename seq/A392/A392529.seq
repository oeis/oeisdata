%o A392529 (PARI) \\ G.f.: Sum_{n>=0} x^n/(1 - x)^A053186(n)
%o A392529 {A053186(n) = n - floor(sqrt(n))^2}
%o A392529 {a(n) = my(A,x = 'x +x*O(x^n));
%o A392529 A = sum(m=0,n, x^m / (1 - x)^A053186(m) ); polcoef(A,n)}
%o A392529 for(n=0,50,print1(a(n),", "))
%o A392529 (PARI) \\ G.f.: S(x) + Sum_{n>=1} x^(2*n-1)/(1-x)^(2*n) * Sum_{k>=n} x^(k^2)
%o A392529 {a(n) = my(A, x = 'x +x*O(x^n));
%o A392529 A = sum(m=0, n, if(m==0,1, x^(2*m-1)/(1-x)^(2*m)) * sum(k=m, floor(sqrt(n+1)), x^(k^2) ) ); polcoef(A, n)}
%o A392529 for(n=0, 50, print1(a(n), ", "))
