%e A392479 For some pairs p is larger than q: 7^3 - 2*3 = 337 = prime(68).
%p A392479 T:= proc(n) option remember; local r,p;
%p A392479       r:= ithprime(n); p:= nextprime(iroot(r,3)-2);
%p A392479       while not (h-> h>0 and h::even and isprime(h/2))(p^3-r)
%p A392479       do p:= nextprime(p) od; p, (p^3-r)/2
%p A392479     end:
%p A392479 seq(T(n), n=1..31);  # _Alois P. Heinz_, Jan 13 2026
