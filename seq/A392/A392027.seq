%F A392027 a(n) = Sum_{i=1..n} A255933(i).
%t A392027 Module[{s = 1, k = 2, m}, Accumulate[Join[{s}, Table[While[k! - 1 <= s, k++]; m = k; While[!Divisible[s, m! - 1], m--]; s += m; m, {n, 2, 100}]]]]
%o A392027 (Python)
%o A392027 from itertools import islice
%o A392027 def A392027_gen(): # generator of terms
%o A392027     an = s = k = factk = 1
%o A392027     while True:
%o A392027         yield s
%o A392027         while factk <= s: k += 1; factk *= k
%o A392027         m, factm = k, factk
%o A392027         while s%(factm-1): factm //= m; m -= 1
%o A392027         an, s = m, s+m
%o A392027 print(list(islice(A392027_gen(), 63))) # _Michael S. Branicky_, Dec 28 2025
