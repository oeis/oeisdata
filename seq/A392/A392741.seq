%F A392741 A392740(n) + a(n) = 4n + 1.
%e A392741 a(1) = 0 because A392739 = 0,1,0,1,1,... and the first 0 is at position 0.
%t A392741 Q[0] = 0; Q[1] = 1; Q[n_] := Q[n] = Which[Mod[n, 4] == 0, Q[Quotient[n, 4]], Mod[n, 4] == 1, 1 - Q[Quotient[n, 4]], Mod[n, 4] == 2, Q[2*Quotient[n, 4]], True, 1 - Q[2*Quotient[n, 4]]]; Select[Range[0, 120], Q[#] == 0 &]
%o A392741 (PARI) Q(n) = if(n<2, n, my(q=n\4, r=n%4); if(r==0, Q(q), r==1, 1-Q(q), r==2, Q(2*q), 1-Q(2*q)));
%o A392741 a(n) = my(k=0, c=0); while(c<n, if(Q(k)==0, c++); if(c<n, k++)); k;
