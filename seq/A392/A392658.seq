%N A392658 Indices of the lower Wythoff partition sequence of the squares; see A392657.
%C A392658 The sequences A392658 and A392659 partition the positive integers, A000027.
%F A392658 a(n) = sqrt(A392656(n)).
%t A392658 Clear[generateSequenceFast];
%t A392658 Range[200]^2  (* User: put your sequence s here. *)
%t A392658 generateSequenceFast[seq_] :=
%t A392658   Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392658    a = ConstantArray[0, seqLen];
%t A392658    b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392658    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392658     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392658     If[pb > seqLen, Break[]];
%t A392658     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392658     While[pa <= seqLen && used[[pa]], pa++];
%t A392658     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]];
%t A392658     used[[pa]] = True; n++];
%t A392658    {Take[a, n], Take[b, n - 1]}];
%t A392658 (* User: put your sequence here: *)
%t A392658 u = generateSequenceFast[s]
