%U A392119 82,85,87,88,89,90,91,94,95,96,97,99,102,103,104,108,109
%N A392119 Indices of the lower Wythoff partition sequence of the primes; see A392117.
%C A392119 The sequences A392119 and A392121 partition the positive integers, A000027.
%F A392119 PrimePi(A392117).
%t A392119 Clear[generateSequenceFast];
%t A392119 s = Prime[Range[200]]  (* User: put your sequence s here. *)
%t A392119 generateSequenceFast[seq_] := Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392119    a = ConstantArray[0, seqLen]; b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392119    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392119     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392119     If[pb > seqLen, Break[]];
%t A392119     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392119     While[pa <= seqLen && used[[pa]], pa++];
%t A392119     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]];
%t A392119     used[[pa]] = True; n++]; {Take[a, n], Take[b, n - 1]}];
%t A392119 u = generateSequenceFast[s]
%t A392119 v = PrimePi[generateSequenceFast[Prime[Range[200]]]]
%t A392119 First[u]  (* A392117*)
%t A392119 Last[u]   (* A392118 *)
%t A392119 First[v]  (* A392119 *)
%t A392119 Last[v]   (* A392121 *)
