%F A392041 A007908(n) <= a(n) <= A053546(n). - _Michael S. Branicky_, Dec 29 2025
%o A392041 (Python)
%o A392041 from gmpy2 import is_prime, mpz
%o A392041 from itertools import count, product
%o A392041 def a(n):
%o A392041     s = "".join(str(i) for i in range(1, n+1))
%o A392041     t = mpz(s)
%o A392041     if is_prime(t): return int(t)
%o A392041     cands = set()
%o A392041     for d in count(1):
%o A392041         for w in product("0123456789", repeat=d):
%o A392041             w = "".join(w)
%o A392041             for i in range(d+1):
%o A392041                 v = w[:i] + s + w[i:]
%o A392041                 if v[0] != "0" and is_prime(t:=mpz(v)):
%o A392041                     cands.add(t)
%o A392041         if cands:
%o A392041             return int(min(cands))
%o A392041 print([a(n) for n in range(1, 19)]) # _Michael S. Branicky_, Dec 29 2025
