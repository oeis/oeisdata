%o A392806 (Python)
%o A392806 from sympy import prime
%o A392806 def Sum(n, p):
%o A392806     s = 0
%o A392806     while n > 0: s, n = s+n, n//p
%o A392806     return s
%o A392806 def PrimePower(p, e):
%o A392806     if e == 0: return 0
%o A392806     else: return p*PrimePower(p, e-1) + 2*p**e - 1
%o A392806 def A392806(n,m):
%o A392806     p, d = prime(m), []
%o A392806     while n > 0: d, n = [n%p]+d, n//p
%o A392806     L = len(d)-1
%o A392806     a, k, ns = 0, 0, 0
%o A392806     while k < L:
%o A392806         ns, k = ns*p+d[k], k+1
%o A392806         if d[k] > 0: a = a + d[k]*max(0,(-PrimePower(p,L-k)+2*p**(L-k)*Sum(ns,p)))
%o A392806     return a # _A.H.M. Smeets_, Feb 05 2026
