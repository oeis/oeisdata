%N A392118 Upper Wythoff partition sequence of the primes; see A392117.
%t A392118 w[s_List] := Module[{u = {}, v = {}, used = <||>, n = 1, i = 1, target, vt},
%t A392118   While[i <= Length@s, While[i <= Length@s && KeyExistsQ[used, i], i++];
%t A392118    If[i > Length@s, Break[]]; AppendTo[u, s[[i]]]; used[i] = True;
%t A392118    target = u[[-1]] + s[[n]]; vt = -1; Do[If[! KeyExistsQ[used, j] && s[[j]] >= target, vt = j;
%t A392118      Break[]], {j, 1, Length@s}]; If[vt == -1, u = Drop[u, -1]; Break[], AppendTo[v, s[[vt]]];
%t A392118     used[vt] = True; n++];]; {u, v}]
%t A392118 s = Prime[Range[200]]; (* User: put your increasing sequence s here *)
%t A392118 u = First[w[s]]  (* lower Wythoff partition sequence of s *)
%t A392118 v = Last[w[s]]   (* upper Wythoff partition sequence of s *)
