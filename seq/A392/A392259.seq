%o A392259 (Python) # Using sample_gauss_kuzmin_distribution function from A391217.
%o A392259 from sympy import continued_fraction_convergents
%o A392259 coeffs = sample_gauss_kuzmin_distribution(101)
%o A392259 convergent_generator = continued_fraction_convergents([0] + coeffs)
%o A392259 next(convergent_generator)
%o A392259 A392259 = [frac.denominator for frac in convergent_generator]
