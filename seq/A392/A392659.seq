%S A392659 2,4,6,9,10,13,14,17,19,21,23,26,28,29,31,34,37,38,40,42,45,47,49,51,
%T A392659 53,55,57,60,62,64,66,68,70,72,74,77,79,81,83,85,87,89,91,94,96,98,
%U A392659 100,102,105,106,108,111,113,115,117,119,122,124,125,128,130,132
%N A392659 Indices of the upper Wythoff partition sequence of the squares; see A392656.
%C A392659 The sequences A392658 and A392659 partition the positive integers, A000027.
%F A392659 Sqrt(A392657).
%t A392659 Clear[generateSequenceFast];
%t A392659 s=Range[200]^2  (* User: put your sequence s here. *)
%t A392659 generateSequenceFast[seq_] :=
%t A392659   Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392659    a = ConstantArray[0, seqLen];
%t A392659    b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392659    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392659     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392659     If[pb > seqLen, Break[]];
%t A392659     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392659     While[pa <= seqLen && used[[pa]], pa++];
%t A392659     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]];
%t A392659     used[[pa]] = True; n++];
%t A392659    {Take[a, n], Take[b, n - 1]}];
%t A392659 u = generateSequenceFast[s]
%t A392659 First[u]  (* A392656 *)
%t A392659 Last[u]   (* A392657 *)
%t A392659 w = Sort[Join[s1, s2]]
%t A392659 Flatten[Flatten[Position[w, #]] & /@ s1] (* A392658 *)
%t A392659 Flatten[Flatten[Position[w, #]] & /@ s2] (* A392659 *)
