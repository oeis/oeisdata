%e A392528 A(t) = 3 at t = 0.5489627750246421219203474323241407265689887623...
%e A392528 A(t) = 4 at t = 0.6001769018172804281709767500838134223639193622...
%e A392528 A(t) = 5 at t = 0.6303589650297074855750910364988318656676429507...
%o A392528 (PARI) \\ G.f.: Sum_{n>=0} x^n/(1 - x)^A002262(n)
%o A392528 {A002024(n) = floor(sqrt(2*n) + 1/2)}
%o A392528 {A002262(n) = n - A002024(n+1) * (A002024(n+1) - 1)/2}
%o A392528 {a(n) = my(A,x = 'x +x*O(x^n));
%o A392528 A = sum(m=0,n, x^m / (1 - x)^A002262(m) ); polcoef(A,n)}
%o A392528 for(n=0,60,print1(a(n),", "))
%o A392528 (PARI) \\ G.f.: Sum_{n>=0} x^n/(1-x)^n * Sum_{k >= n} x^(k*(k+1)/2)
%o A392528 {A002024(n) = floor(sqrt(2*n) + 1/2)}
%o A392528 {a(n) = my(A,x = 'x +x*O(x^n));
%o A392528 A = sum(m=0,n, x^m/(1 - x)^m * sum(k=m,A002024(n+1), x^(k*(k+1)/2) ) ); polcoef(A,n)}
%o A392528 for(n=0,60,print1(a(n),", "))
