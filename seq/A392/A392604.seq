%e A392604 524307 = A048675(A380476(1)) = A048675(4686) = 2^19 + 2^4 + 2^1 + 2^0 = "10000000000000010011" in binary, is a term. See example in A380476.
%o A392604 (PARI) is_A392604(n) = A380467(A019565(n));
%o A392604 (PARI)
%o A392604 search_up_to_2_e = 20;
%o A392604 choosebybits(v,m) = { my(s=vector(hammingweight(m)),i=j=1); while(m>0,if(m%2,s[j] = v[i];j++); i++; m >>= 1); s; };
%o A392604 A276086(n) = { my(m=1, p=2); while(n, m *= (p^(n%p)); n = n\p; p = nextprime(1+p)); (m); };
%o A392604 A359550(n) = { my(pp); forprime(p=2, , pp = p^p; if(!(n%pp), return(0)); if(pp > n, return(1))); };
%o A392604 is_it(ps) = { my(n=factorback(ps),d,m=1); for(i=1,#ps,d=ps[i]; m *= A276086(n/d); if(!A359550(m), return(0))); (1); };
%o A392604 A392604_list(up_to_2_e) = { my(lista=List([]),all_ps=primes(up_to_2_e)); for(w=0, (2^up_to_2_e)-1, if(is_it(choosebybits(all_ps,w)), listput(lista,w))); Vec(lista); };
%o A392604 v392604 = A392604_list(search_up_to_2_e);
%o A392604 A392604(n) = v392604[n];
