%S A392656 1,9,25,49,64,121,144,225,256,324,400,484,576,625,729,900,1024,1089,
%T A392656 1225,1296,1521,1681,1849,1936,2116,2304,2500,2704,2916,3136,3364,
%U A392656 3481,3721,3969,4225,4489,4761,5041,5329,5625,5776,6084,6400,6724,7056,7396,7744
%N A392656 Lower Wythoff partition sequence of the squares; see Comments.
%C A392656 Let s = (s(n)) be an increasing sequence of positive integers, and define sequences u = (u(n)) and v = (v(n)) as follows:
%C A392656    u(1) = s(1);
%C A392656    v(1) = least term of s that is >= u(1) + s(1);
%C A392656    u(n) = least term of s that is not in {u(k) : k < n} and not in {v(k) : k < n};
%C A392656    v(n) = least term of s that is >= u(n) + s(n), for n >= 1.
%C A392656 Then u is the lower Wythoff partition sequence of s, and v is the upper Wythoff partition sequence of s. If s = A000027, then u = A000201 and v = A001950, these being the classical lower and upper Wythoff sequences.
%t A392656 Clear[generateSequenceFast];
%t A392656 s = Range[200]^2  (* User: put your sequence s here. *)
%t A392656 generateSequenceFast[seq_] := Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392656    a = ConstantArray[0, seqLen]; b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392656    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392656     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392656     If[pb > seqLen, Break[]];
%t A392656     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392656     While[pa <= seqLen && used[[pa]], pa++];
%t A392656     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]];
%t A392656     used[[pa]] = True; n++]; {Take[a, n], Take[b, n - 1]}];
%t A392656 (* User: in the next line, put your sequence *)
%t A392656 u = generateSequenceFast[s]
%t A392656 First[u]  (* this sequence *)
%t A392656 Last[u]   (* A392657 *)
