%o A392104 (Python)
%o A392104 from sympy import nextprime, prime
%o A392104 from itertools import count, islice
%o A392104 def agen(NN=10000): # generator of terms for n <= NN
%o A392104     n, pk, residues = 1, 1, "X"*NN
%o A392104     for k in count(1):
%o A392104         pk = nextprime(pk)
%o A392104         resk = pk%3
%o A392104         if resk == 1:
%o A392104             while residues.endswith("2"*n) and n <= NN:
%o A392104                 yield prime(k-n)
%o A392104                 n += 1
%o A392104         residues = residues[1:] + str(resk)
%o A392104 print(list(islice(agen(), 12))) # _Michael S. Branicky_, Jan 11 2026
