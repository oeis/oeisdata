%F A392868 A393058(a(n)) = n.
%o A392868 (PARI) is_A392868(n) = (A391844(n) && is_A392867(n));
%o A392868 (PARI)
%o A392868 A053669(n) = forprime(p=2, , if(n%p, return(p)));
%o A392868 is_A392868(n) = if(n<3 || isprime(n), (n && n<3), my(f=factor(n), u = n*sum(i=1, #f~, f[i, 2]/f[i, 1]), m=1, p=2); if(A053669(u)!=f[1,1], return(0)); while(u, m *= (p^(u%p)); if(m>n, return(0)); u \= p; p = nextprime(1+p)); (1));
