%o A392635 (Python) # uses A002473gen() in A002473
%o A392635 from itertools import islice
%o A392635 from fractions import Fraction
%o A392635 def agen():
%o A392635     g = A002473gen(); k = next(g); record = 0
%o A392635     while True:
%o A392635         nextk = next(g); f = Fraction(k, nextk)
%o A392635         if f > record: yield k; record = f
%o A392635         k = nextk
%o A392635 print(list(islice(agen(), 30)))
