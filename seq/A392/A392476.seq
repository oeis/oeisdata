%o A392476 (Python)
%o A392476 from collections import Counter
%o A392476 from itertools import islice
%o A392476 def agen(): # generator of terms
%o A392476     a, c = [0], Counter()
%o A392476     while True:
%o A392476         yield a[-1]
%o A392476         c[a[-1]] += 1
%o A392476         a.append(a[-1] - a[a[-1]] if c[a[-1]] == 1 else c[a[-1]])
%o A392476 print(list(islice(agen(), 100))) # _Michael S. Branicky_, Feb 15 2026
