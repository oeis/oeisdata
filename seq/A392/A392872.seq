%t A392872 a122111[n_] := Function[l, Product[Prime[Sum[If[j<i, 0, 1], {j, l}]], {i, 1, Max[l]}]][Flatten[Table[Table[PrimePi[f[[1]]], {f[[2]]}], {f, FactorInteger[n]}]]]; a[n_]:=Mod[a122111[n],Prime[1+PrimeOmega[n]]];Array[a,105] (* _James C. McMahon_, Jan 27 2026 *)
%o A392872 (PARI)
%o A392872 A122111(n) = if(1==n,n,my(f=factor(n), es=Vecrev(f[,2]),is=concat(apply(primepi,Vecrev(f[,1])),[0]),pri=0,m=1); for(i=1, #es, pri += es[i]; m *= prime(pri)^(is[i]-is[1+i])); (m));
%o A392872 A392872(n) = (A122111(n)%prime(1+bigomega(n)));
