%T A392660 703,780,861,903,990,1081,1176,1275,1378,1485,1596,1711,1770,1891,
%U A392660 2016,2145,2278,2415,2556,2701,2850,2926,3081,3240,3403,3570,3655,3916,4095
%N A392660 Lower Wythoff partition sequence of the triangular numbers (A003086); see Comments.
%C A392660 Let s = (s(n)) be an increasing sequence of positive integers, and define sequences u = (u(n)) and v = (v(n)) as follows:
%C A392660 u(1) = s(1);
%C A392660 v(1) = least term of s that is >= u(1) + s(1);
%C A392660 u(n) = least term of s that is not in {u(k) : k < n} and not in {v(k) : k < n};
%C A392660 v(n) = least unused term of s that is greater than u(n) + s(n), for n >= 1.
%C A392660 Then u is the lower Wythoff partition sequence of s, and v is the upper Wythoff partition sequence of s. If s = A000027, then u = A000201 and v = A001950, these being the classical lower and upper Wythoff sequences.
%t A392660 Clear[generateSequenceFast];
%t A392660 s = Range[200]*(1 + Range[200])/2  (* User: put your sequence s here.*)
%t A392660 generateSequenceFast[seq_] := Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392660    a = ConstantArray[0, seqLen];
%t A392660    b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392660    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392660     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392660     If[pb > seqLen, Break[]];
%t A392660     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392660     While[pa <= seqLen && used[[pa]], pa++];
%t A392660     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]];
%t A392660     used[[pa]] = True; n++];
%t A392660    {Take[a, n], Take[b, n - 1]}];
%t A392660 (*User:put your sequence here:*)
%t A392660 u = generateSequenceFast[s]
%t A392660 s1 = First[u]  (* A392660*)
%t A392660 s2 = Last[u]   (* A392661*)
