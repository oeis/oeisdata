%T A392663 53,55,57,60,62,64,66,68,70,72,74,77,79,81,83,86,87,89,91,94,96,97,
%U A392663 100,103,105,106,108,112,113,115,117,119,122,124,125,127,130,132
%N A392663 Indices of the upper Wythoff partition sequence of the triangular numbers; see A392660.
%C A392663 The sequences A392662 and A392663 partition the positive integers, A000027.
%F A392663 A003056(A392661).
%t A392663 generateSequenceFast[seq_] := Module[{seqLen = Length[seq], used, a, b, n = 1, pa = 1, pb = 1, bTarget}, used = ConstantArray[False, seqLen];
%t A392663    s = Range[200]*(1 + Range[200])/2; (* User: put your sequence s here. *)
%t A392663    a = ConstantArray[0, seqLen];
%t A392663    b = a; a[[1]] = seq[[1]]; used[[1]] = True;
%t A392663    While[n <= seqLen, bTarget = a[[n]] + seq[[n]];
%t A392663     While[pb <= seqLen && (used[[pb]] || seq[[pb]] < bTarget), pb++];
%t A392663     If[pb > seqLen, Break[]];
%t A392663     b[[n]] = seq[[pb]]; used[[pb]] = True; pa = Max[pa, 2];
%t A392663     While[pa <= seqLen && used[[pa]], pa++];
%t A392663     If[pa > seqLen, Break[]]; a[[n + 1]] = seq[[pa]]; used[[pa]] = True; n++];
%t A392663    {Take[a, n], Take[b, n - 1]}];
%t A392663 u = generateSequenceFast[s]
%t A392663 s1 = First[u]   (* A392660 *)
%t A392663 s2 = Last[u]   (* A392661 *)
%t A392663 w = Sort[Join[s1, s2]]
%t A392663 Flatten[Flatten[Position[w, #]] & /@s1] (* A392662 *)
%t A392663 Flatten[Flatten[Position[w, #]] & /@s2] (* A392663 *)
