%F A081841 a(1)=0; for n>=1 a(n)=floor(r*n-(4*r-1)/(r+1)) where r=1+sqrt(2)
%t A081841 Block[{c, k}, c[_] := False; k = 0; c[0] = True; {k}~Join~Reap[Do[If[c[n], k += 3, k += 2]; c[k] = True; Sow[k], {n, 2, 120}]][[-1, 1]] ] (* _Michael De Vlieger_, Jul 02 2025 *)
