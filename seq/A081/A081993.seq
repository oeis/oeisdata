%o A081993 (Python)
%o A081993 from sympy.utilities.iterables import multiset_permutations
%o A081993 def a(n):
%o A081993     digits = sorted("".join(str(i) for i in range((n-1)*n//2+1, (n-1)*n//2+1+n)))
%o A081993     return sum(int("".join(t)) for t in multiset_permutations(digits) if t[0]!="0")
%o A081993 print([a(n) for n in range(1, 8)]) # _Michael S. Branicky_, Nov 22 2025
