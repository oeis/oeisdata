%t A081319 t[_] := 0; k = -1; While[k > -3100, a = NumberFieldClassNumber@Sqrt@k; If[t[a] == 0, t[a] = -k; Print[{a, -k}]]; k--]; t /@ Range@51 (* _Robert G. Wilson v_, Sep 25 2017 *)
