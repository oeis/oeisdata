%t A259490 $MaxExtraPrecision = 25000; lst = {}; cf = ContinuedFraction[Pi, 10000]; Do[ If[ PrimeQ[ Denominator[ FromContinuedFraction[ Take[ cf, n]] ]], AppendTo[lst, n]], {n, Length[cf]}]; lst
