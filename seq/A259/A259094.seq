%p A259094 G:=n->mul(1/(1-q^(2*i-1)),i=1..n);
%p A259094 M:=41;
%p A259094 G2:=n->seriestolist(series(G(n),q,M));
%p A259094 for n from 1 to 10 do lprint(G2(n)); od:
%p A259094 H:=n->[seq(G2(n-i+1)[i],i=1..n)];
