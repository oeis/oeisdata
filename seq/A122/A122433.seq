%p A122433 end do; # _Peter Bala_, Mar 21 2018
%p A122433 T := (m, n, k) -> (-1)^(k + n)*binomial(n + 1, n - k)*hypergeom([m, k - n], [k + 2], 1); for n from 0 to 9 do seq(simplify(T(4, n, k)), k=0..n) od; # _Peter Luschny_, Sep 23 2024
