%e A087135   01:  [ 1 1 2 6 ]
%e A087135   02:  [ 1 1 3 5 ]
%e A087135   03:  [ 1 1 8 ]
%e A087135   04:  [ 1 2 3 4 ]
%e A087135   05:  [ 1 2 7 ]
%e A087135   06:  [ 1 3 6 ]
%e A087135   07:  [ 1 4 5 ]
%e A087135   08:  [ 1 9 ]
%e A087135   09:  [ 2 2 6 ]
%e A087135   10:  [ 2 3 5 ]
%e A087135   11:  [ 2 8 ]
%e A087135   12:  [ 3 3 4 ]
%e A087135   13:  [ 3 7 ]
%e A087135   14:  [ 4 6 ]
%e A087135   15:  [ 5 5 ]
%e A087135   16:  [ 10 ]
