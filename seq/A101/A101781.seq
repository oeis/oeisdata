%t A101781 Select[Transpose[Select[Partition[Prime[Range[1500]],2,1],Last[#]- First[#] == 2&]][[1]],PrimeQ[FromDigits[Reverse[IntegerDigits[#]]]]&] (* _Harvey P. Dale_, Jun 26 2013 *)
%t A101781 Select[Partition[Prime[Range[1500]],2,1],#[[2]]-#[[1]]==2&&PrimeQ[IntegerReverse[#[[1]]]]&][[;;,1]] (* _Harvey P. Dale_, Sep 01 2025 *)
%o A101781 (PARI) twinl(n) = /* The n-th lower twin prime */ { local(c, x); c=0; x=1; while(c<n, if(isprime(prime(x)+2), c++); x++; ); return(prime(x-1)) }
%o A101781 rev(str) = /* Get the reverse of the input string */ { local(tmp, s, j); tmp = Vec(Str(str)); s=""; forstep(j=length(tmp), 1, -1, s=concat(s, tmp[j])); return(s) }
%o A101781 twlrpr(n) = { for(x=1, n, y=twinl(x); z=eval(rev(y)); if(isprime(z), print1(y", ")) ) }
