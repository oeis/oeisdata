%o A387254 (Python)
%o A387254 from math import isqrt
%o A387254 from sympy import integer_nthroot, factorint, primepi
%o A387254 def A387254(n):
%o A387254     def f(x): return int(n+sum(isqrt(x//k**3) for k in range(1, integer_nthroot(x, 3)[0]+1) if all(d<=1 for d in factorint(k).values()))+primepi(x))
%o A387254     m, k = n, f(n)
%o A387254     while m != k: m, k = k, f(k)
%o A387254     return m**2 # _Chai Wah Wu_, Nov 24 2025
