%t A387298 f[p_, e_] := -(-1)^e*KroneckerSymbol[-3, e]; a[1] = 1; a[n_] := Times @@ f @@@ FactorInteger[n]; Array[a, 100] (* _Amiram Eldar_, Nov 16 2025 *)
%o A387298 (PARI) lista(n) = direuler(p=2,n,(1-X^3)*(1-X^4)/((1-X)*(1-X^6)))
%o A387298 (SageMath)
%o A387298 def a(n: int) -> int:
%o A387298     res = 1
%o A387298     for p, e in factor(n):
%o A387298         res *= -((-1)**e) * kronecker_symbol(-3, e)
%o A387298     return res
