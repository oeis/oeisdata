%o A387882 from math import isqrt
%o A387882 from collections import Counter
%o A387882 from itertools import count, islice
%o A387882 def b(s): # A328447 for string input
%o A387882     t = "".join(sorted(s)).strip("0")
%o A387882     return int(t[0]+"0"*s.count("0")+t[1:])
%o A387882 def agen():
%o A387882     adict, n, pcdict, digs = dict(), 1, Counter(), -1
%o A387882     for d in count(1):
%o A387882         pcdict = Counter()
%o A387882         for m in range(isqrt(10**(d-1)-1)+1, isqrt(10**d-1)+1):
%o A387882             pcdict[b(str(m*m))] += 1
%o A387882         if pcdict:
%o A387882             vals = set(pcdict.values())
%o A387882             for i in range(n, max(vals)+1):
%o A387882                 if i in vals:
%o A387882                     adict[i] = next(k for k in sorted(pcdict) if pcdict[k] == i)
%o A387882                     while n in adict: yield adict[n]; n += 1
%o A387882 print(list(islice(agen(), 30))) # _Michael S. Branicky_, Dec 09 2025
