%o A387730 (Python)
%o A387730 from mpmath import iv
%o A387730 def a_upto(limit):
%o A387730     sum_, lows = iv.mpf('0'), [0]
%o A387730     for i in range(1, 1+limit):
%o A387730         t = 2*i + 1
%o A387730         sum_ += iv.sin(t)**t
%o A387730         if sum_ < 0:
%o A387730             lows.append(i)
%o A387730             sum_ = 0
%o A387730     return lows
%o A387730 print(a_upto(85000)) # _Jwalin Bhatt_, Nov 09 2025
