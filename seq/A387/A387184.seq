%H A387184 P. Erdős and R. L. Graham, <a href="https://fanchung.ucsd.edu/ron/papers/80_11_number_theory.pdf">Old and new problems and results in combinatorial number theory</a>, Monographies de L'Enseignement Mathématique (1980).
%e A387184 6 lies in the sequence because 1!*3!*5!*6! = 720^2 is a perfect square.
%o A387184 (Python) import math
%o A387184 def is_square(n):
%o A387184     r = math.isqrt(n)
%o A387184     return r * r == n
%o A387184 def test(d):
%o A387184   for c in range(1, d):
%o A387184     for b in range(1, c):
%o A387184       for a in range(1, b):
%o A387184         val = math.factorial(d) // math.factorial(c)
%o A387184         val *= math.factorial(b) // math.factorial(a)
%o A387184         if is_square(val):
%o A387184           return True
%o A387184   return False
%o A387184 (Python)
%o A387184 from math import prod
%o A387184 from itertools import count, islice
%o A387184 from sympy import factorint, integer_nthroot, isprime
%o A387184 def A387184_gen(): # generator of terms
%o A387184     g, p, r = [set()], 0, {}
%o A387184     for c in count(2):
%o A387184         f = factorint(c)
%o A387184         d = g[-1]^set(i for i, j in f.items() if j&1)
%o A387184         for h in g:
%o A387184             if (t:=prod(d^h)) not in r:
%o A387184                 r[t] = c-1
%o A387184         g.append(d)
%o A387184         x, y = integer_nthroot(c,2)
%o A387184         if c>4 and y and isprime(x):
%o A387184             yield c
%o A387184         elif c>4 and max(f.values())>1:
%o A387184             yield c
%o A387184         else:
%o A387184             for b in range(c-2,p,-1):
%o A387184                 e = prod(d^g[b])
%o A387184                 if e in r and r[e]<b:
%o A387184                     yield c
%o A387184                     break
%o A387184         if isprime(c):
%o A387184             p = c-2
%o A387184 A387184_list = list(islice(A387184_gen(),50)) # _Chai Wah Wu_, Sep 24 2025, updated Oct 05 2025
