%o A387706 (Python)
%o A387706 from mpmath import iv
%o A387706 def a_upto(limit):
%o A387706     sum_, highs = iv.mpf('0'), [0]
%o A387706     for i in range(1, 1+limit):
%o A387706         t = 2*i + 1
%o A387706         sum_ += iv.sin(t)**t
%o A387706         if sum_ > 0:
%o A387706             highs.append(i)
%o A387706             sum_ = 0
%o A387706     return highs
%o A387706 print(a_upto(14200)) # _Jwalin Bhatt_, Nov 09 2025
