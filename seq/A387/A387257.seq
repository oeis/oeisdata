%t A387257 nn = 600000; mm = Sqrt[nn]; i = 1; fQ[x_] := And[Max[#] > 1, GCD @@ # == 1] &@ FactorInteger[x][[;; , -1]]; MapIndexed[Set[S[First[#2]], #1] &, Select[Range@ Sqrt[nn], fQ]]; Union@ Reap[While[j = 2; While[S[i]^j < nn, If[AllTrue[FactorInteger[#][[;; , -1]], # > 2 &], Sow[#] ] &[S[i]^j]; j++]; j > 2, i++] ][[-1, 1]]
