%p A136289 isAdjac := proc(a,b,c) abs(b-a) = 1 and abs(c-b)=1 and abs(a-c)=1 ; end: neighbrs := proc(x) local y,phi ; y := {} ; for phi from 0 to 5 do y := y union {x+expand(exp(I*phi*Pi/3)) } ; od: end: doesMatch2 := proc(genLin,x) local p ; for p in combinat[choose](genLin,2) do if isAdjac(x,op(1,p),op(2,p)) then RETURN(true) ; fi ; od: RETURN(false) ; end: A136289 := proc(gen) local newgen,o,candid,x,genLin,g ; newgen := {}; genLin := {} ; for g in gen do genLin := genLin union g ; od: for o in op(-1,gen) do candid := neighbrs(o) ; for x in candid do if not x in newgen then if not x in genLin then if doesMatch2(genLin,x) then newgen := newgen union {x} ; fi ; fi ; fi ; od: od: RETURN( [op(gen),newgen] ) ; end: gen := [{0,1,expand(exp(I*Pi/3))}] : for n from 1 do printf("%d,", nops(op(n,gen)) ) ; gen := A136289(gen) od: # _R. J. Mathar_, Apr 15 2008
