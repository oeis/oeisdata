%t A136296 max=6; a={}; For[i=1, i<=10^max, i++, If[Mod[FromDigits[Join[{1}, IntegerDigits[Prime[i]], {1}]], Prime[i]] == 0, AppendTo[a, Prime[i]]]]; a (* _Stefano Spezia_, Mar 26 2023 *)
