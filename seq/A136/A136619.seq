%t A136619 Clear[A, m, n]; m = 3; A[0] = 1; A[1] = (m - 1)/2; A[n_] := A[n] = If[Mod[A[n - 1], 2] == 0, A[n - 1]/2, (m - A[n - 1]) 2]; Table[A[n], {n, 0, 110}]
%t A136619 Join[{1}, PadRight[{}, 110, {1, 4, 2}]] (* _Bruno Berselli_, Nov 30 2013 *)
